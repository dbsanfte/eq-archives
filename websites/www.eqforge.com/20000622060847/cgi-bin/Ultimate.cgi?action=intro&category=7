	document.cookie = "lastLogin=" + CookieLogin + ";expires=" + expireDate.toGMTString() + ";domain=eqforge.com;path=/cgi-bin;";
	document.cookie = "LastLoginDT=" + CookieLoginDT + ";expires=" + expireDate.toGMTString() + ";domain=eqforge.com;path=/cgi-bin;";
	document.cookie = "sessionLogin=" + sessionLogin + ";domain=eqforge.com;path=/cgi-bin;";
	document.cookie = "sessionLoginDT=" + sessionLoginDT + ";domain=eqforge.com;path=/cgi-bin;";
}

function getCookie(name){
var cname = name + "=";               
var dc = document.cookie;
if (dc.length > 0) {              
    begin = dc.indexOf(cname);
	 if (begin != -1) {           
        begin += cname.length;
		end = dc.indexOf(";", begin);
            if (end == -1) end = dc.length;
            return unescape(dc.substring(begin, end));        }
}
return null;
}
