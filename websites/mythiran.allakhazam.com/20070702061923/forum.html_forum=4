    <script type="text/javascript">
      function mtog () {
       if (document.getElementById('mctl').innerHTML != 'Show Menu') {
        document.getElementById('menubar').style.display = 'none';
        document.getElementById('maincontent').style.left = 0 + 'px';
        document.getElementById('maincontent').style.marginLeft = 20 + 'px';
        document.getElementById('maincontent').style.marginRight = 20 + 'px';
        document.getElementById('menutop').style.backgroundImage = 'none';
        document.getElementById('menutop').style.borderBottom = '0px';
        document.getElementById('mctl').innerHTML = 'Show Menu';
        hideMenu = 1;
       } else {
        document.getElementById('menubar').style.display = 'block';
        document.getElementById('maincontent').style.left = 170 + 'px';
        document.getElementById('maincontent').style.marginLeft = 0 + 'px';
        document.getElementById('maincontent').style.marginRight = 5 + 'px';
        document.getElementById('menutop').style.top = 0 + 'px';
        document.getElementById('menutop').style.textAlign = 'right';
        document.getElementById('mctl').innerHTML = 'Hide Menu';
        hideMenu = 0;
       }
      saveMenu(-1,hideMenu);
    }
  </script>
