<script language="JavaScript" type="text/JavaScript">
// (C) 2000 www.CodeLifter.com
// http://www.codelifter.com
// Free for all users, but leave in this  header
// NS4-6,IE4-6
// Fade effect only in IE; degrades gracefully

// $Id: slideshow.inc.php,v 1.6 2004/07/24 15:04:09 gaugau Exp $

// Set slideShowSpeed (milliseconds)
var slideShowSpeed = 5000
// Agent sniffer shamelessly 'stolen' from the excellent X library from cross-browser.com
var xOp7=false,xOp5or6=false,xIE4Up=false,xNN4=false,xUA=navigator.userAgent.toLowerCase();
if(window.opera){
  xOp7=(xUA.indexOf('opera 7')!=-1 || xUA.indexOf('opera/7')!=-1);
  if (!xOp7) xOp5or6=(xUA.indexOf('opera 5')!=-1 || xUA.indexOf('opera/5')!=-1 || xUA.indexOf('opera 6')!=-1 || xUA.indexOf('opera/6')!=-1);
}
else if(document.layers) xNN4=true;
else {xIE4Up=document.all && xUA.indexOf('msie')!=-1 && parseInt(navigator.appVersion)>=4;}

// Duration of crossfade (seconds)
var crossFadeDuration = 3

// Specify the image files
var Pic = new Array() // don't touch this
// to add more images, just continue
// the pattern, adding to the array below
Pic[0] = 'albums/userpics/10008/normal_nagafen1.jpg'
Pic[1] = 'albums/showcase/normal_EQ2_000472.jpg'
Pic[2] = 'albums/userpics/10018/normal_Lightburst%20Mystique.jpg'
Pic[3] = 'albums/characters/normal_alloriaeq2a_153.jpg'
Pic[4] = 'albums/userpics/10031/normal_Old%20School.jpg'
Pic[5] = 'albums/userpics/10010/normal_eq2_000003_199.jpg'
Pic[6] = 'albums/userpics/10016/Darkened%20Mantle.JPG'
Pic[7] = 'albums/characters/faille_185.jpg'
Pic[8] = 'albums/userpics/10037/normal_alloriaeq2%7E0.jpg'
Pic[9] = 'albums/showcase/normal_EQ2_000450.jpg'
Pic[10] = 'albums/showcase/normal_EQ2_000451.jpg'
Pic[11] = 'albums/showcase/normal_EQ2_000480.jpg'
Pic[12] = 'albums/showcase/normal_EQ2_000479.jpg'
Pic[13] = 'albums/showcase/normal_EQ2_000486.jpg'
Pic[14] = 'albums/showcase/normal_EQ2_000497.jpg'
Pic[15] = 'albums/userpics/10032/normal_I%20am%20not%20Scared.jpg'
Pic[16] = 'albums/userpics/10031/normal_Lollipop.jpg'
Pic[17] = 'albums/userpics/10018/normal_Noah_s%20landing.jpg'
Pic[18] = 'albums/userpics/10167/normal_EQ2_000047.jpg'
Pic[19] = 'albums/userpics/10016/normal_Venekor%20Strat.JPG'
Pic[20] = 'albums/userpics/10100/normal_Relapse_by_Eraxer.jpg'
Pic[21] = 'albums/userpics/10016/Vyrdrais%20Claw.JPG'
Pic[22] = 'albums/userpics/10016/normal_DSC00042.JPG'
Pic[23] = 'albums/userpics/10016/normal_DSC00038.JPG'
Pic[24] = 'albums/userpics/10351/normal_DSC00343.JPG'
Pic[25] = 'albums/userpics/10008/normal_demons.jpg'
Pic[26] = 'albums/userpics/10013/Fehm.JPG'
Pic[27] = 'albums/characters/normal_inosolan_161.jpg'
Pic[28] = 'albums/userpics/10018/Kiss.jpg'
Pic[29] = 'albums/userpics/10018/normal_Darathar.jpg'
Pic[30] = 'albums/characters/arnold_153.jpg'
Pic[31] = 'albums/showcase/normal_EQ2_000440.jpg'
Pic[32] = 'albums/userpics/10018/relic%20armor.jpg'
Pic[33] = 'albums/userpics/10016/Insidious%20Leggings%7E0.JPG'
Pic[34] = 'albums/userpics/10016/Soulforged%20Dagger.JPG'
Pic[35] = 'albums/userpics/10351/normal_DSC00364.JPG'
Pic[36] = 'albums/showcase/normal_EQ2_000459.jpg'
Pic[37] = 'albums/userpics/10003/normal_Nagafen-and-Noah.jpg'
Pic[38] = 'albums/userpics/10016/Greatflail%20of%20Kavruul.JPG'
Pic[39] = 'albums/userpics/10018/normal_61-67%20spells.jpg'

var t
var j = 10
var p = Pic.length
var pos = j

var preLoad = new Array()

function preLoadPic(index)
{
        if (Pic[index] != ''){
                window.status='Loading : '+Pic[index]
                preLoad[index] = new Image()
                preLoad[index].src = Pic[index]
                Pic[index] = ''
                window.status=''
        }
}

function runSlideShow(){
   if (xIE4Up){
            document.images.SlideShow.style.filter="blendTrans(duration=2)"
                document.images.SlideShow.style.filter= "blendTrans(duration=crossFadeDuration)"
      document.images.SlideShow.filters.blendTrans.Apply()
        }
        document.images.SlideShow.src = preLoad[j].src
        if (xIE4Up){
           document.images.SlideShow.filters.blendTrans.Play()
        }

        pos = j

        j = j + 1
        if (j > (p-1)) j=0
        t = setTimeout('runSlideShow()', slideShowSpeed)
        preLoadPic(j)
}

function endSlideShow(){
        self.document.location = 'displayimage.php?album=toprated&cat=0&pos='+pos
}

preLoadPic(j)

</script>

