<tr bgcolor="#9C9A9C">
  <td>
    <FONT SIZE="+2"><B><A HREF="/Articles/WebPreferences">Web Preferences</A>  &nbsp;</B></FONT>
  </td><td>
    <a href="/bin/rdiff/Articles/WebPreferences">25 Mar 2003 - 02:56</a> - r1.15 &nbsp;
  </td><td>
    Khelan
  </td>
</tr></table>
<p />
<ul >
<li>
<ul >
<li> Set WEBTOPICLIST = ( <A HREF="/Articles/WebIndex">Index</A> | <A HREF="/Articles/WebSearch">Search</A> | <a href="/Articles/WebPreferences?skin=print">Printable</a> )
</li>
<li> Set AREA = Welcome to DLC
</li>
<li> Set WEBBGCOLOR = #9C9A9C
</li>
<li> Set WEBNAVCOLOR = #CECECE
</li>
<li> Set WEBCOLOR = #DEDFDE
</li>
<li> Set WEBTITLECOLOR = #105AAD
</li>
<li> Set WEBTEXTCOLOR = #000000
</li>
<li> Set WEBLINKCOLOR = #0000CC
</li>
<li> Set WEBVLINKCOLOR = #990000
</li>
<li> Set WEBALINKCOLOR = #0000CC
</li>
<li> Set WEBFORMS = <A HREF="/Articles/ArticlesForm">ArticlesForm</A>
</li>
<li> Set NOSEARCHALL =
</li>
<li> Set DENYWEBCHANGE = 
</li>
<li> Set ALLOWWEBCHANGE = 
</li>
<li> Set ALLOWTOPICCHANGE = 
</li>
<li> Set FINALPREFERENCES = WEBTOPICLIST, DENYWEBVIEW, ALLOWWEBVIEW, DENYWEBCHANGE, ALLOWWEBCHANGE
</li>
</ul>
</li>
</ul>
<p />
<ul >
<li>
<ul >
<li> Set FORMAT = | <strong><span style='background : #FFFFCE;'><font color="#0000FF">$topic</font></span><a href="/bin/edit/Articles/Topic?topicparent=Articles.Index">?</a> ($formfield(<A HREF="/Articles/ArticleSubject">ArticleSubject</A>))</strong> ||$n|<FONT SIZE=-1>&nbsp;&nbsp;$formfield(<A HREF="/Articles/ShortDescription">ShortDescription</A>)  <BR><I><span style='background : #FFFFCE;'><font color="#0000FF">Read more...</font></span><a href="/bin/edit/Articles/Topic?topicparent=Articles.Index">?</a></FONT>||$n| <strong><em><FONT SIZE=-1><A HREF=$formfield(&lt;A HREF="/Articles/DiscussionLink">DiscussionLink</A>)&gt;$formfield(<A HREF="/Articles/DiscussionText">DiscussionText</A>)</A></FONT><em><em>|  _</em><FONT SIZE=-1>Article By: <A HREF='/Articles/SearchArticlesByAuthor?author=$formfield(&lt;A HREF="/Articles/ArticleBy">ArticleBy</A>)'&gt;$formfield(<A HREF="/Articles/ArticleBy">ArticleBy</A>)</A></FONT></em></strong> |$n| <strong><em><FONT SIZE=-2>About: <A HREF='/Articles/SearchArticlesBySubject?subject=$formfield(&lt;A HREF="/Articles/ArticleSubject">ArticleSubject</A>)'&gt;$formfield(<A HREF="/Articles/ArticleSubject">ArticleSubject</A>)</A></FONT></em></strong> |  _</em><FONT SIZE=-2>Created: $formfield(<A HREF="/Articles/PostedDate">PostedDate</A>) (CST)<BR> Last Modified: $date (GMT)</FONT>__| $n| | |
</li>
</ul>
</li>
</ul>
<p />
<ul >
<li>
<ul >
<li> Set SORTBY = <BR>Sort by <SELECT NAME="order"><OPTION VALUE="topic">Article Name <OPTION VALUE="modified">Date Modified <OPTION VALUE="editby">Editor</SELECT>
</li>
</ul>
</li>
</ul>
<p />
<ul >
<li>
<ul >
<li> Set OLDFORMAT = | <strong><span style='background : #FFFFCE;'><font color="#0000FF">$topic</font></span><a href="/bin/edit/Articles/Topic?topicparent=Articles.Index">?</a> ($formfield(<A HREF="/Articles/ArticleSubject">ArticleSubject</A>))</strong> |  *Article By: <A HREF=/Articles/SearchArticlesByAuthor?author=$formfield(&lt;A HREF="/Articles/ArticleBy">ArticleBy</A>)&gt;$formfield(<A HREF="/Articles/ArticleBy">ArticleBy</A>)</A>*|$n|<FONT SIZE=-1>&nbsp;&nbsp;$formfield(<A HREF="/Articles/ShortDescription">ShortDescription</A>)</FONT>||$n| <strong><em><FONT SIZE=-2>About: $formfield(<A HREF="/Articles/ArticleSubject">ArticleSubject</A>)</FONT></em></strong> |  __<FONT SIZE=-2>Created: $formfield(<A HREF="/Articles/PostedDate">PostedDate</A>) (CST)<BR> Last Modified: $date (GMT)</FONT>__| $n| | |
</li>
<li> Set ARTICLETABLE = 
</li>
</ul>
