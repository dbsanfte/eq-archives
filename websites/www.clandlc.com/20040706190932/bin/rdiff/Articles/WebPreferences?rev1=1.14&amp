<li> Set WEBTOPICLIST = (
            <a href="Index">Home</a>
          | <a href="WebIndex">Index</a>
          | <a href="WebSearch">Search</a>
          | Go <input type="text" name="topic" size="16">
        )
</li>
<li> Set WEBBGCOLOR = #E7DFCE
</li>
<li> Set NOSEARCHALL =
</li>
<li> Set DENYWEBCHANGE = 
</li>
<li> Set ALLOWWEBCHANGE = 
</li>
<li> Set ALLOWTOPICCHANGE = 
</li>
<li> Set FINALPREFERENCES = WEBTOPICLIST, DENYWEBVIEW, ALLOWWEBVIEW, DENYWEBCHANGE, ALLOWWEBCHANGE
</li>
</ul>
</li>
