<script language="javascript">

var mbayID = "cfroulette";
var siteName = "http://www.voicephotos.com/cf1/cfroulette.htm";

var expdate = new Date ();
FixCookieDate (expdate);
expdate.setTime (expdate.getTime() + (6 * 60 * 60 * 1000)); // 6 hrs from now
//expdate.setTime (expdate.getTime() + 10000); // 10 seconds from now

if(document.cookie){
	if(mbayID != GetCookie(mbayID)){
		SetCookie (mbayID, mbayID, expdate);
		PopupNewWindow(siteName);
	}
}
else{
	SetCookie (mbayID, mbayID, expdate);
	PopupNewWindow(siteName);
}
</script>
