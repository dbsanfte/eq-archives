		<SCRIPT LANGUAGE="JavaScript">
		<!-- Hide JavaScript from pansy browsers.
		
		numImages = 1;                        // Set this to however many images you have.
		numLoaded = 0;
		queue = new Array();
		
		for(i=0;i<numImages;i++) {            // Initialize the queue to "not loaded" for debugging purposes.
		   queue[i] = "not loaded";
		}
		
		if(document.images) {                 // Preload the file that will get swapped into the status bar.
		   redImg = new Image();
		   redImg.src = "/images/red.gif";
		}
		
		function Push(imgNum) {               // Push image onto queue so that CheckQueue will find it the next time around.
		   queue[imgNum] = "loaded";
		}
		
		function CheckQueue() {               // Check the queue for images that have loaded since the last call of CheckQueue().
		   if(numLoaded < numImages) {
		      for(i=0;i<numImages;i++) {
		         if(queue[i] == "loaded") {
		            queue[i] = "done";
		            document.images["img" + numLoaded].src = "/images/red.gif";
		            numLoaded++;
		         }
		      }
		      setTimeout('CheckQueue()', 100);
		   } else {
		   	document.location.href='printmap.php?map=everfrost_4234';
		   }
		}
		
		CheckQueue();                         // Initialize CheckQueue.
		
		// -->
		</SCRIPT>
		<BODY BGCOLOR="#FFFFFF" TEXT="#000000" LINK="#CC0000" VLINK="#330099" ALINK="#FF3300">

<CENTER>
