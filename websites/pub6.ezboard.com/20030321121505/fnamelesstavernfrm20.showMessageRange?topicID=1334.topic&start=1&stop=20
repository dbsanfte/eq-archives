// sum.cpp : Defines the entry point for the console application.
//
#include &quot;stdafx.h&quot;
#include &lt;functional&gt;
#include &lt;algorithm&gt;
#include &lt;iterator&gt;
#include &lt;vector&gt;
#include &lt;iostream&gt;

using namespace std;

// we need a functor that will sum a range or iterators
class adder : public unary_function&lt;int, void&gt;
{
public:
    int sum;
    adder() : sum(0) {}
    void operator()(int x) {sum+=x;}
};

// we need a functor that will determine if a range of
// iterators is sorted in descending order
class sortcheck : public unary_function&lt;int, void&gt;
{
public:
    int sorted;
    int last;
    sortcheck() : sorted(true),last(0x7FFFFFFF) {}
    void operator()(int x)
    {
        if(x&lt;=last)
        {
&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp last=x;
        }
        else
        {
&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp sorted=false;
        }
    }
};


// this program will read the values from standard input but
// expects the requested sum to come from the command line
int main(int argc,char *argv[])
{
    int count=0;
    int sum = atoi(argv[1]); // first command line parameter
    int size;
    vector&lt;int&gt; data; // holds the data
    vector&lt;int&gt; last; // holds a copy so we can check duplication
    // fill the vector with data from standard input
    for ( int i = 0; i &lt; 10; i++  ) 
&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp data.push_back( i );

    // now do the whole thing starting with 2 numbers and going up
    for(size=2;size&lt;4 ;size++)
    {
        // clear the backup and fill it with zeros
&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp last.clear(); fill_n(back_inserter(last),data.size(),0);
        // always sort before starting a permutation loop
        // so that you get all the permutations
        sort(data.begin(),data.end());
        while(next_permutation(data.begin(),data.end()))
        {
            // now we have the next permutation
            // but if the first X digits are the same as
            // last time then its the same one for us
            if(!equal(data.begin(),data.begin()+size,last.begin()))
&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp {
                sortcheck result=for_each(data.begin(),
&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp data.begin()+size,
&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp sortcheck());
&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp 
                // we must be sure that we only return the
                 // same combination once, so choose the one
                // in descending order
                if(result.sorted)
                {
                    adder result=for_each(data.begin(),
&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp data.begin()+size,
&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp adder());
&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp 
                    // if it adds up then print it
                    if(result.sum==sum)
                    {
                        cout&lt;&lt;&quot;( &quot;;
                        copy(data.begin(),
&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp data.begin()+size,
&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp ostream_iterator&lt;int&gt;(cout,&quot; &quot;));  
                        cout&lt;&lt;&quot;)&quot;;
                        cout&lt;&lt;endl;
                        count++;
                    }
                }
            }
            // before doing the next permutation, save
            // the current one for the 'equals' check
            copy(data.begin(),data.end(),last.begin());
        }
    }
&nbsp &nbsp &nbsp &nbsp cout &lt;&lt; &quot;Total combinations is &quot;&lt;&lt;count&lt;&lt;endl;
    return count;
}
</pre><!--EZCODE CODE END--> <p></p><i></i></td></tr><tr bgcolor=#dcdeed><td nowrap width=20% valign=top align=left><span class=title> <A HREF=http://pub6.ezboard.com/bnamelesstavern.showUserPublicProfile?gid=cinadelos>Cinade LoS</A></span><br> <span class=usertitle>Registered User</span><br>  Posts: 2<br> <font color=#333333>(12/2/02 10:54:10 pm)</font><br>   <a href=http://pub6.ezboard.com/fnamelesstavernfrm20.showAddReplyScreenFromWeb?topicID=1334.topic&index=14>Reply</a>   </td><td valign=top align=left class=m><span class=title> Small change</span><hr size=1> Bah, forgot to remove the debug code.  <br>
