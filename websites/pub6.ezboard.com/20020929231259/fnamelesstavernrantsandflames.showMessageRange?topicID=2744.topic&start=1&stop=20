nttemp = getCookie('NamelessTavernScroller'); if (nttemp == 1) setCookie('NamelessTavernScroller', 0, expiration); else setCookie('NamelessTavernScroller', 1, expiration); window.location.reload();}var expiration = new Date();expiration.setTime(1009843199400);var ntscr = getCookie('NamelessTavernScroller');if (ntscr == null) setCookie('NamelessTavernScroller', 1, expiration);</script></head><body text=#333333 bgcolor=#ffffff link=#333366 vlink=#000066 alink=#333366  ><center> <table width="100%" border="0" cellspacing="0" cellpadding="0">   <tr>    <td>      <table border="0" cellspacing="4" cellpadding="0" align="center">        <tr valign="top"> <td>             <table width="134" border="0" cellspacing="0" cellpadding="0">              <tr align="center">                 <TD width="100%" bgcolor=#666eb0><B><FONT face=Verdana size=2 color="#FFFFFF">Welcome</FONT></B></TD>        </tr>        <tr>          <TD align=left width="100%">            <table border="0" width="100%" cellspacing="0" cellpadding="0">              <tr>                <td width="100%"><a href="http://pub6.ezboard.com/bnamelesstavern"><img src="http://www.acck.freeserve.co.uk/ntlogo_blue.gif" BORDER=0></a></td>              </tr>               </table>          </TD>        </tr></table></td>  <td>             <table width="134" border="0" cellspacing="0" cellpadding="0">              <tr align="center" bgcolor=#666eb0>                 <TD width="100%"><B><FONT face=Verdana size=2 color="#FFFFFF">Spotlight</FONT></B></TD>        </tr>        <tr>          <TD align=left width="100%"><script language='javascript'>ntscr = getCookie('NamelessTavernScroller');if (ntscr == 1) {document.write("<APPLET CODE=RiadaHeadline CODEBASE=\"http://www.acck.freeserve.co.uk/\" ARCHIVE=RiadaHeadline.jar WIDTH=134 HEIGHT=112>");document.write("<PARAM NAME=cabbase VALUE=\"RiadaHeadline.cab\">");document.write("<PARAM N