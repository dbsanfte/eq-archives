<table cellpadding="2" border="1" cellspacing="0" width="98%" ID="Table32">
	     
	     
	<tr>
			<td nowrap bgcolor="#808080" background="http://www.everlore.com/gimages/tb_silver.gif">	
			<a href="/US_Admin.asp?mode=edit&amp;id=583946"><img border="0" src="http://www.everlore.com/gimages/edit.gif"></a>
			</font>
			<font face="Verdana" size="1"><b>
			Submited by:&nbsp;<a href="mailto:Roliusfarrick@aol.com">WALLDO!!!</a></font> 
			<font face="Verdana" size="1">On:&nbsp;7/11/2002 2:49:37 AM</font></b>
			<td nowrap bgcolor="#808080" background="http://www.everlore.com/gimages/tb_silver.gif" align="right">
				<font face="Verdana" size="1"><b>Rank:&nbsp;&nbsp;</font>	
				<font face="Verdana" size="1">worst</font>
				<b><a href="javascript:openWindow(1,583946);"><font face="Verdana" size="1" >1</font></a></b>
				<b><a href="javascript:openWindow(2,583946);"><font face="Verdana" size="1" >2</font></a></b>
				<b><a href="javascript:openWindow(3,583946);"><font face="Verdana" size="1"  Color=black >3</font></a></b>
				<b><a href="javascript:openWindow(4,583946);"><font face="Verdana" size="1" >4</font></a></b>
				<b><a href="javascript:openWindow(5,583946);"><font face="Verdana" size="1" >5</font></a></b>
				<b><a href="javascript:openWindow(6,583946);"><font face="Verdana" size="1" >6</font></a></b>
				<b><a href="javascript:openWindow(7,583946);"><font face="Verdana" size="1" >7</font></a></b>
				<b><a href="javascript:openWindow(8,583946);"><font face="Verdana" size="1" >8</font></a></b>
				<b><a href="javascript:openWindow(9,583946);"><font face="Verdana" size="1" >9</font></a></b>
				<b><a href="javascript:openWindow(10,583946);"><font face="Verdana" size="1" >10</font></a></b>
				<font face="Verdana" size="1"><b>best</b></font>
