		    <td valign="top" width="50%"><font face="Verdana" size="3"><b>Beast Name:</font><br></b>
		    <font face="Verdana" size="2">Tarskuk&nbsp;</font></td>
		    <td valign="top" width="50%"><font face="Verdana" size="3"><b>
		    Location:</font><br></b>
		    <font face="Verdana" size="2"><A href="../geography/default.asp?MAPID=2463">Rathe Mountains</a>&nbsp;</font>
		    </td>
		  </tr>
