						<tr>
							<td width="50%">
							<font face="verdana" size="2">
							Level:<br>
							Rank:<br>
							Target Type:<br>
							Skill:<br>
							Mana:<br>
							Casting Time:<br>
							Recast Time:<br>
							Duration:<br>
							</font>
							</td>
							<td width="50%">
							<font face="verdana" size="2">
							39<br>
							5<br>
							Target<br>
							Abjuration<br>
							50<br>
							4<br>
							<br>
							<br>
							</font>
							</td>
						</tr>
					</table>
				</td>
			</tr>
		</table>

    </td>
  </tr>
  <tr>
    <td valign="top" align="left">
