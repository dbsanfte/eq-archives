		<p align="left">
            <font face="verdana" size="2"><b>Advantages/Disadvantages</b><br>
            </font></font>
        </font>
          <table border="0" width="101%" height="282">
          <tr>
            <td width="7%" height="18" align="left"><b><font size="2" face="Verdana">Races</font></b></td>
            <td width="27%" align="left" height="18">
              <p align="center"><b><font size="2" face="Verdana">Advantages</font></b></p>
            </td>
            <td width="21%" align="left" height="18">
              <p align="center"><b><font size="2" face="Verdana">Disadvantages</font></b></p>
            </td>
          </tr>
		   <font face="verdana" size="2">
          <tr>
            <td width="99%" colspan="3" valign="top" height="21" align="left">
              <hr>
            </td>
          </tr>
          </font>
          <tr>
            <td width="7%" height="18" align="left"><font face="verdana" size="2">Barbarian</font></td>
            <td width="27%" align="left" height="18">
              <p align="left">
		   <font face="verdana" size="2">
          Good, slam, Medium or Large Armor</font></td>
            <td width="21%" align="left" height="18">
              <p align="left">
		   <font face="verdana" size="2">
          No night vision, XP penalty for race/size</font></td>
