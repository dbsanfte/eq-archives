<script language="JavaScript">

function Calculate1 () {
  var Intelligence = parseInt (document.forms.Form1.IQ.value);
  var Wisdom = parseInt (document.forms.Form1.WIS.value);
  var Level = parseInt (document.forms.Form1.LEV.value);
  var Mana;
  var ClassCode;
  for (var i=0; i < document.forms.Form1.Class.length; i++) {
    if (document.forms.Form1.Class[i].checked) {
      break;
      }
    }
  ClassCode = document.forms.Form1.Class[i].value;
  var Attribute = ClassCode.charAt(0);
  var MagicLevel = ClassCode.charAt(1);

  if (MagicLevel == "0") {
    alert("With all the intelligence and wisdom in the world, your total mana is still 0.  Pick a different class.");
    return false;
    }
  else {
    if (Level < MagicLevel) {
      alert("You do not have any mana at this level.  Choose a higher level.")
      return false;
      }
    }
  if (Attribute == "W") {
    Mana = ((Wisdom/5)+2) * (Level - (MagicLevel - 1))
    alert("Your total mana is " + Mana);
    }  
  if (Attribute == "I") {
    Mana = ((Intelligence/5)+2) * (Level - (MagicLevel - 1))
    alert("Your total mana is " + Mana);
    }       
}

function InputCheck(Question){

var inputParameter;

// Determine which textbox is being evaluated
if (Question == "IQ") {
  inputParameter = document.forms.Form1.IQ.value;
  }
if (Question == "WIS") {
  inputParameter = document.forms.Form1.WIS.value;
  }
if (Question == "LEV") {
  inputParameter = document.forms.Form1.LEV.value;
  }

// Verify that the value is a number
if (NumberCheck(inputParameter) == false) {
  alert("Please enter a number");

  // return focus to the appropriate field
  if (Question == "IQ") {
    document.forms.Form1.IQ.focus();
    document.forms.Form1.IQ.select();
    }
  if (Question == "WIS") {
    document.forms.Form1.WIS.focus();
    document.forms.Form1.WIS.select();
    }
  if (Question == "LEV") {
    document.forms.Form1.LEV.focus();
    document.forms.Form1.LEV.select();
    }
  }
}

function NumberCheck(inputVal){

inputStr = inputVal.toString();
for (var i=0; i < inputStr.length; i++) {
  var oneChar=inputStr.charAt(i);
  if (oneChar < "0" || oneChar > "9") {
    return false;
    }
  }
return true;
}

</SCRIPT>
