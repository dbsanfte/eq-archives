  <tr>
    <td valign="top" align="left">
    <font face="verdana" size="5">
    Extinguish Fatigue</font>&nbsp;<a href="../Subscribe.asp?DID=471"><font face="Verdana" size="2">[Subscribe to this document]</font></a></td>
    <td valign="top" align="left"><img src="http://www.everlore.com/gimages/magic.gif" width="216" height="30" align="right">
    </td>
  </tr>
  <tr>
    <td colspan="2">
    <!--<a href="magic.asp?mode=updateform&amp;ID=471">-->
    <a href="../UserUpdate.asp?ID=471">
