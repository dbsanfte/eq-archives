  <tr>
    <td valign="top" ><div style= "padding:4;float: right"> <script language=JavaScript src=http://www.obsidian-tech.com/absolutebm/abmx.aspx?z=26></script></div>
		
			<font face="verdana" size="2">
			
				
					<b>Damage:</b>&nbsp; </font><font face="verdana" size="2">9&nbsp;&nbsp;</font>
					<font face="verdana" size="2"><b>Delay:</b>&nbsp;</font> <font face="verdana" size="2">22&nbsp;&nbsp;</font>
					<font face="verdana" size="2"><b>Ratio:</b>&nbsp;</font> <font face="verdana" size="2">2.44&nbsp;&nbsp;</font>
					<font face="verdana" size="2"><b>Range:</b>&nbsp; <font face="verdana" size="2">-&nbsp;&nbsp;</font>
					
		
		<font face="verdana" size="2"><b>Weight:</b>&nbsp; </font><font face="verdana" size="2">1.0&nbsp;&nbsp;</font>
	
		
		
