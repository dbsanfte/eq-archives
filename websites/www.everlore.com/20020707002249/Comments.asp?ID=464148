		<tr>
		    <td valign="top">
		     <p><font face="Verdana" size="3"><b>Article Title:</b>&nbsp;</font>
		     <font face="Verdana" size="2">Patch Scheduled for July 10th</font><br>
		     <a href = "../Subscribe.asp?DID=464148" ><font face="Verdana" size="2">[Subscribe to this document]</font></a></p>
			 </td>
		</tr>
		
		<tr><td>&nbsp;</tr><tr>
		
