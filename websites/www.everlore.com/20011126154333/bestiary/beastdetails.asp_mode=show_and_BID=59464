		    <select name="ItemTable" onChange="goto_item(this);">
		    <option value="">Select Item</option>
		    
		    <option value="3480">Ghoulbane</option>
		    
		    <option value="20728">Silver-Plated Bracer</option>
		    
		    </select>
		    
