		    <select name="ItemTable" onChange="goto_item(this);">
		    <option value="">Select Item</option>
		    
		    <option value="2368">Gigantic Zweihander</option>
		    
		    <option value="11798">Cloak of the Ice Bear</option>
		    
		    <option value="20586">Lupine Claw Gauntlets</option>
		    
		    </select>
		    
