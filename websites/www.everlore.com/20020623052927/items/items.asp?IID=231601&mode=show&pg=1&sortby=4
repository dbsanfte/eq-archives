<table cellpadding="2" border="1" cellspacing="0" width="98%">
	     
	     
	<tr>
			<td nowrap bgcolor="#808080" background="http://www.everlore.com/gimages/tb_silver.gif">	
			<a href="/US_Admin.asp?mode=edit&amp;id=412112"><img border="0" src="http://www.everlore.com/gimages/edit.gif"></a>
			</font>
			<font face="Verdana" size="1"><b>
			Submited by:&nbsp;<a href="mailto:kirie_raven@yahoo.com">Kyiri Ravenwing</a></font> 
			<font face="Verdana" size="1">On:&nbsp;4/11/2001 4:03:55 PM</font></b>
			<td nowrap bgcolor="#808080" background="http://www.everlore.com/gimages/tb_silver.gif" align="right">
				<font face="Verdana" size="1"><b>Rank:&nbsp;&nbsp;</font>	
				<font face="Verdana" size="1">worst</font>
				<b><a href="javascript:openWindow(1,412112);"><font face="Verdana" size="1" >1</font></a></b>
				<b><a href="javascript:openWindow(2,412112);"><font face="Verdana" size="1" >2</font></a></b>
				<b><a href="javascript:openWindow(3,412112);"><font face="Verdana" size="1" >3</font></a></b>
				<b><a href="javascript:openWindow(4,412112);"><font face="Verdana" size="1" >4</font></a></b>
				<b><a href="javascript:openWindow(5,412112);"><font face="Verdana" size="1" >5</font></a></b>
				<b><a href="javascript:openWindow(6,412112);"><font face="Verdana" size="1" >6</font></a></b>
				<b><a href="javascript:openWindow(7,412112);"><font face="Verdana" size="1" >7</font></a></b>
