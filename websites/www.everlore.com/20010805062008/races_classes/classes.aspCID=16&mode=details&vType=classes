					    &nbsp;<a href="http://www.everlore.com/MB_TOPICS.asp?fid=72&FN=Warrior&sid=7&SN=EverQuest&SURL=http%3A%2F%2Fwww%2Eeverlore%2Ecom&Cat=Classes&PL=2">MB</a>
						&nbsp;<a href="classes.asp?CID=18&amp;mode=details&amp;type=races">Warrior</a><br>
					
					    &nbsp;<a href="http://www.everlore.com/MB_TOPICS.asp?fid=73&FN=Wizard&sid=7&SN=EverQuest&SURL=http%3A%2F%2Fwww%2Eeverlore%2Ecom&Cat=Classes&PL=2">MB</a>
						&nbsp;<a href="classes.asp?CID=19&amp;mode=details&amp;type=races">Wizard</a><br>
					
									
				<br>
				<font face="verdana" size="1">
				<b>Races</b></font><br>
					<font face="verdana" size="1">
					
						&nbsp;<a href="classes.asp?CID=23&amp;mode=details&amp;type=classes">Barbarian</a><br>
					
						&nbsp;<a href="classes.asp?CID=24&amp;mode=details&amp;type=classes">Dark Elf</a><br>
					
						&nbsp;<a href="classes.asp?CID=25&amp;mode=details&amp;type=classes">Dwarf</a><br>
					
						&nbsp;<a href="classes.asp?CID=26&amp;mode=details&amp;type=classes">Erudite</a><br>
					
						&nbsp;<a href="classes.asp?CID=27&amp;mode=details&amp;type=classes">Gnome</a><br>
					
						&nbsp;<a href="classes.asp?CID=28&amp;mode=details&amp;type=classes">Half Elf</a><br>
					
						&nbsp;<a href="classes.asp?CID=29&amp;mode=details&amp;type=classes">Halfling</a><br>
					
						&nbsp;<a href="classes.asp?CID=30&amp;mode=details&amp;type=classes">High Elf</a><br>
					
						&nbsp;<a href="classes.asp?CID=31&amp;mode=details&amp;type=classes">Human</a><br>
					
						&nbsp;<a href="classes.asp?CID=32&amp;mode=details&amp;type=classes">Ogre</a><br>
					
