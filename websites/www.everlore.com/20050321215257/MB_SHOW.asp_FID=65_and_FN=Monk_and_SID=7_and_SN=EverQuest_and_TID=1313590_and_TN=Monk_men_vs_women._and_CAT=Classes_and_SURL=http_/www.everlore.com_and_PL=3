						<td valign="middle" class="main_font" align="center">
							<font size="3"><b></b></font>
					    </td>
					</tr>
			</table>
								
		</td>
								
		<!-- Buffer Table Between Information/News and Menu -->
		<td valign="top">&nbsp;&nbsp;</td>

			
			
