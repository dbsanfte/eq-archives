<script language='VBScript'>
sub DoNothin(sElement)
	if Document.Forms("fmStandings").Elements(sElement).Checked = False then
		Document.Forms("fmStandings").Elements(sElement).Checked = True
	else
		Document.Forms("fmStandings").Elements(sElement).Checked = False
	end if
end sub

sub SetClass()
	sClass = Document.Forms("fmStandings").Elements("Classes").Value
	Document.URL = "epics.asp?Class=" & sClass
end sub
</script>
