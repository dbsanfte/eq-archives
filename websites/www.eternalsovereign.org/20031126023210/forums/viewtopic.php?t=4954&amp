}function WriteItem(url,link,name)
{
	document.write('<a href="javascript:ShowItem(');	document.write("'" + url + "'");	if (name!='') {
		document.write(');">' + name + '</a>');
	} else {		document.write(');">' + link + '</a>');
	}
}

