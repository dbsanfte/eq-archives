        showLogout();
        function showLogout() {
    
            var loginCookie = getCookie('site_617607991');
    
            if(''==loginCookie || 'x'==loginCookie || null==loginCookie) 
            {
                
                document.write('&#187; You are not logged in. ');
                document.write('<a href="http://eternalsovereign.org/6/ubb.x?a=lgn&s=617607991">');
                document.write('Login</a>');
                
                  document.write(' or <a href="http://eternalsovereign.org/6/ubb.x?a=rgi&s=617607991">');
                  document.write('register</a>');
                
                return;
            }

            memberName = loginCookie.substring(2,loginCookie.indexOf('&'));//2 = "u="
            memberName = unescape(memberName);
            memberName = replaceString("+"," ",memberName);
            
            document.write('&#187; Hello ',memberName)
    
            document.write(' [</span><span title="Click here to logout." class="ip-crumbs-font">');
            document.write('<a href="http://eternalsovereign.org/6/ubb.x?a=lgt&s=617607991">');
            
            document.write('logout</a></span><span class="ip-crumbs-font">]');
            

    
        }
        function replaceString(oldS,newS,fullS) {
           for (var i=0; i< fullS.length; i++) {
              if (fullS.substring(i,i+oldS.length) == oldS) {
                 fullS = fullS.substring(0,i)+newS+fullS.substring(i+oldS.length,fullS.length)
              }
           }
           return fullS
        }
        
        function getCookie(name) {
            var cname = name + "=";               
            var dc = document.cookie;
            if (dc.length > 0) {     
                 begin = dc.indexOf(cname);
                 if (begin != -1) {           
                    begin += cname.length;
                    end = dc.indexOf(";", begin);
                        if (end == -1) end = dc.length;
                        return dc.substring(begin, end);
                 }
            }
            return null;
        }    
    
        //--></script>&nbsp;</span></td><td align="right" valign="middle" class="ip-alt-row1"><span class="ip-crumbs-font" id="ip-crumbs-id"><a href="http://eternalsovereign.org">Eternal Sovereign Homepage</a>
        &#187;
        Eternal Sovereign</span></td>
