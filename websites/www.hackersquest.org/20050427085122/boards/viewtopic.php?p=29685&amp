			<tr>
				<td colspan="2"><span class="postbody">This is the script I created for that purpose...
<br />

<br />
</span><table width="90%" cellspacing="1" cellpadding="3" border="0" align="center"><tr> 	  <td><span class="genmed"><b>Code:</b></span></td>	</tr>	<tr>	  <td class="code">#!/bin/sh 
<br />

<br />
BASEDIR=&quot;/path/to/ignbot&quot; 
<br />
PHPBIN=&quot;/usr/local/bin/php&quot; 
<br />
CMD=&quot;$&#123;PHPBIN&#125; $&#123;BASEDIR&#125;/bot.php&quot; 
<br />
# If you want to use your own INI file 
<br />
# set the location of the ini file and 
<br />
# comment out the two following lines 
<br />
#INI=&quot;$&#123;BASEDIR&#125;/php.ini&quot; 
<br />
#CMD=&quot;$&#123;PHPBIN&#125; -c $&#123;INI&#125; $&#123;BASEDIR&#125;/bot.php&quot; 
<br />
NOHUP=/usr/bin/nohup 
<br />

<br />
Error&#40;&#41; &#123; 
<br />
&nbsp; echo &quot;Error&#58; $1&quot; 
<br />
&nbsp; exit 1 
<br />
&#125; 
<br />

<br />
&#91; -d $&#123;BASEDIR&#125; &#93; || Error &quot;Make sure the BASEDIR variable is set 
<br />
&nbsp; The value $&#123;BASEDIR&#125; is not valid&quot; 
<br />

<br />
&#91; -f &quot;$&#123;BASEDIR&#125;/bot.php&quot; &#93; || Error &quot;Make sure the BASEDIR variable is properly set 
<br />
&nbsp; I could not find bot.php in $&#123;BASEDIR&#125;&quot; 
<br />

<br />
&#91; -x $&#123;PHPBIN&#125; &#93; || Error &quot;Make sure the PHPBIN variable is set 
<br />
&nbsp; The value $&#123;PHPBIN&#125; is not valid&quot; 
<br />

<br />
&#91; -x $&#123;NOHUP&#125; &#93; || Error &quot;Make sure the NOHUP variable is set 
<br />
&nbsp; The value $&#123;NOHUP&#125; is not valid&quot; 
<br />

<br />
case $1 
<br />
in 
<br />
start*&#41; 
<br />
&nbsp; PID=`/bin/ps -ao &quot;pid command&quot; | /usr/bin/grep -v grep | /usr/bin/grep &quot;$&#123;CMD&#125;&quot; | awk '&#123; print $1; &#125;'` 
<br />
&nbsp; ANS=0 
<br />
&nbsp; &#91; &quot;$&#123;PID&#125;&quot; != &quot;&quot; &#93; || ANS=1; 
<br />

<br />
&nbsp; if &#91; $ANS -eq 0 &#93; 
<br />
&nbsp; then 
<br />
&nbsp; &nbsp; echo &quot;Bot already running PID=$&#123;PID&#125;&quot; 
<br />
&nbsp; else 
<br />
&nbsp; &nbsp; echo &quot;Starting Bot&quot; 
<br />
&nbsp; &nbsp; cd $&#123;BASEDIR&#125; 
<br />
&nbsp; &nbsp; $&#123;NOHUP&#125; $&#123;CMD&#125; &amp; 
<br />

<br />
&nbsp; &nbsp; PID=`/bin/ps -ao &quot;pid command&quot; | /usr/bin/grep -v grep | /usr/bin/grep &quot;$&#123;CMD&#125;&quot; | awk '&#123;print $1;&#125;'` 
<br />
&nbsp; &nbsp; if &#91; &quot;$PID&quot; = &quot;&quot; &#93; 
<br />
&nbsp; &nbsp; then 
<br />
&nbsp; &nbsp; &nbsp; echo &quot;Failed to start bot&quot; 
<br />
&nbsp; &nbsp; else 
<br />
&nbsp; &nbsp; &nbsp; echo &quot;Bot Started&quot; 
<br />
&nbsp; &nbsp; fi 
<br />
&nbsp; fi 
<br />
;; 
<br />
stop*&#41; 
<br />
&nbsp; PID=`/bin/ps -ao &quot;pid command&quot; | /usr/bin/grep -v grep | /usr/bin/grep &quot;$&#123;CMD&#125;&quot; | awk '&#123; print $1; &#125;'` 
<br />
&nbsp; ANS=0 
<br />
&nbsp; &#91; &quot;$&#123;PID&#125;&quot; != &quot;&quot; &#93; || ANS=1; 
<br />

<br />
&nbsp; if &#91; $ANS -eq 0 &#93; 
<br />
&nbsp; then 
<br />
&nbsp; &nbsp; echo &quot;Stopping bot matching PID=$PID&quot; 
<br />
&nbsp; &nbsp; kill $&#123;PID&#125; 
<br />
&nbsp; else 
<br />
&nbsp; &nbsp; echo &quot;Bot is not running&quot; 
<br />
&nbsp; fi 
<br />
;; 
<br />
status*&#41; 
<br />
&nbsp; PID=`/bin/ps -ao &quot;pid command&quot; | /usr/bin/grep -v grep | /usr/bin/grep &quot;$&#123;CMD&#125;&quot; | awk '&#123; print $1; &#125;'` 
<br />
&nbsp; ANS=0 
<br />
&nbsp; &#91; &quot;$&#123;PID&#125;&quot; != &quot;&quot; &#93; || ANS=1; 
<br />

<br />
&nbsp; if &#91; $ANS -eq 0 &#93; 
<br />
&nbsp; then 
<br />
&nbsp; &nbsp; echo &quot;Bot running PID=$PID&quot; 
<br />
&nbsp; else 
<br />
&nbsp; &nbsp; echo &quot;Bot not running &#58;&#40;&quot; 
<br />
&nbsp; fi 
<br />
;; 
<br />
*&#41; 
<br />
echo &quot;Usage&#58; $0 &#123;start|stop|status&#125;&quot;; 
<br />
;; 
<br />
esac 
<br />
</td>	</tr></table><span class="postbody"></span><span class="gensmall"></span></td>
			</tr>
		</table></td>
	</tr>
