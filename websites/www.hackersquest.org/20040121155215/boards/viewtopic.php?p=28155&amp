&nbsp; &nbsp; // if the data looks like a character reference
<br />

<br />
&nbsp; &nbsp; elseif &#40;$b&#91;6&#93; +$b&#91;5&#93;*256+$b&#91;4&#93;*256*256+$b&#91;3&#93;*256*256*256*256==50000 || $b&#91;5&#93; +$b&#91;4&#93;*256+$b&#91;3&#93;*256*256+$b&#91;2&#93;*256*256*256*256==50000&#41;
<br />
&nbsp; &nbsp; &#123;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; if &#40;$b&#91;6&#93; +$b&#91;5&#93;*256+$b&#91;4&#93;*256*256+$b&#91;3&#93;*256*256*256*256==50000&#41;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &#123;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $item&#91;&quot;type&quot;&#93;=&quot;private tell character reference&quot;;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $item&#91;&quot;length&quot;&#93;=$b&#91;18&#93;+$b&#91;17&#93;*256;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $item&#91;&quot;character number&quot;&#93;=$b&#91;10&#93;+$b&#91;9&#93;*256+$b&#91;8&#93;*256*256+$b&#91;7&#93;*256*256*256*256;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $item&#91;&quot;text&quot;&#93;=substr&#40;$blob,18&#41;;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &#125;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; if &#40;$b&#91;5&#93; +$b&#91;4&#93;*256+$b&#91;3&#93;*256*256+$b&#91;2&#93;*256*256*256*256==50000&#41;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &#123;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $item&#91;&quot;type&quot;&#93;=&quot;chat channel character reference&quot;;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $item&#91;&quot;length&quot;&#93;=$b&#91;17&#93;+$b&#91;16&#93;*256;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $item&#91;&quot;character number&quot;&#93;=$b&#91;9&#93;+$b&#91;8&#93;*256+$b&#91;7&#93;*256*256+$b&#91;6&#93;*256*256*256*256;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $item&#91;&quot;text&quot;&#93;=substr&#40;$blob,17&#41;;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &#125;
<br />
&nbsp; &nbsp; &#125;
<br />

