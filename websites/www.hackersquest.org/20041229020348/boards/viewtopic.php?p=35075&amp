//defining a new level function one time only
<br />
if&#40;!function_exists&#40;'level2'&#41;&#41;&#123;
<br />

<br />
function level2&#40;$sender,$message&#41; 
<br />
&nbsp; &nbsp; &nbsp; &#123; 
<br />
&nbsp; &nbsp;global $statistics,$character; 
<br />
&nbsp; &nbsp;global $level_answer; 
<br />
&nbsp; &nbsp;global $tell_once; 
<br />
&nbsp; &nbsp;$statistics&#91;&quot;levels&quot;&#93;++; 
<br />
&nbsp; &nbsp; &nbsp; list&#40;$word2,$extra&#41;= explode &#40;&quot; &quot;, $message, 2&#41;; 
<br />
&nbsp; &nbsp;$word2=0+$word2; 
<br />
&nbsp; &nbsp;if &#40;$word2&gt;0 &amp;&amp; $word2&lt;221 &amp;&amp; is_numeric&#40;$word2&#41;&#41; 
<br />
&nbsp; &nbsp;&#123;$reply=$level_answer&#91;$word2&#93;;&#125; 
<br />
&nbsp; &nbsp;else&#123;$reply=&quot;give me a number from 1 to 220, do /tell &quot;.$character.&quot; &quot;.$syntax.&quot;level 55&quot;;&#125; 
<br />
&nbsp; &nbsp;return $reply; 
<br />
&nbsp; &nbsp; &nbsp; &#125; 
<br />

<br />
&#125;
<br />

<br />

<br />
//defining a new mission function one time only
<br />

<br />
if&#40;!function_exists&#40;'mission2'&#41;&#41;&#123;
<br />

<br />
function mission2&#40;$message&#41; 
<br />
&nbsp; &nbsp;&#123; 
<br />
&nbsp; &nbsp;global $statistics,$character,$syntax; 
<br />
&nbsp; &nbsp;$statistics&#91;&quot;missions&quot;&#93;++; 
<br />
&nbsp; &nbsp;global $level_answer; 
<br />
&nbsp; &nbsp;$unknown=&quot;Give me a QL mission number from 1 to 250, for example do /tell &quot;.$character.&quot; &quot;.$syntax.&quot;mission 160&quot;; 
<br />
&nbsp; &nbsp;$reply=$unknown; 
<br />
&nbsp; &nbsp;$words=explode&#40;&quot; &quot;,$message&#41;; 
<br />
&nbsp; &nbsp;if &#40;sizeof&#40;$words==1&#41; || $words&#91;0&#93;==$words&#91;1&#93; &amp;&amp; is_numeric&#40;$words&#91;0&#93;&#41;&#41; 
<br />
&nbsp; &nbsp;&#123; 
<br />
&nbsp; &nbsp;if &#40;$words&#91;0&#93;&gt;0 &amp;&amp; $words&#91;0&#93;&lt;251&#41; 
<br />
&nbsp; &nbsp;&#123; 
<br />
&nbsp; &nbsp;$reply=&quot;QL&quot;.$words&#91;0&#93;.&quot; missions maybe from these level players &#40;+/- 1&#41;&#58; &quot;; 
<br />
&nbsp; &nbsp;$target_level=0+$words&#91;0&#93;; 
<br />
&nbsp; &nbsp;foreach &#40;$level_answer as $level&#41; 
<br />
&nbsp; &nbsp;&#123; 
<br />
&nbsp; &nbsp;$level_list=str_replace&#40;&quot;,&quot;,&quot;&quot;,substr&#40;$level,strpos&#40;$level,&quot;missions&quot;&#41;+8&#41;&#41;; 
<br />
&nbsp; &nbsp;$ql_array=explode&#40;&quot; &quot;,$level_list&#41;; 
<br />
&nbsp; &nbsp;foreach&#40;$ql_array as $ql&#41; 
<br />
&nbsp; &nbsp;&#123; 
<br />
&nbsp; &nbsp;if &#40;$ql==$target_level&#41; 
<br />
&nbsp; &nbsp;$reply.=splicedata&#40;$level,&quot;L &quot;,&quot;&#58; team &quot;&#41;.&quot; &quot;; 
<br />
&nbsp; &nbsp;&#125;&#125;&#125;&#125; 
<br />
&nbsp; &nbsp;if &#40;sizeof&#40;$words==2&#41; &amp;&amp; $words&#91;0&#93;!=$words&#91;1&#93;&#41; 
<br />
&nbsp; &nbsp;&#123; 
<br />
&nbsp; &nbsp;if &#40;$words&#91;0&#93;&gt;0 &amp;&amp; $words&#91;0&#93;&lt;251 &amp;&amp; $words&#91;1&#93;&gt;0 &amp;&amp; $words&#91;1&#93;&lt;251 &amp;&amp; is_numeric&#40;$words&#91;0&#93;&#41; &amp;&amp; is_numeric&#40;$words&#91;1&#93;&#41;&#41; 
<br />
&nbsp; &nbsp;&#123; 
<br />
&nbsp; &nbsp;$low=0+$words&#91;0&#93;; 
<br />
&nbsp; &nbsp;$high=0+$words&#91;1&#93;; 
<br />
&nbsp; &nbsp;if &#40;$high&lt;$low&#41; 
<br />
&nbsp; &nbsp;&#123; 
<br />
&nbsp; &nbsp;$t=$low; 
<br />
&nbsp; &nbsp;$low=$high; 
<br />
&nbsp; &nbsp;$high=$t; 
<br />
&nbsp; &nbsp;&#125; 
<br />
&nbsp; &nbsp;if &#40;$high-$low&gt;11&#41; 
<br />
&nbsp; &nbsp;$reply=&quot;That is too large of a range, maximum range is 10 QL's, or the answer becomes too long and meaningless.&quot;; 
<br />
&nbsp; &nbsp;else 
<br />
&nbsp; &nbsp;&#123; 
<br />
&nbsp; &nbsp;$reply=&quot;QL$low-QL$high missions maybe from these level players &#40;+/- 1&#41;&#58; &quot;; 
<br />
&nbsp; &nbsp;foreach &#40;$level_answer as $level&#41; 
<br />
&nbsp; &nbsp;&#123; 
<br />
&nbsp; &nbsp;$level_list=str_replace&#40;&quot;,&quot;,&quot;&quot;,substr&#40;$level,strpos&#40;$level,&quot;missions&quot;&#41;+8&#41;&#41;; 
<br />
&nbsp; &nbsp;$ql_array=explode&#40;&quot; &quot;,$level_list&#41;; 
<br />
&nbsp; &nbsp;$player_level=splicedata&#40;$level,&quot;L &quot;,&quot;&#58; team &quot;&#41;.&quot; &quot;; 
<br />
&nbsp; &nbsp;foreach&#40;$ql_array as $ql&#41; 
<br />
&nbsp; &nbsp;&#123; 
<br />
&nbsp; &nbsp;if &#40;$ql&gt;=$low &amp;&amp; $ql&lt;=$high&#41; 
<br />
&nbsp; &nbsp;$tempreply.=&quot;$player_level &quot;; 
<br />
&nbsp; &nbsp;&#125; 
<br />
&nbsp; &nbsp;&#125; 
<br />
&nbsp; &nbsp;$levelanswers=explode&#40;&quot; &quot;,$tempreply&#41;; 
<br />
&nbsp; &nbsp;sort&#40;$levelanswers&#41;; 
<br />
&nbsp; &nbsp;$tempreply=&quot;&quot;; 
<br />
&nbsp; &nbsp;$last_answer=0; 
<br />
&nbsp; &nbsp;foreach&#40;$levelanswers as $desired_answer&#41; 
<br />
&nbsp; &nbsp;&#123; 
<br />
&nbsp; &nbsp;$x=0+$desired_answer; 
<br />
&nbsp; &nbsp;if &#40;$x&gt;$last_answer&#41; 
<br />
&nbsp; &nbsp;$tempreply.=&quot;$desired_answer &quot;; 
<br />
&nbsp; &nbsp;$last_answer=$x; 
<br />
&nbsp; &nbsp;&#125; 
<br />
&nbsp; &nbsp;$reply.=$tempreply; 
<br />
&nbsp; &nbsp;&#125;&#125;&#125; 
<br />
&nbsp; &nbsp;if &#40;strlen&#40;$reply&#41;&gt;500&#41; 
<br />
&nbsp; &nbsp;$reply=&quot;The reply was too long &#40;&quot;.strlen&#40;$reply&#41;.&quot; chars&#41;.&nbsp; Please try again with a smaller range, maybe.&quot;; 
<br />
&nbsp; &nbsp;return $reply; 
<br />
&nbsp; &nbsp;&#125; 
<br />

<br />
&#125;
<br />

<br />

<br />
send&#40;$sender,$botcolor.mission2&#40;$extra&#41;&#41;;
<br />
$noreply=1;
<br />
?&gt;
