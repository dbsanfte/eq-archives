&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$message_for_queue&#91;&quot;blob&quot;&#93;&nbsp; &nbsp;=$window_contents;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$queue&#91;&#93;=$message_for_queue;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;unset&#40;$message_for_queue&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;else
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$reply=&quot;You must be guildmember, leader, admin, or The Superuser to do this, try /tell $myname whoami&quot;;
<br />
&nbsp; &nbsp;return;
<br />
&#125;</td>	</tr></table><span class="postbody"></span><span class="gensmall"></span></td>
			</tr>
		</table></td>
	</tr>
