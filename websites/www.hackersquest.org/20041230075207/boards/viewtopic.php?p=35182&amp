			<tr>
				<td colspan="2"><span class="postbody">Here is a little plugin i wrote for my guild to pull information off of our teamspeak server.  I don't know how many guilds use teamspeak but i know several that do and found this plugin useful.
<br />

<br />
The functions were from some php website scripts on the teamspeak forums.
<br />

<br />
</span><table width="90%" cellspacing="1" cellpadding="3" border="0" align="center"><tr> 	  <td><span class="genmed"><b>Code:</b></span></td>	</tr>	<tr>	  <td class="code">
<br />

<br />
&lt;?
<br />

<br />
////////////////&nbsp; &#58;&#58;Teamspeak Status Plugin&#58;&#58; by Drevi1 V1.0 //////////////
<br />
//&#40;help&#41;
<br />
//
<br />
//&#40;white&#41;Connects to a teamspeak server and gets information about connected
<br />
//users.
<br />
//&#40;grey&#41;Usage&#58;
<br />
//&#40;blue&#41;/tell &#40;bot&#41; &#40;syntax&#41;&#40;command&#41;
<br />
//&#40;help&#41;
<br />
//////////////
<br />

<br />

<br />
//Teamspeak Server Info
<br />
//Modify this information
<br />
$ip = &quot;192.168.1.1&quot;; //Server IP
<br />
$queryport = &quot;51234&quot;; &nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;//Query Port &#40;51234 by default&#41;
<br />
$serverport = &quot;8767&quot;; &nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;//Server Port &#40;8767 by default&#41;
<br />
$servername = &quot;Blah Blah TS Server&quot;; &nbsp; &nbsp;// Server name
<br />
$displayhost = &quot;ts.domain.com&quot;; //Display ip address
<br />

<br />

<br />

<br />

<br />

<br />

<br />

<br />

<br />
if&#40;!function_exists&#40;'indexOf'&#41;&#41; &#123; function indexOf&#40;$str,$strChar&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;if&#40;strlen&#40;strchr&#40;$str,$strChar&#41;&#41;&gt;0&#41; &#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$position_num = strpos&#40;$str,$strChar&#41; + strlen&#40;$strChar&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;return $position_num;
<br />
&nbsp; &nbsp;&#125; else &#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;return -1;
<br />
&nbsp; &nbsp;&#125;
<br />
&#125;
<br />
&#125;
<br />

<br />
if&#40;!function_exists&#40;'getTSChannelUsers'&#41;&#41;&#123;function getTSChannelUsers&#40;$ip,$port,$tPort&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;$uArray &nbsp; &nbsp;= array&#40;&#41;;
<br />
&nbsp; &nbsp;$innerArray = array&#40;&#41;;
<br />
&nbsp; &nbsp;$out&nbsp; &nbsp;&nbsp; &nbsp;= &quot;&quot;;
<br />
&nbsp; &nbsp;$j&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;= 0;
<br />
&nbsp; &nbsp;$k&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;= 0;
<br />

<br />
&nbsp; &nbsp;$fp = fsockopen&#40;$ip, $tPort, $errno, $errstr, 30&#41;;
<br />
&nbsp; &nbsp;if&#40;$fp&#41; &#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;fputs&#40;$fp, &quot;pl &quot;.$port.&quot;\n&quot;&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;fputs&#40;$fp, &quot;quit\n&quot;&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;while&#40;!feof&#40;$fp&#41;&#41; &#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$out .= fgets&#40;$fp, 1024&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$out&nbsp; &nbsp;= str_replace&#40;&quot;&#91;TS&#93;&quot;, &quot;&quot;, $out&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$out&nbsp; &nbsp;= str_replace&#40;&quot;loginname&quot;, &quot;loginname\t&quot;, $out&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$data &nbsp; &nbsp;= explode&#40;&quot;\t&quot;, $out&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$num &nbsp; &nbsp;= count&#40;$data&#41;;
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;for&#40;$i=0;$i&lt;count&#40;$data&#41;;$i++&#41; &#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$innerArray&#91;$j&#93; = $data&#91;$i&#93;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if&#40;$j&gt;=15&#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$uArray&#91;$k&#93;=$innerArray;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$j = 0;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$k = $k+1;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&#125; else &#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$j++;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;fclose&#40;$fp&#41;;
<br />
&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp; return $uArray;
<br />
&#125;&#125;
<br />
if&#40;!function_exists&#40;'time_convert'&#41;&#41;&#123;function time_convert&#40;$time&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;$hours = floor&#40;$time/3600&#41;;
<br />
&nbsp; &nbsp;$minutes = floor&#40;&#40;$time%3600&#41;/60&#41;;
<br />
&nbsp; &nbsp;$seconds = floor&#40;&#40;$time%3600&#41;%60&#41;;
<br />

<br />
&nbsp; &nbsp;if&#40;$hours&gt;0&#41; $time = $hours.&quot;h &quot;.$minutes.&quot;m &quot;.$seconds.&quot;s&quot;;
<br />
&nbsp; &nbsp;else if&#40;$minutes&gt;0&#41; $time = $minutes.&quot;m &quot;.$seconds.&quot;s&quot;;
<br />
&nbsp; &nbsp;else $time = $seconds.&quot;s&quot;;
<br />

<br />
&nbsp; &nbsp; &nbsp;return $time;
<br />
&#125;&#125;
<br />

<br />

<br />
$reply=&quot;&#40;botcolor&#41;TeamSpeak Server Status&quot;;
<br />
$blob = &quot;&quot;;
<br />
$infolines=&quot;&quot;;
<br />

<br />
$connection1 = fsockopen&#40;$ip, $queryport, $errno, $errstr, 30&#41;;
<br />
if &#40;$connection1&#41; &#123;
<br />
&nbsp; &nbsp;fputs&#40;$connection1,&quot;sel &quot;.$serverport.&quot;\n&quot;&#41;;
<br />
&nbsp; &nbsp;fputs&#40;$connection1,&quot;si\n&quot;&#41;;
<br />
&nbsp; &nbsp;fputs&#40;$connection1,&quot;quit\n&quot;&#41;;
<br />

<br />
&nbsp; &nbsp;while&#40;!feof&#40;$connection1&#41;&#41;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$infolines.=fgets&#40;$connection1,1024&#41;;
<br />
&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;$infolines = str_replace&#40;&quot;&#91;TS&#93;&quot;,&quot;&quot;,$infolines&#41;;
<br />
&nbsp; &nbsp;$infolines = str_replace&#40;&quot;OK&quot;,&quot;&quot;,$infolines&#41;;
<br />
&nbsp; &nbsp;$infolines = trim&#40;$infolines&#41;;
<br />

<br />
&nbsp; &nbsp;// Servername, here but not used, uncomment if you want to use it
<br />
&nbsp; &nbsp;$name=substr&#40;$infolines,indexOf&#40;$infolines,&quot;server_name=&quot;&#41;,strlen&#40;$infolines&#41;&#41;;
<br />
&nbsp; &nbsp;$name=substr&#40;$name,0,indexOf&#40;$name,&quot;server_platform=&quot;&#41;-strlen&#40;&quot;server_platform=&quot;&#41;&#41;;
<br />

<br />
&nbsp; &nbsp;// Connected Users
<br />
&nbsp; &nbsp;$user=substr&#40;$infolines,indexOf&#40;$infolines,&quot;server_currentusers=&quot;&#41;,strlen&#40;$infolines&#41;&#41;;
<br />
&nbsp; &nbsp;$user=substr&#40;$user,0,indexOf&#40;$user,&quot;server_currentchannels=&quot;&#41;-strlen&#40;&quot;server_currentchannels=&quot;&#41;&#41;;
<br />

<br />
&nbsp; &nbsp;// Server Max Users
<br />
&nbsp; &nbsp;$maxusers=substr&#40;$infolines,indexOf&#40;$infolines,&quot;server_maxusers=&quot;&#41;,strlen&#40;$infolines&#41;&#41;;
<br />
&nbsp; &nbsp;$maxusers=substr&#40;$maxusers,0,indexOf&#40;$maxusers,&quot;server_allow_codec_celp51=&quot;&#41;-strlen&#40;&quot;server_allow_codec_celp51=&quot;&#41;&#41;;
<br />

<br />
&nbsp; &nbsp;// Number of channels, here but not used, uncomment if you want to use it
<br />
&nbsp; &nbsp;//$numchan=substr&#40;$infolines,indexOf&#40;$infolines,&quot;server_currentchannels=&quot;&#41;,strlen&#40;$infolines&#41;&#41;;
<br />
&nbsp; &nbsp;//$numchan=substr&#40;$numchan,0,indexOf&#40;$numchan,&quot;server_bwinlastsec=&quot;&#41;-strlen&#40;&quot;server_bwinlastsec=&quot;&#41;&#41;;
<br />

<br />
&#125;
<br />

<br />

<br />
$blob.=&quot;&#40;white&#41;Teamspeak Server Info\n&quot;;
<br />
$blob.=&quot;&#40;grey&#41;Get the client&#58;&nbsp; #L \&quot;http&#58;//www.goteamspeak.com\&quot; \&quot;/start http&#58;//www.goteamspeak.com\&quot;\n\n&quot;;
<br />
$blob.=&quot;&#40;grey&#41;Note&#58; To connect use &quot;.$displayhost.&quot;&#58;&quot;.$serverport.&quot; as the connect address\n&quot;;
<br />
$blob.=&quot;&#40;blue&#41;Server Name&#58;&#40;grey&#41; $servername\n&quot;;
<br />
$blob.=&quot;&#40;blue&#41;Server Address&#58;&#40;grey&#41; $displayhost\n&quot;;
<br />
$blob.=&quot;&#40;blue&#41;Server Port&#58;&#40;grey&#41; $serverport\n\n&quot;;
<br />
$blob.=&quot;&#40;blue&#41;Number of Players Currently Connected&#58; &#40;grey&#41;$user&quot;;
<br />
$blob.=&quot;&#40;blue&#41;Server Maximum&#58; &#40;grey&#41;$maxusers\n&quot;;
<br />
$blob.=&quot;&#40;blue&#41;Players &#40;Time Connected&#41;&#58;\n&quot;;
<br />

<br />
$player_array=getTSChannelUsers&#40;$ip,$serverport,$queryport&#41;;
<br />
$count=count&#40;$player_array&#41;;
<br />
if &#40;$count &gt; 1&#41; &#123;
<br />
$arraycount=1;
<br />
&nbsp; &nbsp;while &#40;$count &gt; 1&#41; &#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$player=$player_array&#91;$arraycount&#93;&#91;14&#93;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$player=str_replace&#40;&quot;\&quot;&quot;,&quot;&quot;,$player&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$player=ucfirst&#40;$player&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$time=$player_array&#91;$arraycount&#93;&#91;8&#93;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$time=time_convert&#40;$time&#41;;
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;$blob.=&quot;&#40;grey&#41;$player &#40;$time&#41;\n&quot;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$count--;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$arraycount++;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;&#125;
<br />

<br />
else &#123;
<br />
&nbsp; &nbsp;$blob.=&quot;&#40;grey&#41;No Players Connected&quot;;
<br />
&nbsp; &nbsp;&#125;
<br />

<br />

<br />
send&#40;$sender,$reply,$blob&#41;;
<br />
$noreply=1;
<br />

<br />

