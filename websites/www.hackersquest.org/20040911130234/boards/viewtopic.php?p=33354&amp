			<tr>
				<td colspan="2"><span class="postbody"></span><table width="90%" cellspacing="1" cellpadding="3" border="0" align="center"><tr> 	  <td><span class="genmed"><b>Code:</b></span></td>	</tr>	<tr>	  <td class="code">&lt;?
<br />

<br />
&nbsp; &nbsp;// CTEXTMAKER.PHP
<br />

<br />
&nbsp; &nbsp;// This will read your custom ctext.txt CSV file
<br />
&nbsp; &nbsp;// Ignoring the third column &#40;original length of the system message&#41;
<br />
&nbsp; &nbsp;// And will recompile a new ctext2.txt that can be renamed in ctext.ldb &#40;backup your original one first&#41;
<br />

<br />
set_time_limit&#40;0&#41;;
<br />

<br />
$fp&nbsp; = fopen&#40;&quot;ctext.txt&quot;,&quot;r&quot;&#41;;
<br />
$fp2 = fopen&#40;&quot;ctext2.txt&quot;,&quot;wb&quot;&#41;;
<br />
&nbsp; &nbsp;fwrite&#40;$fp2,&quot;MLDB&quot;&#41;;
<br />
while&#40;!feof&#40;$fp&#41;&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;if &#40;&#40;$line = fgets&#40;$fp,4096&#41;&#41;==false&#41; break;
<br />
&nbsp; &nbsp;list&#40; $p1, $p2, $len, $str&#41; = explode&#40;&quot;§&quot;, $line&#41;;
<br />
&nbsp; &nbsp;$str = str_replace&#40;chr&#40;13&#41;.chr&#40;10&#41;,&quot;&quot;,$str&#41;;
<br />
&nbsp; &nbsp;$str = str_replace&#40;&quot;/n&quot;,chr&#40;13&#41;.chr&#40;10&#41;,$str&#41;;
<br />
&nbsp; &nbsp;$len = strlen&#40;$str&#41;;
<br />
&nbsp; &nbsp;$ptr1 = pack&#40;&quot;V&quot;, $p1&#41;; //echo &quot;$p1,&quot;;
<br />
&nbsp; &nbsp;$ptr2 = pack&#40;&quot;V&quot;, $p2&#41;; //echo &quot;$p2,&quot;;
<br />
&nbsp; &nbsp;$ptr3 = pack&#40;&quot;V&quot;, $len&#41;; //echo &quot;$len\r\n&quot;;
<br />
&nbsp; &nbsp;fwrite&#40; $fp2, $ptr1.$ptr2.$ptr3.$str&#41;;
<br />
&#125;
<br />
&nbsp; &nbsp;$ending = chr&#40;0&#41;.chr&#40;0&#41;.chr&#40;0&#41;.chr&#40;0&#41;.chr&#40;0&#41;.chr&#40;0&#41;.chr&#40;0&#41;.chr&#40;0&#41;;
<br />
&nbsp; &nbsp;fwrite&#40;$fp2, $ending&#41;;
<br />
fclose&#40;$fp2&#41;;
<br />
fclose&#40;$fp&#41;;
<br />

<br />
&nbsp; &nbsp;// Kir
<br />

<br />
?&gt;</td>	</tr></table><span class="postbody"></span><span class="gensmall"></span></td>
