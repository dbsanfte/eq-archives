			<tr>
				<td colspan="2"><hr /></td>
			</tr>
			<tr>
				<td colspan="2"><span class="postbody"></span><table width="90%" cellspacing="1" cellpadding="3" border="0" align="center"><tr> 	  <td><span class="genmed"><b>Code:</b></span></td>	</tr>	<tr>	  <td class="code">&lt;?
<br />

<br />
&nbsp; &nbsp;// This code will output the content of ctext.ldb.txt into a text file named ctext.txt
<br />
&nbsp; &nbsp;// This file is a CSV style using § as separator
<br />
&nbsp; &nbsp;// The first column contains a 32 bytes value to store the channel type of the system message
<br />
&nbsp; &nbsp;// The second column contains a 32 bytes value to store a sort of ID about the unique system message
<br />
&nbsp; &nbsp;// The third column contains the string length of the system message
<br />
&nbsp; &nbsp;// The Last column contains the string itself &#40;it can contains carriage returns and ; aswell&#41;
<br />

<br />
&nbsp; &nbsp;// The .ldb file is starting with the &quot;MLDB&quot; string header
<br />
&nbsp; &nbsp;// The .ldb file is ended by 8 Bytes set to 0x00
<br />

<br />
set_time_limit&#40;0&#41;;
<br />

<br />
$fp&nbsp; = fopen&#40;&quot;ctext.ldb.txt&quot;,&quot;rb&quot;&#41;;
<br />
$fp2 = fopen&#40;&quot;ctext.txt&quot;,&quot;w&quot;&#41;;
<br />
$header = fread&#40;$fp,4&#41;;
<br />
while&#40;!feof&#40;$fp&#41;&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;$ptr1 = fread&#40;$fp,4&#41;;&nbsp; &nbsp;$p1&nbsp; &nbsp;= ord&#40;$ptr1&#123;0&#125;&#41; | &#40;ord&#40;$ptr1&#123;1&#125;&#41;&lt;&lt;8&#41; | &#40;ord&#40;$ptr1&#123;2&#125;&#41;&lt;&lt;16&#41; | &#40;ord&#40;$ptr1&#123;3&#125;&#41;&lt;&lt;24&#41;; 
<br />
&nbsp; &nbsp;$ptr2 = fread&#40;$fp,4&#41;;&nbsp; &nbsp;$p2&nbsp; &nbsp;= ord&#40;$ptr2&#123;0&#125;&#41; | &#40;ord&#40;$ptr2&#123;1&#125;&#41;&lt;&lt;8&#41; | &#40;ord&#40;$ptr2&#123;2&#125;&#41;&lt;&lt;16&#41; | &#40;ord&#40;$ptr2&#123;3&#125;&#41;&lt;&lt;24&#41;; 
<br />
&nbsp; &nbsp;if &#40;&#40;$ptr3 = fread&#40;$fp,4&#41;&#41;!=false&#41;&nbsp; &nbsp;$len&nbsp; = ord&#40;$ptr3&#123;0&#125;&#41; | &#40;ord&#40;$ptr3&#123;1&#125;&#41;&lt;&lt;8&#41; | &#40;ord&#40;$ptr3&#123;2&#125;&#41;&lt;&lt;16&#41; | &#40;ord&#40;$ptr3&#123;3&#125;&#41;&lt;&lt;24&#41;; 
<br />
&nbsp; &nbsp;else break;
<br />
&nbsp; &nbsp;$str&nbsp; = fread&#40;$fp,$len&#41;;
<br />
&nbsp; &nbsp;$str = str_replace&#40;chr&#40;13&#41;.chr&#40;10&#41;,&quot;/n&quot;,$str&#41;;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;// we use /n to note a carriage return
<br />
&nbsp; &nbsp;fwrite&#40; $fp2, &quot;$p1&quot;.&quot;§&quot;.&quot;$p2&quot;.&quot;§&quot;.&quot;$len&quot;.&quot;§&quot;.&quot;$str\r\n&quot;&#41;;&nbsp; &nbsp;&nbsp; &nbsp;// we use § as a column delimiter
<br />
&#125;
<br />
fclose&#40;$fp2&#41;;
<br />
fclose&#40;$fp&#41;;
<br />

<br />

<br />
&nbsp; &nbsp;// Kir
<br />

<br />
?&gt;
<br />
</td>	</tr></table><span class="postbody"></span><span class="gensmall"></span></td>
			</tr>
		</table></td>
	</tr>
