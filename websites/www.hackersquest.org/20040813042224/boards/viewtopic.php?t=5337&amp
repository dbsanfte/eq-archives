&lt;?php
<br />

<br />
/////checks if ran from a bot in AO.
<br />
if&#40;function_exists&#40;'dynamic'&#41;&#41;&#123;
<br />

<br />
$blob=
<br />
&quot;&#40;white&#41;llllllllll&#40;grey&#41;llllllllll&#40;blue&#41;llllllllll&#40;white&#41; IGN Webcast serverside script module.&#40;grey&#41;
<br />

<br />
This is a IGN Webcast module, meaning its not meant to be
<br />
initiated from your bot but rather installed on your website
<br />
to recieve data from your bot which can be viewed from your
<br />
website, like news, online lists, user items etc.
<br />

<br />
once put up on your server, you can then set up in the bot's
<br />
setup where you have placed the script. with
<br />
/tell &#40;bot&#41; &#40;syntax&#41;setup webcast 'http&#58;//site.address/scriptname.php'
<br />
&#40;without the quotation marks.&#41;&quot;;
<br />

<br />
send&#40;$sender,&quot;&#40;botcolor&#41;IGN Webcast script &#40;notifycolor&#41;&#58;&#58; click to learn how to install.&quot;,$blob&#41;;
<br />

<br />
$noreply=1;
<br />
&#125;
<br />

<br />

<br />
//// serverside part from here.
<br />
else&#123;
<br />
//set the dimension number.
<br />
$dimension=1;
<br />

<br />
//set the file to save the online list to.
<br />
$file=&quot;online.txt&quot;;
<br />
//set a password for the uploading of online list.
<br />
$password=&quot;&quot;;
<br />

<br />
//bot upload reciever part
<br />
if&#40;isset&#40;$_GET&#91;'upload'&#93;&#41;&#41;&#123; $upload = $_GET&#91;'upload'&#93;; &#125;
<br />
if&#40;isset&#40;$_GET&#91;'pass'&#93;&#41;&#41;&#123; $pass = $_GET&#91;'pass'&#93;; &#125;
<br />

<br />
if&#40;!empty&#40;$upload&#41;&#41;&#123;
<br />
&nbsp; &nbsp;if&#40;$pass == $password&#41;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$upload=rawurldecode&#40;$upload&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;if&#40;substr&#40;$upload,0,7&#41;==&quot;&#40;start&#41;&quot; &amp;&amp; file_exists&#40;$file&#41;&#41;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$fo=@fopen&#40;$file,&quot;w&quot;&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;//@unlink&#40;$file&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$upload=substr&#40;$upload,7&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&#125;else&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$fo=@fopen&#40;$file,&quot;aw&quot;&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&#125;&nbsp; &nbsp;&nbsp; &nbsp;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;/*
<br />
&nbsp; &nbsp;&nbsp; &nbsp;if&#40;!file_exists&#40;$file&#41;&#41;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;@touch&#40;$file&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;@chmod&#40;$file,0777&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;*/
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$fr=@fwrite&#40;$fo,$upload&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;@fclose&#40;$fo&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;echo &quot;upload of online lists completed&quot;;
<br />
&nbsp; &nbsp;&#125;else&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;echo &quot;not authorized - password failed&quot;;
<br />
&nbsp; &nbsp;&#125;
<br />
&#125;
<br />

<br />
// old PHP compability
<br />
/*
<br />
if&#40;!function_exists&#40;'file_get_contents'&#41;&#41;&#123;
<br />
function file_get_contents&#40;$file = &quot;&quot;&#41;&#123;
<br />
if&#40;$file==&quot;&quot;&#41;&#123;return FALSE;&#125;
<br />
$fo=@fopen&#40;$file,&quot;r&quot;&#41;;
<br />
$fr=@fread&#40;$fo,filesize&#40;$file&#41;&#41;;
<br />
fclose&#40;$fo&#41;;
<br />
return $fr;
<br />
&#125;&#125;
<br />

<br />
if&#40;!function_exists&#40;'stream_set_timeout'&#41;&#41;&#123;
<br />
function stream_set_timeout&#40;$file = &quot;&quot;,$timer = 1&#41;&#123;
<br />
return;
<br />
&#125;&#125;
<br />
*/
<br />

<br />
//webpage viewing part.
<br />

<br />
if&#40;empty&#40;$upload&#41;&#41;&#123;
<br />
if&#40;isset&#40;$_GET&#91;'view'&#93;&#41;&#41;&#123; $view = $_GET&#91;'view'&#93;; &#125;
<br />
if&#40;isset&#40;$_GET&#91;'image'&#93;&#41;&#41;&#123; $image = $_GET&#91;'image'&#93;; &#125;else&#123; $image = &quot;&quot;; &#125;
<br />
if&#40;empty&#40;$view&#41;&#41;&#123;$view=&quot;online&quot;;&#125;global $frame;
<br />
if&#40;$frame==&quot;&quot;&#41;&#123;$frame=1;&#125;
<br />
if&#40;empty&#40;$c&#41;&#41;&#123;$c=&quot;A5C0DC&quot;;&#125;
<br />
$colors='bgcolor=&quot;#'.$c.'&quot;&gt;';
<br />
$list=file_get_contents&#40;$file&#41;;
<br />
$list=explode&#40;&quot;\r\n&quot;,trim&#40;$list&#41;&#41;;
<br />
if&#40;is_array&#40;$list&#41;&#41;&#123;
<br />
echo '&lt;HTML&gt;&lt;HEAD&gt;&lt;TITLE&gt;Users&lt;/TITLE&gt;&lt;/HEAD&gt;&lt;BODY&gt;&lt;CENTER&gt;&lt;table border='.$frame.' '.$colors;
<br />
foreach&#40;$list as $line&#41;&#123;
<br />
$data=explode&#40;&quot; &quot;,$line&#41;;$pic=&quot;&quot;;
<br />
if&#40;strlen&#40;$data&#91;0&#93;&#41;&gt;2&#41;&#123;$picalign=&quot;left&quot;;
<br />
$data&#91;1&#93;=str_replace&#40;&quot;on&quot;,&quot;online&quot;,str_replace&#40;&quot;off&quot;,&quot;offline&quot;,$data&#91;1&#93;&#41;&#41;;
<br />
if&#40;$data&#91;1&#93;==&quot;online&quot;&#41;&#123;$color='&lt;font color=&quot;#DDFFDD&quot;&gt;';&#125;else&#123;$color='&lt;font color=&quot;#FFCC52&quot;&gt;';&#125;
<br />
if&#40;empty&#40;$search&#41; || stristr&#40;$data&#91;0&#93;,$search&#41; || stristr&#40;$data&#91;2&#93;,$search&#41;&#41;&#123;
<br />
$udata=&quot;&quot;;
<br />
$udata=file_get_contents&#40;'http&#58;//anarchy-online.com/character/bio/d/'.$dimension.'/name/'.strtolower&#40;$data&#91;0&#93;&#41;.'/bio.xml'&#41;;
<br />
$rankurl=explode&#40;'&lt;rank&gt;',$udata&#41;;
<br />
$rank=explode&#40;&quot;&lt;/rank&gt;&quot;,$rankurl&#91;1&#93;&#41;;
<br />
$levelurl=explode&#40;'&lt;level&gt;',$udata&#41;;
<br />
$level=explode&#40;&quot;&lt;/level&gt;&quot;,$levelurl&#91;1&#93;&#41;;
<br />
$professionurl=explode&#40;'&lt;profession&gt;',$udata&#41;;
<br />
$profession=explode&#40;&quot;&lt;/profession&gt;&quot;,$professionurl&#91;1&#93;&#41;;
<br />
if&#40;$image==1 &amp;&amp; $data&#91;1&#93;==$view || $image==1 &amp;&amp; $data&#91;1&#93;==&quot;all&quot;&#41;&#123;
<br />
$picalign=&quot;middle&quot;;
<br />
$picurl=explode&#40;'&lt;pictureurl&gt;',$udata&#41;;
<br />
$pic=explode&#40;&quot;&lt;/pictureurl&gt;&quot;,$picurl&#91;1&#93;&#41;;
<br />
if&#40;!empty&#40;$pic&#91;0&#93;&#41;&#41;&#123;
<br />
if&#40;empty&#40;$w&#41;&#41;&#123;$w=60;&#125;
<br />
if&#40;empty&#40;$h&#41;&#41;&#123;$h=90;&#125;
<br />
$pic='&lt;img src=&quot;'.$pic&#91;0&#93;.'&quot; width=&quot;'.$w.'&quot; height=&quot;'.$h.'&quot;&gt;&lt;br&gt;';&#125;else&#123;$pic=&quot;&quot;;&#125;&#125;
<br />
if&#40;$data&#91;1&#93;==$view || $view==&quot;all&quot;&#41;&#123;
<br />
echo &quot;\r\n&quot;.'&lt;tr border='.$frame.'&gt;&lt;td align=&quot;'.$picalign.'&quot;&gt;'.$pic.'&lt;font face=&quot;Arial&quot;&gt;&lt;b&gt;&nbsp; &nbsp; '.$data&#91;0&#93;.'&nbsp; &nbsp; &lt;/b&gt;&lt;br&gt;&lt;font size=&quot;-2&quot;&gt;'.$level&#91;0&#93;.' '.$profession&#91;0&#93;.' '.$rank&#91;0&#93;.'&nbsp; &nbsp; &nbsp; &lt;td align=&quot;left&quot;&gt;'.$color.$data&#91;1&#93;.' &lt;font size=&quot;2&quot; color=&quot;#000000&quot;&gt;since '.gmstrftime&#40;&quot;%A&quot;&#41;.gmdate&#40;&quot; F j, Y - g&#58;i A&quot;,$data&#91;2&#93;&#41;.&quot; &#40;GMT&#41;.&lt;/br&gt;&lt;/font&gt;&lt;/font&gt;&lt;/font&gt;&lt;/td&gt;&lt;/td&gt;&lt;/tr&gt;\r\n&quot;;&#125;
<br />
&#125;&#125;&#125;&#125;echo &quot;&lt;/TABLE&gt;&lt;/BODY&gt;&lt;/HTML&gt;&lt;/CENTER&gt;&quot;;&#125;exit;&#125;
<br />

<br />
?&gt;
