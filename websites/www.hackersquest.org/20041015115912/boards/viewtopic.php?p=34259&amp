-- Dumps an item in a bit more useful way than
<br />
-- just doing a DumpTable on it.
<br />
function DumpItem&#40; Item &#41;
<br />
&nbsp; &nbsp;if Item.Name == &quot;Nano Crystal &#40;Tap Notum Vein&#58; Scheol&#41;&quot; then
<br />
&nbsp; &nbsp;write &quot;********\n&quot;
<br />

<br />
&nbsp; &nbsp;TotalParsed = TotalParsed + 1
<br />

<br />
&nbsp; &nbsp;keyhi = Item.KeyHi
<br />
&nbsp; &nbsp;keylo = Item.KeyLo
<br />

<br />
&nbsp; &nbsp;if Item.Name then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;write&#40; &quot;Name&#58; &quot;..Item.Name..&quot;\n\n&quot; &#41;
<br />
&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;write&#40; &quot;KeyHi&#58; &quot;..Item.KeyHi..&quot;\n&quot; &#41;
<br />
&nbsp; &nbsp;write&#40; &quot;KeyLo&#58; &quot;..Item.KeyLo..&quot;\n&quot; &#41;
<br />
&nbsp; &nbsp;write&#40; &quot;NumberOfChunks&#58; &quot;..Item.NumberOfChunks..&quot;\n&quot; &#41;
<br />
&nbsp; &nbsp;write&#40; &quot;NumParsedChunks&#58; &quot;..Item.NumParsedChunks..&quot;\n&quot; &#41;
<br />

<br />
&nbsp; &nbsp;if Item.Desc then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;write&#40; &quot;Desc&#58; &quot;..Item.Desc..&quot;\n\n&quot; &#41;
<br />
&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;if Item.Attributes then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;write &quot;** Attributes\n&quot;
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;foreach&#40; Item.Attributes,
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;function &#40; Index, Value &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if type&#40; Value &#41; == &quot;table&quot; then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;write&#40; Index..&quot;&#58; &quot; &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;DumpFlags&#40; Value &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;else
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;write&#40; Index..&quot;&#58; &quot;..Value..&quot;\n&quot; &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;end
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;end &#41;
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;write &quot;\n&quot;
<br />
&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;if Item.SkillsInfo then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;write &quot;** SkillsInfo\n&quot;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;DumpTable&#40; Item.SkillsInfo &#41;
<br />
&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;if Item.Scripts then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;write &quot;** Scripts\n&quot;
<br />
&nbsp; &nbsp;--&nbsp; &nbsp;DumpTable&#40; Item.Scripts &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;DumpScripts&#40; Item.Scripts &#41;
<br />
&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;if Item.Requirements then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;write &quot;** Requirements\n&quot;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;--DumpTable&#40; Item.Requirements &#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;foreach&#40; Item.Requirements,
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;function &#40; Index, Value &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;cond = ConditionToString&#40; Value &#41;
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if&#40; cond &#41; then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;write&#40; Index..&quot;&#58;\n&quot; &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;write&#40; ConditionToString&#40; Value &#41;..&quot;\n&quot; &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;end
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;end &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;write &quot;\n&quot;
<br />
&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;if Item.UnknownChunk then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;write&#40; &quot;UnknownChunk ID&#58; &quot;..Item.UnknownChunk.ChunkID..&quot;\n&quot; &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;if UnknownChunks&#91;Item.UnknownChunk.ChunkID&#93; then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;UnknownChunks&#91;Item.UnknownChunk.ChunkID&#93; = UnknownChunks&#91;Item.UnknownChunk.ChunkID&#93; + 1
<br />
&nbsp; &nbsp;&nbsp; &nbsp;else
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;UnknownChunks&#91;Item.UnknownChunk.ChunkID&#93; = 1
<br />
&nbsp; &nbsp;&nbsp; &nbsp;end
<br />
&nbsp; &nbsp;&nbsp; &nbsp;NumUnparsed = NumUnparsed + 1
<br />
&nbsp; &nbsp;--&nbsp; &nbsp;Dumpbin&#40; Item.KeyLo..&quot;_unparsed.bin&quot;, Item.UnknownChunk.UnparsedData &#41;
<br />
&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;if Item.UnknownFunc then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;write&#40; &quot;UnknownFunc ID&#58; &quot;..Item.UnknownFunc.FuncID..&quot;\n&quot; &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;if UnknownFuncs&#91;Item.UnknownFunc.FuncID&#93; then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;UnknownFuncs&#91;Item.UnknownFunc.FuncID&#93; = UnknownFuncs&#91;Item.UnknownFunc.FuncID&#93; + 1
<br />
&nbsp; &nbsp;&nbsp; &nbsp;else
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;UnknownFuncs&#91;Item.UnknownFunc.FuncID&#93; = 1
<br />
&nbsp; &nbsp;&nbsp; &nbsp;end
<br />
&nbsp; &nbsp;&nbsp; &nbsp;NumUnparsed = NumUnparsed + 1
<br />
&nbsp; &nbsp;--&nbsp; &nbsp;Dumpbin&#40; Item.KeyLo..&quot;_unparsed.bin&quot;, Item.UnknownFunc.UnparsedData &#41;
<br />
&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;write &quot;********\n&quot;
<br />
&nbsp; &nbsp;end
<br />
end
<br />

<br />
-- Dumps an item along with its id
<br />
function Dump&#40; Item &#41;
<br />
&nbsp; &nbsp;write&#40; Item.KeyLo..&quot; &quot;..Item.Name..&quot;\n&quot; &#41;
<br />
end
<br />

<br />
-- Dumps an item if it has a particular unknown chunk
<br />
function DumpUChunk&#40; Item, ChunkID &#41;
<br />
&nbsp; &nbsp;if Item.UnknownChunk and Item.UnknownChunk.ChunkID == ChunkID then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;write&#40; Item.KeyLo..&quot; &quot;..Item.Name..&quot;\n&quot; &#41;
<br />
&nbsp; &nbsp;end
<br />
end
<br />

<br />
-- Dumps an item if it has a particular unknown func
<br />
function DumpUFunc&#40; Item, FuncID &#41;
<br />
&nbsp; &nbsp;if Item.UnknownFunc and Item.UnknownFunc.FuncID == FuncID then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;write&#40; Item.KeyLo..&quot; &quot;..Item.Name..&quot;\n&quot; &#41;
<br />
&nbsp; &nbsp;end
<br />
end
<br />

<br />
-- Dumps an item if it has an unknown func
<br />
function DumpUFunc2&#40; Item &#41;
<br />
&nbsp; &nbsp;if Item.UnknownFunc then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;write&#40; Item.UnknownFunc.FuncID..&quot; &quot;..Item.KeyLo..&quot; &quot;..Item.Name..&quot;\n&quot; &#41;
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;if UnknownFuncs&#91;Item.UnknownFunc.FuncID&#93; then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;UnknownFuncs&#91;Item.UnknownFunc.FuncID&#93; = UnknownFuncs&#91;Item.UnknownFunc.FuncID&#93; + 1
<br />
&nbsp; &nbsp;&nbsp; &nbsp;else
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;UnknownFuncs&#91;Item.UnknownFunc.FuncID&#93; = 1
<br />
&nbsp; &nbsp;&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;end
<br />
end
<br />

<br />
-- Dumps an item if it has an unknown chunk
<br />
function DumpUChunk2&#40; Item &#41;
<br />
&nbsp; &nbsp;if Item.UnknownChunk then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;write&#40; Item.UnknownChunk.ChunkID..&quot; &quot;..Item.KeyLo..&quot; &quot;..Item.Name..&quot;\n&quot; &#41;
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;if UnknownChunks&#91;Item.UnknownChunk.ChunkID&#93; then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;UnknownChunks&#91;Item.UnknownChunk.ChunkID&#93; = UnknownChunks&#91;Item.UnknownChunk.ChunkID&#93; + 1
<br />
&nbsp; &nbsp;&nbsp; &nbsp;else
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;UnknownChunks&#91;Item.UnknownChunk.ChunkID&#93; = 1
<br />
&nbsp; &nbsp;&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;end
<br />
end</td>	</tr></table><span class="postbody">
<br />

<br />
That produces items.txt. 
<br />

<br />
Then, I have a script that reads "new.txt" and "old.txt" (new being the new database, old being the database from the last patch (or whatever). The script loads all of the new items (based on the IDs) into a temporary database, and then I hand-clean those new items, and then let it dump the new items into the old database. 
<br />

<br />
This script dumps all of the new items into a database (and deletes duplicate QLs/names as it would mess Itemsnet up): 
<br />

<br />
</span><table width="90%" cellspacing="1" cellpadding="3" border="0" align="center"><tr> 	  <td><span class="genmed"><b>Code:</b></span></td>	</tr>	<tr>	  <td class="code">$link = mysql_connect&#40;&quot;localhost&quot;, &quot;username&quot;, &quot;password&quot;&#41;;
<br />
mysql_select_db&#40;&quot;database_name&quot;, $link&#41;;
<br />
mysql_query&#40;&quot;DELETE FROM temp&quot;, $link&#41;;
<br />
mysql_query&#40;&quot;DELETE FROM old&quot;, $link&#41;;
<br />
$page = &quot;&quot;; $t = 0; $n = 0; $i = 0;
<br />
$handle = fopen&#40;&quot;old.txt&quot;, &quot;r&quot;&#41;;
<br />
while&#40;!feof&#40;$handle&#41;&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;$page .= fgets&#40;$handle, 4096&#41;;
<br />
&#125;
<br />
$items = explode&#40;&quot;\n&quot;, $page&#41;;
<br />
foreach&#40;$items as $item&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;$i++;
<br />
&nbsp; &nbsp;$stuff = explode&#40;&quot;\t&quot;, $item&#41;;
<br />
&nbsp; &nbsp;mysql_query&#40;&quot;INSERT INTO old&#40;id&#41; VALUES&#40;'$stuff&#91;0&#93;'&#41;&quot;, $link&#41;;
<br />
&#125;
<br />

<br />
$handle = fopen&#40;&quot;new.txt&quot;, &quot;r&quot;&#41;;
<br />
$page = &quot;&quot;;
<br />
while&#40;!feof&#40;$handle&#41;&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;$page .= fgets&#40;$handle, 4096&#41;;
<br />
&#125;
<br />
$items = explode&#40;&quot;\n&quot;, $page&#41;;
<br />
foreach&#40;$items as $item&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;$stuff = explode&#40;&quot;\t&quot;, $item&#41;;
<br />
&nbsp; &nbsp;$row = mysql_fetch_object&#40;mysql_query&#40;&quot;SELECT * FROM old WHERE id = '$stuff&#91;0&#93;'&quot;, $link&#41;&#41;;
<br />
&nbsp; &nbsp;if&#40;!$row-&gt;id&#41;
<br />
&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$t++;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;echo &quot;Adding&#58; $stuff&#91;0&#93;\t$stuff&#91;1&#93;\t$stuff&#91;2&#93;\n&quot;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;mysql_query&#40;&quot;INSERT INTO temp&#40;name, ql, id&#41; VALUES&#40;'&quot;.str_replace&#40;&quot;'&quot;, &quot;&quot;, trim&#40;$stuff&#91;2&#93;&#41;&#41;.&quot;', '$stuff&#91;1&#93;', '&quot;.trim&#40;$stuff&#91;0&#93;&#41;.&quot;'&#41;&quot;, $link&#41;;
<br />
&nbsp; &nbsp;&#125;
<br />
&#125;
<br />
$items = array&#40;&#41;;
<br />
$result = mysql_query&#40;&quot;SELECT * FROM temp&quot;, $link&#41;;
<br />
while&#40;$row = mysql_fetch_object&#40;$result&#41;&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;if&#40;isset&#40;$items&#91;$row-&gt;name&#93;&#91;$row-&gt;ql&#93;&#41;&#41;
<br />
&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;echo &quot;Deleting&#58; $row-&gt;name &#91;$row-&gt;ql&#93;\n&quot;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;mysql_query&#40;&quot;DELETE FROM temp WHERE id = $row-&gt;id&quot;, $link&#41;;
<br />
&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;else
<br />
&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$items&#91;$row-&gt;name&#93;&#91;$row-&gt;ql&#93; = true;
<br />
&nbsp; &nbsp;&#125;
<br />
&#125;
<br />
</td>	</tr></table><span class="postbody">
<br />

<br />

<br />
And this one dumps the stuff into the regular database and makes a pretty dump of all of the items (the items.txt file): 
<br />

<br />
</span><table width="90%" cellspacing="1" cellpadding="3" border="0" align="center"><tr> 	  <td><span class="genmed"><b>Code:</b></span></td>	</tr>	<tr>	  <td class="code">
<br />
$link = mysql_connect&#40;&quot;localhost&quot;, &quot;username&quot;, &quot;password&quot;&#41;;
<br />
mysql_select_db&#40;&quot;database_name&quot;, $link&#41;;
<br />

<br />
$result = mysql_query&#40;&quot;SELECT * FROM temp&quot;, $link&#41;;
<br />
while&#40;$row = mysql_fetch_object&#40;$result&#41;&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;$n++;
<br />
&nbsp; &nbsp;mysql_query&#40;&quot;INSERT INTO items&#40;name, ql, id&#41; VALUES&#40;'$row-&gt;name', '$row-&gt;ql', '$row-&gt;id'&#41;&quot;, $link&#41;;
<br />
&#125;
<br />

<br />
$file = &quot;&quot;;
<br />
$result = mysql_query&#40;&quot;SELECT * FROM items ORDER BY id ASC&quot;, $link&#41;;
<br />
$handle = fopen&#40;&quot;text_dump.txt&quot;, &quot;w&quot;&#41;;
<br />
while&#40;$row = mysql_fetch_object&#40;$result&#41;&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;$file .= $row-&gt;id.&quot;\t&quot;.$row-&gt;ql.&quot;\t&quot;.$row-&gt;name.&quot;\n&quot;;
<br />
&#125;
<br />
fwrite&#40;$handle, $file&#41;;
<br />
fclose&#40;$handle&#41;;</td>	</tr></table><span class="postbody">
<br />

<br />
It's just a quick hack, so don't laugh at the miserable code. -.- 
<br />

<br />

<br />

<br />
As for the person that asked for an update to the old Itemsnet code, I should probably release the new version sometime (although I haven't even made it truly April 5th compliant.. I need to work on it some.) Hope that helps / answers your questions. 
<br />

<br />
-Ethernal</span><span class="gensmall"></span></td>
			</tr>
