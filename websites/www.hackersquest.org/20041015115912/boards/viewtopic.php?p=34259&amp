				<td colspan="2"><span class="postbody">I just use morb's DB dump and the LUA extension that I messed around with (I have no idea how it works, I just played around with it till it worked <img src="images/smiles/icon_razz.gif" alt="Razz" border="0" />) dumps ths name, ql, and id. 
<br />

<br />
</span><table width="90%" cellspacing="1" cellpadding="3" border="0" align="center"><tr> 	  <td><span class="genmed"><b>Code:</b></span></td>	</tr>	<tr>	  <td class="code">dofile &quot;itemdefs.lua&quot;
<br />
dofile &quot;itemutils.lua&quot;
<br />

<br />
dofile &quot;aodbdefs.lua&quot;
<br />

<br />
UnknownChunks = &#123;&#125;
<br />
UnknownFuncs = &#123;&#125;
<br />
TotalParsed = 0
<br />
NumUnparsed = 0
<br />

<br />
-- Table to convert operators into more readable stuff
<br />
OpConvert =
<br />
&#123;
<br />
&nbsp; &nbsp;Operator_Larger = &quot;&gt;&quot;,
<br />
&nbsp; &nbsp;Operator_Less = &quot;&lt;&quot;,
<br />
&nbsp; &nbsp;Operator_Equal = &quot;==&quot;,
<br />
&nbsp; &nbsp;Operator_Unequal = &quot;!=&quot;,
<br />
&nbsp; &nbsp;Operator_And = &quot;and&quot;,
<br />
&nbsp; &nbsp;Operator_BitAnd = &quot;&amp;&quot;
<br />
&#125;
<br />

<br />
-- Convert the provided reverse polnish notation conditional expression
<br />
-- into a tree.
<br />
function ConditionToTree&#40; RPNExpr &#41;
<br />
&nbsp; &nbsp;local stack = &#123;&#125;
<br />
&nbsp; &nbsp;local i = 1
<br />
&nbsp; &nbsp;local stackindex = 1
<br />
&nbsp; &nbsp;local subtree
<br />
--print &quot;conditiontotree rpnexp&#58;&quot;
<br />
--DumpTable&#40; RPNExpr &#41;
<br />
--print &quot;----&quot;
<br />
&nbsp; &nbsp;-- Every string starting with &quot;Operator_&quot; is treated
<br />
&nbsp; &nbsp;-- as a node, everything else is considered atomic
<br />
&nbsp; &nbsp;while RPNExpr&#91;i&#93; do
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;if type&#40; RPNExpr&#91;i&#93; &#41; == &quot;table&quot; then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;print&#40; &quot;ParseConditionExpression error in blob &quot;..keyhi..&quot;, &quot;..keylo &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;if strfind&#40; RPNExpr&#91;i&#93;, &quot;Operator_&quot; &#41; == 1 then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if stackindex &gt;= 3 then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;subtree =
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;Op = RPNExpr&#91;i&#93;,
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&#91;1&#93; = stack&#91; stackindex - 2 &#93;,
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&#91;2&#93; = stack&#91; stackindex - 1 &#93;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&#125;
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;stack&#91; stackindex - 1&#93; = nil
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;stackindex = stackindex - 1
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;stack&#91; stackindex - 1 &#93; = subtree
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;end
<br />
&nbsp; &nbsp;&nbsp; &nbsp;else
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;stack&#91; stackindex &#93; = RPNExpr&#91; i &#93;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;stackindex = stackindex + 1
<br />
&nbsp; &nbsp;&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;i = i + 1
<br />
&nbsp; &nbsp;end
<br />
--DumpTable&#40; stack&#91; 1 &#93; &#41;
<br />

<br />
&nbsp; &nbsp;return stack&#91; 1 &#93;
<br />
end
<br />

<br />
function OutputSubExpr&#40; Operator, SubExpr, ExprString &#41;
<br />
&nbsp; &nbsp;if type&#40; SubExpr &#41; == &quot;table&quot; then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;-- If the operator for this subexpr is different than operator on
<br />
&nbsp; &nbsp;&nbsp; &nbsp;-- the current node, we output it between parenthesis
<br />
&nbsp; &nbsp;&nbsp; &nbsp;if Operator == SubExpr.Op then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;ExprString = ParseSubExpr&#40; SubExpr, ExprString &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;else
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;ExprString = ExprString..&quot;&#40;&quot;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;ExprString = ParseSubExpr&#40; SubExpr, ExprString &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;ExprString = ExprString..&quot;&#41;&quot;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;end
<br />
&nbsp; &nbsp;else
<br />
&nbsp; &nbsp;&nbsp; &nbsp;ExprString = ExprString..SubExpr
<br />
&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;return ExprString
<br />
end
<br />

<br />
function ParseSubExpr&#40; ExprNode, ExprString &#41;
<br />

<br />
&nbsp; &nbsp;if not ExprNode then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;return
<br />
&nbsp; &nbsp;--&nbsp; &nbsp;print&#40; &quot;ParseConditionExpression error in blob &quot;..keyhi..&quot;, &quot;..keylo &#41;
<br />
&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;-- Output first subexpr
<br />
&nbsp; &nbsp;ExprString = OutputSubExpr&#40; ExprNode.Op, ExprNode&#91;1&#93;, ExprString &#41;
<br />

<br />
&nbsp; &nbsp;-- Output operator
<br />
&nbsp; &nbsp;Op = OpConvert&#91; ExprNode.Op &#93; or ExprNode.Op
<br />
&nbsp; &nbsp;ExprString = ExprString..&quot; &quot;..Op..&quot; &quot;
<br />

<br />
&nbsp; &nbsp;-- Output second subexpr
<br />
&nbsp; &nbsp;return OutputSubExpr&#40; ExprNode.Op, ExprNode&#91;2&#93;, ExprString &#41;
<br />
end
<br />

<br />

<br />
-- Output an conditional in algebraic notation from a RPN condition.
<br />
function ConditionToString&#40; RPNExpr &#41;
<br />
&nbsp; &nbsp;local ExprTreeRoot = ConditionToTree&#40; RPNExpr &#41;
<br />
&nbsp; &nbsp;local ExprString = &quot;&quot;
<br />

<br />
&nbsp; &nbsp;return ParseSubExpr&#40; ExprTreeRoot, ExprString &#41;
<br />
end
<br />

<br />
-- Write the provided buffer &#40;string&#41; as a binary file
<br />
function Dumpbin&#40; Filename, Data &#41;
<br />
&nbsp; &nbsp;local fh = openfile&#40; Filename, &quot;wb&quot; &#41;
<br />

<br />
&nbsp; &nbsp;if fh then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;write&#40; fh, Data &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;closefile&#40; fh &#41;
<br />
&nbsp; &nbsp;end
<br />
end
<br />

<br />
-- Recursively dumps a table and its sub-tables, displaying
<br />
-- the indexes and the values.
<br />
function DumpTable&#40; Table &#41;
<br />
&nbsp; &nbsp;foreach&#40; Table,
<br />
&nbsp; &nbsp;&nbsp; &nbsp;function &#40; Index, Value &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if type&#40; Value &#41; == &quot;table&quot; then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;write&#40; Index..&quot;&#58;\n&quot; &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;DumpTable&#40; Value &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;else
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;write&#40; Index..&quot;&#58; &quot;..Value..&quot;\n&quot; &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;end
<br />
&nbsp; &nbsp;&nbsp; &nbsp;end &#41;
<br />
&nbsp; &nbsp;write &quot;\n&quot;
<br />
end
<br />

<br />
-- Dump flags
<br />
function DumpFlags&#40; Table &#41;
<br />
&nbsp; &nbsp;write &quot;&#91; &quot;
<br />
&nbsp; &nbsp;foreachi&#40; Table,
<br />
&nbsp; &nbsp;&nbsp; &nbsp;function &#40; Index, Value &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if Value then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;write&#40; Value..&quot; &quot; &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;end
<br />
&nbsp; &nbsp;&nbsp; &nbsp;end &#41;
<br />
&nbsp; &nbsp;write &quot;&#93;\n&quot;
<br />
end
<br />

<br />
-- Dump the scripts part
<br />
function DumpScripts&#40; Scripts &#41;
<br />
&nbsp; &nbsp;foreachi&#40; Scripts,
<br />
&nbsp; &nbsp;&nbsp; &nbsp;function &#40; Index, Value &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if Value.On then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;write&#40; Value.On..&quot;&#58;\n&quot; &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;foreachi&#40; Value,
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;function&#40; Index, Value &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;local fcnstring = &quot;&quot;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;local i = 1
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if Value.Condition then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;write&#40; &quot;if&#40;&quot;..ConditionToString&#40; Value.Condition &#41;..&quot;&#41;\n&quot; &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;fcnstring = &quot;&nbsp; &quot;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;fcnstring = fcnstring..Value.FunctionName..&quot;&#40; &quot;
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;while Value&#91;i&#93; do
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if i &gt; 1 then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;fcnstring = fcnstring..&quot;, &quot;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if type&#40; Value&#91;i&#93; &#41; == &quot;string&quot; then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;fcnstring = format&#40; &quot;%s%q&quot;, fcnstring, Value&#91;i&#93; &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;else
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;fcnstring = fcnstring..Value&#91;i&#93;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;i = i + 1
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;fcnstring = fcnstring..&quot; &#41;&quot;
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if Value.Target then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;fcnstring = fcnstring..&quot; on &quot;..Value.Target
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;end
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if Value.RepeatCount then
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;fcnstring = fcnstring..&quot; every &quot;..&#40; Value.RepeatDelay / 100 &#41;..&quot;s, &quot;..Value.RepeatCount..&quot; times&quot;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;end
<br />

<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;write&#40; fcnstring..&quot;\n&quot; &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;end &#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;end &#41;
<br />

<br />
&nbsp; &nbsp;write &quot;\n&quot;
<br />
end
<br />

