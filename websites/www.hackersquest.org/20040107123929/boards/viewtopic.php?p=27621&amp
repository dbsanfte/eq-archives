				<td colspan="2"><span class="postbody">here is a script that will give you your results in UBB code:
<br />
</span><table width="90%" cellspacing="1" cellpadding="3" border="0" align="center"><tr> 	  <td><span class="genmed"><b>Code:</b></span></td>	</tr>	<tr>	  <td class="code">&lt;?
<br />
// don't let the script run for more than 5 minutes
<br />
set_time_limit&#40;300&#41;;
<br />
// if the two key files are missing, exit
<br />
if&#40;!file_exists&#40;&quot;myinventory.txt&quot;&#41;|| !file_exists&#40;&quot;itemdatabase.txt&quot;&#41;&#41;
<br />
&#123;&nbsp; &nbsp; echo &quot;either myinventory.txt or itemdatabase.txt is missing\n&quot;;
<br />
&nbsp; &nbsp; exit; &#125;
<br />
//----------------------------------------------
<br />
// open the database file
<br />
echo &quot;opening database file....&quot;;
<br />
$fd = fopen &#40;&quot;itemdatabase.txt&quot;, &quot;r&quot;&#41;;
<br />
while &#40;!feof &#40;$fd&#41;&#41; 
<br />
&#123;
<br />
&nbsp; &nbsp; list&#40;$id,$ql,$name,$value&#41; = explode&#40;&quot;\t&quot;,trim&#40;fgets&#40;$fd, 4096&#41;&#41;,4&#41;;
<br />
&nbsp; &nbsp; &#123;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; $itemtable&#91;$id&#93;&#91;&quot;ql&quot;&#93;&nbsp; =$ql;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; $itemtable&#91;$id&#93;&#91;&quot;name&quot;&#93;=$name;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; $itemtable&#91;$id&#93;&#91;&quot;value&quot;&#93;=0+trim&#40;$value&#41;;
<br />
&nbsp; &nbsp; &#125;&nbsp; &nbsp; 
<br />
&nbsp; &nbsp; //echo &quot;$id\t$ql\t$name\t$value\n&quot;;
<br />
&#125;
<br />
fclose &#40;$fd&#41;;
<br />
echo &quot;done, &quot;.sizeof&#40;$itemtable&#41;.&quot; items read\n&quot;;
<br />
//----------------------------------------------
<br />
// open the data file and write the results
<br />
echo &quot;examining and writing results....&quot;;
<br />
$myfile = fopen&#40;&quot;banktracker_results&#40;UBB_CODE&#41;.txt&quot;,&quot;a+&quot;&#41;;
<br />
$fd = fopen &#40;&quot;myinventory.txt&quot;, &quot;r&quot;&#41;;
<br />
while &#40;!feof &#40;$fd&#41;&#41; 
<br />
&#123;
<br />
&nbsp; &nbsp; $line_of_data=fgets&#40;$fd, 4096&#41;;
<br />
&nbsp; &nbsp; list&#40;$possition,$lowid,$highid,$ql&#41; = explode&#40;&quot; - &quot;,$line_of_data,4&#41;;
<br />
&nbsp; &nbsp; if &#40;$lowid&gt;1000 &amp;&amp; $highid&gt;1000 &amp;&amp; $possition&gt;0&#41;
<br />
&nbsp; &nbsp; &#123;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; $ql=trim&#40;$ql&#41;+0;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; $LowValue =$itemtable&#91;$lowid&#93;&#91;&quot;value&quot;&#93;;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; $LowQL&nbsp; &nbsp; =$itemtable&#91;$lowid&#93;&#91;&quot;ql&quot;&#93;;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; $HighValue=$itemtable&#91;$highid&#93;&#91;&quot;value&quot;&#93;;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; $HighQL&nbsp; &nbsp;=$itemtable&#91;$highid&#93;&#91;&quot;ql&quot;&#93;;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; if &#40;$LowQL&gt;$HighQL&#41;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &#123;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $tempvalue=$LowValue;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $tempQL=$LowQL;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $LowValue =$HighValue;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $LowQL&nbsp; &nbsp; =$HighQL;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $HighValue=$tempvalue;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $HighQL&nbsp; &nbsp;=$tempQL;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &#125;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; if&#40;$LowQL==$HighQL&#41;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $value_interpolated=$HighValue;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; else
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $value_interpolated = round&#40;$LowValue + &#40;$HighValue - $LowValue&#41; / &#40;$HighQL - $LowQL+0.00000001&#41; * &#40;$ql - $LowQL&#41;, 0&#41;;
<br />

<br />
&nbsp; &nbsp; &nbsp; &nbsp; if &#40;abs&#40;$itemtable&#91;$lowid&#93;&#91;&quot;ql&quot;&#93;-$ql&#41;&gt;abs&#40;$itemtable&#91;$highid&#93;&#91;&quot;ql&quot;&#93;-$ql&#41;&#41;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $name=$itemtable&#91;$lowid&#93;&#91;&quot;name&quot;&#93;;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; else
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $name=$itemtable&#91;$highid&#93;&#91;&quot;name&quot;&#93;;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; $resultline=sprintf&#40;&quot;QL %s &#91;url=http&#58;//aodb.info/showitem.asp?LowID=%d&amp;HiID=%d&amp;QL=%d&#93;%s&#91;/url&#93; &#40;slot %s&#41; &#40;store price is %d&#41;&quot;,$ql,$lowid,$highid,$ql,$name,$possition,$value_interpolated&#41;;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; $resultline.=&quot;\n&quot;;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; $fp = fwrite&#40;$myfile,$resultline&#41;;
<br />
&nbsp; &nbsp; &#125;
<br />
&nbsp; &nbsp; elseif&#40;!strstr&#40;$line_of_data,&quot;Position - HighID - LowID - QL&quot;&#41;&#41;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $fp = fwrite&#40;$myfile,&quot;$line_of_data&quot;&#41;;
<br />
&#125;
<br />
fclose &#40;$fd&#41;;
<br />
fclose&#40;$myfile&#41;;
<br />
echo &quot;done\n&quot;;
<br />
?&gt;</td>	</tr></table><span class="postbody">
<br />

<br />

<br />

<br />

<br />
here is an example result:
<br />
</span><table width="90%" cellspacing="1" cellpadding="3" border="0" align="center"><tr> 	  <td><span class="genmed"><b>Code:</b></span></td>	</tr>	<tr>	  <td class="code">
<br />
Backpack Contents 
<br />
QL 1 &#91;url=http&#58;//aodb.info/showitem.asp?LowID=205841&amp;HiID=205841&amp;QL=1&#93;Funny Arrow&#91;/url&#93; &#40;slot 1&#41; &#40;store price is 500000&#41;
<br />
QL 1 &#91;url=http&#58;//aodb.info/showitem.asp?LowID=205839&amp;HiID=205839&amp;QL=1&#93;Karlsson Propellor Cap&#91;/url&#93; &#40;slot 2&#41; &#40;store price is 500000&#41;
<br />
QL 1 &#91;url=http&#58;//aodb.info/showitem.asp?LowID=21605&amp;HiID=21605&amp;QL=1&#93;Bullets&#91;/url&#93; &#40;slot 4&#41; &#40;store price is 25&#41;
<br />
QL 1 &#91;url=http&#58;//aodb.info/showitem.asp?LowID=29738&amp;HiID=29738&amp;QL=1&#93;Night Vision Goggles&#91;/url&#93; &#40;slot 6&#41; &#40;store price is 250&#41;
<br />
QL 38 &#91;url=http&#58;//aodb.info/showitem.asp?LowID=163949&amp;HiID=163950&amp;QL=38&#93;Nova Dillion Armor Helmet&#91;/url&#93; &#40;slot 8&#41; &#40;store price is 133325&#41;
<br />
QL 1 &#91;url=http&#58;//aodb.info/showitem.asp?LowID=21609&amp;HiID=21609&amp;QL=1&#93;Energy Weapon Ammo&#91;/url&#93; &#40;slot 9&#41; &#40;store price is 50&#41;
<br />
