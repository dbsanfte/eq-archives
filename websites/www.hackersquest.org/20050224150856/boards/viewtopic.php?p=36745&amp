preg_replace&#40;'#&lt;a href=&#91;\'&quot;&#93;itemref&#58;//&#40;&#91;0-9&#93;+&#41;/&#40;&#91;0-9&#93;+&#41;/&#40;&#91;0-9&#93;+&#41;&#91;\'&quot;&#93;&gt;&#40;.?&#41;&lt;/a&gt;#i', '\1', $words&#41;; 
