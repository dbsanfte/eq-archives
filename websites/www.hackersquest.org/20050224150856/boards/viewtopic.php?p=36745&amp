$idn&#91;205844&#93; = &quot;Karlsson Propellor Cap&quot;;
<br />
$idn&#91;205843&#93; = &quot;Monster Sunglasses&quot;;
<br />
$idn&#91;205842&#93; = &quot;Funny Arrow&quot;;
<br />

<br />
&#125;
<br />

<br />
/// setting up conversion end.
<br />

<br />
$iteminfo = &quot;Low ID&#58; &quot;.$itemlid.&quot; / High ID&#58; &quot;.$itemhid.&quot; - QL&#58; &quot;.$itemql.&quot; - Result&#58; &quot;;
<br />
$itemresult = &quot;Unknown item - not a Biomaterial.&quot;;
<br />
if &#40;!empty&#40;$idn&#91;$itemlid&#93;&#41;&#41; &#123; $itemresult = $iteminfo.$idn&#91;$itemlid; &#125;
<br />
send&#40;$sender,$itemresult&#41;; 
<br />
$noreply = 1; 
<br />

<br />
?&gt; 
<br />
</td>	</tr></table><span class="postbody">
