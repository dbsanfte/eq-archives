<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; $output = &quot;buddy status&#58; $args&#91;0&#93;\t$temp\t&quot;;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; if &#40;$args&#91;2&#93;==1&#41;
<br />
