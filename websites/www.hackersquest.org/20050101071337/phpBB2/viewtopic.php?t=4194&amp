#### In helpbotfunctions.php add:
<br />

<br />
</span><table width="90%" cellspacing="1" cellpadding="3" border="0" align="center"><tr> 	  <td><span class="genmed"><b>Code:</b></span></td>	</tr>	<tr>	  <td class="code">
<br />
// Dueling
<br />

<br />
function shield_colour &#40;$shield&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;switch &#40;$shield&#41;
<br />
&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;case 0&#58;&nbsp; &nbsp;return &#40;&quot;None&quot;&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;case 1&#58;&nbsp; &nbsp;return &#40;chr&#40;16&#41;.chr&#40;24&#41;.&quot;Red&quot;.chr&#40;16&#41;.chr&#40;15&#41;&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;case 2&#58;&nbsp; &nbsp;return &#40;chr&#40;16&#41;.chr&#40;13&#41;.&quot;Green&quot;.chr&#40;16&#41;.chr&#40;15&#41;&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;case 3&#58;&nbsp; &nbsp;return &#40;chr&#40;16&#41;.chr&#40;14&#41;.&quot;Blue&quot;.chr&#40;16&#41;.chr&#40;15&#41;&#41;;
<br />
&nbsp; &nbsp;&#125;
<br />
&#125;
<br />

<br />

<br />
function duel_status &#40;$you, $opponent, $groupreply&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;global $duels;
<br />

<br />
&nbsp; &nbsp;if &#40;$groupreply==&quot;Y&quot;&#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$reply=&quot;\n&quot;.chr&#40;16&#41;.chr&#40;15&#41;.&quot;&lt;&lt;&lt;&lt;Shields&#58; &quot;.ucfirst&#40;$you&#41;.&quot;&#58; &quot;;
<br />
&nbsp; &nbsp;else
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$reply=&quot;\n&quot;.chr&#40;16&#41;.chr&#40;15&#41;.&quot;&lt;&lt;&lt;&lt;Shields&#58; You&#58; &quot;;
<br />

<br />
&nbsp; &nbsp;$reply=$reply.shield_colour&#40;$duels&#91;$you&#93;&#91;&quot;shields&quot;&#93;&#41;;
<br />
&nbsp; &nbsp;$reply=$reply.&quot; / &quot;.ucfirst&#40;$opponent&#41;.&quot;&#58; &quot;;
<br />
&nbsp; &nbsp;$reply=$reply.shield_colour&#40;$duels&#91;$opponent&#93;&#91;&quot;shields&quot;&#93;&#41;;
<br />
&nbsp; &nbsp;$reply=$reply.&quot;&gt;&gt;&gt;&gt;&quot;;
<br />
&nbsp; &nbsp;return &#40;$reply&#41;;
<br />
&#125;
<br />

<br />

<br />
function process_duel &#40;$sender, $word2&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;global $duels;
<br />
&nbsp; &nbsp;global $aoc;
<br />
&nbsp; &nbsp;global $myguild;
<br />
&nbsp; &nbsp;global $myname;
<br />
&nbsp; &nbsp;global $tournament;
<br />

<br />
&nbsp; &nbsp;$sender = strtolower&#40;$sender&#41;;
<br />
&nbsp; &nbsp;$timenow = gettimeofday&#40;&#41;;
<br />
&nbsp; &nbsp;$opponent=$duels&#91;$sender&#93;&#91;&quot;opponent&quot;&#93;;
<br />
&nbsp; &nbsp;$duels&#91;$sender&#93;&#91;&quot;lasttell&quot;&#93;=$timenow&#91;'sec'&#93;;
<br />

<br />
&nbsp; &nbsp;if &#40;$opponent!=&quot;&quot;&#41;
<br />
&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$timepassed = $timenow&#91;'sec'&#93; - $duels&#91;$opponent&#93;&#91;&quot;lasttell&quot;&#93;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;if &#40;$timepassed &gt; 30&#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if &#40;$timepassed &lt; 60&#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$secstogo=60-$timepassed;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$aoc-&gt;send_tell&#40;$opponent,&quot;You will forfeit Grid Duel in &quot;.$secstogo.&quot;s if you do not respond.&quot;.&quot;\n&quot;&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;else
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$aoc-&gt;send_tell&#40;$opponent,&quot;You have forfeited your Grid Duel with &quot;.ucfirst&#40;$sender&#41;.&quot;.\n&quot;&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$duels&#91;$sender&#93;&#91;&quot;challenged&quot;&#93;=&quot;&quot;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$duels&#91;$sender&#93;&#91;&quot;opponent&quot;&#93;=&quot;&quot;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$duels&#91;$sender&#93;&#91;&quot;shields&quot;&#93;=3;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$duels&#91;$sender&#93;&#91;&quot;move&quot;&#93;=&quot;&quot;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$duels&#91;$opponent&#93;&#91;&quot;challenged&quot;&#93;=&quot;&quot;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$duels&#91;$opponent&#93;&#91;&quot;opponent&quot;&#93;=&quot;&quot;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$duels&#91;$opponent&#93;&#91;&quot;shields&quot;&#93;=3;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$duels&#91;$opponent&#93;&#91;&quot;move&quot;&#93;=&quot;&quot;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;return &#40;ucfirst&#40;$opponent&#41;.&quot; forfeited Grid Duel due to slow response.&quot;&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;&#125;
<br />

<br />
&nbsp; &nbsp;if &#40;$word2==&quot;hit&quot; || $word2==&quot;block&quot; || $word2==&quot;smash&quot;&#41;
<br />
&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;if &#40;$opponent!=&quot;&quot; &amp;&amp; $duels&#91;$opponent&#93;&#91;&quot;opponent&quot;&#93;==$sender&#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$omove=$duels&#91;$opponent&#93;&#91;&quot;move&quot;&#93;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;echo &quot;Opponent's move=&quot;.$omove.&quot;\n&quot;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if &#40;$omove!=&quot;&quot;&#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if &#40;$word2==&quot;block&quot;&#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if &#40;$omove==&quot;block&quot;&#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$reply=&quot;Both combatants block!&nbsp; Shields regenerate.&quot;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$oreply=$reply;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$greply=$reply;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if &#40;$duels&#91;$sender&#93;&#91;&quot;shields&quot;&#93; &lt; 3&#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$duels&#91;$sender&#93;&#91;&quot;shields&quot;&#93;++;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if &#40;$duels&#91;$opponent&#93;&#91;&quot;shields&quot;&#93; &lt; 3&#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$duels&#91;$opponent&#93;&#91;&quot;shields&quot;&#93;++;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&#125;
<br />
