function writedocs&#40;$filename&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;global $docs;
<br />
&nbsp; &nbsp;$myfile = fopen&#40;$filename,&quot;w&quot;&#41;;
<br />
&nbsp; &nbsp;foreach &#40;$docs as $doc =&gt; $doctext&#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$fp = fwrite&#40;$myfile,&quot;$doc\t$doctext\n&quot;&#41;;
<br />
&nbsp; &nbsp;fclose&#40;$myfile&#41;;
<br />
&#125;
<br />

<br />

<br />
function getdocs&#40;$filename&#41;
<br />
&#123;
<br />
&nbsp; &nbsp;global $docs;
<br />
&nbsp; &nbsp;$fd = fopen &#40;$filename, &quot;r&quot;&#41;;
<br />
&nbsp; &nbsp;while &#40;!feof &#40;$fd&#41;&#41; &#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;list&#40;$doc,$doctext&#41; = explode&#40;&quot;\t&quot;,fgets&#40;$fd, 8192&#41;,2&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;if &#40;strlen&#40;$doc&#41;&gt;0 &amp;&amp; strlen&#40;$doctext&#41;&gt;0&#41;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$docs&#91;&quot;$doc&quot;&#93;=trim&#40;$doctext&#41;;
<br />
&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;fclose &#40;$fd&#41;;
<br />
&#125;
