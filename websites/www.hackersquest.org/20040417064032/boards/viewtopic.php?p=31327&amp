<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$blob=str_replace&#40;&quot;&amp;nbsp;&quot;,&quot; &quot;,$blob&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$blob=str_replace&#40;&quot;&amp;lt;&quot;,&quot;&lt;&quot;,$blob&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$blob=str_replace&#40;&quot;&amp;gt;&quot;,&quot;&gt;&quot;,$blob&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$rtopic=str_replace&#40;&quot;&amp;amp;&quot;,&quot;&amp;&quot;,$rtopic&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$rtopic=str_replace&#40;&quot;&amp;quot;&quot;,&quot;''&quot;,$rtopic&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$rtopic=str_replace&#40;&quot;&amp;nbsp;&quot;,&quot; &quot;,$rtopic&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$rtopic=str_replace&#40;&quot;&amp;lt;&quot;,&quot;&lt;&quot;,$rtopic&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$rtopic=str_replace&#40;&quot;&amp;gt;&quot;,&quot;&gt;&quot;,$rtopic&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;echo &quot;&#91;PHPBB&#93; &#58;&#58; &quot;.$fsender.&quot; reading forums &#58;&#58;\r\n&quot;;
