&nbsp; &nbsp;&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;else
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;echo &quot;&#91;PHPBB&#93; &#58;&#58; &quot;.$fsender.&quot; failed reading data &#58;&#58;\r\n&quot;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$rtopic=&quot;&#40;!whi!&#41;Forum Error &#58;&#58; &#40;!gr!&#41;Data missing or timed out, try again.&quot;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$rtopic=str_replace&#40;&quot;&#40;!oran!&#41;&quot;,$fcolor5,$rtopic&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;$rtopic=str_replace&#40;&quot;&#40;!whi!&#41;&quot;,$fcolor1,$rtopic&#41;;
