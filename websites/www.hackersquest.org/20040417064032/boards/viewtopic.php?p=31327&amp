&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$fpages=&quot;\r\n&#40;!whi!&#41;Pages &#40;!bl!&#41;&#40;&#40;!bl!&#41;&quot;.$flastpage.&quot;&#41; &#40;!whi!&#41;&#58;&#58; &quot;.$fpages.&quot;\r\n&quot;;
