<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$blob=str_replace&#40;&quot;&#91;!!L!!&#93;&quot;,'#L &quot;&#40;!gr!&#41;#L&quot; &quot;&quot;',$blob&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$blob=str_replace&#40;&quot;&#91;!L!&#93;&quot;,&quot;#L&quot;,$blob&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$blob=str_replace&#40;&quot;&#91;!S!&#93;&quot;,&quot;#&quot;,$blob&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$blob=str_replace&#40;&quot;&amp;amp;&quot;,&quot;&amp;&quot;,$blob&#41;;
