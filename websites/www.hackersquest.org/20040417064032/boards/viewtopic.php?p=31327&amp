&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&#123;
<br />

<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$blob=str_replace&#40;&quot;\r&quot;,&quot;&quot;,$blob&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$bloblines=explode&#40;&quot;\n&quot;,$blob&#41;;
