&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$blob=str_replace&#40;&quot;\r\n&#40;!bl!&#41;Joined&#58;&quot;,&quot; - &#40;!bl!&#41;Joined&#58;&quot;,$blob&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$blob=str_replace&#40;&quot;\r\n&#40;!bl!&#41;Posts&#58;&quot;,&quot; - &#40;!bl!&#41;Posts&#58;&quot;,$blob&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;$blob=str_replace&#40;'&amp;amp;',&quot;&amp;&quot;,$blob&#41;;
