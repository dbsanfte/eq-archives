				<td colspan="2"><span class="postbody"></span><table width="90%" cellspacing="1" cellpadding="3" border="0" align="center"><tr> 	  <td><span class="genmed"><b>Code:</b></span></td>	</tr>	<tr>	  <td class="code">from string import * 
<br />
import xml.parsers.expat 
<br />
import httplib 
<br />
import urllib 
<br />
#Code and help contributed by syf and #python on irc.openprojects.net 
<br />
#Do not forget to edit the final line to the right directory 
<br />
path='' 
<br />

<br />
class configtest&#58; 
<br />
&nbsp; &nbsp; def __init__ &#40;self, file&#41;&#58; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; self.file = file 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; self.xml_load &#40;&#41; 
<br />

<br />
&nbsp; &nbsp; def xml_load &#40;self&#41;&#58; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; p = xml.parsers.expat.ParserCreate &#40;&#41; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; p.StartElementHandler = self.xml_start 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; p.EndElementHandler = self.xml_end 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; p.CharacterDataHandler = self.xml_data 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; f = open &#40;self.file, 'r'&#41; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; text = f.read &#40;&#41; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; f.close &#40;&#41; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; p.Parse &#40;text&#41; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; del p 
<br />

<br />
&nbsp; &nbsp; def xml_start &#40;self, name, attrs&#41;&#58; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; global path 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; if name == 'Directory'&#58; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; path= &quot;http&#58;//patch.everquest.com&#58;7000/&quot; + attrs&#91;'RemotePath'&#93; + &quot;/&quot; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; urllib.urlopen&#40;path&#41; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; if name == 'File'&#58; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; print attrs&#91;'Name'&#93;, attrs&#91;'Name'&#93;&#91;&#58;-3&#93; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; urllib.urlretrieve&#40;path + attrs&#91;'Name'&#93;, attrs&#91;'Name'&#93;&#91;&#58;-3&#93;&#41; 
<br />

<br />

<br />
&nbsp; &nbsp; def xml_end &#40;self, name&#41;&#58; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; pass 
<br />

<br />
&nbsp; &nbsp; def xml_data &#40;self, data&#41;&#58; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; if data != ''&#58; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; if data != '\n'&#58; 
<br />
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; print 'xml data&#58;', data 
<br />

<br />
test = configtest &#40;'C&#58;\EQ\everquest-changes.xml'&#41; </td>	</tr></table><span class="postbody">
