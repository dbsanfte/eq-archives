<!--EZCODE CODE START--><pre> 
Template&lt;bool YourApathy, unsigned int Others&gt;
class Employer
{
public:
   Employer()
   {  
       _apathy( YourApathy );
       _othersmuckstoworkforyou( others );
   }
   virtual bool getapathy() {return _apathy;}
   virtual void PlayEverquest();
   virtual void talkAboutEverquest();
private:
   bool _apathy;
   unsigned int _othersmuckstoworkforyou;
}

unsigned Char *Pneumatic;

pneumatic = new( Employer&lt;true, 7&gt; );

if ( pneumatic-&gt;GetApathy() )
{
   Int n=rnd(1);
   if ( n == 1 )
   {
      pneumatic-&gt;playEverquest();
   }
   else
   {
      pneumatic-&gt;talkAboutEverquest();
   }

}
</pre><!--EZCODE CODE END--><br>
