function display_title() {
date = new Date();
var hours = date.getHours(); 
var message = '';

 if (hours >= 0) { time_of_day = '<b>Good morning</b>,'; message='Morning Message';}
 if ((hours >= 12) && (hours < 17)) { time_of_day = '<b>Good afternoon</b>,'; message='Afternoon Message'}
 if (hours >= 17) { time_of_day = '<b>Good evening</b>,'; message='Evening Message';} 

document.write('' + time_of_day + '');

 
   }
