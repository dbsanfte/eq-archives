var preloadFlag = false;
function preloadImages() {
	if (document.images) {
		over_profile = newImage(/*URL*/'templates/Charcoal2/images/profile_on.jpg');
		over_faq = newImage(/*URL*/'templates/Charcoal2/images/faq_on.jpg');
