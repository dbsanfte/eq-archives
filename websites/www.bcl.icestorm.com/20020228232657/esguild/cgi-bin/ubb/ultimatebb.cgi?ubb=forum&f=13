document.writeln('<IMG SRC="http://bcl.icestorm.com/esguild/ubb/yellockfolder.gif" BORDER="0">');
} else if (fiery == "yes") {
document.writeln('<IMG SRC="http://bcl.icestorm.com/esguild/ubb/hotclosed.gif" BORDER="0">');
} else {
document.writeln('<IMG SRC="http://bcl.icestorm.com/esguild/ubb/closed.gif" BORDER="0">');
}
}
//-->
</SCRIPT>

</TD>

<!-- icons column -->
