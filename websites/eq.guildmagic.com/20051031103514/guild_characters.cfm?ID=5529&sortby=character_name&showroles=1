<script>
// Make this your homepage only works on Windows IE
var detect = navigator.userAgent.toLowerCase();
var OS,browser,version,total,thestring;

function checkIt(string) {
	place = detect.indexOf(string) + 1;
	thestring = string;
	return place;
}

function addFooterLinks() { // adds links to make homepage and bookmark page
	if (checkIt('msie') && checkIt('win')) {
		document.write("&nbsp;&nbsp;|&nbsp;&nbsp;<span class=\"footerLink\" style=\"text-decoration:underline;cursor:pointer\" onClick=\"this.style.behavior='url(#default#homepage)';this.setHomePage('http://www.guildmagic.com')\">Make This My Homepage</span>&nbsp;&nbsp;|&nbsp;&nbsp;<a class=\"footerLink\" href=\"javascript:void window.external.AddFavorite('http://www.guildmagic.com', 'guildmagic.com')\">Bookmark This Page</a>");
	}	
}

function addD2RLinks() { // adds links to make homepage and bookmark page
	if (checkIt('msie') && checkIt('win')) {
		document.write("<span class=\"topLeftLinks\" onClick=\"this.style.behavior='url(#default#homepage)';this.setHomePage('http://www.guildmagic.com')\">Make This My Homepage</span>&nbsp;&nbsp;|&nbsp;&nbsp;<a href=\"javascript:void window.external.AddFavorite('http://www.guildmagic.com', 'guildmagic.com')\">Bookmark This Page</a>");
	}	
}
