document.links[k].innerHTML="<img src='http://www.msterious.com/flame.gif' width='15' height='15' border=0> Reaper"}

if(document.links[k].href=="http://pub178.ezboard.com/umszoe.showPublicProfile") {
document.links[k].innerHTML="<img src='http://www.msterious.com/transform.gif' width='15' height='15' border=0> Zoe"}


}
}
</script>

