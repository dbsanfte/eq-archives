        ezPub = ezURL[1];
        ezIsBoard = ezURL[2] == "b" ? true : false;
        ezBoardID = ezURL.length > 3 ? ezURL[3]: "";
        if ( ezBoardID.indexOf("frm") != -1 ) {
                ezForum = ezBoardID.substr(ezBoardID.indexOf("frm"));
                ezBoardID = ezBoardID.replace(ezForum,"");
                } else { 
