	<td bgcolor="#A8B8B0"><font color="#FFFFFF">Race:</td><td><font color="#0d014d">Wood Elf</td>
	<td bgcolor="#A8B8B0"><font color="#FFFFFF">Class:</td><td><font color="#0d014d">Druid</td>
	<td bgcolor="#A8B8B0"><font color="#FFFFFF">Gender:</td><td><font color="#0d014d">Male</td>
