<tr valign="top">
	<td class="alt2" width="175">
			
			<div id="postmenu_118571">
				
				<a class="smallusername" href="member.php?s=bfd40fb5873659b707853dddadbb7a84&amp;u=443">Shadowfrost</a>
				<script type="text/javascript"> vbmenu_register("postmenu_118571", true); </script>
				
			</div>
			
			<div class="smallfont"><img src="http://thedruidsgrove.org/forums/images/buttons/guardian.gif"></div>
			
		
			
				<div class="smallfont">
					&nbsp;<br /><a href="member.php?s=bfd40fb5873659b707853dddadbb7a84&amp;u=443"><img src="image.php?s=bfd40fb5873659b707853dddadbb7a84&amp;u=443&amp;dateline=1087744555" alt="Shadowfrost's Avatar" border="0" /></a>
