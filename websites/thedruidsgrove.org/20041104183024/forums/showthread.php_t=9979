<tr valign="top">
	<td class="alt2" width="175">
			
			<div id="postmenu_145846">
				
				<a class="smallusername" href="member.php?s=16f4e6b07aa8f1f8dced5a21e694db34&amp;u=1052">Tappin</a>
				<script type="text/javascript"> vbmenu_register("postmenu_145846", true); </script>
				
			</div>
			
			<div class="smallfont">Tested in Battle</div>
			
		
			
			
			<div class="smallfont">
				&nbsp;<br />
				<div>Join Date: Oct 2003</div>
