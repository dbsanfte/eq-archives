</p><i></i></td></tr><tr bgcolor=#000000><td nowrap width=20% valign=top align=left><span class=title> <A HREF=http://pub78.ezboard.com/uthallas.showPublicProfile?language=EN>Thallas</A></span><br> <span class=usertitle>Adventurer</span><br> <font color=#00F500>(2/19/02 11:10:19 am)</font><br>   <a href=http://pub78.ezboard.com/fwalkersofvazaellefrm4.showAddReplyScreenFromWeb?topicID=489.topic&index=7>Reply</a>   </td><td valign=top align=left class=m><span class=title> Re: Ctrl-V</span><hr size=1> Function ValidroutingNumber(strRoutingNumber As String) As Boolean<br>
<br>
  Dim intMultiplier As Variant<br>
  Dim blnReturnValue As Boolean<br>
  Dim lngTotal As Long<br>
  Dim lngProduct As Long<br>
  Dim n As Integer<br>
  Dim intCheckDigit As Integer<br>
  <br>
  intMultiplier = Array(3, 7, 1, 3, 7, 1, 3, 7)<br>
  <br>
  If Len(Trim(strRoutingNumber)) &lt;&gt; 9 Or Not IsNumeric(strRoutingNumber) Then<br>
     blnReturnValue = False<br>
  Else<br>
     lngTotal = 0<br>
     For n = 1 To 8<br>
         lngProduct = Mid(strRoutingNumber, n, 1) * intMultiplier(n - 1)<br>
         lngTotal = lngTotal + lngProduct<br>
     Next n<br>
     <br>
     If (lngTotal Mod 10) = 0 Then<br>
        intCheckDigit = 0<br>
     Else<br>
        intCheckDigit = (10 - (lngTotal Mod 10))<br>
     End If<br>
     <br>
     If intCheckDigit = Val(Mid(strRoutingNumber, 9, 1)) Then<br>
        blnReturnValue = True<br>
     Else<br>
        blnReturnValue = False<br>
     End If<br>
 End If<br>
 <br>
 ValidroutingNumber = blnReturnValue<br>
  <br>
End Function<br>
