if spell &gt; 0 &#123;
<br />
&nbsp; // do whatever is common among spells and items with effects here 
<br />
&nbsp; if group != 20 &#123;
<br />
&nbsp; &nbsp;// do other item /w effect stuff here
<br />
&nbsp; &nbsp; if effect != 0 or effect !=2 &#123;
<br />
&nbsp; &nbsp; &nbsp; if capacity &gt; 0 &#123;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; casting_time = capacity / 1000
<br />
&nbsp; &nbsp; &nbsp; &#125;
<br />
&nbsp; &nbsp; &nbsp; else &#123;
<br />
&nbsp; &nbsp; &nbsp; &nbsp; casting_time = &quot;instant&quot;
<br />
&nbsp; &nbsp; &nbsp; &#125;
<br />
&nbsp; &nbsp; &#125;
<br />
&nbsp; &#125;
<br />
&nbsp; else &#123;
<br />
&nbsp; &nbsp; // do stuff for spells here
<br />
&nbsp; &#125;
<br />
&#125;
<br />

<br />
</td>	</tr></table><span class="postbody">
<br />

