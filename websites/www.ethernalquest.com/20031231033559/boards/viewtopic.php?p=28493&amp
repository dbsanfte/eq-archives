Public Function calcValue&#40;lval As String, hval As String, diff As Long, ql As Long&#41; As String
<br />
&nbsp; &nbsp; If hval = lval Then
<br />
&nbsp; &nbsp; &nbsp; &nbsp; calcValue = hval
<br />
&nbsp; &nbsp; Else
<br />
&nbsp; &nbsp; &nbsp; &nbsp; calcValue = CStr&#40;CLng&#40;&#40;&#40;hval - lval&#41; / diff&#41; * ql&#41; + CLng&#40;lval&#41;&#41;
<br />
&nbsp; &nbsp; End If
<br />
End Function
<br />
