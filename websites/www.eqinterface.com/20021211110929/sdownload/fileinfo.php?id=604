  <tr><td width="20%" bgcolor="#DFDFDF"><font face="verdana,arial,helvetica" size="1" ><b>File Name:</b></font></td>
  <td bgcolor="#DFDFDF"><a href="http://www.eqinterface.com/sdownload/download.php?id=604" target="picture"><font face="verdana,arial,helvetica" size="1" >AYAUI_500.zip</a></font>   </td></tr>
