document.writeln('<IMG SRC="http://www.eqatlas.com/ubb/hotclosedb.gif" BORDER="0">');
} else {
document.writeln('<IMG SRC="http://www.eqatlas.com/ubb/closedb.gif" BORDER="0">');
}
}  else {
if (closed_topic == "X") {
document.writeln('<IMG SRC="http://www.eqatlas.com/ubb/yellockfolder.gif" BORDER="0">');
} else if (fiery == "yes") {
document.writeln('<IMG SRC="http://www.eqatlas.com/ubb/hotclosed.gif" BORDER="0">');
} else {
document.writeln('<IMG SRC="http://www.eqatlas.com/ubb/closed.gif" BORDER="0">');
}
}
//-->
</SCRIPT>

</TD>

