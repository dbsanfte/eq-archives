<script language="javascript">
function MakeArray() {
   this.length = MakeArray.arguments.length
   for (var i = 0; i < this.length; i++)
   this[i+1] = MakeArray.arguments[i]
}

var url = new MakeArray("","http://www.kaaos.com/cgi-bin/dcforum/dcboard.cgi",
"http://www.kaaos.com/cgi-bin/dcforum/dcboard.cgi?conf=DCConfID1",
"http://www.kaaos.com/cgi-bin/dcforum/dcboard.cgi?az=list&forum=DCForumID1",
