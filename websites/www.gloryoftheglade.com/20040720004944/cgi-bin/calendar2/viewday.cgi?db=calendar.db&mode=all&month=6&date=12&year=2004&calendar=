     months = new Array(12)
     months[1] = "January";
     months[2] = "February";
     months[3] = "March";
     months[4] = "April";
     months[5] = "May";
     months[6] = "June";
     months[7] = "July";
     months[8] = "August";
     months[9] = "September";
     months[10] = "October"; 
     months[11] = "November";
     months[12] = "December";
     today = new Date(); 
     month = months[today.getMonth() + 1]
     date = today.getDate()
     year=today.getYear(); 
 if (year < 2000)
 year = year + 1900;
     document.write ("" + month + " " + date + ", " + year)
