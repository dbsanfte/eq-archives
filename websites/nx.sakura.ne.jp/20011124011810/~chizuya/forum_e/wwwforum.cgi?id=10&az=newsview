	var dtdd = dt.getDate();
	var dthh = dt.getHours();
	var dtnn = dt.getMinutes();
	var dtdp;

	if (dtyy < 99) {
		dtyy += 2000;
	} else if (dtyy < 1900) {
		dtyy += 1900;
	}
	if (dtmm < 10) {
		dtmm = "0" + dtmm;
