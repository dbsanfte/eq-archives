Here is a template in case you or anyone else wants to come up with a color scheme:<br><br>A:link, A:visited, A:active, A:hover { <br>}<br>A:link  { <br>  color: ; <br>}<br>A:visited { <br>  color: ;<br>}<br>A:active {<br>  color: ;<br>}<br>A:HOVER { <br>  color: ; <br>  background-color: ;<br>}<br>.onbody{<br>  color: ; <br>}<br>.onbody A:LINK {<br>  color: ;<br>}<br>.onbody A:VISITED {<br>  color: ;<br>}<br>.onbody A:ACTIVE {<br>  color: ;<br>}<br>BODY,P,TABLE,TD,TR {   <br>  font-size: ;<br>  font-family: ;<br>  color: ;<br>}<br>BODY {<br>  background: ;<br>}<br>PRE {<br>  font-size: ;<br>  font-family: ;<br>}<br>.tableborders {<br>  background: ;<br>}<br>.tdheader {<br>  font-size: ;<br>  font-weight: ;<br>  color: ;<br>  background: ;<br>  font-family: ;<br>}<br>.menubar{<br>  background: ;<br>  color: ;<br>}<br>.navigation{<br>  background: ;<br>  color: ;<br>  font-size: ;<br>  font-family: ;<br>}<br>.post:FIRST-LINE {<br>}<br>.post:FIRST-LETTER {<br>}<br>.new {<br>  font-size: ;<br>  color:     ;<br>}<br>.catandforum {<br>  font-size: ;<br>}<br>.posttime {<br>  font-size: ;<br>  font-style: ;<br>  text-align: ;<br>  background: ;<br>}<br>.threadtotal {<br>  font-size: ;<br>  background: ;<br>}<br>.posttotal {<br>  font-size: ;<br>  background: ;<br>  color: ;<br>}<br>.modcolumn {<br>  font-size: ;<br>  background: ;<br>  color: ;<br>}<br>.small {<br>  font-size: ;<br>  font-family: ;<br>}<br>.standouttext {<br>  color: ;<br>}<br>.pollcolor {<br>  background: ;<br>}<br>.welcome {<br>  background: ;<br>  font-family: ;<br>  color: ;<br>}<br>.forumtitle {<br>}<br>.forumdescript {<br>  font-size: ;<br>}<br>.lighttable {<br>  background: ;<br>  color: ;<br>}<br>.darktable {<br>  background: ;<br>  color: ;<br>}<br>.cleartable {<br>  background: ;<br>}<br>.alternatetable {<br>  background: ;<br>}<br>.subjecttable {<br>  background: ;<br>  color: ;<br>}<br>.footertable {<br>  background: ;<br>}                          <br>.formboxes{<br>  font-family: ;<br>  background-color: ;<br>  color: ;<br>  font-size: ;<br>}<br>.buttons {<br>  font-family: ;<br>  background-color: ;<br>  color: ;<br>  font-size: ;<br>}<br><br>    <br>    <br>    <br><br>     
