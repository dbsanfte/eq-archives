      <table width="100%" bgcolor="#ffffff" border="0" cellspacing="1" cellpadding="4">
        <tr> 
          <td bgcolor="#F1F1F1" rowspan="2" valign="top" nowrap width=120> <a name="post55706"></a> 
             <font face="verdana, arial, helvetica" size="1"><a href="javascript:void openWhoIamWindow('/forums/whoiam.php?id=941')"><b>Darury</b></a></font><br>
            <font face="verdana,arial,helvetica" size="1">Master<br></font><br>
            <!-- BEGIN TEMPLATE: postbit_avatar -->
<img src="http://images.berthall.com/avatars/default/00000175.gif" border="0" alt="">
<!-- END TEMPLATE: postbit_avatar --><font face="verdana,arial,helvetica" size="1"> </font>
<br><a href="http://www.berthall.com/forums/mod/karma.php?action=points&userid=941&postid=55706" target=_blank>Karma: 48</a></td>
          <td bgcolor="#F1F1F1" width="*" valign="top"> <font face="verdana,arial,helvetica" size="1"> 
            <table width="100%" border="0" cellspacing=" 0" cellpadding=" 0">
              <tr>
                <td><img src="images/posticon.gif" border="0" alt="Old Post"> <font face="verdana,arial,helvetica" size="1"><b>
</b></font></td>
                <td align="right" nowrap>&laquo; 06-11-2002 02:26 PM &raquo;</td>
              </tr>
