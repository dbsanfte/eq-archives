	var julians = new Array("2452394.0527", "2452394.0606", "2452394.0823", "2452394.1200", "2452394.1239", "2452394.1356", "2452394.1638", "2452394.2106", "2452407.2230", "2452408.0332", "2452408.0619", "2452408.0656", "2452408.1540", "2452408.1634", "2452408.1657", "2452408.1735", "2452408.1946", "2452409.1554", "2452409.1645", "2452409.1707", "2452409.1848", "2452410.0814", "2452410.0907", "2452410.1127", "2452410.1910", "2452410.1946");
	var posts = new Array("000000", "000001", "000002", "000003", "000004", "000005", "000006", "000007", "000008", "000009", "000010", "000011", "000012", "000013", "000014", "000015", "000016", "000017", "000018", "000019", "000020", "000021", "000022", "000023", "000024", "000025");
