	var replies = julians.length;

	function bluearrow () {

		var counter = 0;
		var baseurl = "http://forums.crgaming.com/cgi-bin/eq/ultimatebb.cgi?ubb=get_topic;f=9;t=007149";
		var shorturl = document.location.href;
		var p = 2;
		var url = "";

		var ploc = shorturl.indexOf("#");
		if(ploc > -1) {
			shorturl = shorturl.substr(0, ploc);
		} // end if

		while(counter < replies) {

			if(session_j > julians[counter]) {
				if((pages[counter] == 1) || (pages[counter] == p)) {
					url = shorturl + "#" + posts[counter];
				} else {
					url = baseurl + ";p=" + pages[counter] + "#" + posts[counter];
				}
				counter++;
				continue;
			} else {
				if(pages[counter] < 1) {
					continue;
				}
				if(pages[counter] == p) {
					url = shorturl + "#" + posts[counter];
				} else {
					url = baseurl + ";p=" + pages[counter] + "#" + posts[counter];
				}
				break;
			} // endif

		} // endwhile
		return url;

	} // endfunc

//-->
