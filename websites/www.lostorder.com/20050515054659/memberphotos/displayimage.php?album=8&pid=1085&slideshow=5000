					<script language="JavaScript" type="text/JavaScript">
// (C) 2000 www.CodeLifter.com
// http://www.codelifter.com
// Free for all users, but leave in this  header
// NS4-6,IE4-6
// Fade effect only in IE; degrades gracefully

// Set slideShowSpeed (milliseconds)
var slideShowSpeed = 5000
// Duration of crossfade (seconds)
var crossFadeDuration = 3

// Specify the image files
var Pic = new Array() // don't touch this
// to add more images, just continue
// the pattern, adding to the array below
Pic[0] = 'albums/userpics/10023/normal_aghundell1.jpg'
Pic[1] = 'albums/userpics/10023/normal_aghundell2.jpg'
Pic[2] = 'albums/userpics/10023/normal_aghundell3.jpg'
Pic[3] = 'albums/userpics/10023/normal_aghundell4.jpg'
Pic[4] = 'albums/userpics/10023/normal_blight1.jpg'
Pic[5] = 'albums/userpics/10023/normal_hz_guild_conclave.jpg'
Pic[6] = 'albums/userpics/10023/normal_loahouse.jpg'
Pic[7] = 'albums/userpics/10023/normal_loahouse1.jpg'
Pic[8] = 'albums/userpics/10023/normal_loahouse2.jpg'
Pic[9] = 'albums/userpics/10023/normal_loahouse3.jpg'
Pic[10] = 'albums/userpics/10023/normal_loahouse4.jpg'
Pic[11] = 'albums/userpics/10023/normal_loahouse5.jpg'
Pic[12] = 'albums/userpics/10023/normal_loahouse6.jpg'
Pic[13] = 'albums/userpics/10023/normal_loahouse7.jpg'
Pic[14] = 'albums/userpics/10023/normal_mah_house1.jpg'
Pic[15] = 'albums/userpics/10023/normal_mah_house10.jpg'
Pic[16] = 'albums/userpics/10023/normal_mah_house10%7E0.jpg'
Pic[17] = 'albums/userpics/10023/normal_mah_house2.jpg'
Pic[18] = 'albums/userpics/10023/normal_mah_house3.jpg'
Pic[19] = 'albums/userpics/10023/normal_mah_house4.jpg'
Pic[20] = 'albums/userpics/10023/normal_mah_house5.jpg'
Pic[21] = 'albums/userpics/10023/normal_mah_house6.jpg'
Pic[22] = 'albums/userpics/10023/normal_mah_house7.jpg'
Pic[23] = 'albums/userpics/10023/normal_mah_house8.jpg'
Pic[24] = 'albums/userpics/10023/normal_mah_house9.jpg'

var t
var j = 6
var p = Pic.length
var pos = j

var preLoad = new Array()

function preLoadPic(index)
{
	if (Pic[index] != ''){
		window.status='Loading : '+Pic[index]
		preLoad[index] = new Image()
		preLoad[index].src = Pic[index]
		Pic[index] = ''
		window.status=''
	}
}

function runSlideShow(){
   if (document.all){
    	document.images.SlideShow.style.filter="blendTrans(duration=2)"
		document.images.SlideShow.style.filter= "blendTrans(duration=crossFadeDuration)"
      document.images.SlideShow.filters.blendTrans.Apply()
	}
	document.images.SlideShow.src = preLoad[j].src
	if (document.all){
	   document.images.SlideShow.filters.blendTrans.Play()
	}
	
	pos = j

	j = j + 1
	if (j > (p-1)) j=0
	t = setTimeout('runSlideShow()', slideShowSpeed)
	preLoadPic(j)
}

function endSlideShow(){
	self.document.location = 'displayimage.php?album=8&pos='+pos
}

preLoadPic(j)

</script>
