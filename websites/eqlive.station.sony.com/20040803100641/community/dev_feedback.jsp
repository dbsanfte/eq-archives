// Makes sure any string representations are safely converted to objects.
function getObj(obj)
{
	if (typeof obj == "string")
	{
		theObj=eval(obj);
	}
	else
	{
		theObj=obj;
	}
	return theObj;
}

/*
	Clears the contents of a select box
*/
function clearSelect(selectObject)
{
	selectObject=getObj(selectObject);

	for (count=selectObject.options.length-1; count>0; count--)
	{
		selectObject.options[count]=null;
	}
}

/*
	 Takes a select box, clears it, then populates it 
	 with a list of elements (an array, populated with new Option(txt, val) )
	 
	 selectObject should be the actual object, not a sting
	 so use populateSelect(document.myForm.mySelect, optListA)
	 not populateSelect("document.myForm.mySelect", optListA)
	 That said, the code can handle the mistake.
*/
function populateSelect(selectObject, list)
{
	selectObject=getObj(selectObject);

	clearSelect(selectObject);

	modifier=0;
	if (list.length>1)
	{
		selectObject.options[0]=new Option("Please choose one....","Please choose one....");
		modifier=1;
	}

	for (count=0; count<list.length; count++)
	{
		selectObject.options[count+modifier]=list[count];
	}
	
	selectObject.selectedIndex=0;
}

function selectChanged(parentSelectObject, childSelectObject, list)
{
	parentSelectObject=getObj(parentSelectObject);
	childSelectObject=getObj(childSelectObject);
	
	parentValue=parentSelectObject.options[parentSelectObject.selectedIndex].value;
	childList=list[parentValue];
	
	populateSelect(childSelectObject, childList);
	
	setDefinition(parentValue);
}

function setDefinition(cat)
{
	if (navigator.userAgent.indexOf("MSIE")!=-1)
	{
		if (parseInt(navigator.appVersion)>=4)
		{
			if (descriptions[cat])
			{
				document.all.catDesc.innerHTML=descriptions[cat];
			}
		}
	}
}

if (document.mailform) {
	selectChanged(document.mailform.category, document.mailform.category2, menu);
}
