                return;
        }
        function pictureThis(from) {
                strSelection = document.selection.createRange().text
                if (strSelection == "") {
                        document.form1.message.focus()
                        strSelection = prompt("Enter the URL of the Image you wish to insert:","http://")
                        if (strSelection == null) return;
                        document.selection.createRange().text = "[img]" + strSelection + "[/img]"
                }
                else document.selection.createRange().text = "[img]" + strSelection + "[/img]"
                return;
        }
        function mail(from) {
                strSelection = document.selection.createRange().text
                if (strSelection == "") {
                        document.form1.message.focus()
                        strSelection = prompt("Enter the Email address to add","")
                        if (strSelection == null) return;
                                document.selection.createRange().text = "[email]" + strSelection + "[/email]"
                }
                else document.selection.createRange().text = "[email]" + strSelection + "[/email]"
                return;
        }
        function code(from) {
                strSelection = document.selection.createRange().text
                if (strSelection == "") {
                        document.form1.message.focus()
                        strSelection = prompt("Enter the code to insert","")
                        if (strSelection == null) return;
                                document.selection.createRange().text = "[code]" + strSelection + "[/code]"
                }
                else document.selection.createRange().text = "[code]" + strSelection + "[/code]"
                return;
        }
                function quotey(from) {
                strSelection = document.selection.createRange().text
                if (strSelection == "") {
                        document.form1.message.focus()
                        strSelection = prompt("Enter the text you want to Quote","")
                        if (strSelection == null) return;
                                document.selection.createRange().text = "[quote]" + strSelection + "[/quote]"
                }
                else document.selection.createRange().text = "[quote]" + strSelection + "[/quote]"
                return;
        }
        function italicThis(from) {
                strSelection = document.selection.createRange().text
                if (strSelection == "") {
                        document.form1.message.focus()
                        strSelection = prompt("Enter the text you want Italic","")
                        if (strSelection == null) return;
                                document.selection.createRange().text = "[i]" + strSelection + "[/i]"
                }
                else document.selection.createRange().text = "[i]" + strSelection + "[/i]"
                return;
        }
        </script>
