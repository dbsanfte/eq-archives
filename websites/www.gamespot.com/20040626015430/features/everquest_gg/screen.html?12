<!--begin split-->
<script language="JavaScript">
    function setCookie(name, value, hours) {
        if (!hours) hours = 24;
        var expdate = new Date();
        expdate.setTime(expdate.getTime() + hours*3600000);
        document.cookie = name + "=" + escape(value) + "; expires=" + expdate.toGMTString() + "; domain=.gamespot.com; path=/;";
        }

    function readCookie(name) {
        var cookiename = name + "=";
        var ca = document.cookie.split(';');
        for(var i=0;i < ca.length;i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1,c.length);
            if (c.indexOf(cookiename) == 0) return c.substring(cookiename.length,c.length);
            }
        return null;
        }
</script>

<script language="JavaScript">
    var now = new Date();
    var seed = now.getSeconds();
    var random_number = Math.random(seed);
    setCookie('gssplit', random_number, 10000);
</script>
<!--end split-->



