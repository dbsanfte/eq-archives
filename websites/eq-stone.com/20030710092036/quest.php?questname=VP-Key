var myMsg =  "/GUILDMOTD - ";
    myMsg += ": Keep going were almost there 44 Keyed for Emperor and more tonight! FARM FARM FARM!!!                    ";
var i = 0;
function scrollMsg() {
	frontPart = myMsg.substring(i,myMsg.length);
	backPart = myMsg.substring(0,i);
	document.thisform.thisbanner.value = frontPart + backPart;
	if (i < myMsg.length)
		{
		i++;
		}
	else
		{
		i = 0;
		}
	setTimeout("scrollMsg()", 150);
	}
