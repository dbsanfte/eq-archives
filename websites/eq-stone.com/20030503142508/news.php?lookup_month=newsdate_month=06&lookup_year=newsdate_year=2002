var myMsg =  "/GUILDMOTD - ";
    myMsg += "The Q: Welcome our new trial members for May!  Arsene!  Drevonor!  Ehgreiz!  Fistnoll!  Gias!  Saphira!  Scottpero!  Well done and good luck to you all!          ";
var i = 0;
function scrollMsg() {
	frontPart = myMsg.substring(i,myMsg.length);
	backPart = myMsg.substring(0,i);
	document.thisform.thisbanner.value = frontPart + backPart;
	if (i < myMsg.length)
		{
		i++;
		}
	else
		{
		i = 0;
		}
	setTimeout("scrollMsg()", 150);
	}
//-->


