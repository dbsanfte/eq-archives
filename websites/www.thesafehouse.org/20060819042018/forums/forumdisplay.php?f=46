var SESSIONURL = "s=1b7b7e0d5c4723c7d5456675aa9179d4&";
var IMGDIR_MISC = "images/misc";
// -->
function showSpoiler(block) {
   // by The Small One
   block.nextSibling.nextSibling.style.display ="block";
   block.parentNode.removeChild(block);
}
</script>

