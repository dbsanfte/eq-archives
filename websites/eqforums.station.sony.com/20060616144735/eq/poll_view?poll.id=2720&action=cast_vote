	function adminv3_show_help(el) {
		var isHeading = false;
		if (el.indexOf("H") == 0) {
			isHeading = true;
		}

		if (document.all) {
			var elImg = document.all[el + "IMG"];
			el = document.all[el];
			if (el != null) {
				el.style.display='';
				if (isHeading == true) {
					elImg.src = adminv3_help_heading_on;
				} else {
					elImg.src = adminv3_help_on;
				}
			}
		} else if (document.getElementById) {
			var elImg = document.getElementById[el + "IMG"];
			el = document.getElementById(el);
			if (el != null) {
				el.style.display='';
				if (isHeading == true) {
					elImg.src = adminv3_help_heading_on;
				} else {
					elImg.src = adminv3_help_on;
				}
			}
		}
	}

	function adminv3_hide_help(el) {
		var isHeading = false;
		if (el.indexOf("H") == 0) {
			isHeading = true;
		}

		if (document.all) {
			var elImg = document.all[el + "IMG"];
			el = document.all[el];
			if (el != null) {
				el.style.display='none';
				if (isHeading == true) {
					elImg.src = adminv3_help_heading_off;
				} else {
					elImg.src = adminv3_help_off;
				}
			}
		} else if (document.getElementById) {
			var elImg = document.getElementById[el + "IMG"];
			el = document.getElementById(el);
			if (el != null) {
				el.style.display='none';
				if (isHeading == true) {
					elImg.src = adminv3_help_heading_off;
				} else {
					elImg.src = adminv3_help_off;
				}
			}
		}
	}

