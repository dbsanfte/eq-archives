	var adminv3_help_on = "/i/adminv3/help_on.gif";
	var adminv3_help_off = "/i/adminv3/help_off.gif";
	var adminv3_help_heading_on = "/i/adminv3/help_heading_on.gif";
	var adminv3_help_heading_off = "/i/adminv3/help_heading_off.gif";

	<!--
	function adminv3_toggle_help(el) {
		var isHeading = false;
		if (el.indexOf("H") == 0) {
			isHeading = true;
		}

		if (document.all) {
			var elImg = document.all[el + "IMG"];
			el = document.all[el];
			
			if (el != null) {
				if (el.style.display == '') {
					el.style.display='none';
					if (isHeading == true) {
						elImg.src = adminv3_help_heading_off;
					} else {
						elImg.src = adminv3_help_off;
					}
				} else {
					el.style.display='';
					if (isHeading == true) {
						elImg.src = adminv3_help_heading_on;
					} else {
						elImg.src = adminv3_help_on;
					}
				}
			}
		} else if (document.layers) {
			var elImg = document.layers[el + "IMG"];
			el = document.layers[el];

			if (el != null) {
				if (el.display == '') {
					el.display='none';
					if (isHeading == true) {
						elImg.src = adminv3_help_heading_off;
					} else {
						elImg.src = adminv3_help_off;
					}
				} else {
					el.display='';
					if (isHeading == true) {
						elImg.src = adminv3_help_heading_on;
					} else {
						elImg.src = adminv3_help_on;
					}
				}
			}
		} else if (document.getElementById) {
			var elImg = document.getElementById[el + "IMG"];
			el = document.getElementById(el);
			
			if (el != null) {
				if (el.style.display == '') {
					el.style.display='none';
					if (isHeading == true) {
						elImg.src = adminv3_help_heading_off;
					} else {
						elImg.src = adminv3_help_off;
					}
