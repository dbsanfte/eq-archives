	<td bgcolor="#050A0E" width=1><img src="/i/p.gif" height=1 width=1 border=0 alt=""><br></td>
	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: One last glimpse of EQ's original magic

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bprogressionm16980_table" 
		
			onClick="setId('16980');showLiMenu(this,'io_bprogressionm16980','right',false);" 
			onDblClick="setId('16980');showLiMenu(this,'io_bprogressionm16980','right',false);" 
			
		>
		<tr class=clickable>
			
				<td nowrap><a class=subjectbar_text style='font-size: 8pt;'>Options</a></td>
				
			<td>&nbsp;&nbsp;</td>
