	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Additional PvP changes

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bdevcornerm8808_table" 
		
			onClick="setId('8808');showLiMenu(this,'io_bdevcornerm8808','right',false);" 
			onDblClick="setId('8808');showLiMenu(this,'io_bdevcornerm8808','right',false);" 
			
