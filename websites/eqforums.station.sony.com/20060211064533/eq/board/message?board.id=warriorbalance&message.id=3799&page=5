	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Discuss Class Re-Envisionment

			&nbsp; <nobr><span style="font-size: 8pt;">[ Edited ]</span></nobr> 
			
	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bwarriorbalancem4241_table" 
		
			onClick="setId('4241');showLiMenu(this,'io_bwarriorbalancem4241','right',false);" 
			onDblClick="setId('4241');showLiMenu(this,'io_bwarriorbalancem4241','right',false);" 
			
