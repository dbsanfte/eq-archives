	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Need better 50-55 xp spots

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bveteransm123_table" 
		
			onClick="setId('123');showLiMenu(this,'io_bveteransm123','right',false);" 
