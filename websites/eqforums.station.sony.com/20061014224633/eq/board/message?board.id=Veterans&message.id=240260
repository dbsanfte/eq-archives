	<td bgcolor="#050A0E" width=1><img src="/i/p.gif" height=1 width=1 border=0 alt=""><br></td>
	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Post your 6/13 patch day issues here (in one place for easy tracking)

			&nbsp; <nobr><span style="font-size: 8pt;">[ Edited ]</span></nobr> 
			
	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bveteransm240216_table" 
		
			onClick="setId('240216');showLiMenu(this,'io_bveteransm240216','right',false);" 
			onDblClick="setId('240216');showLiMenu(this,'io_bveteransm240216','right',false);" 
			
		>
		<tr class=clickable>
			
