	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		In-game guild roster

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bdevcornerm974_table" 
		
			onClick="setId('974');showLiMenu(this,'io_bdevcornerm974','right',false);" 
