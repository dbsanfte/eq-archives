	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		1% of population misleading?

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bveteransm168005_table" 
		
			onClick="setId('168005');showLiMenu(this,'io_bveteransm168005','right',false);" 
