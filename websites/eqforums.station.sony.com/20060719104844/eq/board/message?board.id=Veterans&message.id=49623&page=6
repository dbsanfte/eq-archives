	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: What commands do you wish you knew a long time ago?

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bveteransm80731_table" 
		
			onClick="setId('80731');showLiMenu(this,'io_bveteransm80731','right',false);" 
			onDblClick="setId('80731');showLiMenu(this,'io_bveteransm80731','right',false);" 
			
