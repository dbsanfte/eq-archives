	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Rogue DPS concerns... trying to think outside the box

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_broguebalancem4623_table" 
		
			onClick="setId('4623');showLiMenu(this,'io_broguebalancem4623','right',false);" 
			onDblClick="setId('4623');showLiMenu(this,'io_broguebalancem4623','right',false);" 
			
		>
		<tr class=clickable>
			
				<td nowrap><a class=subjectbar_text style='font-size: 8pt;'>Options</a></td>
				
			<td>&nbsp;&nbsp;</td>
