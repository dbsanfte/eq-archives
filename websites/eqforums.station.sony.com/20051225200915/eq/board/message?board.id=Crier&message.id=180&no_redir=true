	<tr id="M180"><td colspan=1>
		<a name="M180"></a>
				<table cellpadding=0 cellspacing=0 border=0 width="100%">
		
						<tr>
			
					<td class="subjectbar" align=left style="padding:0px;padding-left: 10px;">
		
					&nbsp;</td>
					<td bgcolor="#050A0E" width=1><img src="/i/p.gif" height=1 width=1 border=0 alt=""><br></td>
			<td class="subjectbar"  style="font-size: 10pt;padding-left:10px;" width="100%">
									HoC Dev Chat Logs - 8/25/2005
																	</td>
							<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">		
		<table cellpadding=0 cellspacing=0 border=0 
		id="individual_options_180_table" 
		onClick="setId('180');show(this,'individual_options_180','right',false);" 
		onDblClick="setId('180');show(this,'individual_options_180','right',false);" 
