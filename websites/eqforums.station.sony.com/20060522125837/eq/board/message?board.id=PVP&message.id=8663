	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Other issues that need addressed on pvp servers

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bpvpm8655_table" 
		
			onClick="setId('8655');showLiMenu(this,'io_bpvpm8655','right',false);" 
