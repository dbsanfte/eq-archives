		<tr class=clickable>
			<td>&nbsp;</td>
		
			<td><a href="javascript:go('https://secure.station.sony.com/login/login/station/login_no_popup.jsp?returnURL=http%3A%2F%2Feqforums.station.sony.com%2Feq%2Fboard%2Fpost%3Fboard.id%3DVeterans%26message.reply_to_id%3D76784');"><img src='/i/skins/default/button_reply.gif' border=0 alt='Reply' title='Reply' width=18 height=28></a></td>
			<td>&nbsp;</td>
			
			<td nowrap><a href="javascript:go('https://secure.station.sony.com/login/login/station/login_no_popup.jsp?returnURL=http%3A%2F%2Feqforums.station.sony.com%2Feq%2Fboard%2Fpost%3Fboard.id%3DVeterans%26message.reply_to_id%3D76784');"><span class=subjectbar_text style='font-size: 8pt'>Reply</span></a></td>
			<td>&nbsp;</td>
			
