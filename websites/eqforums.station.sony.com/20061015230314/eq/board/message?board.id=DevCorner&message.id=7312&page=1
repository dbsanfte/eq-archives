	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		PvP Enhancements on Test

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bdevcornerm7312_table" 
		
			onClick="setId('7312');showLiMenu(this,'io_bdevcornerm7312','right',false);" 
			onDblClick="setId('7312');showLiMenu(this,'io_bdevcornerm7312','right',false);" 
			
