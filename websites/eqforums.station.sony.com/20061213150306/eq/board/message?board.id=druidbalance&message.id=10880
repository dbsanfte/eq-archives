	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Druid TSS BP focus / clicky

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bdruidbalancem10877_table" 
		
			onClick="setId('10877');showLiMenu(this,'io_bdruidbalancem10877','right',false);" 
