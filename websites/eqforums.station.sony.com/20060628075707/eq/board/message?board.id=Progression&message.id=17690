	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: What Killed Everquest

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bprogressionm17712_table" 
		
			onClick="setId('17712');showLiMenu(this,'io_bprogressionm17712','right',false);" 
