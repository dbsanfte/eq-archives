	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Who Owns High-End PVP

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bpvpm11992_table" 
		
			onClick="setId('11992');showLiMenu(this,'io_bpvpm11992','right',false);" 
			onDblClick="setId('11992');showLiMenu(this,'io_bpvpm11992','right',false);" 
			
		>
		<tr class=clickable>
			
				<td nowrap><a class=subjectbar_text style='font-size: 8pt;'>Options</a></td>
				
			<td>&nbsp;&nbsp;</td>
