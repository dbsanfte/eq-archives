	<td bgcolor="#050A0E" width=1><img src="/i/p.gif" height=1 width=1 border=0 alt=""><br></td>
	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Roles of the healing classes.

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bclericbalancem16_table" 
		
			onClick="setId('16');showLiMenu(this,'io_bclericbalancem16','right',false);" 
			onDblClick="setId('16');showLiMenu(this,'io_bclericbalancem16','right',false);" 
			
		>
		<tr class=clickable>
			
				<td nowrap><a class=subjectbar_text style='font-size: 8pt;'>Options</a></td>
				
			<td>&nbsp;&nbsp;</td>
			<td><img src='/i/skins/default/button_options.gif' border=0 alt='Options' title='Options' width=11 height=28></td>
			<td>&nbsp;</td></tr>
		</table>
		
		</td></tr>
		
	<tr>
	
		<td class="msg_user_cell" width="150" valign=top>
		
			 









<a href='/eq/view_profile?user.id=6399'  class="auth_text"  style="font-weight:bold;" >	
<span style='color:#9933CC;font-weight:bold; '>Hurk</span></a>
			<br>
			
			<span style="font-size: 8pt;">
			Guardian<br>
			
				Posts: 5223<br>
				
				Registered: 03-12-2004
				
			</span>
			


			
