	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: suggestions for new hotzones

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bveteransm181199_table" 
		
			onClick="setId('181199');showLiMenu(this,'io_bveteransm181199','right',false);" 
