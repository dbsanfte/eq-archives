	<td bgcolor="#050A0E" width=1><img src="/i/p.gif" height=1 width=1 border=0 alt=""><br></td>
	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Suggested Improvements for Mid-level Warriors

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bwarriorbalancem26018_table" 
		
			onClick="setId('26018');showLiMenu(this,'io_bwarriorbalancem26018','right',false);" 
			onDblClick="setId('26018');showLiMenu(this,'io_bwarriorbalancem26018','right',false);" 
			
		>
		<tr class=clickable>
			
				<td nowrap><a class=subjectbar_text style='font-size: 8pt;'>Options</a></td>
				
			<td>&nbsp;&nbsp;</td>
			<td><img src='/i/skins/default/button_options.gif' border=0 alt='Options' title='Options' width=11 height=28></td>
			<td>&nbsp;</td></tr>
		</table>
		
		</td></tr>
		
	<tr>
	
		<td class="msg_user_cell" width="150" valign=top>
		
			 









<a href='/eq/view_profile?user.id=80701'  class="auth_text"  style="font-weight:bold;" >	
<span style='color:#CC6600;font-weight:bold; '>JazzyJoyce</span></a>
			<br>
			
			<span style="font-size: 8pt;">
			Master<br>
			
				Posts: 866<br>
				
