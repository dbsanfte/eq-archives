		<script language="JavaScript">
			function getRealTopPos(el) {
				iPos = 0
				while (el!=null) {
				 	iPos += el.offsetTop
					el = el.offsetParent
				}
				return iPos
			}

			toMsg = document.getElementById("M109815");
			scrollTo(0, getRealTopPos(toMsg));
		</script>
		
	


