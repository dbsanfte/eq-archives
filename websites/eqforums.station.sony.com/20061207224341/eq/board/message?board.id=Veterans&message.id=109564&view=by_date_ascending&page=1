	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		What is Sony doing about exploit/cheaters?

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bveteransm109564_table" 
		
			onClick="setId('109564');showLiMenu(this,'io_bveteransm109564','right',false);" 
			onDblClick="setId('109564');showLiMenu(this,'io_bveteransm109564','right',false);" 
			
		>
		<tr class=clickable>
			
				<td nowrap><a class=subjectbar_text style='font-size: 8pt;'>Options</a></td>
				
			<td>&nbsp;&nbsp;</td>
			<td><img src='/i/skins/default/button_options.gif' border=0 alt='Options' title='Options' width=11 height=28></td>
			<td>&nbsp;</td></tr>
		</table>
		
		</td></tr>
		
	<tr>
	
		<td class="msg_user_cell" width="150" valign=top>
		
			 









<a href='/eq/view_profile?user.id=53957'  class="auth_text"  style="font-weight:bold;" >	
<span style='color:#FF00FF;'>fretwire</span></a>
			<br>
			
