	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Rez and Call to Corpse

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bpaladinbalancem24949_table" 
		
			onClick="setId('24949');showLiMenu(this,'io_bpaladinbalancem24949','right',false);" 
