	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Clicky items

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bveteransm197402_table" 
		
			onClick="setId('197402');showLiMenu(this,'io_bveteransm197402','right',false);" 
