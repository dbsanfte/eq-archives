	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Run Slow on Dell Precision M70 LapTop

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_btechm100105_table" 
		
			onClick="setId('100105');showLiMenu(this,'io_btechm100105','right',false);" 
