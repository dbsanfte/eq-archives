									<td bgcolor="#092640" width=1><img src="/i/p.gif" height=1 width=1 border=0 alt=""><br></td>
			<td class="subjectbar"  style="font-size: 10pt;padding-left:10px;" width="100%">
									Problems with Cleave 3
																	</td>
							<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">		
		<table cellpadding=0 cellspacing=0 border=0 
		id="individual_options_4632_table" 
		onClick="setId('4632');show(this,'individual_options_4632','right',false);" 
		onDblClick="setId('4632');show(this,'individual_options_4632','right',false);" 
	 >
	<tr class=clickable>
					<td nowrap><a class=subjectbar style='font-size: 8pt;'>Options</a></td>
				<td>&nbsp;&nbsp;</td>
