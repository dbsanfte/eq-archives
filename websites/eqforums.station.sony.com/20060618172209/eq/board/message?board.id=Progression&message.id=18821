	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Magician or Rogue?

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bprogressionm18732_table" 
		
			onClick="setId('18732');showLiMenu(this,'io_bprogressionm18732','right',false);" 
