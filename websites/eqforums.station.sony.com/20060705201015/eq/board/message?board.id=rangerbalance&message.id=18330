	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Class issues lists – Updated Version (Ranger)

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_brangerbalancem15228_table" 
		
			onClick="setId('15228');showLiMenu(this,'io_brangerbalancem15228','right',false);" 
			onDblClick="setId('15228');showLiMenu(this,'io_brangerbalancem15228','right',false);" 
			
