	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Canticle's Notes: The Sporali

			&nbsp; <nobr><span style="font-size: 8pt;">[ Edited ]</span></nobr> 
			
	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_blorem8776_table" 
		
			onClick="setId('8776');showLiMenu(this,'io_blorem8776','right',false);" 
			onDblClick="setId('8776');showLiMenu(this,'io_blorem8776','right',false);" 
			
		>
		<tr class=clickable>
			
				<td nowrap><a class=subjectbar_text style='font-size: 8pt;'>Options</a></td>
				
