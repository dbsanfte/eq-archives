      <pubDate>Sat, 22 May 2004 13:16:21 GMT</pubDate>
      <author>chighal_KoZ</author>
      <guid>http://eqforums.station.sony.com/eq/board/message?board.id=druidbalance&amp;message.id=540#M540</guid>
      <dc:creator>chighal_KoZ</dc:creator>
