	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Improvements to soloing/tasks (January Summit)

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bveteransm51839_table" 
		
			onClick="setId('51839');showLiMenu(this,'io_bveteransm51839','right',false);" 
			onDblClick="setId('51839');showLiMenu(this,'io_bveteransm51839','right',false);" 
			
		>
		<tr class=clickable>
			
				<td nowrap><a class=subjectbar_text style='font-size: 8pt;'>Options</a></td>
				
			<td>&nbsp;&nbsp;</td>
