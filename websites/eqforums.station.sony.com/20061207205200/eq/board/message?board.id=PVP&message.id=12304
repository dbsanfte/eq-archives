	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Where did the Anti-PK guilds go?

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bpvpm12268_table" 
		
			onClick="setId('12268');showLiMenu(this,'io_bpvpm12268','right',false);" 
			onDblClick="setId('12268');showLiMenu(this,'io_bpvpm12268','right',false);" 
			
