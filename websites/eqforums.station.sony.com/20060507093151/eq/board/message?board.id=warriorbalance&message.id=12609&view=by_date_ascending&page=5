	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Design-a-Weapon catagory 1H Shieldmate

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bwarriorbalancem12737_table" 
		
			onClick="setId('12737');showLiMenu(this,'io_bwarriorbalancem12737','right',false);" 
			onDblClick="setId('12737');showLiMenu(this,'io_bwarriorbalancem12737','right',false);" 
			
		>
		<tr class=clickable>
			
