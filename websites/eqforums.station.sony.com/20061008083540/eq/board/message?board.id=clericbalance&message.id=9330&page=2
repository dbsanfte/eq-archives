	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: New Turn Undead maxes out at 500dmg/tick? Not acceptable!!!

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bclericbalancem9459_table" 
		
			onClick="setId('9459');showLiMenu(this,'io_bclericbalancem9459','right',false);" 
			onDblClick="setId('9459');showLiMenu(this,'io_bclericbalancem9459','right',false);" 
			
