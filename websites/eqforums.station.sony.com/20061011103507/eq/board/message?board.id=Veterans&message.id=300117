	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Drakkin Enchanter Hair = Bald Illusion

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bveteransm300117_table" 
		
			onClick="setId('300117');showLiMenu(this,'io_bveteransm300117','right',false);" 
