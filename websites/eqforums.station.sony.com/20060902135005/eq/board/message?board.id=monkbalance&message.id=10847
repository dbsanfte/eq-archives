	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Monk DPS

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bmonkbalancem10831_table" 
		
			onClick="setId('10831');showLiMenu(this,'io_bmonkbalancem10831','right',false);" 
			onDblClick="setId('10831');showLiMenu(this,'io_bmonkbalancem10831','right',false);" 
			
