	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Quick Project: Cities, Demeanors

			&nbsp; <nobr><span style="font-size: 8pt;">[ Edited ]</span></nobr> 
			
	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_blorem7656_table" 
		
			onClick="setId('7656');showLiMenu(this,'io_blorem7656','right',false);" 
			onDblClick="setId('7656');showLiMenu(this,'io_blorem7656','right',false);" 
			
