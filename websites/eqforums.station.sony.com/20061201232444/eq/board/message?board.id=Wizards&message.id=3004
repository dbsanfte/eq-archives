	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Wizard DPS - a real class balance problem

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bwizardsm3067_table" 
		
			onClick="setId('3067');showLiMenu(this,'io_bwizardsm3067','right',false);" 
			onDblClick="setId('3067');showLiMenu(this,'io_bwizardsm3067','right',false);" 
			
