	<td bgcolor="#050A0E" width=1><img src="/i/p.gif" height=1 width=1 border=0 alt=""><br></td>
	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Removing Defensive/Evasive from the game

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bwarriorbalancem443_table" 
		
			onClick="setId('443');showLiMenu(this,'io_bwarriorbalancem443','right',false);" 
			onDblClick="setId('443');showLiMenu(this,'io_bwarriorbalancem443','right',false);" 
			
