	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Death by Exodus & TSS

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bdruidbalancem9972_table" 
		
			onClick="setId('9972');showLiMenu(this,'io_bdruidbalancem9972','right',false);" 
