	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: All the clerics will hate me

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bveteransm180233_table" 
		
			onClick="setId('180233');showLiMenu(this,'io_bveteransm180233','right',false);" 
