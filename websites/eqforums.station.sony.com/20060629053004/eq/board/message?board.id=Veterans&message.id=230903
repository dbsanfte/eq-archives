	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Gear Gap Between Raiders and Casuals Too Wide

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bveteransm229497_table" 
		
			onClick="setId('229497');showLiMenu(this,'io_bveteransm229497','right',false);" 
			onDblClick="setId('229497');showLiMenu(this,'io_bveteransm229497','right',false);" 
			
