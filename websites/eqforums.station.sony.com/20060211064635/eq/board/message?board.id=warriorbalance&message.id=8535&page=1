	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Level 66 Warrior opinion on DoN items and Augments.

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bwarriorbalancem10480_table" 
		
			onClick="setId('10480');showLiMenu(this,'io_bwarriorbalancem10480','right',false);" 
			onDblClick="setId('10480');showLiMenu(this,'io_bwarriorbalancem10480','right',false);" 
			
