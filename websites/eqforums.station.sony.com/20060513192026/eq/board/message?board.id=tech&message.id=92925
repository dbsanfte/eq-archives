	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Strange Desktop Issues - Is it just me?

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_btechm92912_table" 
		
			onClick="setId('92912');showLiMenu(this,'io_btechm92912','right',false);" 
			onDblClick="setId('92912');showLiMenu(this,'io_btechm92912','right',false);" 
			
