	<td bgcolor="#050A0E" width=1><img src="/i/p.gif" height=1 width=1 border=0 alt=""><br></td>
	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Everquest's fundamental growth problem

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bsoem4396_table" 
		
			onClick="setId('4396');showLiMenu(this,'io_bsoem4396','right',false);" 
			onDblClick="setId('4396');showLiMenu(this,'io_bsoem4396','right',false);" 
			
