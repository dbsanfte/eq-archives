	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Everquest's fundamental growth problem

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bsoem4762_table" 
		
			onClick="setId('4762');showLiMenu(this,'io_bsoem4762','right',false);" 
