	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Upcoming Fixes and Features for EQPlayers

			&nbsp; <nobr><span style="font-size: 8pt;">[ Edited ]</span></nobr> 
			
	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_beqplayersm2632_table" 
		
			onClick="setId('2632');showLiMenu(this,'io_beqplayersm2632','right',false);" 
			onDblClick="setId('2632');showLiMenu(this,'io_beqplayersm2632','right',false);" 
			
