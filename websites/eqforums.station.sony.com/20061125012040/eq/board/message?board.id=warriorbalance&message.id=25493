	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Newest Warrior Question:  9-6-06

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bwarriorbalancem25493_table" 
		
			onClick="setId('25493');showLiMenu(this,'io_bwarriorbalancem25493','right',false);" 
