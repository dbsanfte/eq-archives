	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		EQ emulation servers make me sad and mad.

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bveteransm76008_table" 
		
			onClick="setId('76008');showLiMenu(this,'io_bveteransm76008','right',false);" 
