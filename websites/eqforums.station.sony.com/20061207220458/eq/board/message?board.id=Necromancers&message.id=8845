	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		New Expansion Wish List

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bnecromancersm8845_table" 
		
			onClick="setId('8845');showLiMenu(this,'io_bnecromancersm8845','right',false);" 
