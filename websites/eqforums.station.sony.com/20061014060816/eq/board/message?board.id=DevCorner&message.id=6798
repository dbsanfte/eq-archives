	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		PvP Beta Random Discussion

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bdevcornerm6798_table" 
		
			onClick="setId('6798');showLiMenu(this,'io_bdevcornerm6798','right',false);" 
