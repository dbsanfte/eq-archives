	<tr id="M21417"><td colspan=1>
		<a name="M21417"></a>
	
		<table cellpadding=0 cellspacing=0 border=0 width="100%">
		
	<tr>
	
		<td class="subjectbar" align=left style="padding:0px;padding-left: 10px;padding-right: 10px;">
		
			








		<table cellpadding=0 cellspacing=0 border=0>
		<tr class=clickable>
			<td>&nbsp;</td>
		
			<td><a href="javascript:go('https://secure.station.sony.com/login/login/station/login_no_popup.jsp?returnURL=http%3A%2F%2Feqforums.station.sony.com%2Feq%2Fboard%2Fpost%3Fboard.id%3Dwarriorbalance%26message.reply_to_id%3D21417');"><img src='/i/skins/default/button_reply.gif' border=0 alt='Reply' title='Reply' width=18 height=28></a></td>
