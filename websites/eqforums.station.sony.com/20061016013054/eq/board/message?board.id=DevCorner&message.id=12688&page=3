	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: The Importance of Stats

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bdevcornerm12747_table" 
		
			onClick="setId('12747');showLiMenu(this,'io_bdevcornerm12747','right',false);" 
			onDblClick="setId('12747');showLiMenu(this,'io_bdevcornerm12747','right',false);" 
			
		>
		<tr class=clickable>
			
				<td nowrap><a class=subjectbar_text style='font-size: 8pt;'>Options</a></td>
				
			<td>&nbsp;&nbsp;</td>
