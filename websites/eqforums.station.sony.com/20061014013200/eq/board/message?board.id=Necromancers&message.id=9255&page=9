	<td class="subjectbar" height="20" style="font-size: 8pt;padding-left:10px;" width="100%">
		Re: Please repair Lich, and reassess Affliction Efficiency

	</td>
	
		<td class="subjectbar" align=right style="padding:0px;padding-right:10px;">












		<table cellpadding=0 cellspacing=0 border=0 id="io_bnecromancersm9455_table" 
		
			onClick="setId('9455');showLiMenu(this,'io_bnecromancersm9455','right',false);" 
			onDblClick="setId('9455');showLiMenu(this,'io_bnecromancersm9455','right',false);" 
			
