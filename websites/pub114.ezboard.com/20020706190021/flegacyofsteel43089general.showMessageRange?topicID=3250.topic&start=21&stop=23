<br>
I gotta make it next just throwing in some infiles so it can get the points back from a prior run and add on to the loot list... the raids thing is just the first 6 or so things that popped into my head<br>
<br>
#include &lt;iostream.h&gt;<br>
#include &lt;apstring.h&gt;<br>
#include &lt;fstream.h&gt;<br>
<br>
<br>
void addraid(int &amp;points, int raid);<br>
void addloot(int &amp;points, apstring &amp;loot ,int &amp;pointslooted);<br>
<br>
<br>
void main ()<br>
{<br>
&nbsp &nbsp &nbsp &nbsp ofstream outfile;<br>
&nbsp &nbsp &nbsp &nbsp ofstream outfile2;<br>
<br>
&nbsp &nbsp &nbsp &nbsp int points = 0;<br>
&nbsp &nbsp &nbsp &nbsp int pointslooted=0;<br>
&nbsp &nbsp &nbsp &nbsp int raid;<br>
&nbsp &nbsp &nbsp &nbsp apstring name, name2,name3;<br>
&nbsp &nbsp &nbsp &nbsp char YN='Y';<br>
&nbsp &nbsp &nbsp &nbsp char didloot;<br>
&nbsp &nbsp &nbsp &nbsp apstring loot=&quot;&quot;;<br>
<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt;&quot;Enter your current RaidPoints&quot;&lt;&lt;endl;<br>
&nbsp &nbsp &nbsp &nbsp cin&gt;&gt;points;<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt;&quot;You have &quot;&lt;&lt;points&lt;&lt;&quot; Raidpoints&quot;&lt;&lt;endl;<br>
<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt;&quot;Enter your character name&quot;&lt;&lt;endl;<br>
&nbsp &nbsp &nbsp &nbsp cin&gt;&gt;name;<br>
&nbsp &nbsp &nbsp &nbsp name2=name;<br>
&nbsp &nbsp &nbsp &nbsp name3=name;<br>
&nbsp &nbsp &nbsp &nbsp name3=name3+&quot;.txt&quot;;<br>
&nbsp &nbsp &nbsp &nbsp outfile.open(name3.c_str());<br>
&nbsp &nbsp &nbsp &nbsp name2=name2+&quot;loot&quot;;<br>
&nbsp &nbsp &nbsp &nbsp name2=name2+&quot;.txt&quot;;<br>
&nbsp &nbsp &nbsp &nbsp outfile2.open(name2.c_str());<br>
&nbsp &nbsp &nbsp &nbsp <br>
&nbsp &nbsp &nbsp &nbsp while(YN!='N'&amp;&amp;YN!='n')<br>
&nbsp &nbsp &nbsp &nbsp {<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt; &quot;Enter raid number&quot; &lt;&lt;endl;<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt; &quot;Doomshade 1 &quot; &lt;&lt;endl;<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt; &quot;Vindicator 2 &quot; &lt;&lt;endl;<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt; &quot;Temple of Veeshan 3 &quot; &lt;&lt;endl;<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt; &quot;Velketor 4 &quot; &lt;&lt;endl;<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt; &quot;Ragefire 5 &quot; &lt;&lt;endl;<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt; &quot;Severillous 6 &quot; &lt;&lt;endl;<br>
&nbsp &nbsp &nbsp &nbsp cin&gt;&gt;raid;<br>
<br>
<br>
&nbsp &nbsp &nbsp &nbsp addraid(points, raid);<br>
<br>
&nbsp &nbsp &nbsp &nbsp <br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt;name&lt;&lt;&quot;'s new raid points are &quot;&lt;&lt;points&lt;&lt;endl;<br>
<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt;&quot;Did &quot;&lt;&lt;name&lt;&lt;&quot; get any loot on this raid? Y/N&quot;&lt;&lt;endl;<br>
<br>
&nbsp &nbsp &nbsp &nbsp cin&gt;&gt;didloot;<br>
<br>
&nbsp &nbsp &nbsp &nbsp if (didloot=='Y'||didloot=='y')<br>
&nbsp &nbsp &nbsp &nbsp {<br>
&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp addloot(points, loot,pointslooted);<br>
&nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp <br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt;name&lt;&lt;&quot;'s new raid points are &quot;&lt;&lt;points&lt;&lt;endl;<br>
&nbsp &nbsp &nbsp &nbsp }<br>
<br>
&nbsp &nbsp &nbsp &nbsp if (pointslooted!=0)<br>
&nbsp &nbsp &nbsp &nbsp {cout&lt;&lt;name&lt;&lt;&quot; has looted to date : &quot;&lt;&lt;endl&lt;&lt;endl&lt;&lt;loot&lt;&lt;endl;<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt;&quot;Total point value of looted items: &quot;&lt;&lt;pointslooted&lt;&lt;endl&lt;&lt;endl;<br>
&nbsp &nbsp &nbsp &nbsp }<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt;&quot;Do you wish to add another raid? Y/N &quot;&lt;&lt;endl;<br>
&nbsp &nbsp &nbsp &nbsp cin&gt;&gt;YN;<br>
<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt;endl;<br>
<br>
<br>
&nbsp &nbsp &nbsp &nbsp }<br>
outfile&lt;&lt;points&lt;&lt;endl&lt;&lt;pointslooted;<br>
outfile2&lt;&lt;loot;<br>
<br>
outfile.close();<br>
outfile2.close();<br>
}<br>
<br>
<br>
void addraid(int &amp;points, int raid)<br>
{<br>
&nbsp &nbsp &nbsp &nbsp if (raid==1)<br>
&nbsp &nbsp &nbsp &nbsp points=points+15;<br>
<br>
&nbsp &nbsp &nbsp &nbsp else if (raid==2)<br>
&nbsp &nbsp &nbsp &nbsp points=points+10;<br>
<br>
&nbsp &nbsp &nbsp &nbsp else if (raid==3)<br>
&nbsp &nbsp &nbsp &nbsp points=points+20;<br>
<br>
&nbsp &nbsp &nbsp &nbsp else if (raid==4)<br>
&nbsp &nbsp &nbsp &nbsp points=points+30;<br>
<br>
&nbsp &nbsp &nbsp &nbsp else if (raid==5)<br>
&nbsp &nbsp &nbsp &nbsp points=points+40;<br>
<br>
&nbsp &nbsp &nbsp &nbsp else if (raid==6)<br>
&nbsp &nbsp &nbsp &nbsp points=points+50;<br>
<br>
&nbsp &nbsp &nbsp &nbsp else <br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt;&quot;Invalid raid number&quot;&lt;&lt;endl;<br>
}<br>
<br>
void addloot(int &amp;points, apstring &amp;loot, int &amp;pointslooted)<br>
{<br>
&nbsp &nbsp &nbsp &nbsp int deduct;<br>
&nbsp &nbsp &nbsp &nbsp char more='Y';<br>
&nbsp &nbsp &nbsp &nbsp apstring newloot,junk;<br>
while (more=='Y'||more=='y')<br>
{<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt;&quot;Enter name of loot&quot;&lt;&lt;endl;<br>
&nbsp &nbsp &nbsp &nbsp getline(cin,junk);<br>
&nbsp &nbsp &nbsp &nbsp getline(cin,newloot);<br>
&nbsp &nbsp &nbsp &nbsp loot=loot+newloot+&quot;\n&quot;;<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt;&quot;Enter points value of loot&quot;&lt;&lt;endl;<br>
&nbsp &nbsp &nbsp &nbsp cin&gt;&gt;deduct;<br>
&nbsp &nbsp &nbsp &nbsp points=points-deduct;<br>
&nbsp &nbsp &nbsp &nbsp pointslooted=pointslooted+deduct;<br>
&nbsp &nbsp &nbsp &nbsp cout&lt;&lt;&quot;More loot? Y/N&quot;&lt;&lt;endl;<br>
&nbsp &nbsp &nbsp &nbsp cin&gt;&gt;more;<br>
}<br>
<br>
