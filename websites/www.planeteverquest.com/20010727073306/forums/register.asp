function hexEncode(str) {
        str = zreplace(str, "%", "%25");
        str = zreplace(str, "/", "%2F");
        str = zreplace(str, "+", "%2B");
        str = zreplace(str, " ", "+");
        str = zreplace(str, "\"", "%22");
