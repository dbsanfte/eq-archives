OpenWin = this.open(page,"CtrlWindow","toolbar=no,menubar=no,location=no,scrollbars=yes,resizable=yes,width=600,height=500,screenX=0,screenY=0,top=0,left=0,alwaysRaised=yes");
}
// End -->
