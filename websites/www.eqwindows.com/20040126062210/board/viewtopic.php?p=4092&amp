EQExe=C:\Program Files\Sony\EverQuest Trilogy\Everquest.exe
<br />
FixEQIni=TRUE
<br />
[Hotkeys]
<br />
ReleaseMouse=F1
<br />
ReloadIni=Ctrl Alt I
<br />
EQWSwitch=F2
<br />
[Positions]
<br />
800by600X=344
<br />
800by600Y=-1
<br />
640by480X=443
<br />
640by480Y=95
<br />

