	<script>
	function changelink(x){window.status=x;return true;}
	function returnlink(){window.status='';}
	function setCookie(name, value) {   
		var expire = new Date();
		var today = new Date();
		expire.setTime(today.getTime() + 1000 * 60 * 60 * 24 * 365);
		document.cookie = name + "=" + escape(value) + "; //path=/"   + ((expire == null) ? "" : ("; expires=" + expire.toGMTString()))
	}
	function set24HourCookie(name, value) {   
		var expire = new Date();
		var today = new Date();
		expire.setTime(today.getTime() + 1000 * 60 * 60 * 24);
		document.cookie = name + "=" + escape(value) + "; //path=/"   + ((expire == null) ? "" : ("; expires=" + expire.toGMTString()))
	}
	function getCookie(Name) {   
		var search = Name + "="   
		if (document.cookie.length > 0) {// if there are any cookies      
			offset = document.cookie.indexOf(search)       
			if (offset != -1) { // if cookie exists          
			offset += search.length; 
			// set index of beginning of value         
			end = document.cookie.indexOf(";", offset)
			// set index of end of cookie value         
			if (end == -1)             
			end = document.cookie.length         
			return unescape(document.cookie.substring(offset, end))      
			}    
		}
	}
	flag = 1
	function mhppop(){
		var strCookieName = "seen_search_pops";
		var cookieExist = getCookie(strCookieName);
		if ((cookieExist != "yes") && (flag == 1)){
		setCookie(strCookieName, "yes");
		mhp.style.behavior="url(#default#homepage)";
		mhp.setHomePage("http://www.netster.com");
		}
	}
	function openlink(strLink){
		window.open(strLink); 
	}
	function resultpop(){
		var strCookieName = "seen_result_pops";
		var cookieExist = getCookie(strCookieName);
		if (cookieExist != "yes"){
		set24HourCookie(strCookieName, "yes");
		window.open('pop.asp?KeyWords=advertising', '', 'width=550,height=445,toolbar=0,location=0,directories=0,status=0,menuBar=0,scrollBars=1,resizable=0' )
		}
	}	
	</script>
