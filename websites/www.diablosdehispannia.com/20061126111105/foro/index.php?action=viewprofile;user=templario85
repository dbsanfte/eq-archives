function OpenAdsDetail(FileName,WindowName)
{
	
WindowFeature="Resizable=No,ScrollBars=Yes,MenuBar=No,Directories=No,ToolBar=No,Location=No,Status=No,Width=660,Height=550,ScreenX=0,ScreenY=0,Top=0,Left=0"

