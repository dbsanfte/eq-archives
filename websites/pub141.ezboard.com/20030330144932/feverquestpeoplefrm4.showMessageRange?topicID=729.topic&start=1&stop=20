oad" style="visibility: hidden;" style="width:0; height:1;">
document.MoveItem = function MoveItem(loMoveObject) {
var myObj; myObj = loMoveObject; var oDragItem = null
function doMouseMove() {if ((1 == event.button) && (oDragItem != null)) {
var intTop = event.clientY+document.body.scrollTop;var intLeft = event.clientX + document.body.scrollLeft;var intLessTop  = 0; var intLessLeft = 0;var oCurrent = oDragItem.offsetParent;
while (oCurrent.offsetParent!=null) {intLessTop+=oCurrent.offsetTop;intLessLeft+=oCurrent.offsetLeft;oCurrent = oCurrent.offsetParent;}
oDragItem.style.pixelTop = intTop  - intLessTop - oDragItem.y;oDragItem.style.pixelLeft = intLeft - intLessLeft  - oDragItem.x;event.returnValue = false;
}}function doMouseDown(oMoveObj) {var oCurrent = oMoveObj;
if (null!=oCurrent) {oDragItem = oCurrent;oDragItem.x = event.offsetX;oDragItem.y = event.offsetY;var op = oMoveObj;if ((oDragItem!=op.offsetParent) && (oDragItem!=event.srcElement)) {while (op!=oDragItem) {oDragItem.x+=op.offsetLeft;oDragItem.y+=op.offsetTop;op=op.offsetParent;}}}}
document.onmousemove = doMouseMove; document.onselectstart = new Function("oDragItem!=null"); document.onmouseup = new Function(
"oDragItem=null; document.onmousemove = ''; document.onselectstart = '';");doMouseDown(myObj);}
</textarea>

