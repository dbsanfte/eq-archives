<!--
	function remplace(expr, orig, subst) {
		var i = 0;
		while (i != -1) {
			i = expr.indexOf(orig, i);
			if ( i >= 0) {
				expr = expr.substring(0, i) + subst + expr.substring(i + orig.length);
				i += subst.length;
			}
		}
		return expr;
	}

	function email(adresse) {
		var href;
		if (! window.focus)
			return true;
		href = remplace(adresse, ' point ', '.');
		href = 'mailto:' + remplace(href, ' chez ', '@');
		location = href;
	}
	
	function fiche(url,parametres) { 
		var fenetre = window.open(url,'zoom',parametres); 
	}
//-->
