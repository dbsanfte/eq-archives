function DoPrompt(action) {
var revisedMessage;
var currentMessage = document.coolsus.message.value;

if (action == "url") {
var thisURL = prompt("Enter the URL for the link you want to add.", "http://");
var thisTitle = prompt("Enter the web site title", "Page Title");
