function jump () {
  if (document.s2.zone.selectedIndex != 0) {
    document.s2.submit();
  }
}
function jump3 () {
  if (document.s3.faction.selectedIndex != 0) {
    document.s3.submit();
  }
}



// -->
</script>
</html>
