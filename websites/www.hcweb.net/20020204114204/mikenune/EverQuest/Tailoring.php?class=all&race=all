<SCRIPT TYPE="text/javascript" LANGUAGE="javascript1.2">
    s = screen.width;
    v = navigator.appName

    if (v != "Netscape") {
        c = screen.colorDepth
    } else {
        c = screen.pixelDepth
    }

    j = navigator.javaEnabled()
</SCRIPT>

<SCRIPT TYPE="text/javascript" LANGUAGE="javascript">
    function pr(n) {
        document.write(n,"\n");
    }

    NS2Ch = 0
    if (navigator.appName == "Netscape" && navigator.appVersion.charAt(0) == "2") {
        NS2Ch = 1
    }
    if (NS2Ch == 0) {
        r = "&size=" + s + "&colors=" + c + "&referer=" + f + "&java=" + j + ""
        pr("<A HREF=\"http://www.thecounter.com/\" TARGET=\"_top\"><IMG BORDER=\"0\" HEIGHT=\"31\" SRC=\"http://c3.thecounter.com/id=2367949" + r + "\" WIDTH=\"88\"><\/A>")
    }
</SCRIPT>

