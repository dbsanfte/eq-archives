<script language="JavaScript">

function open_picwin(picfile)
{
	var win1;
	win1=window.open("","","");
	win1.document.open("text/html");
	win1.document.bgColor="#000000";
	win1.document.write("<img src='"+picfile+"'>");
}

</script>

