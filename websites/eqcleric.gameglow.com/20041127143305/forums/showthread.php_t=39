				<option value="1" >1 : Terrible</option>
			</optgroup>
		</select><!-- BEGIN TEMPLATE: gobutton -->
<!-- go button -->
<input type="submit" class="button" value="Go"  />
<!-- END TEMPLATE: gobutton -->
	</div>
	</form>
	<!-- / rate this thread -->
	
	</td>
	
