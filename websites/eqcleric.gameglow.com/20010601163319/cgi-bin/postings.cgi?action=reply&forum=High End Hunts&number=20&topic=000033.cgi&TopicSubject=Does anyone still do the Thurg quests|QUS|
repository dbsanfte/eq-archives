<INPUT TYPE="HIDDEN" NAME="TopicSubject" VALUE="Does anyone still do the Thurg quests?">
<INPUT TYPE="HIDDEN" NAME="forum" VALUE="High End Hunts">
<INPUT TYPE="HIDDEN" NAME="number" VALUE="20">
<INPUT TYPE="HIDDEN" NAME="topic" VALUE="000033.cgi">
<CENTER>
 <FONT SIZE="2" FACE="Verdana, Arial" COLOR="#000080">
