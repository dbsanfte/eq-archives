   function jumpPage(form) {
      i = form.category.selectedIndex;
      window.location.href = url[i+1];
   }

   function MakeArray() {
      this.length = MakeArray.arguments.length
      for (var i = 0; i < this.length; i++)
      this[i+1] = MakeArray.arguments[i]
   }

   var url = new MakeArray(
"http://www.hawk.igs.net/~darkender/cgi-bin/dclinks/dclinks.cgi?action=view_category&category=EQ-Links",
"http://www.hawk.igs.net/~darkender/cgi-bin/dclinks/dclinks.cgi?action=view_category&category=Guilds"
   )
   </script>

