<script>
function handleEvent() {
	if (window.event.altKey) {
		
		if (window.event.keyCode == 76) {
			self.location = 'login.cfm';
		}
		
		
				if (window.event.keyCode == 80) {document.messagepostform.submit();}
			
	}
}

document.onkeydown = handleEvent;
