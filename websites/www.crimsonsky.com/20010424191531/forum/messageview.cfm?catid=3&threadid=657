	function getPermission(cid,tid,mid)
	{
		if(window.confirm('Are you sure that you want to approve this message?'))
		{
			location.href='messageview.cfm?catid='+cid+'&threadid='+tid+'&messid='+mid+'&ApproveMess=y';
		}
	}
