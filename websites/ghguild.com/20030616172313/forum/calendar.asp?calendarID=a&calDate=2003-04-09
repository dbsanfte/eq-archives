	<script language="JavaScript1.2">
	function displayPermission(){
	var permissionString = 
	
	"Read Calendar: True\n"+
	"Add Events: False\n"+
	"Add Recurring Events: False\n"+
	"Discuss Events: False\n"+
	"Calendar Moderator: False\n"+
	"Upload Size Allowed: 0KB\n"+
	"Upload Format Allowed: \n"+
	"Delete Event: False\n"+
	"Subscribe Calendar: False\n"
	 alert(permissionString);
	 return false;
	}
	function gotoDate(){
		var theform = document.calendar
		var ySelect = theform.yy
		var mSelect = theform.mm
		var baseURL = "calendar.asp?calendarID=a&calDate="+ySelect.options[ySelect.selectedIndex].value+"-"+mSelect.options[mSelect.selectedIndex].value
		var dSelect = (theform.dd) ? "-"+theform.dd.options[theform.dd.selectedIndex].value+"&types=viewday" : "-01"
		self.location.href = baseURL+dSelect
	}
	</script>
