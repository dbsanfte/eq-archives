<script language="JavaScript">


var appMPG = 25

function showPicture(lastU,isLocked,hits,replies){
	var checkDate = new Date("7/22/2003 7:51:13 PM")
	var checkDate1= new Date(lastU)
	var lastVisit=checkDate.getTime()
	var lastUpdate=checkDate1.getTime()
	if ((lastUpdate-lastVisit)>0) {
		if (isLocked==1) {
			document.write ("<img src='/forum/image/board_snlocked.gif' border='0' alt='New Messages'>")
		} 
		else if (hits>=10*appMPG || replies>=appMPG){
			document.write ("<img src='/forum/image/board_snh.gif' border='0' alt='Hot Topic!, New Messages'>")
		}
		else {
			document.write ("<img src='/forum/image/board_sn.gif' border='0' alt='New Messages'>")
		}
	}
	else {
		if (isLocked==1) {
			document.write ("<img src='/forum/image/board_slocked.gif' border='0' alt='No New Messages'>")
		} 
		else if (hits>=10*appMPG || replies>=appMPG){
			document.write ("<img src='/forum/image/board_sh.gif' border='0' alt='Hot Topic!, No New Messages'>")
		}		
		else {
			document.write ("<img src='/forum/image/board_s.gif' border='0' alt='No New Messages'>")
		}
	}
} 



var userLevelNameMod = "Moderator"
var userLevelNameAdmin = "Administrator"
var starPicSuffix = "star.gif"
var modStar = "5starM.gif"
var adminStar = "5starA.gif"
var strTitle = new Array()
strTitle [1] = "25&&5Plebeian1"
strTitle [2] = "100&&15Citizen2"
strTitle [3] = "250&&50Patrician3"
strTitle [4] = "500&&100Legionnaire4"
strTitle [5] = "1337&&500Senator5"

var forumdir = "/forum/"
//-->
</script>
