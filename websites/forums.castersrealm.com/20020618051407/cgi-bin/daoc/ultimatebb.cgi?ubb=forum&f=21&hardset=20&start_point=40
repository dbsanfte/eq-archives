document.writeln('<IMG SRC="http://forums.castersrealm.com/daoc/hotclosed.gif" BORDER="0">');
} else {
document.writeln('<IMG SRC="http://forums.castersrealm.com/daoc/closed.gif" BORDER="0">');
}
}
//-->
</SCRIPT>

</TD>

