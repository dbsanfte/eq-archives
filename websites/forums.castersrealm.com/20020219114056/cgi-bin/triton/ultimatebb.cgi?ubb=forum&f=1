if (closed_topic == "X") {
document.writeln('<IMG SRC="http://forums.castersrealm.com/tritonforum/redlockfolder.gif" BORDER="0">');
} else if (fiery == "yes") {
document.writeln('<IMG SRC="http://forums.castersrealm.com/tritonforum/hotclosedb.gif" BORDER="0">');
} else {
document.writeln('<IMG SRC="http://forums.castersrealm.com/tritonforum/closedb.gif" BORDER="0">');
}
}  else {
if (closed_topic == "X") {
document.writeln('<IMG SRC="http://forums.castersrealm.com/tritonforum/yellockfolder.gif" BORDER="0">');
} else if (fiery == "yes") {
document.writeln('<IMG SRC="http://forums.castersrealm.com/tritonforum/hotclosed.gif" BORDER="0">');
} else {
document.writeln('<IMG SRC="http://forums.castersrealm.com/tritonforum/closed.gif" BORDER="0">');
}
}
//-->
</SCRIPT>

</TD>

