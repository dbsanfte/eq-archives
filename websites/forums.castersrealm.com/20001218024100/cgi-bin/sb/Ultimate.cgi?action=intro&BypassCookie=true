	document.cookie = "sessionLogin=" + sessionLogin + ";domain=forums.castersrealm.com;path=/cgi-bin/sb;";
	document.cookie = "sessionLoginDT=" + sessionLoginDT + ";domain=forums.castersrealm.com;path=/cgi-bin/sb;";
}

function getCookie(name){
var cname = name + "=";               
var dc = document.cookie;
if (dc.length > 0) {              
    begin = dc.indexOf(cname);
	 if (begin != -1) {           
        begin += cname.length;
		end = dc.indexOf(";", begin);
            if (end == -1) end = dc.length;
            return unescape(dc.substring(begin, end));        }
}
return null;
}
