	if (DaysPrune == null) {
		var DaysPrune = "5";
	}
} else {
var DaysPrune = "5";
}

expireDate = new Date;
if (expireDate.getYear() < 1900) {
expireDate.setYear(expireDate.getYear() + 1900 + 1);
} else {
expireDate.setYear(expireDate.getYear()+1);
}

if (noNewLogin != "true") {

var CookieLogin = escape("2451895.221");
