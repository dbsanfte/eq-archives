document.writeln('<IMG SRC="http://forums.castersrealm.com/daoc/hotclosedb.gif" BORDER="0">');
} else {
document.writeln('<IMG SRC="http://forums.castersrealm.com/daoc/closedb.gif" BORDER="0">');
}
}  else {
if (closed_topic == "X") {
document.writeln('<IMG SRC="http://forums.castersrealm.com/daoc/yellockfolder.gif" BORDER="0">');
} else if (fiery == "yes") {
document.writeln('<IMG SRC="http://forums.castersrealm.com/daoc/hotclosed.gif" BORDER="0">');
} else {
document.writeln('<IMG SRC="http://forums.castersrealm.com/daoc/closed.gif" BORDER="0">');
}
}
//-->
</SCRIPT>

</TD>

