img1 = new Image();
img1.src = "/images/welcome.gif";

img2 = new Image();
img2.src = "/images/docs.gif";

img3 = new Image();
img3.src = "/images/events.gif";

img4 = new Image();
img4.src = "/images/pics.gif";

img5 = new Image();
