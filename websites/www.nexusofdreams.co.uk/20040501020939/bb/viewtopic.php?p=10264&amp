_img6 = new Image();
_img6.src = "/images/_forum.gif";

function change(imgName) {
imgOn = eval("_" + imgName + ".src");
document [imgName].src = imgOn;
}

function unchange(imgName) {
imgOff = eval(imgName + ".src");
document [imgName].src = imgOff;
}

