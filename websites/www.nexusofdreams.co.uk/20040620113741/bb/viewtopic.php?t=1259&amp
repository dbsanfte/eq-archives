			window.open("../calendar.php", "Guild", "width=575, height=585, resizable=yes, scrollbars=yes");
			window.status="Guild Calendar";
		}
		function RostWin() {
			window.open("../roster.php", "Guild", "width=575, height=585, resizable=yes, scrollbars=yes");
