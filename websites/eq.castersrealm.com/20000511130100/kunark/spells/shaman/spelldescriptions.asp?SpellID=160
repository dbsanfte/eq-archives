	<tr><td valign='top' width='200'><font face='Arial'><small><b>Number of Components Needed: </font></small></b></td><td valign='top'  width='400' colspan='2'><font face='Arial'><small>0.</font></small></td></tr>
	<tr><td valign='top' width='200'><font face='Arial'><small><b>Cost of Component: </font></small></b></td><td valign='top'  width='400' colspan='2'><font face='Arial'><small>.</font></small></td></tr>
	<tr><td valign='top' width='200'><font face='Arial'><small><b>Type: </font></small></b></td><td valign='top'  width='400' colspan='2'><font face='Arial'><small>Buffing Spell for Use on Others.</font></small></td></tr>
