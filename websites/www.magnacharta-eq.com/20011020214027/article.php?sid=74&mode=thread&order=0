var myScroller1 = new Scroller(0, 0, 180, 42, 1, 0);
myScroller1.setColors("#000000", "#FFFFFF", "#000000");
myScroller1.setFont("Verdana,Arial,Helvetica", 1);
myScroller1.addItem("<center><a href=\"article.php?sid=74\">Magna Charta sucht DICH!</a></center>");
myScroller1.addItem("<center><a href=\"article.php?sid=72\">We are with you.</a></center>");
myScroller1.addItem("<center><a href=\"article.php?sid=58\">EverQuest Fan Faire in Europe</a></center>");

function init() {

var img;
var x, y;
img = getImage("placeholder");
x = getImagePageLeft(img);
y = getImagePageTop(img);
myScroller1.create();
myScroller1.hide();
myScroller1.moveTo(x, y);
myScroller1.setzIndex(100);
myScroller1.show();

}
