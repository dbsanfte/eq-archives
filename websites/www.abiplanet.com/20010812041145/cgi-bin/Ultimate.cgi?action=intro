	if (DaysPrune == null) {
		var DaysPrune = "20";
	}
} else {
var DaysPrune = "20";
}

expireDate = new Date;
if (expireDate.getYear() < 1900) {
expireDate.setYear(expireDate.getYear() + 1900 + 1);
} else {
expireDate.setYear(expireDate.getYear()+1);
}

if (noNewLogin != "true") {

var CookieLogin = escape("2452133.2111");
