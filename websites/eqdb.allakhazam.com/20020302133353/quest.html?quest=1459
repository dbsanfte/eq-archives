	     function CheckIt() {
	       if (document.pm.subject.value == ""){
		 alert('You Need to Enter a Subject');
		 return false;
	       }
	       document.pm.submit();
	     }
	     </script><p align=center style='font-size:10px;'>Forum system by <a href=mailto:illia@nector.com>Illia</a></p></ul>
</td></tr>
