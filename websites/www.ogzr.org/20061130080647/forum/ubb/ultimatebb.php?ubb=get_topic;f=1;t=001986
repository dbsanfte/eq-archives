function getCookie(name) {
	var cname = name + "=";
	var dc = document.cookie;

	if (dc.length > 0) {
		begin = dc.indexOf(cname);
		if (begin != -1) {
			begin += cname.length;
			end = dc.indexOf(";", begin);
			if (end == -1)  {
				end = dc.length;
			} // end if
			return (dc.substring(begin, end));
		} // end if
	} // end if
	return null;
}


var u_cookie = getCookie('ubber2452763.0000');
