document.writeln('<IMG SRC="http://innercircle.at.stomped.com/ubb/hotclosedb.gif" BORDER="0">');
} else {
document.writeln('<IMG SRC="http://innercircle.at.stomped.com/ubb/closedb.gif" BORDER="0">');
}
}  else {
if (closed_topic == "X") {
document.writeln('<IMG SRC="http://innercircle.at.stomped.com/ubb/yellockfolder.gif" BORDER="0">');
} else if (fiery == "yes") {
document.writeln('<IMG SRC="http://innercircle.at.stomped.com/ubb/hotclosed.gif" BORDER="0">');
} else {
document.writeln('<IMG SRC="http://innercircle.at.stomped.com/ubb/closed.gif" BORDER="0">');
}
}
//-->
</SCRIPT>

</TD>

