function lootWin(url) {

	MyWindow=window.open(url,'MyWindow','toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no,width=420,height=175,left=25,top=25');

	MyWindow.focus();

}

