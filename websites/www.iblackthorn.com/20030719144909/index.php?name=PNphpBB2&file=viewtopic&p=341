/* Based on the original Style Sheet for the PNTheme v2 Theme for phpBB version 2+
Edited by Wunjo  - http://www.iblackthorn.com */


/* General text */
.gensmall{font-size:10px}
.genmed{font-size:11px;color:#FFFFFF;border-color:#FFFFFF} 




/* titles for the topics:could specify viewed link colour too */
.topictitle{font-size:11px;font-weight:bold}
a.topictitle:visited{color:}
a.topictitle:hover{color:}

/* Name of poster in viewmsg.php and viewtopic.php and other places */
.name{font-size:11px;font-weight: bold}

/* Added for Mozilla - still need to look at the black background problem though */
iframe{border:0}

hr{border: 0px solid #1D1D1D;border-top-width:1px;height:0px}

/* Form elements */
form{display:inline}

input{margin-bottom:2px;margin-top:2px;text-indent:2px}

input,textarea,select{border-color:;color:;
font-size:11px}

/* Fancy form styles for IE */
input,textarea,select,input.button{border-width:1px;font:normal 11px Verdana,Arial,Helvetica,sans-serif}

/* The buttons used for bbCode styling in message post */
input.button{background:#2e2e2e;color:#FFFFFF}
input.catbutton{background:#2e2e2e;color:#FFFFFF;font-size:10px}

/* None-bold submit button */
input.liteoption{background: ;color:#ffffff}
/* The main submit button option */
input.mainoption{background:;color:#FFFFFF;font-weight:bold}
/* The text input fields background colour */
input.post,textarea.post,select{background: ;color:#ffffff}

/* Category gradients*/
td.cat{font-weight:bold;letter-spacing:1px;color:;background:#2e2e2e ;height:29px}

/* Main table cell colours and backgrounds */
td.row1{background:#2e2e2e;color:}
td.row2{background:#2e2e2e;color:}
td.row3{background:#272727;color:}
td.spacerow{background:;color:}

/* This is for the table cell above the Topics,Post & Last posts on the index.php */
td.rowpic{background:#2e2e2e repeat-y}

/* Header cells - the blue and silver gradient backgrounds */
th{background: #272727;color:;font-size:11px;
font-weight:bold;height:28px;white-space:nowrap;text-align:center}

/* Code blocks */
.code{background:#1D1D1D;border:1px solid #272727;color:;
font:11px Courier,"Courier New",sans-serif;padding:5px}

/* This is for the error messages that pop up */
.errorline{background:#2e2e2e;color:;border:1px solid }

/* This is the border line & background colour round the entire page */
.bodyline{background:#2e2e2e;color:;border:1px solid #272727}

/* This is the outline round the main forum tables */
.forumline{background:#272727;color:;border:1px solid }

/* This is the line in the posting page which shows the rollover
help line. This is actually a text box,but if set to be the same
colour as the background no one will know;)*/
.helpline{background:#2e2e2e;border-style:none;color:#FFFFFF}

/* The largest text used in the index page title and toptic title etc. */
.maintitle,h1{
font:bold 20px/120% "Trebuchet MS",Verdana,Arial,Helvetica,sans-serif;text-decoration:none}

/* Used for the navigation text,(Page 1,2,3 etc) and the navigation bar when in a forum */
.nav{font-size:11px;font-weight:bold}
.postbody{font:12px/125% Verdana,Arial,Helvetica,sans-serif}

/* Location,number of posts,post date etc */
.postdetails{color:;font-size:10px}

/* Quote blocks */
.quote{background:#1D1D1D;border:1px solid ;color:#FFFFFF;
font:11px/125% Verdana,Arial,Helvetica,sans-serif}

.subtitle,h2{color:;font:bold 18px/180% "Trebuchet MS",Verdana,Arial,Helvetica,sans-serif;
text-decoration:none}

/* This is the gradient background at the top of the page */
.topbkg{background: #2e2e2e repeat-x}
