			<script type="text/javascript"><!-- // --><![CDATA[

			// Only want to load up jQuery 1 time here!
			if(!window.jQuery)
			{
			   var script = document.createElement("script");
			   script.type = "text/javascript";
			   script.async = false;
			   script.src = "//ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js";
			   var oScripts = document.getElementsByTagName("script");
			   var s = oScripts[0];

			   s.parentNode.insertBefore(script, s);
			}
			function dpmodPostAnimateblog_mod_7()
			{
				jQuery(document).ready(function($){
						$(".leftblog_mod_7").click(function() {
							slideRight_blog_mod_7("showblog_mod_7", 0);
						});

						$(".rightblog_mod_7").click(function() {
							slideLeft_blog_mod_7("showblog_mod_7", 0);
						});
					$(".container_blog_mod_7").css({minHeight: $(".wrapper_blog_mod_7").outerHeight() + "px"});
					$(".showblog_mod_7").css({width: $(".container_blog_mod_7").width() + "px"});
				});
			}
			function slideRight_blog_mod_7(oEle, delay) {
				if ($("." + oEle + ":animated").length == 0)
				{
					delay = delay | 0;
					var lastBlock = $("." + oEle + ":last-child");
					var curWidth = $(".container_blog_mod_7").width();
					var cloned = lastBlock.clone();
					cloned.css({marginLeft: "-" + curWidth + "px"});
					$("." + oEle + ":first-child").before(cloned);
					cloned.delay(delay).animate({
						marginLeft: 0
					}, "5000", function() {
						lastBlock.remove();
						if (delay != 0)
							slideRight_blog_mod_7(oEle, delay);
					});
				}
			}

				function slideLeft_blog_mod_7(oEle, delay) {
					if ($("." + oEle + ":animated").length == 0)
					{
						delay = delay | 0;
						var curWidth = $(".container_blog_mod_7").width();
						var firstBlock = $("." + oEle + ":first-child");
						var cloned = firstBlock.clone();
						cloned.css({width: curWidth + "px"});
						$("." + oEle + ":last-child").after(cloned);
						firstBlock.delay(delay).animate({
							marginLeft: "-" + curWidth
						}, "5000", function () {
							firstBlock.remove();
							if (delay != 0)
								slideLeft_blog_mod_7(oEle, delay);
						});
					}
				}
					addLoadEvent(dpmodPostAnimateblog_mod_7);
			// ]]></script>
