var now = new Date();
var seed = now.getTime() % 0xffffffff;

function rand(n) {
seed = (0x015a4e35 * seed) % 0x7fffffff;
return ( seed >> 16 ) % n;
} 

var num = rand(16);

document.write("<img alt='" + alt[num] + "' name='main_image' src='" + pic[num] + "'  border='0' ></a>"); 

