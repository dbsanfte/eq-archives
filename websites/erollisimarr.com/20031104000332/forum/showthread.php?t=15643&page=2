function reputation(postid)
{
	window.open("reputation.php?" + SESSIONURL + "p=" + postid, "Reputation", "toolbar=no,scrollbars=yes,resizable=yes,width=400,height=300");
}
function changemode(mode, which)
{
	if (mode == "hybrid")
	{
		window.location = "showthread.php?" + SESSIONURL + "mode=hybrid&t=15643";
		return false;
	}
	else if (which == "first")
	{
		window.location = "showthread.php?" + SESSIONURL + "p=" + FIRSTPOSTID + "&mode=" + mode + "#post" + FIRSTPOSTID;
		return false;
	}
	else if (which == "last")
	{
		window.location = "showthread.php?" + SESSIONURL + "p=" + LASTPOSTID + "&mode=" + mode + "#post" + LASTPOSTID;
		return false;
	}
}
var FIRSTPOSTID = "164463";
var LASTPOSTID = "165206";
