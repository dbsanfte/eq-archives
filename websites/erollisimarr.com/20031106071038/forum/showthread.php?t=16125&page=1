	<!-- 2 -->
	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b></b></td>
		<td class="smallfont" align="right" nowrap="nowrap">Post #<b><a href="showpost.php?s=72ae96832b74fee17e72daa54ffe9822&amp;p=170859&amp;postcount=6" target="new">6</a></b></td>
