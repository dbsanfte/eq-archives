	<!-- 2 -->
	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b>Zendell</b></td>
		<td class="smallfont" align="right" nowrap="nowrap">Post #<b><a href="showpost.php?s=d8a8d43809e5e9028927b108f8437d4a&amp;p=169093&amp;postcount=24" target="new">24</a></b></td>
