function changemode(mode, which)
{
	if (mode == "hybrid")
	{
		window.location = "showthread.php?" + SESSIONURL + "mode=hybrid&t=16138";
		return false;
	}
	else if (which == "first")
	{
		window.location = "showthread.php?" + SESSIONURL + "p=" + FIRSTPOSTID + "&mode=" + mode + "#post" + FIRSTPOSTID;
		return false;
	}
	else if (which == "last")
	{
		window.location = "showthread.php?" + SESSIONURL + "p=" + LASTPOSTID + "&mode=" + mode + "#post" + LASTPOSTID;
		return false;
	}
}
