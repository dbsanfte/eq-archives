	<!-- message, attachments, sig -->
	
		
			<!-- icon and title -->
			<div class="smallfont">
				
				<span class="time"><strong>Re: EQ2 only allows 4 characters per account</strong></span>
			</div>
			<hr size="1" style="color:#606B73" />
