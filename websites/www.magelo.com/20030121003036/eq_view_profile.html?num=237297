	function getColor(bonus)
	{
		var color="#E0E0E0";
		if(bonus<0)
			color="#F00000";
		else if(bonus>0)
			color="00F000";
		return color;
	}
	
	// affichage des valeurs dans les emplacements prvus
	document.getElementById("hp").appendChild(document.createTextNode((2607)+"/"+(2607)));
	document.getElementById("ac").style.color="#00F000";
	document.getElementById("ac").innerHTML=("<span style='color:"+getColor(368)+";'>"+758+"<\/span><\/td>");
	document.getElementById("atk").style.color="#00F000";
