	function getColor(bonus)
	{
		var color="#E0E0E0";
		if(bonus<0)
			color="#F00000";
		else if(bonus>0)
			color="00F000";
		return color;
	}
	
	// affichage des valeurs dans les emplacements prvus
	document.getElementById("hp").appendChild(document.createTextNode((3934)+"/"+(3934)));
	document.getElementById("ac").style.color="#00F000";
	document.getElementById("ac").innerHTML=("<span style='color:"+getColor(390)+";'>"+1025+"<\/span><\/td>");
	document.getElementById("atk").style.color="#00F000";
	document.getElementById("atk").innerHTML=("<span style='color:"+getColor(170)+";'>"+1203+"<\/span><\/td>");
	
	document.getElementById("str-label").style.color=getColor(106);
	document.getElementById("str-label").appendChild(document.createTextNode(196));
	document.getElementById("sta").style.color=getColor(134);
	document.getElementById("sta").appendChild(document.createTextNode(219));
	document.getElementById("agi").style.color=getColor(48);
	document.getElementById("agi").appendChild(document.createTextNode(143));
	document.getElementById("dex").style.color=getColor(43);
	document.getElementById("dex").appendChild(document.createTextNode(113));
