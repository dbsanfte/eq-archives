	function getColor(bonus)
	{
		var color="#E0E0E0";
		if(bonus<0)
			color="#F00000";
		else if(bonus>0)
			color="00F000";
		return color;
	}
	
	// affichage des valeurs dans les emplacements prvus
	document.getElementById("hp").appendChild(document.createTextNode((2962)+"/"+(2962)));
	document.getElementById("ac").style.color="#00F000";
	document.getElementById("ac").innerHTML=("<span style='color:"+getColor(580)+";'>"+1360+"<\/span><\/td>");
	document.getElementById("atk").style.color="#00F000";
	document.getElementById("atk").innerHTML=("<span style='color:"+getColor(466)+";'>"+1168+"<\/span><\/td>");
	
	document.getElementById("str-label").style.color=getColor(144);
