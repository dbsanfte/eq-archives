	var mod_cat="hp";
	function processMod()
	{
		document.getElementById("mod-header").innerHTML="Stat: <span style='color: #00FF00'>"+mod_cat.toUpperCase()+"<\/span>";
		var buffer="<table style='width: 270px;color: #E0E0E0;font-family: Arial;font-size: 11px;' cellpadding=0 cellspacing=0>";
		var total = 0;
		for(var i=1;i<=tabMod.length;i++)
		{
			
			if(tabMod[i].composant==mod_cat)
			{
				buffer+="<tr><td><span style='cursor: pointer;color: #739FC1;' onclick='showItem("+tabMod[i].slot+")'>"+tabItem[tabMod[i].slot].name+"<\/span><\/td><td align='center' style='width:45px'>"+slot_name[tabMod[i].slot].toLowerCase()+"<\/td><td align='center' style='width:35px'>"+tabMod[i].org+"<\/td><td align='center' style='width:35px'>"+tabMod[i].value_+"<\/td><\/tr>";
				total+=tabMod[i].value_;
			}
		}
	
		if(total!=0)
			buffer+="<tr><td colspan='4'><hr><\/td><\/tr><tr><td>TOTAL :<\/td><td colspan='2'><\/td><td align='center'>"+total+"<\/td><\/tr>";
		buffer+="<\/table>";
		document.getElementById("mod-data").innerHTML=buffer;
		showMod();
	}
	
	
