	function getColor(bonus)
	{
		var color="#E0E0E0";
		if(bonus<0)
			color="#F00000";
		else if(bonus>0)
			color="00F000";
		return color;
	}
	
	// affichage des valeurs dans les emplacements prvus
	document.getElementById("hp").appendChild(document.createTextNode((30)+"/"+(30)));
	document.getElementById("ac").style.color="#00F000";
	document.getElementById("ac").innerHTML=("<span style='color:"+getColor(4)+";'>"+18+"<\/span><\/td>");
	document.getElementById("atk").style.color="#00F000";
	document.getElementById("atk").innerHTML=("<span style='color:"+getColor(0)+";'>"+-1+"<\/span><\/td>");
	
	document.getElementById("str-label").style.color=getColor(0);
	document.getElementById("str-label").appendChild(document.createTextNode(65));
	document.getElementById("sta").style.color=getColor(0);
	document.getElementById("sta").appendChild(document.createTextNode(70));
	document.getElementById("agi").style.color=getColor(0);
	document.getElementById("agi").appendChild(document.createTextNode(85));
	document.getElementById("dex").style.color=getColor(0);
	document.getElementById("dex").appendChild(document.createTextNode(70));
	document.getElementById("wis").style.color=getColor(0);
