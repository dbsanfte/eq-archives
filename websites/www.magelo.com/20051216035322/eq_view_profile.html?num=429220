	slot_name[13]="PRIMARY";
	slot_name[14]="SEC.";
	slot_name[15]="FINGER";
	slot_name[16]="FINGER";
	slot_name[17]="CHEST";
	slot_name[18]="LEGS";
	slot_name[19]="FEET";
	slot_name[20]="WAIST";
