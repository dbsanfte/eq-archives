,'stat',5);tabMod[55]=new mod('mr',18,'stat',3);tabMod[56]=new mod('mr',12,'stat',3);tabMod[57]=new mod('pr',14,'stat',9);tabMod[58]=new mod('pr',9,'stat',5);tabMod[59]=new mod('pr',12,'stat',3);tabMod[60]=new mod('sta',13,'stat',6);tabMod[61]=new mod('sta',7,'stat',5);tabMod[62]=new mod('sta',8,'stat',4);tabMod[63]=new mod('sta',5,'stat',3);tabMod[64]=new mod('sta',18,'stat',2);tabMod[65]=new mod('str',9,'stat',10);tabMod[66]=new mod('str',17,'stat',9);tabMod[67]=new mod('str',19,'stat',7);tabMod[68]=new mod('str',2,'stat',6);tabMod[69]=new mod('str',13,'stat',6);tabMod[70]=new mod('str',7,'stat',5);tabMod[71]=new mod('str',6,'stat',4);tabMod[72]=new mod('str',8,'stat',4);tabMod[73]=new mod('str',1,'stat',3);tabMod[74]=new mod('str',3,'stat',3);tabMod[75]=new mod('str',4,'stat',3);tabMod[76]=new mod('str',5,'stat',3);tabMod[77]=new mod('str',12,'stat',3);tabMod[78]=new mod('str',10,'stat',3);tabMod[79]=new mod('wis',2,'stat',6);tabMod[80]=new mod('wis',10,'stat',5);tabMod[81]=new mod('wis',6,'stat',4);tabMod[82]=new mod('wis',8,'stat',4);
	
	var mod_cat="hp";
	function processMod()
	{
		document.getElementById("mod-header").innerHTML="Stat: <span style='color: #00FF00;font-size:9px'>"+mod_cat.toUpperCase()+"<\/span>";
		var buffer="<table style='width: 270px;color: #E0E0E0;font-family: Arial;font-size: 11px;' cellpadding=0 cellspacing=0>";
		var total = 0;
		for(var i=1;i<=tabMod.length;i++)
		{
			
			if(tabMod[i].composant==mod_cat)
			{
				buffer+="<tr><td><span style='cursor: pointer;color: #739FC1;' onclick='showItem("+tabMod[i].slot+")'>"+tabItem[tabMod[i].slot].name+"<\/span><\/td><td align='center' style='width:45px'>"+slot_name[tabMod[i].slot].toLowerCase()+"<\/td><td align='center' style='width:35px'>"+tabMod[i].org+"<\/td><td align='center' style='width:35px'>"+tabMod[i].value_+"<\/td><\/tr>";
				total+=tabMod[i].value_;
			}
		}
	
		if(total!=0)
			buffer+="<tr><td colspan='4'><hr><\/td><\/tr><tr><td>TOTAL :<\/td><td colspan='2'><\/td><td align='center'>"+total+"<\/td><\/tr>";
		buffer+="<\/table>";
		document.getElementById("mod-data").innerHTML=buffer;
		showMod();
	}
	
	
