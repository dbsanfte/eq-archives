<script type='text/javascript'>
	if(typeof(pr_bonus)=='undefined')
		document.location="./profile_error.html";
	
	if(background_opt==1)
			document.getElementById("body").style.backgroundPosition = "center";
	else if(background_opt==2)
		document.getElementById("body").style.backgroundRepeat = "repeat";
	if(background_url!=null)
		document.getElementById("body").style.backgroundImage = 'url('+background_url+')';
	
	var ie_win32=(document.all!=null && navigator.platform == "Win32");
	var ie = (document.all!=null);
	var cursor="pointer";
	if(ie==true)
		cursor="hand";
	
	var title=name+" "+surname+" - "+race+" "+class_eq;
	if(guild!=null)
		title+=" of "+guild;
	title+="    ["+server+"]";
	document.title=title;
	
	var slot_name = new Array();
	slot_name[1]="EAR";
	slot_name[2]="NECK";
	slot_name[3]="FACE";
	slot_name[4]="HEAD";
	slot_name[5]="EAR";
	slot_name[6]="FINGER";
	slot_name[7]="WRIST";
	slot_name[8]="ARMS";
	slot_name[9]="HANDS";
	slot_name[10]="WRIST";
	slot_name[11]="FINGER";
	slot_name[12]="SHOULD.";
	slot_name[13]="CHEST";
	slot_name[14]="BACK";
	slot_name[15]="WAIST";
	slot_name[16]="LEGS";
	slot_name[17]="FEET";
	slot_name[18]="PRIMARY";
	slot_name[19]="SEC.";
	slot_name[20]="RANGE";
	slot_name[21]="AMMO";
		
	var layer="";
	function drag_drop(e)
	{
	   	if (!document.all)
	    {
	        document.getElementById(layer).style.left = tempx + e.clientX - offsetx;
	        document.getElementById(layer).style.top  = tempy + e.clientY - offsety;
	    }
	    else
	    {
	        document.getElementById(layer).style.left = tempx + event.clientX - offsetx;
	        document.getElementById(layer).style.top  = tempy + event.clientY - offsety;
			if(ie_win32==true)
			{
				document.getElementById(layer+"-trans").style.left = tempx + event.clientX - offsetx;
	        	document.getElementById(layer+"-trans").style.top  = tempy + event.clientY - offsety;
	    	}
		} 
	}
	
	function startdnd(event)
	{
	    offsetx = event.clientX;
	    offsety = event.clientY;
			    
	    tempx = parseInt( document.getElementById(layer).style.left );
	    tempy = parseInt( document.getElementById(layer).style.top );
	    
	    document.onmousemove=drag_drop;
	}
	
	function stopdnd(e)
	{
	    document.onmousemove='';
	}
	
	function setAlphaChannel(id)
	{
	    if (ie_win32==false)
	    	document.getElementById(id).style.backgroundImage="url("+repository+"/misc/alpha50_black.png)";
	}
		
	
	function show_item(num)
	{
		if(num==0)
		{
			document.getElementById("item-info").style.visibility='hidden';
			document.getElementById("details").style.visibility='visible';
			document.getElementById("upperbox2").style.visibility='visible';
		}
		else
		{
			document.getElementById("item-info").innerHTML=(tab_item[num].text);
			document.getElementById("item-info").style.visibility='visible';
			document.getElementById("details").style.visibility='hidden';
			document.getElementById("upperbox2").style.visibility='hidden';
		}
	}
	
	function open_bag(index)
	{
		var buffer="";
		
		
				
		buffer+=("<div id='upperbox' onMousedown=\"layer='alpha-bag';startdnd(event);\" onMouseup='stopdnd();' onSelectStart='return false;'><\/div>");
		for(var i=1;i<=8;i++)
		{
			buffer+=("<div id='slot-bag-"+i+"' class='slot'>");
			if(tab_item[index+i]==null)
				buffer+=("<img style='cursor: "+cursor+"' border=0 alt='' src='"+repository+"/misc/money_mask.png'>");
			else
				buffer+=("<img style='cursor: "+cursor+"' border=0 align='' src='"+tab_item[index+i].icon+"'  onmousedown='show_item("+(index+i)+");' onmouseup='show_item(0);' onmouseout='show_item(0);'>");
			buffer+=("<\/div>");
		}
		buffer+=("<div id='slot-bag-done'><img style='cursor: "+cursor+"' alt='done' src='"+repository+"/misc/done_off.png' onmousedown=\"this.src='"+repository+"/misc/done_on.png'\" onclick=\"this.src='"+repository+"/misc/done_off.png';close_bag();\"><\/div>");
		
		document.getElementById("alpha-bag").innerHTML=buffer;
		document.getElementById("alpha-bag").style.visibility='visible';
		document.getElementById("alpha-bag-trans").style.visibility='visible';
	}
	
	function close_bag()
	{
		document.getElementById("alpha-bag").style.visibility='hidden';
		document.getElementById("alpha-bag-trans").style.visibility='hidden';
	}
	
	var skill_index=-1;
	function show_skill()
	{
		document.getElementById("skill").innerHTML=("<img alt='skill' src='"+repository+"/misc/skill_on.png'>");
		document.getElementById("stat").innerHTML=("<img style='cursor: "+cursor+"' alt='stat' src='"+repository+"/misc/stat_off.png' onclick='show_stat()'>");
		
		document.getElementById("skill-area").style.visibility='visible';
		
		document.getElementById("block1").style.visibility='hidden';
		document.getElementById("block2").style.visibility='hidden';
		document.getElementById("next-level").style.visibility='hidden';
		document.getElementById("bulle-xp").style.visibility='hidden';
		document.getElementById("str").style.visibility='hidden';
		document.getElementById("pr").style.visibility='hidden';
		
		var buffer="";
		for(var i=skill_index*6+1; i<=skill_index*6+6 && i<=tab_skill.length ;i++)
		{
			var level_name="";
			if(tab_skill[i].nbr_point>=100)
				level_name="master";
			else if(tab_skill[i].nbr_point>=90 && tab_skill[i].nbr_point<100)
				level_name="excellent";
			else if(tab_skill[i].nbr_point>=80 && tab_skill[i].nbr_point<90)
				level_name="verygood";
			else if(tab_skill[i].nbr_point>=70 && tab_skill[i].nbr_point<80)
				level_name="good";
			else if(tab_skill[i].nbr_point>=60 && tab_skill[i].nbr_point<70)
				level_name="aboveaverage";
			else if(tab_skill[i].nbr_point>=50 && tab_skill[i].nbr_point<60)
				level_name="average";
			else if(tab_skill[i].nbr_point>=40 && tab_skill[i].nbr_point<50)
				level_name="belowaverage";
			else if(tab_skill[i].nbr_point>=30 && tab_skill[i].nbr_point<40)
				level_name="feeble";
			else if(tab_skill[i].nbr_point>=20 && tab_skill[i].nbr_point<30)
				level_name="verybad";
			else if(tab_skill[i].nbr_point>=10 && tab_skill[i].nbr_point<20)
				level_name="bad";
			else
				level_name="awful";
			
			buffer+="<div style='position: relative;'><img alt='' src='"+repository+"/skill/skill_bck.png'><div id='skill-name'><img alt='' src='"+repository+"/skill/"+tab_skill[i].num+".png'></div><div id='skill-level'>"+tab_skill[i].nbr_point+"</div><div id='skill-level-name'><img alt='' src='"+repository+"/skill/skill_"+level_name+".png'><\/div><\/div>";
		
		
		}
		buffer+="<div id='skill-up'><img style='cursor: "+cursor+"' alt='' src='"+repository+"/skill/up_off.png' onmousedown=\"this.src='"+repository+"/skill/up_on.png'\" onmouseup=\"this.src='"+repository+"/skill/up_off.png'\" onclick='skill_up();'></div><div id='skill-down'><img style='cursor: "+cursor+"' alt='' src='"+repository+"/skill/down_off.png' onmousedown=\"this.src='"+repository+"/skill/down_on.png'\" onmouseup=\"this.src='"+repository+"/skill/down_off.png'\" onclick='skill_down();'><\/div>"
		document.getElementById("skill-area").innerHTML=buffer;
	}
	
	function show_stat()
	{
		document.getElementById("skill").innerHTML=("<img style='cursor: "+cursor+"' alt='skill' src='"+repository+"/misc/skill_off.png' onclick='skill_index=0;show_skill();'>");
		document.getElementById("stat").innerHTML=("<img alt='stat' src='"+repository+"/misc/stat_on.png'>");
		
		document.getElementById("skill-area").style.visibility='hidden';
		skill_index=-1;
		
		document.getElementById("block1").style.visibility='visible';
		document.getElementById("block2").style.visibility='visible';
		document.getElementById("next-level").style.visibility='visible';
		document.getElementById("bulle-xp").style.visibility='visible';
		document.getElementById("str").style.visibility='visible';
		document.getElementById("pr").style.visibility='visible';
	}
	
	function skill_up()
	{
		if(skill_index!=0)
			skill_index--;
		show_skill();
	}
	
	function skill_down()
	{
		if( skill_index<Math.floor(tab_skill.length/6))
			skill_index++;
		show_skill();
	}
	
	function show_bio()
	{
		document.getElementById("main").style.visibility='hidden';
		document.getElementById("skill-area").style.visibility='hidden';
		
		document.getElementById("block1").style.visibility='hidden';
		document.getElementById("block2").style.visibility='hidden';
		document.getElementById("next-level").style.visibility='hidden';
		document.getElementById("bulle-xp").style.visibility='hidden';
		document.getElementById("str").style.visibility='hidden';
		document.getElementById("pr").style.visibility='hidden';
			
		document.getElementById("bio").style.visibility='visible';
	}
	
	function show_adv()
	{
		document.getElementById("main").style.visibility='hidden';
		document.getElementById("skill-area").style.visibility='hidden';
		
		document.getElementById("block1").style.visibility='hidden';
		document.getElementById("block2").style.visibility='hidden';
		document.getElementById("next-level").style.visibility='hidden';
		document.getElementById("bulle-xp").style.visibility='hidden';
		document.getElementById("str").style.visibility='hidden';
		document.getElementById("pr").style.visibility='hidden';
			
		document.getElementById("adv").style.visibility='visible';
	}
	
	function show_main()
	{
		document.getElementById("bio").style.visibility='hidden';
		document.getElementById("adv").style.visibility='hidden';
		document.getElementById("main").style.visibility='visible';
		if(skill_index==-1)
		{
			document.getElementById("block1").style.visibility='visible';
			document.getElementById("block2").style.visibility='visible';
			document.getElementById("next-level").style.visibility='visible';
			document.getElementById("bulle-xp").style.visibility='visible';
			document.getElementById("str").style.visibility='visible';
			document.getElementById("pr").style.visibility='visible';
		}
		else
			document.getElementById("skill-area").style.visibility='visible';
	}
	
	var ability_cat=1;
	function process_adv()
	{
		var ability_cat_name="";
		switch(ability_cat)
		{
			
			case 1:
				ability_cat_name="General Abilities";
				document.getElementById("adv-general").innerHTML=("<img alt='' src='"+repository+"/misc/aa_general_on.png'>");
				document.getElementById("adv-arch").innerHTML=("<img style='cursor: "+cursor+"' alt='' src='"+repository+"/misc/aa_arch_off.png' onmousedown=\"this.src='"+repository+"/misc/aa_arch_on.png'\" onclick='ability_cat=2;process_adv();'>");
				document.getElementById("adv-class").innerHTML=("<img style='cursor: "+cursor+"' alt='' src='"+repository+"/misc/aa_class_off.png' onmousedown=\"this.src='"+repository+"/misc/aa_class_on.png'\" onclick='ability_cat=3;process_adv();'>");
				break;
			case 2:
				ability_cat_name="Archetype Abilities";
				document.getElementById("adv-arch").innerHTML=("<img alt='' src='"+repository+"/misc/aa_arch_on.png'>");
				document.getElementById("adv-general").innerHTML=("<img style='cursor: "+cursor+"' alt='' src='"+repository+"/misc/aa_general_off.png' onmousedown=\"this.src='"+repository+"/misc/aa_general_on.png'\" onclick='ability_cat=1;process_adv();'>");
				document.getElementById("adv-class").innerHTML=("<img style='cursor: "+cursor+"' alt='' src='"+repository+"/misc/aa_class_off.png' onmousedown=\"this.src='"+repository+"/misc/aa_class_on.png'\" onclick='ability_cat=3;process_adv();'>");
				break;
			case 3:
				ability_cat_name="Class Specific Abilities";
				document.getElementById("adv-class").innerHTML=("<img alt='' src='"+repository+"/misc/aa_class_on.png'>");
				document.getElementById("adv-general").innerHTML=("<img style='cursor: "+cursor+"' alt='' src='"+repository+"/misc/aa_general_off.png' onmousedown=\"this.src='"+repository+"/misc/aa_general_on.png'\" onclick='ability_cat=1;process_adv();'>");
				document.getElementById("adv-arch").innerHTML=("<img style='cursor: "+cursor+"' alt='' src='"+repository+"/misc/aa_arch_off.png' onmousedown=\"this.src='"+repository+"/misc/aa_arch_on.png'\" onclick='ability_cat=2;process_adv();'>");
				break;
		}
		document.getElementById("adv-cat").innerHTML=("<table style='width: 246px;color: white;	font-family: Arial;	font-size: 11px;' cellpadding=0 cellspacing=0><tr><td>"+ability_cat_name+"<\/td><td style='width: 19px' align='right'>Cur<\/td><td align='center' style='width: 8px'>/<\/td><td style='width: 22px'>Max<\/td><\/tr><\/table>");
		
		var buffer="<table style='width: 246px;color: #E0E0E0;font-family: Arial;	font-size: 11px;' cellpadding=0 cellspacing=0>";
		for(var i=1;i<=tab_ability.length;i++)
		{
			if(tab_ability[i].cat==ability_cat)
			{
				buffer+="<tr><td>"+tab_ability[i].name+"<\/td><td style='width: 19px;";
				if(tab_ability[i].nbr_point>0)
					buffer+="color: #00FF00;";
				buffer+="' align='right'>"+tab_ability[i].nbr_point+"<\/td><td align='center' style='width: 8px'>/<\/td><td style='width: 22px'>"+tab_ability[i].nbr_point_max+"<\/td><\/tr>";
			}
		}
		buffer+="<\/table>";
		document.getElementById("adv-data").innerHTML=buffer;
	}
		
	function getColor(bonus)
	{
		var color="#E0E0E0";
		if(bonus==0)
			color="#F00000";
		else if(bonus==2)
			color="00F000";
		return color;
	}
	
	function page_setup(txt)
	{
		var tmp="";
		for(var i=0;i<txt.length;i++)
		{
			if(txt.charAt(i)=="\n")
				tmp+="<br>";
			else
				tmp+=txt.charAt(i);
		}
		return tmp;
	}
			
