	document.getElementById("RD").appendChild(document.createTextNode(65));
	document.getElementById("RF").style.color=getColor(9);
	document.getElementById("RF").appendChild(document.createTextNode(34));
	document.getElementById("RC").style.color=getColor(14);
	document.getElementById("RC").appendChild(document.createTextNode(39));
</script>


