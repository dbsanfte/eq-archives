	document.getElementById("RF").style.color=getColor(184);
	document.getElementById("RF").appendChild(document.createTextNode(209));
	document.getElementById("RC").style.color=getColor(138);
	document.getElementById("RC").appendChild(document.createTextNode(163));
</script>

