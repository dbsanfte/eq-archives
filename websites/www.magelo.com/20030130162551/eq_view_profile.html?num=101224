	<script type="text/javascript">
		for(var i=2000;i<2008;i++)
		{
			document.write("<div id='bank-slot-"+(i-1999)+"' class='slot'>");
			if(tabItem[i]==null)
				document.write("<img border=0 alt='' src='/res/eq/misc/money_mask.png'>");
			else if(tabItem[i].capacite==0)
			{
				document.write("<img style='cursor: pointer' title=\""+tabItem[i].name+"\" border=0 align='' src='/res/eq/itemicon/"+tabItem[i].icon+".png'  onclick='showItem("+i+");'>");
				if(tabItem[i].stackable==true)
				{
					if(ieWin32)
						document.write("<div style='position: absolute;padding-top: 2px;text-align: center;width:19px;height:15px;top:26px;left:22px;z-index:6;background-image:url(/res/eq/misc/qt.png)'>"+tabItem[i].qt+"<\/div>");
					else
						document.write("<div style='position: absolute;padding-top: 2px;text-align: center;width:19px;height:15px;top:24px;left:22px;z-index:6;background-image:url(/res/eq/misc/qt.png)'>"+tabItem[i].qt+"<\/div>");
				}
			}
			else 	
				document.write("<img style='cursor: pointer' title=\""+tabItem[i].name+"\" border=0 alt='' src='/res/eq/itemicon/"+tabItem[i].icon+".png' onclick='openBag("+i+");'>");
			document.write("<\/div>");
		}
	</script>
	
