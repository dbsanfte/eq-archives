	var ability_cat=1;
	function processAdv()
	{
		var ability_cat_name="";
		switch(ability_cat)
		{
			case 1:
				ability_cat_name="General Abilities";
				break;
			case 2:
				ability_cat_name="Archetype Abilities";
				break;
			case 3:
				ability_cat_name="Class Specific Abilities";
				break;
			case 4:
				ability_cat_name="PoP Advance";
				break;
			case 5:
				ability_cat_name="PoP Abilities";
				break;
			case 6:
				ability_cat_name="GoD Abilities";
				break;
			case 7:
				ability_cat_name="Omens Abilities";
				break;
		}
		document.getElementById("adv-cat").innerHTML=("<table style='width: 220px;color: white;	font-family: Arial;	font-size: 11px;' cellpadding=0 cellspacing=0><tr><td>"+ability_cat_name+"<\/td><td style='width: 19px' align='right'>Cur<\/td><td align='center' style='width: 8px'>/<\/td><td style='width: 22px'>Max<\/td><td align='center' style='width: 5px'><\/td><td style='width: 22px'>Cost<\/td><\/tr><\/table>");
		
		var buffer="<table style='width: 220px;color: #E0E0E0;font-family: Arial;	font-size: 11px;' cellpadding=0 cellspacing=0>";
		for(var i=1;i<=tabAbility.length;i++)
		{
			if(tabAbility[i].cat==ability_cat)
			{
				buffer+="<tr><td>"+tabAbility[i].name+"<\/td><td style='width: 19px;";
				if(tabAbility[i].nbr_point>0)
					buffer+="color: #00FF00;";
				buffer+="' align='right'>"+tabAbility[i].nbr_point+"<\/td><td align='center' style='width: 8px'>/<\/td><td style='width: 22px'>"+tabAbility[i].nbr_point_max+"<\/td><td align='center' style='width: 5px'><\/td><td align='center' style='width: 22px'>"+tabAbility[i].next_point_cost+"<\/td><\/tr>";
			}
		}
		buffer+="<\/table>";
		document.getElementById("adv-data").innerHTML=buffer;
		
		showAdv();
	}
	
	var leader_cat=10;
	function processLeader()
	{
		var leader_cat_name="";
		switch(leader_cat)
		{
			case 10:
				leader_cat_name="Leadership Group Abilities";
				break;
			case 11:
				leader_cat_name="Leadership Raid Abilities";
				break;
		}
		document.getElementById("leader-cat").innerHTML=("<table style='width: 220px;color: white;	font-family: Arial;	font-size: 11px;' cellpadding=0 cellspacing=0><tr><td>"+leader_cat_name+"<\/td><td style='width: 19px' align='right'>Cur<\/td><td align='center' style='width: 8px'>/<\/td><td style='width: 22px'>Max<\/td><td align='center' style='width: 5px'><\/td><td style='width: 22px'>Cost<\/td><\/tr><\/table>");
		
		var buffer="<table style='width: 220px;color: #E0E0E0;font-family: Arial;	font-size: 11px;' cellpadding=0 cellspacing=0>";
		for(var i=1;i<=tabAbility.length;i++)
		{
			if(tabAbility[i].cat==leader_cat)
			{
				buffer+="<tr><td>"+tabAbility[i].name+"<\/td><td style='width: 19px;";
				if(tabAbility[i].nbr_point>0)
					buffer+="color: #00FF00;";
				buffer+="' align='right'>"+tabAbility[i].nbr_point+"<\/td><td align='center' style='width: 8px'>/<\/td><td style='width: 22px'>"+tabAbility[i].nbr_point_max+"<\/td><td align='center' style='width: 5px'><\/td><td align='center' style='width: 22px'>"+tabAbility[i].next_point_cost+"<\/td><\/tr>";
			}
		}
		buffer+="<\/table>";
		document.getElementById("leader-data").innerHTML=buffer;
		
		showLeader();
	}
	
	function mod(composant,slot,org,value_){this.composant=composant;this.slot=slot;this.org=org;this.value_=value_;}
	var tabMod = new makeArray(1);
	tabMod[1]=new mod('ac',13,'stat',10);
	
	function modSet(mod,calc,cap){this.mod=mod;this.calc=calc;this.cap=cap;}
	var tabModSet = new makeArray(21);
	tabModSet[1]=new modSet('cha','sum','218');tabModSet[2]=new modSet('sta','sum','108');tabModSet[3]=new modSet('mr','sum','458');tabModSet[4]=new modSet('wis','sum','188');tabModSet[5]=new modSet('sing','max','0');tabModSet[6]=new modSet('haste','max','0');tabModSet[7]=new modSet('dr','sum','485');tabModSet[8]=new modSet('mana+','sum','15');tabModSet[9]=new modSet('int','sum','195');tabModSet[10]=new modSet('hp+','sum','35');tabModSet[11]=new modSet('pr','sum','485');tabModSet[12]=new modSet('agi','sum','180');tabModSet[13]=new modSet('str','sum','105');tabModSet[14]=new modSet('percu','max','0');tabModSet[15]=new modSet('wind','max','0');tabModSet[16]=new modSet('cr','sum','475');tabModSet[17]=new modSet('atk','sum','250');tabModSet[18]=new modSet('string','max','0');tabModSet[19]=new modSet('brass','max','0');tabModSet[20]=new modSet('dex','sum','185');tabModSet[21]=new modSet('fr','sum','475');
	
	var mod_cat="hp";
	function processMod()
	{
		document.getElementById("mod-header").innerHTML="Stat: <span style='color: #00FF00;font-size:8px'>"+mod_cat.toUpperCase()+"<\/span>";
		var buffer="<table style='width: 267px;color: #E0E0E0;font-family: Arial;font-size: 11px;' cellpadding=0 cellspacing=0>";
		var total = 0, calc='sum', cap=0;
		
		var modFound = false;
		for(var i=1;i<=tabModSet.length && modFound==false;i++)
		{
			if(tabModSet[i].mod==mod_cat)
			{
				modFound=true;
				calc = tabModSet[i].calc;
				cap = tabModSet[i].cap;
			}
		}
		
		for(var i=1;i<=tabMod.length;i++)
		{
			
			if(tabMod[i].composant==mod_cat)
			{
				buffer+="<tr><td><span style='cursor: pointer;color: #739FC1;' onclick='showItem("+tabMod[i].slot+")'>";
				if(tabItem[tabMod[i].slot].name.replace("(Augmented)","*").length>26)
					buffer+=tabItem[tabMod[i].slot].name.replace("(Augmented)","*").substring(0,23)+"...";
				else
					buffer+=tabItem[tabMod[i].slot].name.replace("(Augmented)","*");
				buffer+="<\/span><\/td><td align='center' style='width:45px'>"+slot_name[tabMod[i].slot].toLowerCase()+"<\/td><td align='center' style='width:35px'>"+tabMod[i].org+"<\/td><td align='center' style='width:35px'>"+tabMod[i].value_+"<\/td><\/tr>";
				if(calc=='sum')
					total+=tabMod[i].value_;
				else
					total=Math.max(total,tabMod[i].value_);
			}
		}
		
		if(total!=0)
		{
			buffer+="<tr><td colspan='4'><hr><\/td><\/tr><tr><td>";
			if(calc=='sum')
				buffer+="TOTAL";
			else
				buffer+="MAX";
			buffer+=":<\/td><td colspan='2'><\/td><td align='center'>"+total;
			if(cap!=0)
				buffer+="/"+cap;
			buffer+="<\/td><\/tr>";
		}
		
		buffer+="<\/table>";
		document.getElementById("mod-data").innerHTML=buffer;
		showMod();
	}
	
	var mod2_cat="hp+";
	function processMod2()
	{
		document.getElementById("mod2-header").innerHTML="Stat: <span style='color: #00FF00;font-size:8px'>"+mod2_cat.toUpperCase()+"<\/span>";
		var buffer="<table style='width: 267px;color: #E0E0E0;font-family: Arial;font-size: 11px;' cellpadding=0 cellspacing=0>";
		var total = 0, calc='sum', cap=0;
		
		var modFound = false;
		for(var i=1;i<=tabModSet.length && modFound==false;i++)
		{
			if(tabModSet[i].mod==mod2_cat)
			{
				modFound=true;
				calc = tabModSet[i].calc;
				cap = tabModSet[i].cap;
			}
		}
		
		for(var i=1;i<=tabMod.length;i++)
		{
			
			if(tabMod[i].composant==mod2_cat)
			{
				buffer+="<tr><td><span style='cursor: pointer;color: #739FC1;' onclick='showItem("+tabMod[i].slot+")'>";
				if(tabItem[tabMod[i].slot].name.replace("(Augmented)","*").length>26)
					buffer+=tabItem[tabMod[i].slot].name.replace("(Augmented)","*").substring(0,23)+"...";
				else
					buffer+=tabItem[tabMod[i].slot].name.replace("(Augmented)","*");
				buffer+="<\/span><\/td><td align='center' style='width:45px'>"+slot_name[tabMod[i].slot].toLowerCase()+"<\/td><td align='center' style='width:35px'>"+tabMod[i].org+"<\/td><td align='center' style='width:35px'>"+tabMod[i].value_+"<\/td><\/tr>";
				if(calc=='sum')
					total+=tabMod[i].value_;
				else
					total=Math.max(total,tabMod[i].value_);
			}
		}
		
		if(total!=0)
		{
			buffer+="<tr><td colspan='4'><hr><\/td><\/tr><tr><td>";
			if(calc=='sum')
				buffer+="TOTAL";
			else
				buffer+="MAX";
			buffer+=":<\/td><td colspan='2'><\/td><td align='center'>"+total;
			if(cap!=0)
				buffer+="/"+cap;
			buffer+="<\/td><\/tr>";
		}
		
		buffer+="<\/table>";
		document.getElementById("mod2-data").innerHTML=buffer;
		showMod2();
	}
	
	
</script>






