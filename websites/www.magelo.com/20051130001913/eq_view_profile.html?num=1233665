		document.getElementById("skill-header").style.width  = '345px';
		document.getElementById("spell-header").style.width  = '345px';
		document.getElementById("focus-header").style.width  = '345px';
		document.getElementById("keys-header").style.width  = '345px';
		document.getElementById("flags-header").style.width  = '345px';
		
		
		document.getElementById("screenshot-text").style.lineHeight = '0.95';

		//XP
		document.getElementById("xp").childNodes.item(1).style.width  = '54px';
		document.getElementById("altXp").childNodes.item(1).style.width  = '34px';
		document.getElementById("aaXp").childNodes.item(1).style.width  = '34px';
		document.getElementById("grpXp").childNodes.item(1).style.width  = '0px';
		document.getElementById("raidXp").childNodes.item(1).style.width  = '0px';
	}
	else
	{
		//XP
		document.getElementById("xp").childNodes.item(3).style.width  = '54px';
		document.getElementById("altXp").childNodes.item(3).style.width  = '34px';
		document.getElementById("aaXp").childNodes.item(3).style.width  = '34px';
