		document.getElementById("debug").style.width=334;
		
		document.getElementById("macaron").style.top=256;
		
		document.getElementById("bio-text").style.width=315;
		
		document.getElementById("mod-header").style.height=398;
		document.getElementById("mod-table-header").style.left=69;
		document.getElementById("mod-data").style.left=79;
		
	}
	
	function getColor(bonus)
	{
		var color="#E0E0E0";
		if(bonus<0)
			color="#F00000";
		else if(bonus>0)
			color="00F000";
		return color;
	}
	
	// affichage des valeurs dans les emplacements prvus
