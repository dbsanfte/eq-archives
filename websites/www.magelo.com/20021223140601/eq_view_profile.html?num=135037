<script type="text/javascript">
	
	if(ieWin32)
	{
		document.getElementById("character-alpha").style.backgroundColor='black';
		document.getElementById("magelo-alpha").style.backgroundColor='black';
		document.getElementById("desc-alpha").style.backgroundColor='black';
		document.getElementById("bag-alpha").style.backgroundColor='black';
		
		document.getElementById("adv-cat").style.width  = '345px';
		document.getElementById("skill-header").style.width  = '345px';
		document.getElementById("spell-header").style.width  = '345px';
		document.getElementById("focus-header").style.width  = '345px';
		
		document.getElementById("screenshot-text").style.lineHeight = '0.95';
	}
	else
	{
		document.getElementById("character-alpha").style.backgroundImage='url(/res/eq/misc/alpha50_black.png)';
		document.getElementById("character-alpha").style.width=346;
		document.getElementById("character-alpha").style.height=401;
			
		document.getElementById("magelo-alpha").style.backgroundImage='url(/res/eq/misc/alpha50_black.png)';
		document.getElementById("magelo-alpha").style.width=335;
		document.getElementById("magelo-alpha").style.height=151;
		document.getElementById("screenshot-text").style.width=325;
		
		document.getElementById("desc-alpha").style.backgroundImage='url(/res/eq/misc/alpha50_black.png)';
		document.getElementById("desc-alpha").style.width=335;
		document.getElementById("desc-alpha").style.height=241;
		document.getElementById("desc-body").style.width=330;
		
		document.getElementById("bag-alpha").style.backgroundImage='url(/res/eq/misc/alpha50_black.png)'
		document.getElementById("bag-alpha").style.width=97;
		
		document.getElementById("money-gold").style.top=196;
		document.getElementById("money-silver").style.top=219;
		document.getElementById("money-copper").style.top=242;
	}
	
	function getColor(bonus)
	{
		var color="#E0E0E0";
		if(bonus<0)
			color="#F00000";
		else if(bonus>0)
			color="00F000";
		return color;
	}
	
