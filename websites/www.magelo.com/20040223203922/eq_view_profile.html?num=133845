,'stat',3);tabMod[55]=new mod('mr',16,'stat',2);tabMod[56]=new mod('pr',2,'stat',15);tabMod[57]=new mod('pr',5,'stat',7);tabMod[58]=new mod('pr',17,'stat',5);tabMod[59]=new mod('sta',20,'stat',3);tabMod[60]=new mod('sta',18,'stat',2);tabMod[61]=new mod('sta',9,'stat',2);tabMod[62]=new mod('str',17,'stat',10);tabMod[63]=new mod('str',5,'stat',7);tabMod[64]=new mod('str',11,'stat',5);tabMod[65]=new mod('str',10,'stat',5);tabMod[66]=new mod('str',12,'stat',4);tabMod[67]=new mod('str',1,'stat',3);tabMod[68]=new mod('str',20,'stat',3);tabMod[69]=new mod('str',6,'stat',2);tabMod[70]=new mod('str',15,'stat',2);tabMod[71]=new mod('str',9,'stat',2);tabMod[72]=new mod('wis',12,'stat',3);tabMod[73]=new mod('wis',20,'stat',3);tabMod[74]=new mod('wis',9,'stat',2);
	
	var mod_cat="hp";
	function processMod()
	{
		document.getElementById("mod-header").innerHTML="Stat: <span style='color: #00FF00;font-size:9px'>"+mod_cat.toUpperCase()+"<\/span>";
		var buffer="<table style='width: 270px;color: #E0E0E0;font-family: Arial;font-size: 11px;' cellpadding=0 cellspacing=0>";
		var total = 0;
		for(var i=1;i<=tabMod.length;i++)
		{
			
			if(tabMod[i].composant==mod_cat)
			{
				buffer+="<tr><td><span style='cursor: pointer;color: #739FC1;' onclick='showItem("+tabMod[i].slot+")'>"+tabItem[tabMod[i].slot].name+"<\/span><\/td><td align='center' style='width:45px'>"+slot_name[tabMod[i].slot].toLowerCase()+"<\/td><td align='center' style='width:35px'>"+tabMod[i].org+"<\/td><td align='center' style='width:35px'>"+tabMod[i].value_+"<\/td><\/tr>";
				total+=tabMod[i].value_;
			}
		}
		
		if(mod_cat=="haste")
			total=0;
	
		if(total!=0)
			buffer+="<tr><td colspan='4'><hr><\/td><\/tr><tr><td>TOTAL :<\/td><td colspan='2'><\/td><td align='center'>"+total+"<\/td><\/tr>";
		buffer+="<\/table>";
		document.getElementById("mod-data").innerHTML=buffer;
		showMod();
	}
	
	
