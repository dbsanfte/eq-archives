	function filtre(txt)
	{
		var tmp="";
		for(var i=0;i<txt.length;i++)
		{
			if(txt.charAt(i)=="\'")
				tmp+="\\\'";
			else
				tmp+=txt.charAt(i);
		}
		return tmp;
	}
	
	function showItem(position)
	{
		document.getElementById("screenshot-text").innerHTML=tabItem[position].desc;
		showScreenshot()
	}
	
	function getNumSlot(capaciteBag,indice)
	{
		if(indice<=capaciteBag/2)
			return indice;
		else
			return (indice+5-(capaciteBag/2));
	}
	
	function openBag(position)
	{
		showItem(position);
		
		var buffer="";
		for(var i=1;i<=tabItem[position].capacite;i++)
		{
			var index = 0;
			if(position <2000)
				index = 250 + 10*(position-22) + i -1;
			else
				index = (position-2000)*10+2030+i-1;
				
			buffer+=("<div id='bag-slot-"+getNumSlot(tabItem[position].capacite,i)+"' class='slot'><img ");
			if(tabItem[index]!=null)
			{
				buffer+=("style='cursor: pointer' title=\""+tabItem[index].name+"\" border=0 src='/res/eq/itemicon/"+tabItem[index].icon+".png'  onmousedown='showItem("+index+");'>");
				if(tabItem[index].stackable==true)
				{
					if(ieWin32)
						buffer+=("<div style='position: absolute;padding-top: 2px;text-align: center;width:19px;height:15px;top:26px;left:22px;z-index:6;background-image:url(/res/eq/misc/qt.png)'>"+tabItem[index].qt+"<\/div>");
					else
						buffer+=("<div style='position: absolute;padding-top: 2px;text-align: center;width:19px;height:15px;top:24px;left:22px;z-index:6;background-image:url(/res/eq/misc/qt.png)'>"+tabItem[index].qt+"<\/div>");
				}
			}
			else
				buffer+=("border=0 src='/res/eq/misc/money_mask.png'>");
								
			buffer+=("<\/div>");
		}
		
		buffer+=("<div id='bag-button' style='position: absolute;left:25px' class='button' onmouseover=\"this.style.backgroundImage='url(/res/eq/misc/test2.png)';\" onmouseout=\"this.style.backgroundImage='url(/res/eq/misc/test.png)';\" onclick='closeBag();'>Done<\/div>");
		
		document.getElementById("bag").innerHTML=buffer;
		
		if(ieWin32)
			document.getElementById("bag-alpha").style.height=(tabItem[position].capacite/2*40+40);
		else
			document.getElementById("bag-alpha").style.height=(tabItem[position].capacite/2*40+41);
		document.getElementById("bag").style.height=(tabItem[position].capacite/2*40+40);
		document.getElementById("bag-button").style.top=(tabItem[position].capacite/2*40+17);
		
		document.getElementById("bag-alpha").style.visibility='visible';
		document.getElementById("bag").style.visibility='visible';
	}
	
	function closeBag()
	{
		document.getElementById("bag-alpha").style.visibility='hidden';
		document.getElementById("bag").style.visibility='hidden';
		hideScreenshot();
	
	}
	
