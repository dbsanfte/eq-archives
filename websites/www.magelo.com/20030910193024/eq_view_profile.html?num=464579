	var tabAbility = new makeArray(68);function ability(name,cat,nbr_point,nbr_point_max,next_point_cost){this.name=name;this.cat=cat;this.nbr_point=nbr_point;this.nbr_point_max=nbr_point_max;this.next_point_cost=next_point_cost;}tabAbility[1]=new ability('Innate Strength',1,0,5,'1');tabAbility[2]=new ability('Innate Stamina',1,4,5,'1');tabAbility[3]=new ability('Innate Agility',1,0,5,'1');tabAbility[4]=new ability('Innate Dexterity',1,0,5,'1');tabAbility[5]=new ability('Innate Intelligence',1,0,5,'1');tabAbility[6]=new ability('Innate Wisdom',1,0,5,'1');tabAbility[7]=new ability('Innate Charisma',1,0,5,'1');tabAbility[8]=new ability('Innate Fire Protection',1,0,5,'1');tabAbility[9]=new ability('Innate Cold Protection',1,0,5,'1');tabAbility[10]=new ability('Innate Magic Protection',1,0,5,'1');tabAbility[11]=new ability('Innate Poison Protection',1,0,5,'1');tabAbility[12]=new ability('Innate Disease Protection',1,0,5,'1');tabAbility[13]=new ability('First Aid',1,0,3,'1');tabAbility[14]=new ability('Innate Run Speed',1,3,3,'-');tabAbility[15]=new ability('Innate Lung Capacity',1,0,3,'1');tabAbility[16]=new ability('Innate Metabolism',1,3,3,'-');tabAbility[17]=new ability('Innate Regeneration',1,0,3,'1');tabAbility[18]=new ability('New Tanaan Crafting Mastery',1,0,6,'3');tabAbility[19]=new ability('Channeling Focus',2,0,3,'2');tabAbility[20]=new ability('Mental Clarity',2,0,3,'2');tabAbility[21]=new ability('Spell Casting Fury',2,0,3,'2');tabAbility[22]=new ability('Spell Casting Reinforcement',2,0,3,'2');tabAbility[23]=new ability('Combat Agility',2,3,3,'-');tabAbility[24]=new ability('Combat Fury',2,3,3,'-');tabAbility[25]=new ability('Combat Stability',2,3,3,'-');tabAbility[26]=new ability('Fear Resistance',2,0,3,'2');tabAbility[27]=new ability('Finishing Blow',2,0,3,'2');tabAbility[28]=new ability('Natural Durability',2,3,3,'-');tabAbility[29]=new ability('Natural Healing',2,0,3,'2');tabAbility[30]=new ability('Healing Adept',2,3,3,'-');tabAbility[31]=new ability('Hea