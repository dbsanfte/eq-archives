	var tabMod = new makeArray(21);
	tabMod[1]=new mod('ac',17,'stat',9);tabMod[2]=new mod('ac',14,'stat',5);tabMod[3]=new mod('ac',2,'stat',3);tabMod[4]=new mod('ac',7,'stat',3);tabMod[5]=new mod('ac',5,'stat',3);tabMod[6]=new mod('ac',18,'stat',3);tabMod[7]=new mod('ac',12,'stat',3);tabMod[8]=new mod('ac',8,'stat',3);tabMod[9]=new mod('ac',9,'stat',3);tabMod[10]=new mod('ac',19,'stat',3);tabMod[11]=new mod('ac',6,'stat',2);tabMod[12]=new mod('ac',3,'stat',1);tabMod[13]=new mod('ac',20,'stat',1);tabMod[14]=new mod('ac',10,'stat',1);tabMod[15]=new mod('dex',9,'stat',3);tabMod[16]=new mod('pr',17,'stat',10);tabMod[17]=new mod('sta',5,'stat',1);tabMod[18]=new mod('str',5,'stat',1);tabMod[19]=new mod('str',9,'stat',1);tabMod[20]=new mod('wis',15,'stat',2);tabMod[21]=new mod('wis',16,'stat',2);
	
	function modSet(mod,calc,cap){this.mod=mod;this.calc=calc;this.cap=cap;}
	var tabModSet = new makeArray(21);
	tabModSet[1]=new modSet('cha','sum','180');tabModSet[2]=new modSet('sta','sum','180');tabModSet[3]=new modSet('mr','sum','475');tabModSet[4]=new modSet('wis','sum','140');tabModSet[5]=new modSet('sing','max','0');tabModSet[6]=new modSet('haste','max','0');tabModSet[7]=new modSet('dr','sum','485');tabModSet[8]=new modSet('mana+','sum','15');tabModSet[9]=new modSet('int','sum','180');tabModSet[10]=new modSet('hp+','sum','35');tabModSet[11]=new modSet('pr','sum','485');tabModSet[12]=new modSet('agi','sum','155');tabModSet[13]=new modSet('str','sum','190');tabModSet[14]=new modSet('percu','max','0');tabModSet[15]=new modSet('wind','max','0');tabModSet[16]=new modSet('cr','sum','475');tabModSet[17]=new modSet('atk','sum','250');tabModSet[18]=new modSet('string','max','0');tabModSet[19]=new modSet('brass','max','0');tabModSet[20]=new modSet('dex','sum','175');tabModSet[21]=new modSet('fr','sum','475');
	
	var mod_cat="hp";
	function processMod()
	{
		document.getElementById("mod-header").innerHTML="Stat: <span style='color: #00FF00;font-size:8px'>"+mod_cat.toUpperCase()+"<\/span>";
		var buffer="<table style='width: 267px;color: #E0E0E0;font-family: Arial;font-size: 11px;' cellpadding=0 cellspacing=0>";
		var total = 0, calc='sum', cap=0;
		
		var modFound = false;
		for(var i=1;i<=tabModSet.length && modFound==false;i++)
		{
			if(tabModSet[i].mod==mod_cat)
			{
				modFound=true;
				calc = tabModSet[i].calc;
				cap = tabModSet[i].cap;
			}
		}
		
		for(var i=1;i<=tabMod.length;i++)
		{
			
			if(tabMod[i].composant==mod_cat)
			{
				buffer+="<tr><td><span style='cursor: pointer;color: #739FC1;' onclick='showItem("+tabMod[i].slot+")'>";
				if(tabItem[tabMod[i].slot].name.replace("(Augmented)","*").length>26)
					buffer+=tabItem[tabMod[i].slot].name.replace("(Augmented)","*").substring(0,23)+"...";
				else
					buffer+=tabItem[tabMod[i].slot].name.replace("(Augmented)","*");
				buffer+="<\/span><\/td><td align='center' style='width:45px'>"+slot_name[tabMod[i].slot].toLowerCase()+"<\/td><td align='center' style='width:35px'>"+tabMod[i].org+"<\/td><td align='center' style='width:35px'>"+tabMod[i].value_+"<\/td><\/tr>";
				if(calc=='sum')
					total+=tabMod[i].value_;
				else
					total=Math.max(total,tabMod[i].value_);
			}
		}
		
		if(total!=0)
		{
			buffer+="<tr><td colspan='4'><hr><\/td><\/tr><tr><td>";
			if(calc=='sum')
				buffer+="TOTAL";
			else
				buffer+="MAX";
			buffer+=":<\/td><td colspan='2'><\/td><td align='center'>"+total;
			if(cap!=0)
				buffer+="/"+cap;
			buffer+="<\/td><\/tr>";
		}
		
		buffer+="<\/table>";
		document.getElementById("mod-data").innerHTML=buffer;
		showMod();
	}
	
	var mod2_cat="hp+";
	function processMod2()
	{
		document.getElementById("mod2-header").innerHTML="Stat: <span style='color: #00FF00;font-size:8px'>"+mod2_cat.toUpperCase()+"<\/span>";
		var buffer="<table style='width: 267px;color: #E0E0E0;font-family: Arial;font-size: 11px;' cellpadding=0 cellspacing=0>";
		var total = 0, calc='sum', cap=0;
		
		var modFound = false;
		for(var i=1;i<=tabModSet.length && modFound==false;i++)
		{
			if(tabModSet[i].mod==mod2_cat)
			{
				modFound=true;
				calc = tabModSet[i].calc;
				cap = tabModSet[i].cap;
			}
		}
		
		for(var i=1;i<=tabMod.length;i++)
		{
			
			if(tabMod[i].composant==mod2_cat)
			{
				buffer+="<tr><td><span style='cursor: pointer;color: #739FC1;' onclick='showItem("+tabMod[i].slot+")'>";
				if(tabItem[tabMod[i].slot].name.replace("(Augmented)","*").length>26)
					buffer+=tabItem[tabMod[i].slot].name.replace("(Augmented)","*").substring(0,23)+"...";
				else
					buffer+=tabItem[tabMod[i].slot].name.replace("(Augmented)","*");
				buffer+="<\/span><\/td><td align='center' style='width:45px'>"+slot_name[tabMod[i].slot].toLowerCase()+"<\/td><td align='center' style='width:35px'>"+tabMod[i].org+"<\/td><td align='center' style='width:35px'>"+tabMod[i].value_+"<\/td><\/tr>";
				if(calc=='sum')
					total+=tabMod[i].value_;
				else
					total=Math.max(total,tabMod[i].value_);
			}
		}
		
		if(total!=0)
		{
			buffer+="<tr><td colspan='4'><hr><\/td><\/tr><tr><td>";
			if(calc=='sum')
				buffer+="TOTAL";
			else
				buffer+="MAX";
			buffer+=":<\/td><td colspan='2'><\/td><td align='center'>"+total;
			if(cap!=0)
				buffer+="/"+cap;
			buffer+="<\/td><\/tr>";
		}
		
		buffer+="<\/table>";
		document.getElementById("mod2-data").innerHTML=buffer;
		showMod2();
	}
	
	
</script>






