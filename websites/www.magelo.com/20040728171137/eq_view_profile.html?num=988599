<script type="text/javascript">

	if(getParameter("debug")=="true")
	{
		document.getElementById("debug-alpha").style.visibility='visible';
		document.getElementById("debug").style.visibility='visible';
	}
	
	if(ieWin32)
	{
		document.getElementById("character-alpha").style.backgroundColor='black';
		document.getElementById("magelo-alpha").style.backgroundColor='black';
		document.getElementById("bag-alpha").style.backgroundColor='black';
		document.getElementById("bank-alpha").style.backgroundColor='black';
		document.getElementById("debug-alpha").style.backgroundColor='black';
		
		document.getElementById("adv-cat").style.width  = '345px';
		document.getElementById("skill-header").style.width  = '345px';
		document.getElementById("spell-header").style.width  = '345px';
		document.getElementById("focus-header").style.width  = '345px';
		document.getElementById("keys-header").style.width  = '345px';
		document.getElementById("flags-header").style.width  = '345px';
		
		
		document.getElementById("screenshot-text").style.lineHeight = '0.95';
	}
	else
	{
		document.getElementById("character-alpha").style.backgroundImage='url(/res/eq/misc/alpha50_black.png)';
