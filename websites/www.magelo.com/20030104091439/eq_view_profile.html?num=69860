	function getColor(bonus)
	{
		var color="#E0E0E0";
		if(bonus<0)
			color="#F00000";
		else if(bonus>0)
			color="00F000";
		return color;
	}
	
	// affichage des valeurs dans les emplacements prvus
	document.getElementById("hp").appendChild(document.createTextNode((3447)+"/"+(3447)));
	document.getElementById("ac").style.color="#00F000";
	document.getElementById("ac").innerHTML=("<span style='color:"+getColor(620)+";'>"+1270+"<\/span><\/td>");
	document.getElementById("atk").style.color="#00F000";
	document.getElementById("atk").innerHTML=("<span style='color:"+getColor(84)+";'>"+1117+"<\/span><\/td>");
	
