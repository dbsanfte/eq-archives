	document.getElementById("RF").style.color=getColor(75);
	document.getElementById("RF").appendChild(document.createTextNode(120));
	document.getElementById("RC").style.color=getColor(77);
	document.getElementById("RC").appendChild(document.createTextNode(122));
</script>


