	function getParameter(param)
	{
		param+='=';
		var loc=''+document.location;
		var deb=loc.indexOf(param)+param.length;
		var fin=-1;
		var value='';
		if(deb!=(-1+param.length))
		{
			fin=loc.indexOf('&',deb);
			if(fin!=-1)
				value=loc.substring(deb,fin);
			else
				value=loc.substring(deb,loc.length);
		}
		return unescape(value);
	}
	
	if(getParameter("resize")=="true")
		window.resizeTo(800,740);
	
	function makeArray(n){this.length=n;return this;}

	document.title="Sengie The Rising Sun - Dark Elf Cleric [Stromm]";
	document.getElementById('body').style.backgroundPosition = 'center';document.getElementById('body').style.backgroundImage = 'url(http://i5.photobucket.com/albums/y178/porkalator/sengie.gif)';document.getElementById('body').style.backgroundColor = 'white';
	
