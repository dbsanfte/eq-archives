	document.getElementById("RM").appendChild(document.createTextNode(138));
	document.getElementById("RD").style.color=getColor(158);
	document.getElementById("RD").appendChild(document.createTextNode(173));
	document.getElementById("RF").style.color=getColor(103);
	document.getElementById("RF").appendChild(document.createTextNode(148));
	document.getElementById("RC").style.color=getColor(128);
	document.getElementById("RC").appendChild(document.createTextNode(173));
</script>


