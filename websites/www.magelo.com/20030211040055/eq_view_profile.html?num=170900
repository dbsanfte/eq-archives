	function getColor(bonus)
	{
		var color="#E0E0E0";
		if(bonus<0)
			color="#F00000";
		else if(bonus>0)
			color="00F000";
		return color;
	}
	
	// affichage des valeurs dans les emplacements prvus
	document.getElementById("hp").appendChild(document.createTextNode((2409)+"/"+(2409)));
	document.getElementById("ac").style.color="#00F000";
	document.getElementById("ac").innerHTML=("<span style='color:"+getColor(504)+";'>"+1050+"<\/span><\/td>");
	document.getElementById("atk").style.color="#00F000";
