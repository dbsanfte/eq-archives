		//XP
		document.getElementById("xp").childNodes.item(1).style.width  = '16px';
		document.getElementById("altXp").childNodes.item(1).style.width  = '0px';
		document.getElementById("aaXp").childNodes.item(1).style.width  = '0px';
		document.getElementById("grpXp").childNodes.item(1).style.width  = '1px';
		document.getElementById("raidXp").childNodes.item(1).style.width  = '0px';
	}
	else
	{
		//XP
