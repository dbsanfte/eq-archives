	document.getElementById("RM").appendChild(document.createTextNode(69));
	document.getElementById("RD").style.color=getColor(23);
	document.getElementById("RD").appendChild(document.createTextNode(50));
	document.getElementById("RF").style.color=getColor(35);
	document.getElementById("RF").appendChild(document.createTextNode(81));
	document.getElementById("RC").style.color=getColor(54);
	document.getElementById("RC").appendChild(document.createTextNode(79));
</script>


