	{
		document.getElementById("screenshot").style.visibility='hidden';
		document.getElementById("magelo-info").style.visibility='visible';
	}
	
	function showAdv()
	{
		document.getElementById("stat").style.visibility='hidden';
		document.getElementById("adv").style.visibility='visible';
	}
	
