<script type="text/javascript">
	var slot_name = new Array();
	slot_name[0]="CHARM";
	slot_name[1]="EAR";
	slot_name[2]="HEAD";
	slot_name[3]="FACE";
	slot_name[4]="EAR";
	slot_name[5]="NECK";
