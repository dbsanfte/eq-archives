	function getColor(bonus)
	{
		var color="#E0E0E0";
		if(bonus<0)
			color="#F00000";
		else if(bonus>0)
			color="00F000";
		return color;
	}
	
	// affichage des valeurs dans les emplacements pr?vus
	document.getElementById("hp").appendChild(document.createTextNode((8324)+"/"+(8324)));
	document.getElementById("ac").style.color="#00F000";
