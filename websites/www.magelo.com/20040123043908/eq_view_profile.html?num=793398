	var ability_cat=1;
	function processAdv()
	{
		var ability_cat_name="";
		switch(ability_cat)
		{
			case 1:
				ability_cat_name="General Abilities";
				break;
			case 2:
				ability_cat_name="Archetype Abilities";
				break;
			case 3:
				ability_cat_name="Class Specific Abilities";
				break;
			case 4:
				ability_cat_name="PoP Advance";
				break;
			case 5:
				ability_cat_name="PoP Ability";
				break;
		}
		document.getElementById("adv-cat").innerHTML=("<table style='width: 220px;color: white;	font-family: Arial;	font-size: 11px;' cellpadding=0 cellspacing=0><tr><td>"+ability_cat_name+"<\/td><td style='width: 19px' align='right'>Cur<\/td><td align='center' style='width: 8px'>/<\/td><td style='width: 22px'>Max<\/td><td align='center' style='width: 5px'><\/td><td style='width: 22px'>Cost<\/td><\/tr><\/table>");
		
		var buffer="<table style='width: 220px;color: #E0E0E0;font-family: Arial;	font-size: 11px;' cellpadding=0 cellspacing=0>";
		for(var i=1;i<=tabAbility.length;i++)
		{
			if(tabAbility[i].cat==ability_cat)
			{
				buffer+="<tr><td>"+tabAbility[i].name+"<\/td><td style='width: 19px;";
				if(tabAbility[i].nbr_point>0)
					buffer+="color: #00FF00;";
				buffer+="' align='right'>"+tabAbility[i].nbr_point+"<\/td><td align='center' style='width: 8px'>/<\/td><td style='width: 22px'>"+tabAbility[i].nbr_point_max+"<\/td><td align='center' style='width: 5px'><\/td><td align='center' style='width: 22px'>"+tabAbility[i].next_point_cost+"<\/td><\/tr>";
			}
		}
		buffer+="<\/table>";
		document.getElementById("adv-data").innerHTML=buffer;
		
		showAdv();
	}
	
	function mod(composant,slot,org,value_){this.composant=composant;this.slot=slot;this.org=org;this.value_=value_;}
	var tabMod = new makeArray(1);
	tabMod[1]=new mod('ac',17,'stat',2);
	
	var mod_cat="hp";
	function processMod()
	{
		document.getElementById("mod-header").innerHTML="Stat: <span style='color: #00FF00;font-size:9px'>"+mod_cat.toUpperCase()+"<\/span>";
		var buffer="<table style='width: 270px;color: #E0E0E0;font-family: Arial;font-size: 11px;' cellpadding=0 cellspacing=0>";
		var total = 0;
		for(var i=1;i<=tabMod.length;i++)
		{
			
			if(tabMod[i].composant==mod_cat)
			{
				buffer+="<tr><td><span style='cursor: pointer;color: #739FC1;' onclick='showItem("+tabMod[i].slot+")'>"+tabItem[tabMod[i].slot].name+"<\/span><\/td><td align='center' style='width:45px'>"+slot_name[tabMod[i].slot].toLowerCase()+"<\/td><td align='center' style='width:35px'>"+tabMod[i].org+"<\/td><td align='center' style='width:35px'>"+tabMod[i].value_+"<\/td><\/tr>";
				total+=tabMod[i].value_;
			}
		}
		
		if(mod_cat=="haste")
			total=0;
	
		if(total!=0)
			buffer+="<tr><td colspan='4'><hr><\/td><\/tr><tr><td>TOTAL :<\/td><td colspan='2'><\/td><td align='center'>"+total+"<\/td><\/tr>";
		buffer+="<\/table>";
		document.getElementById("mod-data").innerHTML=buffer;
		showMod();
	}
	
	
</script>
