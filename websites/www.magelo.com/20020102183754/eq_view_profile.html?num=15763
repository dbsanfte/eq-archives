<script type='text/javascript' language='JavaScript'>
	var title=name+" "+surname+" - "+race+" "+class_eq;
	if(guild!=null)
		title+=" of "+guild;
	title+="    ["+server+"]";
	document.title=title;
	
	function show_item(num)
	{
		if(num==0)
		{
			document.getElementById("itemText").style.visibility='hidden';
			document.getElementById("itemBackground").style.visibility='hidden';
		}
		else
		{
			document.getElementById("itemText").innerHTML=("<span class='itemFont'>"+tab_item[num].text+"</span>");
			document.getElementById("itemText").style.visibility='visible';
			document.getElementById("itemBackground").style.visibility='visible';
		}
	}
</script>

