<script type='text/javascript' language='JavaScript'>
	for(var i=1;i<=21;i++)
	{
		document.write("<div id='item"+i+"'>");
		if(tab_item[i]!=null)
			document.write("<img style='cursor: hand' border=0 src='"+tab_item[i].icon+"'  onmousedown='show_item("+i+");' onmouseup='show_item(0);' onmouseout='show_item(0);'>");
		document.write("</div>");	
	}
	
	for(var i=22;i<=30;i++)
	{
		document.write("<div id='key"+(i-21)+"'>");
		if(tab_item[i]!=null)
			document.write("<img style='cursor: hand' border=0 src='"+tab_item[i].icon+"'  onmousedown='show_item("+i+");' onmouseup='show_item(0);' onmouseout='show_item(0);'>");
		document.write("</div>");	
	}
</script>

