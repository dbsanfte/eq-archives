	var ability_cat=1;
	function processAdv()
	{
		var ability_cat_name="";
		switch(ability_cat)
		{
			case 1:
				ability_cat_name="General Abilities";
				break;
			case 2:
				ability_cat_name="Archetype Abilities";
				break;
			case 3:
				ability_cat_name="Class Specific Abilities";
				break;
			case 4:
				ability_cat_name="PoP Advance";
				break;
			case 5:
				ability_cat_name="PoP Ability";
				break;
		}
		document.getElementById("adv-cat").innerHTML=("<table style='width: 230px;color: white;	font-family: Arial;	font-size: 11px;' cellpadding=0 cellspacing=0><tr><td>"+ability_cat_name+"<\/td><td style='width: 19px' align='right'>Cur<\/td><td align='center' style='width: 8px'>/<\/td><td style='width: 22px'>Max<\/td><\/tr><\/table>");
		
		var buffer="<table style='width: 230px;color: #E0E0E0;font-family: Arial;	font-size: 11px;' cellpadding=0 cellspacing=0>";
		for(var i=1;i<=tabAbility.length;i++)
		{
			if(tabAbility[i].cat==ability_cat)
			{
				buffer+="<tr><td>"+tabAbility[i].name+"<\/td><td style='width: 19px;";
				if(tabAbility[i].nbr_point>0)
					buffer+="color: #00FF00;";
				buffer+="' align='right'>"+tabAbility[i].nbr_point+"<\/td><td align='center' style='width: 8px'>/<\/td><td style='width: 22px'>"+tabAbility[i].nbr_point_max+"<\/td><\/tr>";
			}
		}
		buffer+="<\/table>";
		document.getElementById("adv-data").innerHTML=buffer;
		
		showAdv();
	}
	
	
	
