	document.getElementById("RF").appendChild(document.createTextNode(32));
	document.getElementById("RC").style.color=getColor(2);
	document.getElementById("RC").appendChild(document.createTextNode(27));
</script>


</body>
</html>		
			
			
			
			
			
			