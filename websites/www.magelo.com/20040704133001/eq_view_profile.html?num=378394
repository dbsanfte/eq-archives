	var ability_cat=1;
	function processAdv()
	{
		var ability_cat_name="";
		switch(ability_cat)
		{
			case 1:
				ability_cat_name="General Abilities";
				break;
			case 2:
				ability_cat_name="Archetype Abilities";
				break;
			case 3:
				ability_cat_name="Class Specific Abilities";
				break;
			case 4:
				ability_cat_name="PoP Advance";
				break;
			case 5:
				ability_cat_name="PoP Abilities";
				break;
			case 6:
				ability_cat_name="GoD Abilities";
				break;
		}
		document.getElementById("adv-cat").innerHTML=("<table style='width: 220px;color: white;	font-family: Arial;	font-size: 11px;' cellpadding=0 cellspacing=0><tr><td>"+ability_cat_name+"<\/td><td style='width: 19px' align='right'>Cur<\/td><td align='center' style='width: 8px'>/<\/td><td style='width: 22px'>Max<\/td><td align='center' style='width: 5px'><\/td><td style='width: 22px'>Cost<\/td><\/tr><\/table>");
		
		var buffer="<table style='width: 220px;color: #E0E0E0;font-family: Arial;	font-size: 11px;' cellpadding=0 cellspacing=0>";
		for(var i=1;i<=tabAbility.length;i++)
		{
			if(tabAbility[i].cat==ability_cat)
			{
				buffer+="<tr><td>"+tabAbility[i].name+"<\/td><td style='width: 19px;";
				if(tabAbility[i].nbr_point>0)
					buffer+="color: #00FF00;";
				buffer+="' align='right'>"+tabAbility[i].nbr_point+"<\/td><td align='center' style='width: 8px'>/<\/td><td style='width: 22px'>"+tabAbility[i].nbr_point_max+"<\/td><td align='center' style='width: 5px'><\/td><td align='center' style='width: 22px'>"+tabAbility[i].next_point_cost+"<\/td><\/tr>";
			}
		}
		buffer+="<\/table>";
		document.getElementById("adv-data").innerHTML=buffer;
		
		showAdv();
	}
	
	var leader_cat=7;
	function processLeader()
	{
		var leader_cat_name="";
		switch(leader_cat)
		{
			case 7:
				leader_cat_name="Leadership Group Abilities";
				break;
			case 8:
				leader_cat_name="Leadership Raid Abilities";
				break;
		}
		document.getElementById("leader-cat").innerHTML=("<table style='width: 220px;color: white;	font-family: Arial;	font-size: 11px;' cellpadding=0 cellspacing=0><tr><td>"+leader_cat_name+"<\/td><td style='width: 19px' align='right'>Cur<\/td><td align='center' style='width: 8px'>/<\/td><td style='width: 22px'>Max<\/td><td align='center' style='width: 5px'><\/td><td style='width: 22px'>Cost<\/td><\/tr><\/table>");
		
		var buffer="<table style='width: 220px;color: #E0E0E0;font-family: Arial;	font-size: 11px;' cellpadding=0 cellspacing=0>";
		for(var i=1;i<=tabAbility.length;i++)
		{
			if(tabAbility[i].cat==leader_cat)
			{
				buffer+="<tr><td>"+tabAbility[i].name+"<\/td><td style='width: 19px;";
				if(tabAbility[i].nbr_point>0)
					buffer+="color: #00FF00;";
				buffer+="' align='right'>"+tabAbility[i].nbr_point+"<\/td><td align='center' style='width: 8px'>/<\/td><td style='width: 22px'>"+tabAbility[i].nbr_point_max+"<\/td><td align='center' style='width: 5px'><\/td><td align='center' style='width: 22px'>"+tabAbility[i].next_point_cost+"<\/td><\/tr>";
			}
		}
		buffer+="<\/table>";
		document.getElementById("leader-data").innerHTML=buffer;
		
		showLeader();
	}
	
	function mod(composant,slot,org,value_){this.composant=composant;this.slot=slot;this.org=org;this.value_=value_;}
	var tabMod = new makeArray(50);
	tabMod[1]=new mod('ac',17,'stat',8);tabMod[2]=new mod('ac',15,'stat',5);tabMod[3]=new mod('ac',8,'stat',5);tabMod[4]=new mod('ac',19,'stat',5);tabMod[5]=new mod('ac',6,'stat',4);tabMod[6]=new mod('ac',3,'stat',3);tabMod[7]=new mod('ac',16,'stat',2);tabMod[8]=new mod('ac',12,'stat',2);tabMod[9]=new mod('ac',9,'stat',2);tabMod[10]=new mod('ac',20,'stat',2);tabMod[11]=new mod('ac',10,'stat',1);tabMod[12]=new mod('agi',4,'stat',4);tabMod[13]=new mod('agi',19,'stat',3);tabMod[14]=new mod('cha',13,'stat',25);tabMod[15]=new mod('cha',14,'stat',15);tabMod[16]=new mod('cha',5,'stat',5);tabMod[17]=new mod('cha',17,'stat',5);tabMod[18]=new mod('cha',16,'stat',3);tabMod[19]=new mod('cr',17,'stat',10);tabMod[20]=new mod('dex',5,'stat',2);tabMod[21]=new mod('dr',6,'stat',-10);tabMod[22]=new mod('hp',16,'stat',35);tabMod[23]=new mod('hp',15,'stat',25);tabMod[24]=new mod('hp',10,'stat',12);tabMod[25]=new mod('hp',19,'stat',10);tabMod[26]=new mod('hp',20,'stat',8);tabMod[27]=new mod('int',5,'stat',6);tabMod[28]=new mod('int',17,'stat',6);tabMod[29]=new mod('int',12,'stat',5);tabMod[30]=new mod('int',20,'stat',4);tabMod[31]=new mod('int',1,'stat',3);tabMod[32]=new mod('int',3,'stat',3);tabMod[33]=new mod('int',9,'stat',3);tabMod[34]=new mod('int',10,'stat',3);tabMod[35]=new mod('int',6,'stat',2);tabMod[36]=new mod('int',4,'stat',2);tabMod[37]=new mod('int',15,'stat',2);tabMod[38]=new mod('mana',16,'stat',35);tabMod[39]=new mod('mana',15,'stat',25);tabMod[40]=new mod('mana',6,'stat',10);tabMod[41]=new mod('mana',12,'stat',10);tabMod[42]=new mod('mana',13,'stat',10);tabMod[43]=new mod('mr',8,'stat',5);tabMod[44]=new mod('pr',15,'stat',10);tabMod[45]=new mod('pr',8,'stat',5);tabMod[46]=new mod('sta',14,'stat',10);tabMod[47]=new mod('str',6,'stat',-2);tabMod[48]=new mod('wis',9,'stat',3);tabMod[49]=new mod('wis',6,'stat',2);tabMod[50]=new mod('wis',15,'stat',2);
	
	var mod_cat="hp";
	function processMod()
	{
		document.getElementById("mod-header").innerHTML="Stat: <span style='color: #00FF00;font-size:9px'>"+mod_cat.toUpperCase()+"<\/span>";
		var buffer="<table style='width: 270px;color: #E0E0E0;font-family: Arial;font-size: 11px;' cellpadding=0 cellspacing=0>";
		var total = 0;
		for(var i=1;i<=tabMod.length;i++)
		{
			
			if(tabMod[i].composant==mod_cat)
			{
				buffer+="<tr><td><span style='cursor: pointer;color: #739FC1;' onclick='showItem("+tabMod[i].slot+")'>"+tabItem[tabMod[i].slot].name.replace("(Augmented)","*")+"<\/span><\/td><td align='center' style='width:45px'>"+slot_name[tabMod[i].slot].toLowerCase()+"<\/td><td align='center' style='width:35px'>"+tabMod[i].org+"<\/td><td align='center' style='width:35px'>"+tabMod[i].value_+"<\/td><\/tr>";
				total+=tabMod[i].value_;
			}
		}
		
		if(mod_cat=="haste")
			total=0;
	
		if(total!=0)
			buffer+="<tr><td colspan='4'><hr><\/td><\/tr><tr><td>TOTAL :<\/td><td colspan='2'><\/td><td align='center'>"+total+"<\/td><\/tr>";
		buffer+="<\/table>";
		document.getElementById("mod-data").innerHTML=buffer;
		showMod();
	}
	
	
