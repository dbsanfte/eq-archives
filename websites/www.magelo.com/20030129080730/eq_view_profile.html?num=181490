	document.getElementById("RF").style.color=getColor(36);
	document.getElementById("RF").appendChild(document.createTextNode(61));
	document.getElementById("RC").style.color=getColor(58);
	document.getElementById("RC").appendChild(document.createTextNode(93));
</script>


</body>
