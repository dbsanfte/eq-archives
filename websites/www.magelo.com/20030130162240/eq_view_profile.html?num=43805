	function getColor(bonus)
	{
		var color="#E0E0E0";
		if(bonus<0)
			color="#F00000";
		else if(bonus>0)
			color="00F000";
		return color;
	}
	
	// affichage des valeurs dans les emplacements prvus
	document.getElementById("hp").appendChild(document.createTextNode((1902)+"/"+(1902)));
	document.getElementById("ac").style.color="#00F000";
	document.getElementById("ac").innerHTML=("<span style='color:"+getColor(323)+";'>"+879+"<\/span><\/td>");
	document.getElementById("atk").style.color="#00F000";
	document.getElementById("atk").innerHTML=("<span style='color:"+getColor(77)+";'>"+957+"<\/span><\/td>");
	
