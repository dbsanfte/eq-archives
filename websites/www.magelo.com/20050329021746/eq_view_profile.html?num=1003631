	document.getElementById("RF").style.color=getColor(217);
	document.getElementById("RF").appendChild(document.createTextNode(242));
	document.getElementById("RC").style.color=getColor(239);
	document.getElementById("RC").appendChild(document.createTextNode(264));
</script>

