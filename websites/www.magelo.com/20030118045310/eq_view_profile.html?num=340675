	function getColor(bonus)
	{
		var color="#E0E0E0";
		if(bonus<0)
			color="#F00000";
		else if(bonus>0)
			color="00F000";
		return color;
	}
	
	// affichage des valeurs dans les emplacements prvus
	document.getElementById("hp").appendChild(document.createTextNode((2808)+"/"+(2808)));
	document.getElementById("ac").style.color="#00F000";
	document.getElementById("ac").innerHTML=("<span style='color:"+getColor(181)+";'>"+590+"<\/span><\/td>");
	document.getElementById("atk").style.color="#00F000";
	document.getElementById("atk").innerHTML=("<span style='color:"+getColor(30)+";'>"+515+"<\/span><\/td>");
	
	document.getElementById("str-label").style.color=getColor(22);
	document.getElementById("str-label").appendChild(document.createTextNode(97));
	document.getElementById("sta").style.color=getColor(115);
	document.getElementById("sta").appendChild(document.createTextNode(190));
	document.getElementById("agi").style.color=getColor(30);
	document.getElementById("agi").appendChild(document.createTextNode(105));
	document.getElementById("dex").style.color=getColor(21);
	document.getElementById("dex").appendChild(document.createTextNode(111));
