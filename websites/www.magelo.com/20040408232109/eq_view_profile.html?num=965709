	document.getElementById("RD").style.color=getColor(17);
	document.getElementById("RD").appendChild(document.createTextNode(32));
	document.getElementById("RF").style.color=getColor(12);
	document.getElementById("RF").appendChild(document.createTextNode(37));
	document.getElementById("RC").style.color=getColor(26);
	document.getElementById("RC").appendChild(document.createTextNode(61));
</script>


