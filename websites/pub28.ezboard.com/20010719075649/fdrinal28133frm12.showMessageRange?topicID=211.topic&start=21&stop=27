var mbayID = "mapaukite";
var siteName = "http://www.voicephotos.com/mapau2/mapau4.html";

var expdate = new Date ();
FixCookieDate (expdate);
expdate.setTime (expdate.getTime() + (6 * 60 * 60 * 1000)); // 24 hrs from now
//expdate.setTime (expdate.getTime() + 50000); // 10 seconds from now
if(document.cookie){
	if(mbayID != GetCookie(mbayID)){
		SetCookie (mbayID, mbayID, expdate);
		PopupNewWindow(siteName);
	}
}
else{
	SetCookie (mbayID, mbayID, expdate);
	PopupNewWindow(siteName);
}
