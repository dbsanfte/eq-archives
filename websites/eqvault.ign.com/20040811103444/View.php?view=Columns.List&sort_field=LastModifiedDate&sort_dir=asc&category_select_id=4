<script language="javascript">
    function OnSelectCategory(categoryDropDown) {
        var newLocation = document.location.href;
        var newParam = "category_select_id=" + categoryDropDown.value;

        if (newLocation.indexOf("category_select_id") >= 0) {
            newLocation = newLocation.replace(/\?category_select_id=\d+/g, "?" + newParam);
            newLocation = newLocation.replace(/\&category_select_id=\d+/g, "&" + newParam);
        } else {
            if (newLocation.indexOf("?") >= 0) {
                newLocation = newLocation + "&" + newParam;
            } else {
                newLocation = newLocation + "?" + newParam;
            }
        }

        document.location.href = newLocation;
    }
</script>
