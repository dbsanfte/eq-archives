      <td width="672" valign="top" style="background-image:  url(images/sub_bg.jpg); background-repeat: no-repeat;"> 
        <!-- InstanceBeginEditable name="Header Image" --><img src="images/f_creatures_head.jpg" width="672" height="56" alt="" border="0"><br><!-- InstanceEndEditable -->
