            <B>4 - Select Stats</B><BR>
            <SELECT name="stats" style="width:12em;">
              <OPTION value="any">Any
              <OPTION value="none">None
              <OPTION value="AGI">+AGI
              <OPTION value="DEX">+DEX
              <OPTION value="INT">+INT
              <OPTION value="STA">+STA
              <OPTION value="STR">+STR
