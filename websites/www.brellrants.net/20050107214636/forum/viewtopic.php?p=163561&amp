<br />
if &#40;v=='B'&#41; &#123;
<br />
b = b + 1;
<br />
breal = breal + 1; 
<br />
&#125; else if &#40;v=='K'&#41; &#123;
<br />
k = k + 1;
<br />
kreal = kreal + 1;
<br />
// This is not a vote for b, so steal some votes&#58;
<br />
robOhio&#40;kreal, &k&#41;;
<br />
&#125; else if &#40;v=='O'&#41; &#123;
<br />
