<tr valign="top">
	<td class="alt2" width="175">
			
			<div id="postmenu_207073">
				<a class="bigusername" href="member.php?s=5d9947021710a01effe3b6d6a5a3ad2b&amp;u=2727">Aligaurius2</a>
				<script type="text/javascript"> vbmenu_register("postmenu_207073", true); </script>
			</div>
                        


			<div class="smallfont">Forum Apprentice</div>
			
			
			<!-- Avatars -->		
			
			<!-- /Avatars -->
			<div class="smallfont">
<br>



<br />
<div>
<a href="http://www.magelo.com/eq_view_profile.html?num=149394" target="blank">Magelo Profile</a></div>

</div>
				
				<div>
					
					
				</div>
				<div>   </div>
			</div>					
		
