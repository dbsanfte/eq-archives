<li class="postbitlegacy postbitim postcontainer" id="post_1016875">
<!-- see bottom of postbit.css for .userinfo .popupmenu styles -->

	<div class="posthead">
		<label for="post_imod_checkbox_1016875">
		<span class="postdate old">
		
		<span class="date">November 1st, 2006&nbsp;<span class="time">06:24 AM</span></span>
		
		</span>
		</label>
		<span class="nodecontrols">
		
			<a name="post1016875" href="showthread.php?44505-F-F-X-I-I&amp;s=5e2918e1fc49993622f06171dcedaa03&amp;p=1016875&amp;viewfull=1#post1016875" class="postcounter">#9</a>
		
		
		</span>
	</div>
	<div class="postdetails">
		<div class="userinfo">
			<div class="username_container">
			
				<div class="popupmenu memberaction">
	<a class="username offline popupctrl" href="member.php?5195-Droigan&amp;s=5e2918e1fc49993622f06171dcedaa03" title="Droigan is offline"><strong>Droigan</strong></a>
	<ul class="popupbody popuphover memberaction_body">
		<li class="left">
			<a href="member.php?5195-Droigan&amp;s=5e2918e1fc49993622f06171dcedaa03" class="siteicon_profile">
