<tr valign="top">
	<td class="alt2" width="175">
			
			<div id="postmenu_352234">
				<a class="bigusername" href="member.php?s=cc188be129921e058622f3392c3d3b69&amp;u=3">Kaelon</a>
				<script type="text/javascript"> vbmenu_register("postmenu_352234", true); </script>
			</div>
                        
			<div class="smallfont">Wizard of Eternity</div>
			
					
			
				<div class="smallfont">
					&nbsp;<br /><a href="member.php?s=cc188be129921e058622f3392c3d3b69&amp;u=3"><img src="image.php?s=cc188be129921e058622f3392c3d3b69&amp;u=3&amp;dateline=1064703098" alt="Kaelon's Avatar" border="0" /></a>
				</div>
			
			
			<div class="smallfont">
				&nbsp;<br />
<div>
<a href="http://www.magelo.com/eq_view_profile.html?num=62579" target="blank">Magelo Profile</a></div>

<div>Join Date: Jun 2003</div>
				
