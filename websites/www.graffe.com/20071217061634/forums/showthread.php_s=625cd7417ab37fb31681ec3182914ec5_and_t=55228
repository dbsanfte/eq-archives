                                  <!-- WoW Style Avatars --><br><table cellspacing ='0' cellpadding ='0' border ='0'><tr><td colspan ='3'><img src ='/forums/images/wowborder/portrait-top.gif' width ='82' height ='8'></td></tr><tr><td><img src ='/forums/images/wowborder/portrait-left.gif' width ='8' height ='64'></td><td background='' style='background-repeat: no-repeat; background-position: center center;'><table cellspacing ='0' cellpadding ='0' border ='0'><tr><td background ='/forums/images/wowborder/level-circle.gif'><div style ='height:64px; width:64px;'><div style ='position: relative; top:52px; left: 44px; width:28px; text-align:center; color: #FFD823;'><b><small>60</small></b></div></td></tr></table></td><td><img src='/forums/images/wowborder/portrait-right.gif' Border='0' width =' 10' height =' 64'></td></tr><tr><td colspan =' 3'><img src ='/forums/images/wowborder/portrait-bot.gif' width =' 82' height =' 11'></td></tr></table><!-- / WoW Style Avatars -->		
				
				
			


			<div class="smallfont">

<!-- Profiles -->
<table width=100%><tr><td align=right width=25%><div class='smallfont'><b>Game</b>:</div></td><td width=75%><div class='smallfont'>EVE</div></td></tr><tr><td align=right width=25%><div class='smallfont'><b>Name</b>:</div></td><td width=75%><div class='smallfont'>Sat Thu</div></td></tr><tr><td align=left colspan=2><div id='postprofile_1271384'><span class='smallfont' style='cursor: pointer;'><b>EVE Profile</b></span><script type='text/javascript'> vbmenu_register('postprofile_1271384', false); </script></div></td></tr></table> 
<!-- / Profiles -->



