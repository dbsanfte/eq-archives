<table id="post1264758" class="tborder" cellpadding="6" cellspacing="0" border="0" width="100%" align="center">
<tr>
	
		<td class="thead" style="font-weight:normal" >		
			<!-- status icon and date -->
			<a name="post1264758"><img class="inlineimg" src="images/legacy_images/statusicon/post_old.gif" alt="Old" border="0" /></a>
