Parser<br />
<div style="margin:20px; margin-top:5px">
	<div class="smallfont" style="margin-bottom:2px">Code:</div>
	<pre class="alt2" style="margin:0px; padding:6px; border:1px inset; width:640px; height:210px; overflow:auto"><div dir="ltr" style="text-align:left;"># Usage: perl crits.pl LOGFILENAME CASTER
# perl crits.pl eqlog_Rikluz_saryrn.txt Rikluz
$crits=0;
$casts=0;
open(LOGFILE, $ARGV[0]) or die &quot;Can't open file $ARGV[0] for reading.\n&quot;;
while(&lt;LOGFILE&gt;)
{
    $crits++ if /You deliver a critical blast!/;
    $casts++ if /$ARGV[1] hit .* points of non-melee damage/;
}
close(LOGFILE);
print &quot;$ARGV[1]: $crits/$casts=&quot; . int(10000*$crits/$casts)/100 . &quot;%\n&quot;;</div></pre>
</div></div>
