<script language="javascript">
	function disp1(){
		var mymonth=document.calendarform.month.options[document.calendarform.month.selectedIndex].value;
		var myyear=document.calendarform.year.options[document.calendarform.year.selectedIndex].value;
	//	var mymode=document.calendarform.mode.value;
		window.location="index.cfm?fuseaction=calendar&year="+myyear+"&month="+mymonth;
	}
	function disp2(){
		var mymonth=document.calendarform.month.options[document.calendarform.month.selectedIndex].value;
		var myyear=document.calendarform.year.options[document.calendarform.year.selectedIndex].value;	
	//	var mymode=document.calendarform.mode.value;
		window.location="index.cfm?fuseaction=calendar&year="+myyear+"&month="+mymonth;
	}
	function nextMonth(){
		var mymonth=document.calendarform.month.options[document.calendarform.month.selectedIndex].value;
		var myyear=document.calendarform.year.options[document.calendarform.year.selectedIndex].value;	
	//	var mymode=document.calendarform.mode.value;
		mymonth++;
		if (mymonth > 12) 
		{
		    myyear++;
			mymonth=1;
		}
		window.location="index.cfm?fuseaction=calendar&year="+myyear+"&month="+mymonth;
	}
	function prevMonth(){
		var mymonth=document.calendarform.month.options[document.calendarform.month.selectedIndex].value;
		var myyear=document.calendarform.year.options[document.calendarform.year.selectedIndex].value;	
	//	var mymode=document.calendarform.mode.value;
		mymonth--;
		if (mymonth < 1) 
		{
		    myyear--;
			mymonth=12;
		}
		window.location="index.cfm?fuseaction=calendar&year="+myyear+"&month="+mymonth;
	}
</script>
	
