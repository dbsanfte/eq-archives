<tr valign="top">
	<td class="alt2" style="border-right: 1px solid #181B16;"  width="160">
		<div id="postmenu_97091">
				
				<a class="bigusername" href="member.php?s=4d97018afe0f180f4236a7ca1f2b9045&amp;u=1064"><div style="font-size:14pt; width:140px;"> richardkent</div></a>
				<script type="text/javascript"> vbmenu_register("postmenu_97091", true); </script>
				
			</div>
			
		    <div class="smallfont">Registered User</div>
			
		
			
				<div class="smallfont">
					&nbsp;<br /><a href="member.php?s=4d97018afe0f180f4236a7ca1f2b9045&amp;u=1064"><img src="images/avatars/0/anon.gif" alt="richardkent's Avatar" border="0" /></a>
