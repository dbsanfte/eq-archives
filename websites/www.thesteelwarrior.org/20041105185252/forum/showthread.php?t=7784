<tr valign="top">
	<td class="alt2" style="border-right: 1px solid #181B16;"  width="160">
		<div id="postmenu_116777">
				
				<a class="bigusername" href="member.php?s=8343a7ddae88fb14308323b609b7b830&amp;u=94"><div style="font-size:14pt; width:140px;"> Glatius2</div></a>
				<script type="text/javascript"> vbmenu_register("postmenu_116777", true); </script>
				
