<!-- open content container -->

	<div align="center">
		<div class="page" style="width:100%; text-align:left">
			<div style="padding:0px 25px 0px 25px">
<div style="padding:0px 0px 6px 0px">


<br />
 <!-- top category table setup -->
<table width="100%" style="background-image:url(http://www.thesteelwarrior.org/forum/images/tren_z/misc/cat_back.gif)" border="0" cellpadding="0" cellspacing="0">
  <tr>
    <td width="88" height="26" align="left" valign="top"><img src="http://www.thesteelwarrior.org/forum/images/tren_z/misc/tren_z_front_top_table_ls.gif" width="88" height="26" alt=" " border="0" /></td>
    <td width="100%" class="tcat" style="background-image:url(http://www.thesteelwarrior.org/forum/images/tren_z/misc/cat_back.gif)" valign="middle"><div class="smallfont" align="center"> <strong></strong></div></td>
     <td width="88" height="26" align="right" valign="top"><img src="http://www.thesteelwarrior.org/forum/images/tren_z/misc/tren_z_front_top_table_rs.gif" width="88" height="26" alt=" " border="0" /></td>
