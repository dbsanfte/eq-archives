<table class="border" cellpadding="6" cellspacing="0" border="0" width="100%" align="center">
<tr>
	<td class="thead" style="font-weight:normal" >		
			<!-- status icon and date -->
			<a name="post73520"><img class="inlineimg" src="http://www.thesteelwarrior.org/forum/images/tren_z/statusicon/post_old.gif" alt="Old" border="0" /></a>
			
				01-09-2004
			
			
			<!-- / status icon and date -->				
	</td>
	<td class="thead" style="font-weight:normal" align="right">
		&nbsp;
		#<a href="showpost.php?s=256c56e9ec5f552209f21f4b0e840834&amp;p=73520&amp;postcount=8" target="new"><strong>8</strong></a>
