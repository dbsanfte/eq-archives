<table class="border" cellpadding="6" cellspacing="0" border="0" width="100%" align="center">
<tr>
	<td class="thead" style="font-weight:normal" >		
			<!-- status icon and date -->
			<a name="post126943"><img class="inlineimg" src="http://www.thesteelwarrior.org/forum/images/tren_z/statusicon/post_old.gif" alt="Old" border="0" /></a>
			
				08-04-2004
			
			
			<!-- / status icon and date -->				
	</td>
	<td class="thead" style="font-weight:normal" align="right">
		&nbsp;
		#<a href="showpost.php?s=b2c6c0573f03790689e0c270c8e99eae&amp;p=126943&amp;postcount=2" target="new"><strong>2</strong></a>
	</td>
