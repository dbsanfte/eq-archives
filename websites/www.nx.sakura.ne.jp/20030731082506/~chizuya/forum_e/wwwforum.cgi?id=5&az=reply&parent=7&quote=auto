<script language="JavaScript">
function postcheck( f ) {
	if( null == f.name.value || '' == f.name.value ) {
		alert('お名前を入力して下さい');
		f.name.focus();
		return false;
	}

	if( null == f.subject.value || '' == f.subject.value ) {
		alert('記事タイトルを入力して下さい');
		f.subject.focus();
		return false;
	}

	if( null == f.content.value || '' == f.content.value ) {
		alert('記事内容を入力して下さい');
		f.content.focus();
		return false;
	}

	return confirm('この内容で送信してよろしいですか？');
}

</script>
