<SCRIPT LANGUAGE="JavaScript">
function edit_event(id) {
	var theform = document.forms["mainform"];
	theform.command.value = "edit_event_form";
	theform.template.value = "add_edit_event.html";
	theform.EVENT_id.value = id;
	theform.submit();
	}
function delete_event(id,occurrence_id, recurrence_id, recurring) {
	var theform = document.forms["mainform"];
	var c = false;
//	if (occurrence_id) {
//		c = confirm("This event is scheduled on multiple dates!\n\nAre you sure you want to delete every occurrence of this event?\n\nTHIS ACTION CANNOT BE UNDONE!");
//		}
	if (recurring==1) {
		c = confirm("This is a recurring event!\n\nAre you sure you want to delete every occurrence of this event?\n\nTHIS ACTION CANNOT BE UNDONE!");
		}
	else {
		c = confirm("Are you sure you want to delete this event?\nTHIS ACTION CANNOT BE UNDONE!");
		}
	if (c) {
		theform.command.value = "delete_event";
		theform.template.value = "edit_delete_events.html";
		theform.EVENT_id.value = id;
		theform.submit();
		}
	}
function reschedule_event(id,occurrence_id, recurrence_id, recurring) {
	var theform = document.forms["mainform"];
	var c = true;
//	if (occurrence_id) {
//		c = confirm("This event is scheduled on multiple dates!\n\nSchedule changes will affect all occurrences of this event!");
//		}
	if (recurring==1) {
		c = confirm("This is a recurring event!\n\nSchedule changes will affect all occurrences of this event!");
		}
	if (c) {
		theform.template_value.value = "edit_delete_events.html";
		theform.command.value = "reschedule_event";
		if (recurring==1) {
			theform.template.value = "schedule_event_recurring.html";
			}
		else {
			theform.template.value = "schedule_event_non_recur.html";
			}
		theform.EVENT_id.value = id;
		theform.submit();
		}
	}
function dosearch(obj) {
	var theform = obj.form;
	if (theform.newsearch.value=="") {
		alert("Search cannot be blank!");
		return false;
		}
	theform.search.value = theform.newsearch.value;
	theform.month.value = theform.newmonth.options[theform.newmonth.selectedIndex].value;
	theform.year.value = theform.newyear.options[theform.newyear.selectedIndex].value;
	theform.command.value = "";
	theform.template.value = "edit_delete_events.html";
	theform.submit();
	}
function doSubmit(theform) {
	if (theform.newsearch.value!="") {
		dosearch(theform.newsearch);
		}
	return true;
	}
function next_month() {
	var theform = document.forms["mainform"];
	theform.month.value++;
	if (theform.month.value > 12) {
		theform.month.value=1;
		theform.year.value++;
		}
	theform.search.value = "";
	theform.command.value = "";
	theform.template.value = "edit_delete_events.html";
	theform.submit();
	}
function last_month() {
	var theform = document.forms["mainform"];
	theform.month.value--;
	if (theform.month.value < 1) {
		theform.month.value=12;
		theform.year.value--;
		}
	theform.search.value = "";
	theform.command.value = "";
	theform.template.value = "edit_delete_events.html";
	theform.submit();
	}
function gotoYear() {
	var theform = document.forms["mainform"];
	theform.month.value = theform.newmonth.options[theform.newmonth.selectedIndex].value;
	theform.year.value = theform.newyear.options[theform.newyear.selectedIndex].value;
	theform.search.value = "";
	theform.command.value = "";
	theform.template.value = "edit_delete_events.html";
	theform.submit();
	}
</SCRIPT>
