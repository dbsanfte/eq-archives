    <script language="JavaScript">
		   var fN = 3
			 var visible = false;
			 var changed = false;
			 var centre;
			 var divPos1;
			 var divPos2;
			 var divPos3;
			 var content;
//evaluate the positioning for the form elements in Div tags
      function setPosition(){
			   for(i=1; i<=3; i++){
				   if (navigator.appName == 'Netscape'){
						 isNN = true
	         }else{
		          isNN = false
	         }
	         if(isNN){
						 width = window.innerWidth
						 centre = eval(width / 2)
						 divPos1 = eval(centre - 290)
						 divPos2 = eval(centre - 383)
						 divPos3 = eval(centre - 354)
						 eval('document.fieldSet'+i+'.left=divPos'+i)
            eval('document.fieldSet1.top="198"')
            eval('document.fieldSet2.top="195"')
            eval('document.fieldSet3.top="190"')
	          }else{
							 width = document.body.clientWidth
							 centre = eval(width / 2)
							 divPos1 = eval(centre - 282)
							 divPos2 = eval(centre - 375)
							 divPos3 = eval(centre - 346)
							 eval('document.all.fieldSet'+i+'.style.left=divPos'+i)
		        }
	       }
      }
	     function showFields(fieldSet){
			   document.parent.p_amount.value="";
		     if (navigator.appName == 'Netscape'){
			     isNS = true;
		     }else{
			      isNS = false;
			   }
		     if (isNS){
					 document.fieldSet3.visibility="hidden";
					 document.fieldSet2.visibility="hidden";
					 eval('document.'+ fieldSet +'.visibility="visible"');
					 visible = true;
				 }else{
						document.all.fieldSet3.style.visibility="hidden";
						document.all.fieldSet2.style.visibility="hidden";
						eval('document.all.'+ fieldSet +'.style.visibility="visible"');
						visible = true;
				 }
	     }
			 function hideFields(){ 
				 document.parent.p_amount.value="";
				 for(i=1; i<=fN; i++){
					 if (navigator.appName == 'Netscape'){
						 isNS = true
          }else{
             isNS = false
          }
          if (isNS){
            eval('document.fieldSet'+i+'.visibility="hidden"')
          }else{
             eval('document.all.fieldSet'+i+'.style.visibility="hidden"');
          }
        }
        visible = false;
      }       
		   function pass2(val){
        document.parent.p_amount.value = val;
		     if(changed) {
			     changed = false;
		     }
      }
