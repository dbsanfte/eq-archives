				<td colspan="2"><span class="postbody">compile this and the one above in the same directory:
<br />

<br />
</span><table width="90%" cellspacing="1" cellpadding="3" border="0" align="center"><tr> 	  <td><span class="genmed"><b>Code:</b></span></td>	</tr>	<tr>	  <td class="code">
<br />
import java.util.Date;
<br />
import java.util.Random;
<br />

<br />
public class RunRandomAPlusGood extends RunDoubleAPlusGood&#123;
<br />
&nbsp; &nbsp;private static boolean DEBUG = true;
<br />
&nbsp; &nbsp;private Random rand = new Random&#40;new Date&#40;&#41;.getTime&#40;&#41;&#41;;
<br />
&nbsp; &nbsp;protected void spew&#40;&#41;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;try&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;super.spew&#40;new Integer&#40;rand.nextInt&#40;Integer.MAX_VALUE&#41;&#41;.toString&#40;&#41;&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&#125;catch&#40;Exception e&#41;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;if&#40;DEBUG&#41;System.err.println&#40;e&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;protected void spew&#40;String guess&#41;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;super.spew&#40;guess&#41;;
<br />
&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;public static void main&#40;String&#91;&#93; args&#41; &#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;RunRandomAPlusGood me = new RunRandomAPlusGood&#40;&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;if&#40;args.length&lt;1&#41;&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;me.spew&#40;&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&#125;else&#123;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;me.spew&#40;args&#91;0&#93;&#41;;
<br />
&nbsp; &nbsp;&nbsp; &nbsp;&#125;
<br />
&nbsp; &nbsp;&#125;
<br />
&#125;
