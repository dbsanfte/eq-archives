			rslt = new Image();
			rslt.src = arg;
			return rslt;
		}
}

function changeImages() {
		if (document.images && (preloadFlag == true)) {
			for (var i=0; i<changeImages.arguments.length; i+=2) {
				document[changeImages.arguments[i]].src = changeImages.arguments[i+1];
			}
		}
}

var preloadFlag = false;

function preloadImages() {
		if (document.images) {
			homeButton_over = newImage("themes/orion1s2/images/homeButton-over.gif");
			calendarButton_over = newImage("themes/orion1s2/images/calendarButton-over.gif");
			guildsButton_over = newImage("themes/orion1s2/images/guildsButton-over.gif");
			forumButton_over = newImage("themes/orion1s2/images/forumButton-over.gif");
			mailboxButton_over = newImage("themes/orion1s2/images/mailboxButton-over.gif");
			chatButton_over = newImage("themes/orion1s2/images/chatButton-over.gif");
			pollsButton_over = newImage("themes/orion1s2/images/pollsButton-over.gif");
			linksButton_over = newImage("themes/orion1s2/images/linksButton-over.gif");
			preloadFlag = true;
		}
}

function popupwin(url, name, properties) {
	popChild=window.open(url, name, properties);
		if(popChild) {
			popChild.focus();
			}
}

function breakout() {
 if (window.top != window.self) {
   window.top.location="index.php";
 }
}

breakout();

-->
</script>
