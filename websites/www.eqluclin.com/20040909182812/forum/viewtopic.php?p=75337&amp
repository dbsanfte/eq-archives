<br />
EAX=ffffffff CS=0167 EIP=024b506a EFLGS=00010202
<br />
EBX=00000000 SS=016f ESP=00aef9d4 EBP=00000000
<br />
ECX=00000000 DS=016f ESI=0000001f FS=8537
<br />
