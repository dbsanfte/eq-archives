Classes: <a href="/cleric/weapons/2h_blunt.shtml">CLR</a>, <a href="/druid/weapons/2h_blunt.shtml">DRU</a>, <a href="/enchanter/weapons/2h_blunt.shtml">ENC</a>, <a href="/magician/weapons/2h_blunt.shtml">MAG</a>, <a href="/monk/weapons/2h_blunt.shtml">MNK</a>, <a href="/necromancer/weapons/2h_blunt.shtml">NEC</a>, <a href="/shadow_knight/weapons/2h_blunt.shtml">SHD</a>, <a href="/shaman/weapons/2h_blunt.shtml">SHM</a>, <a href="/warrior/weapons/2h_blunt.shtml">WAR</a>, <a href="/wizard/weapons/2h_blunt.shtml">WIZ</a><BR>
