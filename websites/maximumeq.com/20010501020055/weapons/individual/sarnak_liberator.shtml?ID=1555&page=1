Classes: <a href="/bard/weapons/1h_slash.shtml">BRD</a>, <a href="/cleric/weapons/1h_slash.shtml">CLR</a>, <a href="/druid/weapons/1h_slash.shtml">DRU</a>, <a href="/enchanter/weapons/1h_slash.shtml">ENC</a>, <a href="/magician/weapons/1h_slash.shtml">MAG</a>, <a href="/monk/weapons/1h_slash.shtml">MNK</a>, <a href="/necromancer/weapons/1h_slash.shtml">NEC</a>, <a href="/paladin/weapons/1h_slash.shtml">PAL</a>, <a href="/ranger/weapons/1h_slash.shtml">RNG</a>, <a href="/rogue/weapons/1h_slash.shtml">ROG</a>, <a href="/shadow_knight/weapons/1h_slash.shtml">SHD</a>, <a href="/shaman/weapons/1h_slash.shtml">SHM</a>, <a href="/warrior/weapons/1h_slash.shtml">WAR</a>, <a href="/wizard/weapons/1h_slash.shtml">WIZ</a><BR>
