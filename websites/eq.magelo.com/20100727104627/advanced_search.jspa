	<script type="text/javascript">
		
		function init()
		{
			DWREngine.setPreHook(function()
				{
					jQuery('#searchWndAjaxImg').show();
				});
				
			DWREngine.setPostHook(function()
				{
					jQuery('#searchWndAjaxImg').hide();
				});
		}
		addLoadEvent(init);
		
		function selectCharacter()
		{
			if(DWRUtil.getValue('character') != "")
				DWRActionUtil.execute({namespace: '/eq', action: 'adv_search_get_char_info', executeResult: 'false'}, 'character', onSelectCharacter);
			else
			{
				DWRUtil.setValue('playableRaceId',"");
				DWRUtil.setValue('classeId',"");	
				DWRUtil.setValue('deityId',"");	
				DWRUtil.setValue('level',"");	
			}
		}
		
		function onSelectCharacter(action)
		{
			DWRUtil.setValue('playableRaceId',action.playableRaceId);
			DWRUtil.setValue('classeId',action.classeId);	
			DWRUtil.setValue('deityId',action.deityId);	
			DWRUtil.setValue('level',action.level);	
		}
		
		function handleAugSlotList()
		{
			if(DWRUtil.getValue('augSlotList') == "54")
				jQuery('#augSlotLabel').html("Aug type");
			else
				jQuery('#augSlotLabel').html("Aug slot");
		
		}
		
		function rollSectionImg(event,section)
		{
			if (!event)
				event = window.event;
			
			if(jQuery('#'+section+'Content').is(":visible"))
			{
				if(event.type=="mouseover")
					rollImage(section+'Img','http://www.magelocdn.com/images/website/localized/picto/eq_manage_roll.png?v=8889');
				else
					rollImage(section+'Img','http://www.magelocdn.com/images/website/localized/picto/eq_deroule_on.png?v=8889');
			}
			else
			{
				if(event.type=="mouseover")
					rollImage(section+'Img','http://www.magelocdn.com/images/website/localized/picto/eq_deroule_roll.png?v=8889');
				else
					rollImage(section+'Img','http://www.magelocdn.com/images/website/localized/picto/eq_manage_off.png?v=8889');
			}
		}
		
		function toggleSection(section)
		{
			if(jQuery('#'+section+'Content').is(":visible"))
			{
				jQuery('#'+section+'Content').hide();
				rollImage(section+'Img','http://www.magelocdn.com/images/website/localized/picto/eq_manage_off.png?v=8889');
			}
			else
			{
				jQuery('#'+section+'Content').show();
				rollImage(section+'Img','http://www.magelocdn.com/images/website/localized/picto/eq_deroule_on.png?v=8889');
			}
		}
		
		function showCharContentTooltip(event,charId,slotId,buildContent,useOriginalDesc)
		{
			if(buildContent)
			{
				var content = eval("contents"+charId+"["+slotId+"]");
				if(content!=undefined)
					showItemTooltip(event,content,buildContent,useOriginalDesc);
				else
					showItemTooltip(event,null,buildContent);
			}
			else
				showItemTooltip(event,null,buildContent);
			
		}
		
		function uncheckExp()
		{
			$(".expansion").attr('checked', false);
		}
		
		function checkExp()
		{
			$(".expansion").attr('checked', true);
		}
