		<script type="text/html" id="ttp-locinfo">
<tpl for="values"> <tpl if="xindex==3"><br></tpl> <div id="cat{[xindex]}" class="cat"> <tpl if="xindex==1">Inventory</tpl><tpl if="xindex==2">Bank</tpl><tpl if="xindex==3"><br>Shared Bank</tpl><br> <div class='grp'> <tpl for="values"><div class='slot {[values!=0? "":"empty"]}' ><tpl if="values != 0">{[values]}</tpl></div>{[xindex%2 == 0 ? "<br>" : ""]}{[parent.length>10 && xindex%8 == 0 ? "</div><div class='grp' style='padding-left:5px;'>" : ""]}</tpl> </div> </div> </tpl>		</script>
	
	<script type="text/javascript">
			var pageTracker;

        if (typeof(jQuery) != "undefined") {
            jQuery(document).ready(function(){

		$("#scope").change(function() { this.form.submit(); });
        Magelo.ToolTip.addContentProvider(function(ttp) {

                {
                    var params = ttp.key.split(":");
                    if (params.length == 4 && params[0] == "eq" && params[1] == "invloc") {

                        var itemId = parseInt(params[2]),charId = parseInt(params[3]), locData = null;
                        for(var i=0;i<data.length;i++) {
                            if(data[i][0] == itemId) {
                                locData = data[i][4];
                                break;
                            }
                        }

                        if(locData) {
                            for(var i=0;i<locData.length;i++) {
                                if(locData[i][0][0] == charId) {
                                    locData = locData[i][1];
                                    break;
                                }
                            }

                            var inv = [],bank=[],shared=[];
                            for(var i=0;i<10;i++)
                                inv.push(0);
                            for(var i=0;i<24;i++)
                                bank.push(0);
                            for(var i=0;i<2;i++)
                                shared.push(0);

                            for(var grp=0;grp<locData.length;grp++) {
                                for(var i=0;i<locData[grp][2].length;i++) {
                                    if(locData[grp][0]=='inventory')
                                        inv[locData[grp][2][i][0]] = locData[grp][2][i][1];
                                    else if(locData[grp][0]=='bank')
                                       bank[locData[grp][2][i][0]] = locData[grp][2][i][1];
                                    else
                                       shared[locData[grp][2][i][0]] = locData[grp][2][i][1];
                                }
                            }

                            var result = [];
                            result.push(inv);
                            result.push(bank);
                            result.push(shared);
                            ttp.setContent(jQuery.Template.from("#ttp-locinfo").apply(result));
                        }

                    }

                }
			});

					pageTracker = _gat._getTracker("UA-271030-4");
			pageTracker._trackPageview();

            });
        } else {

            Ext.onReady(function() {
            });
        }

	</script>
