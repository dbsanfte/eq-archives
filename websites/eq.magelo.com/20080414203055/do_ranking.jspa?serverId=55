	<script type='text/javascript'>
		function handleServerList()
		{
			var regexp = new RegExp("serverId=.*");
			document.getElementsByClassName("rankLink").each(
				function(element)
				{
					element.href=element.href.replace(regexp,"serverId="+DWRUtil.getValue('serverList'));
					//element.href="dedede";
				}
			);
		}
	</script>

