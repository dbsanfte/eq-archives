			function fixFlash(id)
			{
				document.getElementById(id).outerHTML = document.getElementById(id).outerHTML;
			}
			
			function getChildNode(element,position)
			{
				var child = element.childNodes[position];
				if(child.nodeType==3)
					child = element.childNodes[position+1];
				return child;
			}
											function qsfocus()
											{
												var q = Ext.get("qsquery");
												if (q.dom.value == "Quick search")
												{
													q.dom.value = "";
													q.removeClass("default");
												}
											}
											
											function qsblur()
											{
												var q = Ext.get("qsquery");
												if (q.dom.value == "")
												{
													q.addClass("default");
													q.dom.value = "Quick search";
												}
											}
		
		Ext.onReady(function() {
		});
