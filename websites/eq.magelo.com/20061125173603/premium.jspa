		<script type="text/javascript">
		
		var currentWindow;
		function showWin(winId)
		{
			var win = $(winId);
			if(currentWindow == undefined)
				currentWindow = $('1');
			
			Element.hide(currentWindow); 
			switchOff(currentWindow.id);
			Element.show(win); 
			switchOn(winId);
			
			currentWindow=win;
		}
		
		function switchOn(winId)
		{
			var imgId = 'sel'+winId;
			$(imgId).src='images/v1/localized/premium_offer/number_on.png';
			$(winId).focus();
		}
		
		function switchOff(winId)
		{
			var imgId = 'sel'+winId;
			$(imgId).src='images/v1/shared/premium_offer/number_off.png';
		}
		
	</script>
	
