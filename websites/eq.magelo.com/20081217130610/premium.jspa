	<script type="text/javascript">
		
		var currentWindow;
		function showWin(winId)
		{
			var win = $(winId);
			if(currentWindow == undefined)
				currentWindow = $('1');
			
			Element.hide(currentWindow); 
			switchOff(currentWindow.id);
			Element.show(win); 
			switchOn(winId);
			
			currentWindow=win;
		}
		
		function switchOn(winId)
		{
			var imgId = 'sel'+winId;
			$(imgId).src='http://cdn.magelo.com/images/website/localized/premium_offer/eq_number_on.png?v=21';
			$(winId).focus();
		}
		
		function switchOff(winId)
		{
			var imgId = 'sel'+winId;
			$(imgId).src='http://cdn.magelo.com/images/website/shared/premium_offer/number_off.png?v=21';
		}
		
	</script>
	
