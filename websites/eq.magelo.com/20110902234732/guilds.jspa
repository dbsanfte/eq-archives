		function updateNeededClasses()
		{
			var params = {};
			if (typeof(arguments) != "undefined" && arguments.length == 1) {
				var combo = arguments[0];
				params = { serverId : combo.options[combo.selectedIndex].value };
			}
            DWRActionUtil.execute({namespace: '/eq', action: 'lookup_neededClasses', executeResult: 'false'}, params, onUpdateNeededClasses);
		}

		function onUpdateNeededClasses(result)
		{
			jQuery('#guildCount').html(result.nbrRecruitingGuilds);
						
			if(result.nbrRecruitingGuilds > 0)
			{
				jQuery("#noGuildsMsg").hide();
				jQuery("#tickers .ticker-class").each(function()
					{
						var classeId = this.id.replace("ticker-class-","");
						if(result.neededClasses[classeId] ==undefined)
							jQuery(this).hide();
						else
						{
							jQuery("#ticker-class-" + classeId + " .ticker-class-count").html(result.neededClasses[classeId]);
	  						jQuery("#ticker-class-" + classeId + " .ticker-class-link").each(function() {
                                this.href="do_matching_guilds.jspa?classeId="+classeId+"&serverId="+DWRUtil.getValue("server");
                            });
	  						
	  						jQuery(this).show();
						}
					});
			}
			else
			{
				jQuery("#tickers .ticker-class").hide();
				jQuery("#noGuildsMsg").show();
			}
		}
		
