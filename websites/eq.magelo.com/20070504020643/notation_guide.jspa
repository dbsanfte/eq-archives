		<script type="text/javascript">
		
		var currentWindow;
		function showWin(winId)
		{
			var win = $(winId);
			if(currentWindow == undefined)
				currentWindow = $('headings');
			
			Element.hide(currentWindow); 
			switchOff(currentWindow.id);
			Element.show(win); 
			switchOn(winId);
			
			currentWindow=win;
		}
		
		function switchOn(winId)
		{
			var linkId = winId+'_link';
			var imgId = winId+'_img';
			$(linkId).className='on';
			$(imgId).src='images/v6/shared/picto/folder_on.png';
			$(winId).focus();
		}
		
		function switchOff(winId)
		{
			var linkId = winId+'_link';
			var imgId = winId+'_img';
			$(linkId).className='off';
			$(imgId).src='images/v6/shared/picto/folder_off.png';
		}
		
	</script>
	
	<style>
		.on {font-weight: bold;color: #517C8C;font-size: 12px;}
		.on:hover {text-decoration: none;color: #517C8C;}
		.off {cursor: pointer; }
	
