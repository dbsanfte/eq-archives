	<script type="text/javascript">
			
			function getChildNode(element,position)
			{
				var child = element.childNodes[position];
				if(child.nodeType==3)
					child = element.childNodes[position+1];
				return child;
			}
											function qsfocus()
											{
												var q = jQuery("#qsquery");
												if (q.val() == "Quick search")
												{
													q.val("");
													q.removeClass("default");
												}
											}
											
											function qsblur()
											{
												var q = jQuery("#qsquery");
												if (q.val() == "")
												{
													q.addClass("default");
													q.val("Quick search");
												}
											}
			var pageTracker;

        if (typeof(jQuery) != "undefined") {
            jQuery(document).ready(function(){

					pageTracker = _gat._getTracker("UA-271030-4");
			pageTracker._trackPageview();

            });
        } else {

            Ext.onReady(function() {
            });
        }

