					identity = new Identity($.cookie("m"));
					try
					{
						if(identity.isLogged())
						{
							$("#logText").html(identity.getName());
							$("#logLink").attr("href", "/logout.jspa");
							$("#log").attr("title", "Click to logout");
							
							if(!identity.isPremium() && $.cookie("c") != "1")
								$("#lmenu-premium").show();	
						}
						else if($.cookie("c") != "1")
						{
							$("#lmenu-register").show();
						}
					}
					catch(e){}
