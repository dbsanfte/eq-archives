											function qsfocus()
											{
												var q = jQuery("#qsquery");
												if (q.val() == "Quick search")
												{
													q.val("");
													q.removeClass("default");
												}
											}
											
											function qsblur()
											{
												var q = jQuery("#qsquery");
												if (q.val() == "")
												{
													q.addClass("default");
													q.val("Quick search");
												}
											}

