	<script type='text/javascript'>
		
		Ext.Element.prototype.visibilityMode = Ext.Element.DISPLAY;
		
		
		function init()
		{
			DWREngine.setPreHook(function()
			{
				$('guildRecruitWndAjaxImg').show();
			});
			
			DWREngine.setPostHook(function()
			{
				$('guildRecruitWndAjaxImg').hide();
			});
		}
	
		function updateNeededClasses()
		{
			var params = {};
			if (typeof(arguments) != "undefined" && arguments.length == 1) {
				var combo = arguments[0];
				params = { recruitmentServerId : combo.options[combo.selectedIndex].value }; 
			}
            DWRActionUtil.execute({namespace: '/eq', action: 'lookup_neededClasses', executeResult: 'false'}, params, onUpdateNeededClasses);
		}

		function onUpdateNeededClasses(result)
		{
			Ext.get('guildCount').update(result.nbrRecruitingGuilds);
						
			if(result.nbrRecruitingGuilds > 0)
			{
				Ext.get("noGuildsMsg").hide();
				Ext.select(".ticker-class",false,"tickers").each(function(el,caller,index)
					{
						var classeId = el.dom.id.replace("ticker-class-","");
						if(result.neededClasses[classeId] ==undefined)
							el.hide();
						else
						{
							Ext.select("#ticker-class-" + classeId + " .ticker-class-count",false,"tickers").update(result.neededClasses[classeId]);
	  						Ext.select("#ticker-class-" + classeId + " .ticker-class-link",false,"tickers").each(function(el,caller,index)
	  							{
	  								el.dom.href="do_matching_guilds.jspa?classeId="+classeId+"&serverId="+DWRUtil.getValue("server");
	  							});
	  						
	  						el.show();	
						}
					});
			}
			else
			{
				Ext.select(".ticker-class",false,"tickers").hide();
				Ext.get("noGuildsMsg").show();
			}
		}
		
	</script>
	

