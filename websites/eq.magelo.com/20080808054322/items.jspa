													q.dom.value = "";
													q.removeClass("default");
												}
											}
											
											function qsblur()
											{
												var q = Ext.get("qsquery");
												if (q.dom.value == "")
												{
													q.addClass("default");
													q.dom.value = "Quick Search";
												}
											}
		
