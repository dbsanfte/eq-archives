	<div id="edit192445" style="padding:0px 0px 6px 0px">
		


<table id="post192445" class="tborder" cellpadding="6" cellspacing="0" border="0" width="100%" align="center">
<tr>
	
		<td class="thead" style="font-weight:normal"  id="currentPost">		
			<!-- status icon and date -->
			<a name="post192445"><img class="inlineimg" src="images/statusicon/post_old.gif" alt="Old" border="0" /></a>
