<!-- / post #15095 --><!-- post #15096 -->

	<!-- open content container -->

<div align="center">
	<div class="page" style="width:100%; text-align:left">
		<div style="padding:0px 25px 0px 25px">

	<div id="edit15096" style="padding:0px 0px 6px 0px">
	<!-- this is not the last post shown on the page -->	


<table id="post15096" class="tborder" cellpadding="6" cellspacing="0" border="0" width="100%" align="center">
<tr>
	
		<td class="thead" style="font-weight:normal" >		
			<!-- status icon and date -->
			<a name="post15096"><img class="inlineimg" src="images/statusicon/post_old.gif" alt="Old" border="0" /></a>
			01-06-2003, 04:29 PM
			
			<!-- / status icon and date -->				
		</td>
		<td class="thead" style="font-weight:normal" align="right">
			&nbsp;
			#<a href="showpost.php?s=c45d51017128184c1f3ff4980bab25ce&amp;p=15096&amp;postcount=6" target="new" rel="nofollow" id="postcount15096" name="6"><strong>6</strong></a>
			
		</td>
	
</tr>
<tr valign="top">
	<td class="alt2" width="175">
			
			<div id="postmenu_15096">
				
				<a class="bigusername" href="member.php?s=c45d51017128184c1f3ff4980bab25ce&amp;u=10293">Arou</a>
				<script type="text/javascript"> vbmenu_register("postmenu_15096", true); </script>
				
			</div>
			
			<div class="smallfont">Visitor</div>
			
		
			
			
			<div class="smallfont">
				&nbsp;<br />
				<div>Join Date: Dec 2002</div>
				
				
