<tr valign="top">
	<td class="alt2" width="175">
			
			<div id="postmenu_15141">
				
				<a class="bigusername" href="member.php?s=c45d51017128184c1f3ff4980bab25ce&amp;u=8299">sir0die</a>
				<script type="text/javascript"> vbmenu_register("postmenu_15141", true); </script>
				
			</div>
			
			<div class="smallfont">Apprentice Trader</div>
			
		
			
			
			<div class="smallfont">
