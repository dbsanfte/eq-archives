var eqc = new Array(15);
eqc['BRD'] = 'Bard';
eqc['BST'] = 'Beastlord';
eqc['CLR'] = 'Cleric';
eqc['DRU'] = 'Druid';
eqc['ENC'] = 'Enchanter';
eqc['MAG'] = 'Magician';
eqc['MNK'] = 'Monk';
eqc['NEC'] = 'Necro';
eqc['PAL'] = 'Paladin';
