var eqc = new Array(15);
eqc['BRD'] = 'Bard';
eqc['BST'] = 'Beastlord';
eqc['CLR'] = 'Cleric';
eqc['DRU'] = 'Druid';
eqc['ENC'] = 'Enchanter';
eqc['MAG'] = 'Magician';
eqc['MNK'] = 'Monk';
eqc['NEC'] = 'Necro';
eqc['PAL'] = 'Paladin';
eqc['RNG'] = 'Ranger';
eqc['ROG'] = 'Rogue';
eqc['SHD'] = 'Shadow Knight';
eqc['SHM'] = 'Shaman';
eqc['WAR'] = 'Warrior';
eqc['WIZ'] = 'Wizard';

var eqr = new Array(15);
eqr['HUM'] = new Array('BRD','CLR','DRU','ENC','MAG','MNK','NEC','PAL','RNG','ROG','SHD','WAR','WIZ');
