document.writeln('<IMG SRC="http://www.drow.org/forums/ubb/yellockfolder.gif" BORDER="0">');
} else if (fiery == "yes") {
document.writeln('<IMG SRC="http://www.drow.org/forums/ubb/hotclosed.gif" BORDER="0">');
} else {
document.writeln('<IMG SRC="http://www.drow.org/forums/ubb/closed.gif" BORDER="0">');
}
}
//-->
</SCRIPT>

