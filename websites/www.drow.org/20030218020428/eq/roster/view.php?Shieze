var eqc = new Array(15);
eqc['BRD'] = 'Bard';
eqc['BST'] = 'Beastlord';
eqc['CLR'] = 'Cleric';
eqc['DRU'] = 'Druid';
eqc['ENC'] = 'Enchanter';
eqc['MAG'] = 'Magician';
