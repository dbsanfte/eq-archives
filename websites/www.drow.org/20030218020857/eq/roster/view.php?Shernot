var eqc = new Array(15);
eqc['BRD'] = 'Bard';
eqc['BST'] = 'Beastlord';
eqc['CLR'] = 'Cleric';
eqc['DRU'] = 'Druid';
eqc['ENC'] = 'Enchanter';
eqc['MAG'] = 'Magician';
eqc['MNK'] = 'Monk';
eqc['NEC'] = 'Necro';
eqc['PAL'] = 'Paladin';
eqc['RNG'] = 'Ranger';
eqc['ROG'] = 'Rogue';
eqc['SHD'] = 'Shadow Knight';
eqc['SHM'] = 'Shaman';
eqc['WAR'] = 'Warrior';
eqc['WIZ'] = 'Wizard';

var eqr = new Array(15);
eqr['HUM'] = new Array('BRD','CLR','DRU','ENC','MAG','MNK','NEC','PAL','RNG','ROG','SHD','WAR','WIZ');
eqr['BAR'] = new Array('BST','ROG','SHM','WAR');
eqr['DEF'] = new Array('CLR','ENC','MAG','NEC','ROG','SHD','WAR','WIZ');
eqr['DWF'] = new Array('CLR','PAL','ROG','WAR');
eqr['ERU'] = new Array('CLR','ENC','MAG','NEC','PAL','SHD','WIZ');
eqr['GNM'] = new Array('CLR','ENC','MAG','NEC','PAL','ROG','SHD','WAR','WIZ');
eqr['HEF'] = new Array('BRD','DRU','PAL','RNG','ROG','WAR');
eqr['HFL'] = new Array('CLR','DRU','PAL','RNG','ROG','WAR');
eqr['HIE'] = new Array('CLR','ENC','MAG','PAL','WIZ');
eqr['OGR'] = new Array('BST','SHD','SHM','WAR');
eqr['TRL'] = new Array('BST','SHD','SHM','WAR');
eqr['ELF'] = new Array('BRD','DRU','RNG','ROG','WAR');
eqr['IKS'] = new Array('BST','MNK','NEC','SHD','SHM','WAR');
eqr['VAH'] = new Array('BRD','BST','ROG','SHM','WAR');

var eqt = new Array(15);
eqt['BRD'] = new Array('Baron','Baroness','Muse');
eqt['BST'] = new Array('Baron','Baroness','Venerable','Elder');
eqt['CLR'] = new Array('Baron','Baroness','Venerable','Exarch');
eqt['DRU'] = new Array('Baron','Baroness','Venerable','Elder');
eqt['ENC'] = new Array('Baron','Baroness','Master','Mistress','Sage','Arcane');
eqt['MAG'] = new Array('Baron','Baroness','Master','Mistress','Sage','Arcane');
eqt['MNK'] = new Array('Baron','Baroness','Brother','Sister','Sensei');
eqt['NEC'] = new Array('Baron','Baroness','Sage','Lich');
eqt['PAL'] = new Array('Baron','Baroness','Sir','Lady','Duke','Duchess');
eqt['ROG'] = new Array('Baron','Baroness','Veteran','Marauder');
eqt['RNG'] = new Array('Baron','Baroness','Veteran','Hunter','Huntress','Forest Stalker');
eqt['SHD'] = new Array('Baron','Baroness','Sir','Lady','Duke','Duchess');
eqt['SHM'] = new Array('Baron','Baroness','Venerable','Elder');
eqt['WAR'] = new Array('Baron','Baroness','Veteran','Marshall');
eqt['WIZ'] = new Array('Baron','Baroness','Master','Mistress','Sage','Arcane');

function fillClass()
 {
  var classlist = document.post.eqclass;
  var racelist = document.post.eqrace;
  var sel = -1;

  var race = racelist.options[racelist.selectedIndex].value;
  classlist.options.length = 0;
  classlist.options[0] = new Option('', '', true, true);

  for (var i=0; i < eqr[race].length; i++)
   {
    var element = new Option(eqc[eqr[race][i]], eqr[race][i]);
    classlist.options[i+1] = element;
    if (document.post.eqclass_h.value == eqr[race][i])
     {
      sel = i+1;
     }
   }

  if (sel == -1) classlist.options[0].selected=true;
  else classlist.options[sel].selected=true;
 }
function fillTitle()
 {
  var classlist = document.post.eqclass;
  var titlelist = document.post.eqtitle;
  var sel = -1;

  var eqclass = classlist.options[classlist.selectedIndex].value;
  titlelist.options.length = 0;
  titlelist.options[0] = new Option('', '', true, true);

  for (var i=0; i < eqt[eqclass].length; i++)
   {
    var element = new Option(eqt[eqclass][i], eqt[eqclass][i]);
    titlelist.options[i+1] = element;
    if (document.post.eqtitle_h.value == eqt[eqclass][i])
     {
      sel = i+1;
     }
   }

  if (sel == -1) titlelist.options[0].selected=true;
  else titlelist.options[sel].selected=true;
 }
function switchStats(slot)
 {
  document.getElementById('stats').innerHTML = eval("slot" + slot);
 }
