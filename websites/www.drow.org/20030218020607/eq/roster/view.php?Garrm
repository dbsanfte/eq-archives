eqr['GNM'] = new Array('CLR','ENC','MAG','NEC','PAL','ROG','SHD','WAR','WIZ');
eqr['HEF'] = new Array('BRD','DRU','PAL','RNG','ROG','WAR');
eqr['HFL'] = new Array('CLR','DRU','PAL','RNG','ROG','WAR');
eqr['HIE'] = new Array('CLR','ENC','MAG','PAL','WIZ');
eqr['OGR'] = new Array('BST','SHD','SHM','WAR');
eqr['TRL'] = new Array('BST','SHD','SHM','WAR');
eqr['ELF'] = new Array('BRD','DRU','RNG','ROG','WAR');
eqr['IKS'] = new Array('BST','MNK','NEC','SHD','SHM','WAR');
eqr['VAH'] = new Array('BRD','BST','ROG','SHM','WAR');

var eqt = new Array(15);
eqt['BRD'] = new Array('Baron','Baroness','Muse');
eqt['BST'] = new Array('Baron','Baroness','Venerable','Elder');
eqt['CLR'] = new Array('Baron','Baroness','Venerable','Exarch');
eqt['DRU'] = new Array('Baron','Baroness','Venerable','Elder');
eqt['ENC'] = new Array('Baron','Baroness','Master','Mistress','Sage','Arcane');
eqt['MAG'] = new Array('Baron','Baroness','Master','Mistress','Sage','Arcane');
eqt['MNK'] = new Array('Baron','Baroness','Brother','Sister','Sensei');
eqt['NEC'] = new Array('Baron','Baroness','Sage','Lich');
eqt['PAL'] = new Array('Baron','Baroness','Sir','Lady','Duke','Duchess');
eqt['ROG'] = new Array('Baron','Baroness','Veteran','Marauder');
eqt['RNG'] = new Array('Baron','Baroness','Veteran','Hunter','Huntress','Forest Stalker');
