<meta NAME="keywords" CONTENT="Everquest,everquest,eq,verant,Verant,Sony,sony,norrath,quests,eqlizer,eq lizer,equipment,items,bestiary,monsters,spawns,spoilers,cheatsgames,mmo,online,mmorpg,massive,massive multiplayer,massive multiplayer online,massive multiplayer online,Everquest,gameznet,role playing,persistant world,internet,rpg,RPG">
<meta NAME="description" CONTENT="Welcome to EQLizer v3.0 - The original Everquest strategy and spoiler web site.">
<title>EQ'Lizer Full News v3.0</title>



	
	<script language="Javascript">
		function setCookie(name, value, expires, path, domain, secure) {
		  var curCookie = name + "=" + escape(value) +
			  ((expires) ? "; expires=" + expires.toGMTString() : "") +
			  ((path) ? "; path=" + path : "") +
			  ((domain) ? "; domain=" + domain : "") +
			  ((secure) ? "; secure" : "");
		  document.cookie = curCookie;
		}
		
		function getCookie(name) {
		  var dc = document.cookie;
		  var prefix = name + "=";
		  var begin = dc.indexOf("; " + prefix);
		  if (begin == -1) {
			begin = dc.indexOf(prefix);
			if (begin != 0) return null;
		  } else
			begin += 2;
		  var end = document.cookie.indexOf(";", begin);
		  if (end == -1)
			end = dc.length;
		  return unescape(dc.substring(begin + prefix.length, end));
		}
		
		// test to make sure cookies are allowed
		setCookie('BFFLSID','TEST','','/');
		if (getCookie('BFFLSID') == 'TEST') { // cookies are allowed
			var sessid = (getCookie('FLSID')) ? ('&PHPSESSID=' + getCookie('FLSID')) : '';
			location.replace('http://www.flashlink.com/bf.php?s=57'
				+ sessid
				+ '&h=www.gameznet.com'
				+ '&u=/eq/index.shtml/item.php%3fid=15536');
		} // else no bannerfree (value set above)
	</script>



