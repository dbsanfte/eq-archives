  <tr> 
     <td width="100%" height="30" align="center" colspan="2" class="big" bgcolor="#A34D1E">Dawnshroud Cider</td> 
   </tr> 
  <tr bgcolor="#4A2A12"> 
     <td width="150" class="smy" align="right">Properties</td> 
     <td width="100%"> QUEST-RELATED &nbsp;STORE-BOUGHT &nbsp;QUEST-REWARD &nbsp; </td> 
   </tr> 
  <tr bgcolor="#54341C"> 
     <td class="smy" align="right">Equipment: Slots</td> 
     <td>Misc: Inventory</td> 
   </tr> 
   
  <tr bgcolor="#4A2A12"> 
     <td class="smy" align="right">Statistics</td> 
     <td> None </td> 
   </tr> 
  <tr bgcolor="#54341C"> 
