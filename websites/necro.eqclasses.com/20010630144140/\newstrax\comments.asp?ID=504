        }
  }	
  function DelWin(stuff){
        AdmWin = window.open("admdel.asp?ID="+stuff,"AdmWin","toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=no,WIDTH=680,HEIGHT=540");
  
  }
