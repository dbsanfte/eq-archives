                  <table width="100%" height="100%" border="0" cellspacing="0" cellpadding="0">
                    <tr>
                      <td colspan="2" valign="top">
                      <img src="images/icon_mi_1.gif" width="15" height="15" border="0" alt="" title="" hspace="3" /><font color="#FFFFFF" face="arial" size="2">Posted&nbsp;-&nbsp;04/22/2003&nbsp;:&nbsp; 06:59:10 AM</font>
                      &nbsp;<a href="pop_profile.asp?mode=display&id=5347"><img src="images/icon_profile.gif" width="15" height="15" border="0" alt="Show Profile" title="Show Profile" align="absmiddle" hspace="6" /></a>
