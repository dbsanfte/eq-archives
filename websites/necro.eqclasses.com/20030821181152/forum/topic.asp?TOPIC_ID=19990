                  <table width="100%" height="100%" border="0" cellspacing="0" cellpadding="0">
                    <tr>
                      <td colspan="2" valign="top">
                      <img src="images/icon_mi_1.gif" width="15" height="15" border="0" alt="" title="" hspace="3" /><font color="#000000" face="arial" size="2">Posted&nbsp;-&nbsp;07/06/2003&nbsp;:&nbsp; 4:58:02 PM</font>
                      &nbsp;<a href="pop_profile.asp?mode=display&id=23832"><img src="images/icon_profile.gif" width="15" height="15" border="0" alt="Show Profile" title="Show Profile" align="absmiddle" hspace="6" /></a>
                      &nbsp;<a href="post.asp?method=ReplyQuote&REPLY_ID=185449&TOPIC_ID=19990&FORUM_ID=2"><img src="images/icon_reply_topic.gif" width="15" height="15" border="0" alt="Reply with Quote" title="Reply with Quote" align="absmiddle" hspace="6" /></a>
                      <hr noshade size="2"></td>
