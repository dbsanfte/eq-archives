<style type="text/css"><!--

#mylayer {position:absolute;top:100;alignment:center;}
    .head {font-family:Arial,Verdana,sans-serif;}
    .content {font-family:Arial,Verdana,sans-serif;font-size:11px;
    .about {position:absolute;top:100;alignment:center;}
//--></style>
<script LANGUAGE="JavaScript">

var x=90;
var delay=40;
var speed=1;

function doanim() {
    if(document.all) {
        document.all.mylayer.style.top=x;
    } else {
        document.layers.mylayer.top=x;
    }


    x-=speed;
    if(x<-800) x=90;
    setTimeout("doanim()",delay);
}

setTimeout('window.focus();',500);

</script>

</head>
<body bgcolor="#F6F6FF" onLoad="doanim()">
<div class="head"><strong><b>SPChat Version 0.8.1.7 (Revision: 175)</b></strong></div>
