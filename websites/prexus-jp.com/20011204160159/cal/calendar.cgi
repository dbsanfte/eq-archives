<!-- Add An Entry Form -->
  <TABLE BORDER=1 CELLSPACING=0 CELLPADDING=3 bordercolor="#003366">
    <form action="/cal/calendar.cgi" method=post>
	<input type="hidden" name="ACTION" value="DO_ADD">
	<input type="hidden" name="config" value="calendar.cfg">
	  <TR bgcolor="#003366"> 
        <TD ALIGN=CENTER COLSPAN=4><font color="white" size=+1><B><font face="Verdana, Arial, Helvetica, sans-serif" size="2">Add 
          an Event</font></B></font></TD>
	</TR>
	  <TR bgcolor="#FFFFFF"> 
        <TH><font size="1" face="Verdana, Arial, Helvetica, sans-serif">Date</font></TH>
		<TD> 
          <input name="Month" size=2 maxlength=2 value="12"> / <input name="Date" size=2 maxlength=2 value="5"> / <input name="Year" size=4 maxlength=4 value="2001"><br>
			<input type="checkbox" name="annual" value="1" onClick="if(form.annual.checked == true){form.Year.value='0000'}else{form.Year.value='2001'}">
          <font size="1" face="Verdana, Arial, Helvetica, sans-serif">Every Year 
          </font></TD>
		<TH><font face="Verdana, Arial, Helvetica, sans-serif" size="1">Heading</font></TH>
		<TD> 
          <input name="heading" size=30></TD>
	</TR>
	  <TR bgcolor="#FFFFFF"> 
        <TH><font size="1" face="Verdana, Arial, Helvetica, sans-serif">Description</font></TH>
		<TD COLSPAN=3> 
          <textarea name="description" cols=50 rows=3></textarea></TD>
	</TR>
	  <TR bgcolor="#FFFFFF"> 
        <TD COLSPAN=4 ALIGN=RIGHT> 
          <input type="reset" CLASS="button" value="Cancel">&nbsp;&nbsp;&nbsp;<input type="submit" CLASS="button" value="Save"></TD>
	</TR>
	</form>
</TABLE>
