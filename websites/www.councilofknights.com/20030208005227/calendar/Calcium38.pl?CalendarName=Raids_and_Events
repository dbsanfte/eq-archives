<SCRIPT LANGUAGE="JavaScript"><!-- 

        function PopupWindow (calName, date, id, source, width, height) {
            if (width < 100) {
               width = Math.round (screen.width * width / 100);
            }
            if (height < 100) {
               height = Math.round (screen.height * height / 100);
            }
            win = window.open ("Calcium38.pl?Op=PopupWindow&Amount=Month&NavType=Absolute&Type=Block" +
                               "&CalendarName=" + calName +
                               "&Date=" + escape (date) +
                               "&ID=" + id +
                               "&Source=" + source,
                               "PopupWindow" + calName + id,
                               "scrollbars,resizable," +
                               "width=" + width + "," +
                               "height=" + height)
            win.focus()
       }
        
// --></SCRIPT>

