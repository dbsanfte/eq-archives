	  function checkSelected(checkbox, category) {  
	  
			if(checkbox.value == category) {
				if (checkbox.checked) {
						if (category=="C") {
						divClass.className = "clsShow";
						}
						if (category=="S") {
						divServer.className = "clsShow";
						}
						if (category=="GU") {
						divGuild.className = "clsShow";
						}
					}
				else {
						if (category=="C") {
						divClass.className = "clsHide";
						}
						if (category=="S") {
						divServer.className = "clsHide";
						}
						if (category=="GU") {
						divGuild.className = "clsHide";
						}
				}
			}
			  	
	  }
	  
		function errorCheck() {
			var requiredFieldMsg = "";
			var siteNameEntered = "";
			var siteURLEntered = "";
			var classSelected = "";
			var serverSelected = "";
			var guildEntered = "";
			
			siteNameEntered 		= document.frmSite.txtSiteName.value
			siteURLEntered 		= document.frmSite.txtSiteURL.value
			classSelected 			= document.frmSite.cboClass.value
			serverSelected 		= document.frmSite.cboServer.value
			guildEntered 			= document.frmSite.txtGuild.value
			
			if ( (!siteNameEntered) || (!siteURLEntered) ) {
				if (!siteNameEntered) {
					requiredFieldMsg = " - site name\r"
				}
				if (!siteURLEntered) {
					requiredFieldMsg = requiredFieldMsg + " - site URL\r"
				}
			}
			
			if (divClass.className == "clsShow" && classSelected == 'default') {
				requiredFieldMsg = requiredFieldMsg + " - please select a class.\r";
			}
			
			if (divServer.className == "clsShow" && serverSelected == 'default') {
				requiredFieldMsg = requiredFieldMsg + " - please select a server.\r";
			}
			
			if (divGuild.className == "clsShow" && (!guildEntered)) {
				requiredFieldMsg = requiredFieldMsg + " - please enter your guild name.\r";
			}
			
			if (requiredFieldMsg) {
				alertMessage = "Please fill out the following required information\r";
				alertMessage = alertMessage + requiredFieldMsg;
				
				alert(alertMessage);
			} else {
				document.frmSite.submit();
			}
			
	  	}
	  
	//-->
