<script language=javascript>

function verif_form() {
	var form_ok=1;
	var msg_erreur="Fehler!\n";

	
	if (document.formulaire.ref_email.value=="" || verif_mail(document.formulaire.ref_email.value)>0) {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Ihre eMail ist falsch\n";
	}
	
	if (document.formulaire.ref_username.value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Ihr Stationsname muss angegeben werden\n";
	}
	
	if (document.formulaire.ref_firstname.value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Ihr Name muss komplett sein\n";
	}
	
	
	if (document.formulaire.jour[document.formulaire.jour.selectedIndex].value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Wählen Sie den Tag Ihrer Anmeldung aus\n";
	}

	if (document.formulaire.mois[document.formulaire.mois.selectedIndex].value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Wählen Sie das Monat Ihrer Anmeldung aus\n";
	}

	if (document.formulaire.annee[document.formulaire.annee.selectedIndex].value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Wählen Sie das Jahr Ihrer Anmeldung aus\n";
	}
	
	if (document.formulaire.ref_refbyname.value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Der Name Ihres Sponsors muss komplett angegeben sein\n";
	}
	
	
	if (document.formulaire.ref_refbyemail.value=="" || verif_mail(document.formulaire.ref_refbyemail.value)>0) {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Die eMail Ihres Sponsors ist falsch\n";
	}
	
	
	if (form_ok==1) {
		document.formulaire.submit();
	}
	else
	{
		alert(msg_erreur);
	}
}



function verif_mail(email)
{
	var erreur = 0;
	if(email == ""){
		//alert("L'adresse e-mail est obligatoire.");
		erreur = 1;
		}
	else {
		var chaine=new String(email);
		re = /[a-zA-Z0-9_\.\-]{2,}@[a-zA-Z0-9_\-]{2,}.[a-zA-Z]{2,4}/
		if((chaine.match(re))== null){
		erreur = 2;
		}
		else{
			decoupe=chaine.split('@')
			decoupe1=decoupe[1].split('.')
			if (decoupe1[decoupe1.length-1].length > 3){
			erreur = 2;
			}
		}
	}
return erreur;
}
</script>

