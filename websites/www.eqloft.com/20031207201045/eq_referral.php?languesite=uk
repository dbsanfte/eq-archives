<script language=javascript>

function verif_form() {
	var form_ok=1;
	var msg_erreur="Error !\n";

	
	if (document.formulaire.ref_email.value=="" || verif_mail(document.formulaire.ref_email.value)>0) {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Wrong e-mail address\n";
	}
	
	if (document.formulaire.ref_username.value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Please enter your Station Name\n";
	}
	
	if (document.formulaire.ref_firstname.value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Please enter your name\n";
	}
	
	
	if (document.formulaire.jour[document.formulaire.jour.selectedIndex].value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Select the day of your subscription\n";
	}

	if (document.formulaire.mois[document.formulaire.mois.selectedIndex].value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Select the month of your subscription\n";
	}

	if (document.formulaire.annee[document.formulaire.annee.selectedIndex].value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Select the year of your subscription\n";
	}
	
	if (document.formulaire.ref_refbyname.value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Please enter your sponsor's name\n";
	}
	
	
	if (document.formulaire.ref_refbyemail.value=="" || verif_mail(document.formulaire.ref_refbyemail.value)>0) {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Your sponsor's e-mail address is not valid\n";
	}
	
	
	if (form_ok==1) {
		document.formulaire.submit();
	}
	else
	{
		alert(msg_erreur);
	}
}



function verif_mail(email)
{
	var erreur = 0;
	if(email == ""){
		//alert("L'adresse e-mail est obligatoire.");
		erreur = 1;
		}
	else {
		var chaine=new String(email);
		re = /[a-zA-Z0-9_\.\-]{2,}@[a-zA-Z0-9_\-]{2,}.[a-zA-Z]{2,4}/
		if((chaine.match(re))== null){
		erreur = 2;
		}
		else{
			decoupe=chaine.split('@')
			decoupe1=decoupe[1].split('.')
			if (decoupe1[decoupe1.length-1].length > 3){
			erreur = 2;
			}
		}
	}
return erreur;
}
</script>

