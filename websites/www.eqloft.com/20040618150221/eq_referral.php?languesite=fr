<script language=javascript>

function verif_form() {
	var form_ok=1;
	var msg_erreur="Erreur !\n";

	
	if (document.formulaire.ref_email.value=="" || verif_mail(document.formulaire.ref_email.value)>0) {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Votre email est incorrect\n";
	}
	
	if (document.formulaire.ref_username.value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Votre Nom de Station doit être completé\n";
	}
	
	if (document.formulaire.ref_firstname.value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Votre nom doit être completé\n";
	}
	
	
	if (document.formulaire.jour[document.formulaire.jour.selectedIndex].value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Sélectionnez le jour de votre inscription\n";
	}

	if (document.formulaire.mois[document.formulaire.mois.selectedIndex].value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Sélectionnez le mois de votre inscription\n";
	}

	if (document.formulaire.annee[document.formulaire.annee.selectedIndex].value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Sélectionnez l'année de votre inscription\n";
	}
	
	if (document.formulaire.ref_refbyname.value=="") {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-Le nom de votre parrain doit être rempli\n";
	}
	
	
	if (document.formulaire.ref_refbyemail.value=="" || verif_mail(document.formulaire.ref_refbyemail.value)>0) {
		var form_ok=0;
		var msg_erreur=msg_erreur+"-L'email de votre parrain est invalide\n";
	}
	
	
	if (form_ok==1) {
		document.formulaire.submit();
	}
	else
	{
		alert(msg_erreur);
	}
}



function verif_mail(email)
{
	var erreur = 0;
	if(email == ""){
		//alert("L'adresse e-mail est obligatoire.");
		erreur = 1;
		}
	else {
		var chaine=new String(email);
		re = /[a-zA-Z0-9_\.\-]{2,}@[a-zA-Z0-9_\-]{2,}.[a-zA-Z]{2,4}/
		if((chaine.match(re))== null){
		erreur = 2;
		}
		else{
			decoupe=chaine.split('@')
			decoupe1=decoupe[1].split('.')
			if (decoupe1[decoupe1.length-1].length > 3){
			erreur = 2;
			}
		}
	}
return erreur;
}
</script>

