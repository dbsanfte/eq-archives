sub HTML_ProfileForm{
	print "    <CENTER>\n";
	print "      <FORM ACTION=\"$scriptfile\" METHOD=\"POST\">\n";
	print "        <BR>\n";
	print "        <TABLE WIDTH=\"600\" BORDER=\"0\" CELLSPACING=\"2\" CELLPADDING=\"2\">\n";
	print "          <TR ALIGN=\"center\">\n";
	print "            <TD COLSPAN=\"5\" HEIGHT=\"10\"><FONT FACE=\"Arial, Helvetica, sans-serif\" SIZE=\"2\" COLOR=\"ffffff\"><B>Name: <INPUT TYPE=\"text\" NAME=\"name\" SIZE=\"30\" VALUE=\"$FileInfo[2]\"></FONT></B>\n";
	print "            </TD>\n";
	print "          </TR>\n";
	print "          <TR>\n";
	print "            <td width=\"203\" rowspan=\"16\" align=\"left\" valign=\"top\">\n";
	
	# Greg Jones 2/12/2000
	#
	$greg = $FileInfo[14];
	chop($greg);
	if ($greg ne "") {
		print "              <font size=\"2\"><img src=\"$FileInfo[14]\" width=\"200\" height=\"350\"></font>\n";
		} else {
		print "               <font size=\"3\">No Photo at this time.<br><img src=\"$blackimg\" width=\"200\" height=\"350\"></font>\n";
		};

	print "            </td>\n";
	print "            <TD WIDTH=\"83\" NOWRAP HEIGHT=\"10\"><B><FONT FACE=\"Arial, Helvetica, sans-serif\" SIZE=\"2\" COLOR=\"ffffff\">Strength:</FONT></B></TD>\n";
	print "            <TD WIDTH=\"60\" NOWRAP HEIGHT=\"10\"><FONT FACE=\"Arial, Helvetica, sans-serif\" SIZE=\"2\"><INPUT TYPE=\"text\" NAME=\"strength\" SIZE=\"3\" VALUE=\"$FileInfo[15]\"></FONT></TD>\n";
	print "            <TD width=\"71\" NOWRAP HEIGHT=\"2\"><B><FONT FACE=\"Arial, Helvetica, sans-serif\" size=\"2\" COLOR=\"ffffff\">Class:</FONT></B></TD>\n";
	print "          <TD WIDTH=\"160\" NOWRAP HEIGHT=\"10\">\n";
	print "            <FONT FACE=\"Arial, Helvetica, sans-serif\" size=\"2\">\n";
	print "              <SELECT NAME=\"class\">\n";
	
	if ($FileInfo[5] eq "ShadowKnight\n") {
		print "                <OPTION VALUE=\"ShadowKnight\" SELECTED=\"true\">ShadowKnight</OPTION>\n";
	}else {
		print "                <OPTION VALUE=\"ShadowKnight\">ShadowKnight</OPTION>\n";
	}
		
	if ($FileInfo[5] eq "Bard\n") {
		print "                <OPTION VALUE=\"Bard\" SELECTED=\"true\">Bard</OPTION>\n";
	}else {
		print "                <OPTION VALUE=\"Bard\">Bard</OPTION>\n";
	}

	if ($FileInfo[5] eq "Ranger\n") {
		print "                <OPTION VALUE=\"Ranger\" SELECTED=\"true\">Ranger</OPTION>\n";
	}else {
		print "                <OPTION VALUE=\"Ranger\">Ranger</OPTION>\n";
	}
		
	if ($FileInfo[5] eq "Paladin\n") {
		print "                <OPTION VALUE=\"Paladin\" SELECTED=\"true\">Paladin</OPTION>\n";
	}else {
		print "                <OPTION VALUE=\"Paladin\">Paladin</OPTION>\n";
	}
		
	if ($FileInfo[5] eq "Warrior\n") {
		print "                <OPTION VALUE=\"Warrior\" SELECTED=\"true\">Warrior</OPTION>\n";
	}else {
		print "                <OPTION VALUE=\"Warrior\">Warrior</OPTION>\n";
	}
		
	if ($FileInfo[5] eq "Monk\n") {
		print "                <OPTION VALUE=\"Monk\" SELECTED=\"true\">Monk</OPTION>\n";
	}else {
		print "                <OPTION VALUE=\"Monk\">Monk</OPTION>\n";
	}
		
	if ($FileInfo[5] eq "Rogue\n") {
		print "                <OPTION VALUE=\"Rogue\" SELECTED=\"true\">Rogue</OPTION>\n";
	}else {
		print "                <OPTION VALUE=\"Rogue\">Rogue</OPTION>\n";
	}
		
	if ($FileInfo[5] eq "Cleric\n") {
		print "                <OPTION VALUE=\"Cleric\" SELECTED=\"true\">Cleric</OPTION>\n";
	}else {
		print "                <OPTION VALUE=\"Cleric\">Cleric</OPTION>\n";
