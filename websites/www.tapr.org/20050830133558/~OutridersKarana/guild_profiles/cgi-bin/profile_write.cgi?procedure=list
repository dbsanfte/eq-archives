#!/usr/bin/perl
# ********************************************************************************* #
#                     EverQuest Character Profile Script                            #
#                        Developed by Matt Harrison                                 #
#                            February 9th, 2000                                     #
# ********************************************************************************* #
#                                                                                   #
# ********************************************************************************* #
$pfile_directory = 'profiles/';
$datafile = $pfile_directory;
$datafile = $datafile.'profiledat.dat';
$scriptfile = 'profile_write.cgi';
$Mainpwrd = 'dokutt';
$Logo = 'http://www.tapr.org/~OutridersKarana/jpeg/profile.jpeg';
$blackimg = 'http://www.tapr.org/~OutridersKarana/jpeg/blackimg.jpeg';

print "Content-type: text/html\n\n";
&Get_Form_Input;
&Parse_Data;

# Storing profile data.
if ($procedure eq "write"){
	&Write_File;

	$article_file = $lastfilenum;
	$article_file = $article_file.'.pfl';

	&Open_File;
	&HTML_Header;
	&HTML_ProfileView;
	&HTML_Footer;
	
# Viewing someone's profile.
} elsif ($procedure eq "view"){
	$article_file = $usrfile;
	$article_file = $article_file.'.pfl';

	&Open_File;
	&HTML_Header;
	&HTML_ProfileView;
	&HTML_Footer;
	
# Display Search Form.
} elsif ($procedure eq "list"){
	&HTML_Header;
	&HTML_Search;
	&HTML_List;
	&HTML_Footer;
	
# Listing available profiles to view.
} elsif ($procedure eq "search"){
	&read_dir;
	&HTML_Header;
	&List_Header;
	foreach $article_file (@foundfiles) {
		&List_File;
	}
	&List_Footer;
	&HTML_List;
	&HTML_Footer;
	
# Print a form to create a profile.
} elsif ($procedure eq "create"){
	if ($field{'pword'} eq $Mainpwrd) {
		&HTML_Header;
		&HTML_ProfileForm;
		&HTML_Footer;
	}else {
		print "Badd Password\n";
	}
	
# Print a create form with info filled in on the appropriate character.
} elsif ($procedure eq "edit"){
	$article_file = $usrfile;
	$article_file = $article_file.'.pfl';

	&Open_File;
	&HTML_Header;
	&HTML_ProfileForm;
	&HTML_Footer;
}
# ********************************************************************************* #
#                                   Procedures.                                     #
# ********************************************************************************* #
sub Open_File{
	open (FILEHANDLE, "$pfile_directory$article_file");
		@FileInfo = <FILEHANDLE>;
	close FILEHANDLE;
}

sub read_dir{
	# open incoming directory, read all files found, and push them to an array
	
	opendir (DIR, $pfile_directory) || die "Directory Read Failed \n";
		while (defined($scanfile = readdir(DIR) ) ){
			if ($scanfile ne '.' && $scanfile ne '..'){
				if (substr($scanfile, length($scanfile) - 3, 3) eq "pfl"){
#					if (length($scanfile) > '4'){
						push (@foundfiles, $scanfile);
#					}
				}
			}
		}
	closedir(DIR);
}

sub List_File{
	$oldpassword = "";
	&Open_File;
	$siRace = 0;
	$siClass = 0;
	$siLevel = 0;
	$siTZ = 0;

	if  ($field{'RaceSearch'} eq "Race"){
		$siRace = 1;
	}else{
		if($field{'RaceSearch'} eq substr($FileInfo[6],0,length($FileInfo[6])-1)){
			$siRace = 1;
		}
	}		
	
	if  ($field{'ClassSearch'} eq "Class"){
		$siClass = 1;
	}else{
		if($field{'ClassSearch'} eq substr($FileInfo[5],0,length($FileInfo[5])-1)){
			$siClass = 1;
		}
	}		

	if  ($field{'TZSearch'} eq "TimeZone"){
		$siLevel = 1;
	}else{
		if($field{'TZSearch'} eq substr($FileInfo[13],0,length($FileInfo[13])-1)){
			$siLevel = 1;
		}
	}		

	if  ($field{'LevelSearch'} eq "0"){
		$siTZ = 1;
	}else{
		if($field{'LevelSearch'} eq "1" && substr($FileInfo[4],0,length($FileInfo[4])-1) > "0" && substr($FileInfo[4],0,length($FileInfo[4])-1) < "11"){
			$siTZ = 1;
		}
		elsif ($field{'LevelSearch'} eq "2" && substr($FileInfo[4],0,length($FileInfo[4])-1) > "10" && substr($FileInfo[4],0,length($FileInfo[4])-1) < "21"){
			$siTZ = 1;
		}
		elsif ($field{'LevelSearch'} eq "3" && substr($FileInfo[4],0,length($FileInfo[4])-1) > "20" && substr($FileInfo[4],0,length($FileInfo[4])-1) < "31"){
			$siTZ = 1;
		}
		elsif ($field{'LevelSearch'} eq "4" && substr($FileInfo[4],0,length($FileInfo[4])-1) > "30" && substr($FileInfo[4],0,length($FileInfo[4])-1) < "41"){
			$siTZ = 1;
		}
		elsif ($field{'LevelSearch'} eq "5" && substr($FileInfo[4],0,length($FileInfo[4])-1) > "40" && substr($FileInfo[4],0,length($FileInfo[4])-1) < "51"){
			$siTZ = 1;
		}
	}		
	
	if ($siRace == "1" && $siClass == "1" && $siLevel == "1" && $siTZ == "1"){
		&Add_To_List;
	}
}

sub Add_To_List{
	# *************************************
	print "              <TR>\n";
	print "                <TD><A HREF=\"$scriptfile?procedure=view&filename=$FileInfo[1]\">$FileInfo[2]</A></TD>\n";
	print "                <TD>Level $FileInfo[4] $FileInfo[5]</TD>\n";
	print "                <TD>$FileInfo[3]</TD>\n";
	print "              </TR>\n";
}

sub View_File{
	open (FILEHANDLE, "$pfile_directory$usrfile");
		@FileInfo = <FILEHANDLE>;
	close FILEHANDLE;

	print @FileInfo;	
}

sub Parse_Data{
	$usrfile = $field{'filename'};
	$hp = $field{'hp'};
	$ac = $field{'ac'};
	$atk = $field{'atk'};
	$lastfilenum = $usrfile;
	$procedure = $field{'procedure'};
	$name = $field{'name'};	 
	$class = $field{'class'};
	$strength = $field{'strength'};	 
	$server = $field{'server'};	 
	$stamina = $field{'stamina'};	 
	$deity = $field{'deity'};	 
	$dexterity = $field{'dexterity'};	 
	$roleplay = $field{'roleplay'};	 
	$agility = $field{'agility'};	 
	$tz = $field{'tz'};	 
	$wisdom = $field{'wisdom'};	 
	$race = $field{'race'};	 
	$intelligence = $field{'intelligence'};	 
	$sex = $field{'sex'};	 
	$charisma = $field{'charisma'};	 
	$level = $field{'level'};	 
	$guild = $field{'guild'};	 
	$email = $field{'email'};	 
	$web = $field{'web'};	 
	$ear = $field{'ear'};	 
	$shoulders = $field{'shoulders'};	 
	$ear2 = $field{'ear2'};	 
	$chest = $field{'chest'};	 
	$neck = $field{'neck'};	 
	$back = $field{'back'};	 
	$face = $field{'face'};	 
	$waist = $field{'waist'};	 
	$head = $field{'head'};	 
	$legs = $field{'legs'};	 
	$finger = $field{'finger'};	 
	$feet = $field{'feet'};	 
	$finger2 = $field{'finger2'};	 
	$weapon = $field{'weapon'};	 
	$wrist = $field{'wrist'};	 
	$weapon2 = $field{'weapon2'};	 
	$wrist2 = $field{'wrist2'};	 
	$ranged = $field{'ranged'};	 
	$hands = $field{'hands'};	 
	$ammo = $field{'ammo'};	 
	$arms = $field{'arms'};	 
	$baking = $field{'baking'};	 
	$brewing = $field{'brewing'};	 
	$fletching = $field{'fletching'};	 
	$jewelcraft = $field{'jewelcraft'};	 
	$pottery = $field{'pottery'};	 
	$smithing = $field{'smithing'};	 
	$tailoring = $field{'tailoring'};	 
	$history = $field{'history'};	 
	$journal = $field{'journal'};	 
	$pword = $field{'pword'};	 
	$submit = $field{'submit'};	 
	$reset = $field{'reset'};	 
	$picture = $field{'picture'};	 

	$message = "";
	$found_err = "";

	if ($procedure eq "write") {
		$errmsg = "<p>Field 'Name' must be filled in.</p>\n";
		if ($name eq "") {
		    $message = $message.$errmsg;
		    $found_err = 1; 
		}
		
		$errmsg = "<p>Field 'Password' must be filled in.</p>\n";
		if ($pword eq "") {
		    $message = $message.$errmsg;
		    $found_err = 1; 
		}
		
		if ($found_err) {
		    &PrintError; 
		}
	}
}

sub PrintError{ 
    print $message;

    exit 0;
    return 1; 
}

sub Get_Form_Input{
	(*fval) = @_ if @_ ;
	local ($buf);
	if ($ENV{'REQUEST_METHOD'} eq 'POST') {
		read(STDIN,$buf,$ENV{'CONTENT_LENGTH'});
	}else {
		$buf=$ENV{'QUERY_STRING'};
	}
	if ($buf eq "") {
			return 0 ;
	}else {
 		@fval=split(/&/,$buf);
		foreach $i (0 .. $#fval){
			($name,$val)=split (/=/,$fval[$i],2);
			$val=~tr/+/ /;
			$val=~ s/%(..)/pack("c",hex($1))/ge;
			$name=~tr/+/ /;
			$name=~ s/%(..)/pack("c",hex($1))/ge;

			if (!defined($field{$name})) {
				$field{$name}=$val;
			}else {
				$field{$name} .= ",$val";
		 	
		 		# if you want multi-selects to goto into an array change to:
				# $field{$name} .= "\0$val";
			}
		}
	}
    return 1;
}

sub Write_File{
	if ($pword eq ""){
		print "You must enter a password\n";
		return 0;
	}
	if ($usrfile eq ""){
		$lastfilenum = 0;
		open (FILEHANDLE, "$datafile") || die "bad file\n";
			$lastfilenum = <FILEHANDLE>;
		close FILEHANDLE;
		$lastfilenum++;
		$usrfile = $lastfilenum;
		open (FILEHANDLE, ">$datafile");
			print FILEHANDLE $lastfilenum;
		close FILEHANDLE;
	}else {
		$lastfilenum = $usrfile; #substr($usrfile, 0, length($usrfile)-1);
	}
	$usrfile = $usrfile.'.pfl';
	$oldpassword = "";
	open (FILEHANDLE, "$pfile_directory$usrfile");
		@FileInfo = <FILEHANDLE>;
	close FILEHANDLE;
	
	$oldpword = substr($FileInfo[0], 0, length($FileInfo[0])-1);
	$oldpassword = $oldpword;
	if ($oldpassword eq ""){
		$oldpassword = $pword;
	}
	if ($pword ne $oldpassword){
		print "Bad Password\n";
	}else{
		open (FILEHANDLE, ">$pfile_directory$usrfile") || die "Could not open article for writing\n" ;
			$writefile = "";
			# Main Char Data.
			$writefile = "$writefile$field{'pword'}\n";	 
			$writefile = "$writefile$lastfilenum\n";
			$writefile = "$writefile$field{'name'}\n";	 
			$writefile = "$writefile$field{'server'}\n";	 
			$writefile = "$writefile$field{'level'}\n";	 
			$writefile = "$writefile$field{'class'}\n";
			$writefile = "$writefile$field{'race'}\n";	 
			$writefile = "$writefile$field{'deity'}\n";	 
			$writefile = "$writefile$field{'sex'}\n";
			$writefile = "$writefile$field{'guild'}\n";	 
			$writefile = "$writefile$field{'email'}\n";	 
			$writefile = "$writefile$field{'web'}\n";	 
			$writefile = "$writefile$field{'roleplay'}\n";	 
			$writefile = "$writefile$field{'tz'}\n";	 
			$writefile = "$writefile$field{'picture'}\n";	 
			
			# Stats.
			$writefile = "$writefile$field{'strength'}\n";	 
			$writefile = "$writefile$field{'stamina'}\n";	 
			$writefile = "$writefile$field{'dexterity'}\n";	 
			$writefile = "$writefile$field{'agility'}\n";	 
			$writefile = "$writefile$field{'wisdom'}\n";	 
			$writefile = "$writefile$field{'intelligence'}\n";	 
			$writefile = "$writefile$field{'charisma'}\n";	 
			
			# Equipment.
			$writefile = "$writefile$field{'ear'}\n";	 
			$writefile = "$writefile$field{'neck'}\n";	 
			$writefile = "$writefile$field{'face'}\n";	 
			$writefile = "$writefile$field{'head'}\n";	 
			$writefile = "$writefile$field{'ear2'}\n";	 

			$writefile = "$writefile$field{'finger'}\n";	 
			$writefile = "$writefile$field{'wrist'}\n";	 
			$writefile = "$writefile$field{'hands'}\n";	 
			$writefile = "$writefile$field{'arms'}\n";	 
			$writefile = "$writefile$field{'wrist2'}\n";	 
			$writefile = "$writefile$field{'finger2'}\n";	 

			$writefile = "$writefile$field{'shoulders'}\n";	 
			$writefile = "$writefile$field{'back'}\n";	 
			$writefile = "$writefile$field{'waist'}\n";	 
			$writefile = "$writefile$field{'chest'}\n";	 
			$writefile = "$writefile$field{'legs'}\n";	 
			$writefile = "$writefile$field{'feet'}\n";	 

			$writefile = "$writefile$field{'weapon'}\n";	 
			$writefile = "$writefile$field{'weapon2'}\n";	 
			$writefile = "$writefile$field{'ranged'}\n";	 
			$writefile = "$writefile$field{'ammo'}\n";	 
			
			# Misc.
			$writefile = "$writefile$field{'history'}\n";	 
			$writefile = "$writefile$field{'journal'}\n";	 
			$writefile = "$writefile$field{'hp'}\n";
			$writefile = "$writefile$field{'ac'}\n";
			$writefile = "$writefile$field{'atk'}\n";
			print FILEHANDLE $writefile;
		close (FILEHANDLE);
	}
}

# ********************************************************************************* #
#                                   HTML Output.                                    #
# ********************************************************************************* #
sub HTML_Header{
	print "<STYLE type=\"text/css\">\n";
	print "  <!--\n";
	print "    A:link {text-decoration: none} A:visited {text-decoration: none}\n";
	print "    A:active {text-decoration: none}\n";
	print "  -->\n";
	print "</STYLE>\n";
	print "<HTML>\n";
	print "  <TITLE> Character Profiles </TITLE>\n";
	print "  <BODY TEXT=\"#C0C0C0\" BGCOLOR=\"#000000\" LINK=\"#7070FF\" VLINK=\"#7070FF\" ALINK=\"#FF0000\">\n";
	print "    <table WIDTH=\"800\">\n";
	print "      <TR>\n";
	print "        <TD>\n";
	print "          <center><IMG SRC=\"$Logo\"></center><BR>\n";
}

sub List_Header{
	print "          <CENTER>\n";
	print "            <TABLE WIDTH=\"650\">\n";
	print "              <H1>Character Profiles</H1><BR>\n";
}
	
sub List_Footer{
	print "            </TABLE>\n";
	print "          <p><hr width=75%></CENTER>\n";
}

sub HTML_Footer{
	print "          <BR><BR>\n";
	print "          <CENTER>\n";
	print "            <HR width=75%>\n";
	print "            <I>\n";
	print "              <H5>\n";
	print "                Script created and maintained by: <A HREF=\"mailto:faerin\@idirect.com\">Matt Harrison</A><BR>\n";
	print "                Last updated on Wednesday, February 9th, 2000 12:44:00\n";
	print "              </H5>\n";
	print "            </I>\n";
	print "          </CENTER>\n";
	print "        </TD>\n";
	print "      </TR>\n";
	print "    </TABLE>\n";
	print "  </BODY>\n";
	print "</HTML>\n";
}

sub HTML_List{
	print "      <FORM ACTION=\"$scriptfile\" METHOD=\"POST\">\n";
	print "        <BR>\n";
	print "        <TABLE WIDTH=\"600\" BORDER=\"0\" CELLSPACING=\"2\" CELLPADDING=\"2\">\n";
	print "          <TR ALIGN=\"center\">\n";
	print "            <TD COLSPAN=\"5\" HEIGHT=\"10\" ALIGN=\"center\"><CENTER><FONT FACE=\"Arial, Helvetica, sans-serif\" SIZE=\"2\" COLOR=\"ffffff\"><B> Username: <INPUT TYPE=\"text\" NAME=\"name\" SIZE=\"10\"><BR>Password : <INPUT TYPE=\"password\" NAME=\"pword\" SIZE=\"10\"></FONT></CENTER></B>\n";
	print "            </TD>\n";
	print "          </TR>\n";
	print "          <TR ALIGN=\"cebter\">\n";
	print "            <TD COLSPAN=\"5\" HEIGHT=\"10\" ALIGN=\"center\">\n";
	print "              <CENTER><input type=\"submit\" name=\"submit\" value=\"Create a new Profile\"></CENTER>\n";
	print "            </TD>\n";
	print "          </TR>\n";
	print "        </table><input type=\"hidden\" name=\"procedure\" value=\"create\">\n";
	print "      </form>\n";
}

