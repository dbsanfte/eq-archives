<tr valign="top">
	<td class="alt2" width="175">
			
			<div id="postmenu_162997">
				
				<a href="member.php?s=3117088ca62980c66ca5b76c0ae2c998&amp;u=7516">Bakatengu</a>
				<script type="text/javascript"> vbmenu_register("postmenu_162997", true); </script>
				
