<tr valign="top">
	<td class="alt2" width="175">
			
			<div id="postmenu_210989">
				
				<a href="member.php?s=683e3f3c8335aa02f76e0d8e6eb75d80&amp;u=35">Duke Vendric Ravenkrest</a>
				<script type="text/javascript"> vbmenu_register("postmenu_210989", true); </script>
				
