<tr valign="top">
	<td class="alt2" width="175">
			
			<div id="postmenu_302220">
				
				<a href="member.php?s=4667ffe797951187e1a03aca7630a3bf&amp;u=4067">Knevil</a>
				<script type="text/javascript"> vbmenu_register("postmenu_302220", true); </script>
				
			</div>
			
			<div class="smallfont">Registered User</div>
			
		
			
			
			<div class="smallfont">
				&nbsp;<br />
				<div>Join Date: Oct 2002</div>
				
				
				<div>

					
				</div>
