<tr valign="top">
	<td class="alt2" width="175">
			
			<div id="postmenu_237146">
				
				<a href="member.php?s=c27cb25756d980759518a2219420da31&amp;u=273">Klotar AngedeMorts</a>
				<script type="text/javascript"> vbmenu_register("postmenu_237146", true); </script>
				
			</div>
			
			<div class="smallfont">Registered User</div>
			
		
			
			
