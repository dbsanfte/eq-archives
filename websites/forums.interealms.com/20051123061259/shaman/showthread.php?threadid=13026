function changemode(mode, which)
{
	if (mode == "hybrid")
	{
		window.location = "showthread.php?" + SESSIONURL + "mode=hybrid&threadid=13026";
		return false;
	}
	else if (which == "first")
	{
		window.location = "showthread.php?" + SESSIONURL + "postid=" + FIRSTPOSTID + "&mode=" + mode + "#post" + FIRSTPOSTID;
		return false;
