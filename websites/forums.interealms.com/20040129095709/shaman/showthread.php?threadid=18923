	<td class="tcat" colspan="2"><img src="images/clear.gif" alt="" width="1" height="1" /></td>
</tr><tr title="Post 212462">
	<td class="thead" width="175" align="center">
	
	<!-- 1 -->
	<table cellpadding="0" cellspacing="0" border="0">
	<tr>
		<td><img src="images/posticon.gif" alt="folder icon" title="Old Post" border="0" /></td>
		<td class="smallfont"><a name="post212462">&nbsp;</a> 01-10-2004, 08:17 AM </td>
	</tr>
	</table>
	<!-- /1 -->
	
	</td>
	<td class="thead">
	
	<!-- 2 -->
	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b></b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=86f5cf258a4b542b7283be000868a23d&amp;postid=212462&postcount=10" target="new">10</a></b></td>
	</tr>
	</table>
	<!-- /2 -->
	
	</td>
