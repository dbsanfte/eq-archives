	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b></b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=5d19667afc90a94c382e87e4499a87af&amp;postid=278005&postcount=25" target="new">25</a></b></td>
