	<tr>
		
		<td class="smallfont"><b>raids and difficulty</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=fd5ec24a4a9f564162087e9fbdabd431&amp;postid=180870&postcount=33" target="new">33</a></b></td>
