	<td class="thead">
	
	<!-- 2 -->
	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b></b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=380928af29c4fea8451011f0d65406eb&amp;postid=238695&postcount=40" target="new">40</a></b></td>
