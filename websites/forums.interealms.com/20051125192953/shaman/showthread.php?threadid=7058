	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b>Recap: Chapters 1-4</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=6280e51de941ecae658f9a659798643f&amp;postid=82282&postcount=18" target="new">18</a></b></td>
