	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b></b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=7762271a6e54790699329a508c363b07&amp;postid=87082&postcount=27" target="new">27</a></b></td>
