	<tr>
		
		<td class="smallfont"><b>Dulak Cave Rares</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=af47759b91dd375b9c4eea51568191f1&amp;postid=192367&postcount=1" target="new">1</a></b></td>
