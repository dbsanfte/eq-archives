	<tr>
		
		<td class="smallfont"><b>My LDoN Ramblin</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=d7ccf0baa7a88668bb17ca76c6b64db8&amp;postid=196903&postcount=39" target="new">39</a></b></td>
