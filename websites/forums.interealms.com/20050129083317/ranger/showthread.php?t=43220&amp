<tr valign="top">
	<td class="alt2" width="175">
			
			<div id="postmenu_667815">
				
				<a class="bigusername" href="member.php?s=9c7580dd50d0515642caf3c2aeb5a20f&amp;u=14136">Maegril Pathstrider</a>
				<script type="text/javascript"> vbmenu_register("postmenu_667815", true); </script>
				
			</div>
			
			<div class="smallfont">Senior Member</div>
			
		
			
			
			<div class="smallfont">
