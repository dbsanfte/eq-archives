	<!-- 2 -->
	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b>To SUM UP!</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=e3e790b4eeba12e86fa88e72be309ebc&amp;postid=205163&postcount=36" target="new">36</a></b></td>
	</tr>
