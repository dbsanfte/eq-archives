	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b>Proposal for Shaman Improvements</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=674b203b7d60257ab8b63a60f83bac12&amp;postid=216981&postcount=1" target="new">1</a></b></td>
