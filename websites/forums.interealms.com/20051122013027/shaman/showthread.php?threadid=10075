	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b></b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=be1f30348556454add031cde553cfc34&amp;postid=112786&postcount=13" target="new">13</a></b></td>
