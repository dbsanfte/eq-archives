	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b>Re: Luclin at 24</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=be1f30348556454add031cde553cfc34&amp;postid=160526&postcount=14" target="new">14</a></b></td>
