	<tr>
		
		<td class="smallfont"><b>Epic faction conversation w/ GM</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=47a5d37b497d282f462d8766ae0809a9&amp;postid=25507&postcount=31" target="new">31</a></b></td>
	</tr>
