	<!-- 2 -->
	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b>Don't Pretend</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=baea81f7fedbdd080b5b6f9400ea7adf&amp;postid=185809&postcount=37" target="new">37</a></b></td>
