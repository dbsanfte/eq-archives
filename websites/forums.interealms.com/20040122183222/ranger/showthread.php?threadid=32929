	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b>Class Balance Proposal</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=492c3e7717e9b206f5c8633f571c56fc&amp;postid=469864&postcount=1" target="new">1</a></b></td>
	</tr>
