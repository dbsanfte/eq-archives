	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b>ShowEQ declares War</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=e8b90a289c7b5f8712c002b479e55356&amp;postid=83622&postcount=1" target="new">1</a></b></td>
