	<!-- 2 -->
	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b>Another New Shammy Post</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=6280e51de941ecae658f9a659798643f&amp;postid=123473&postcount=1" target="new">1</a></b></td>
	</tr>
