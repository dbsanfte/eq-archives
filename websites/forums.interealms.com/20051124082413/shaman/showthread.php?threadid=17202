	<!-- 2 -->
	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b></b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=1baf9eea0470300951099c1227a42fce&amp;postid=189023&postcount=13" target="new">13</a></b></td>
