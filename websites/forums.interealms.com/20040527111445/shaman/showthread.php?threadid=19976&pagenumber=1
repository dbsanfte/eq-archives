	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b></b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=173d270a4eedf05247a354557911a09b&amp;postid=228750&postcount=12" target="new">12</a></b></td>
