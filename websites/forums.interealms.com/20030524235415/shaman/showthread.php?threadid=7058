	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b>Recap: Chapters 1-4</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=0817038a8ce656ecd644fe2c5ba1f816&amp;postid=82282&postcount=18" target="new">18</a></b></td>
