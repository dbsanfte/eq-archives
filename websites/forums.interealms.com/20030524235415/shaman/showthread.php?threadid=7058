	<tr>
		
		<td class="smallfont"><b>Chapter 4</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=0817038a8ce656ecd644fe2c5ba1f816&amp;postid=82280&postcount=17" target="new">17</a></b></td>
