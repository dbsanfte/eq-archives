	<!-- 2 -->
	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b>Chapter 3</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=0817038a8ce656ecd644fe2c5ba1f816&amp;postid=80167&postcount=9" target="new">9</a></b></td>
	</tr>
	</table>
	<!-- /2 -->
	
	</td>
