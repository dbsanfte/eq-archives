	<tr>
		
		<td class="smallfont"><b>Good Plvling spots.</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=6280e51de941ecae658f9a659798643f&amp;postid=168512&postcount=14" target="new">14</a></b></td>
	</tr>
	</table>
	<!-- /2 -->
	
	</td>
