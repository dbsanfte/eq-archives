	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b>GoD</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=3017cfb1c34f62a09a768d93ae0b3455&amp;postid=228027&postcount=14" target="new">14</a></b></td>
