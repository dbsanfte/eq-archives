	<td class="thead">
	
	<!-- 2 -->
	<table cellpadding="0" cellspacing="0" border="0" width="100%">
	<tr>
		
		<td class="smallfont"><b></b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=125fabf87c7e199455122352a3987f9b&amp;postid=327629&postcount=40" target="new">40</a></b></td>
