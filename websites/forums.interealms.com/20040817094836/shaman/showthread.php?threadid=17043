	<tr>
		
		<td class="smallfont"><b>Different focus</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=92497192143a6f02d84d680f36d0393f&amp;postid=187272&postcount=7" target="new">7</a></b></td>
