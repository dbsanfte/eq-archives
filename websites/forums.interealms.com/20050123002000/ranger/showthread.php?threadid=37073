function reputation(postid)
{
	window.open("reputation.php?" + SESSIONURL + "postid=" + postid, "Reputation", "toolbar=no,scrollbars=yes,resizable=yes,width=400,height=300");
}
function changemode(mode, which)
{
	if (mode == "hybrid")
	{
		window.location = "showthread.php?" + SESSIONURL + "mode=hybrid&threadid=37073";
		return false;
	}
	else if (which == "first")
	{
		window.location = "showthread.php?" + SESSIONURL + "postid=" + FIRSTPOSTID + "&mode=" + mode + "#post" + FIRSTPOSTID;
		return false;
	}
	else if (which == "last")
	{
		window.location = "showthread.php?" + SESSIONURL + "postid=" + LASTPOSTID + "&mode=" + mode + "#post" + LASTPOSTID;
		return false;
	}
}
var FIRSTPOSTID = "548552";
var LASTPOSTID = "551229";
