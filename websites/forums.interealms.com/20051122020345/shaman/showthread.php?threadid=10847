	<tr>
		
		<td class="smallfont"><b>Why Necros Love Shaman</b></td>
		<td class="smallfont" align="right" wrap="nowrap">Post #<b><a href="showpost.php?s=dcc0c6dbab3f3cd4941f8f081eb6adb8&amp;postid=120706&postcount=1" target="new">1</a></b></td>
