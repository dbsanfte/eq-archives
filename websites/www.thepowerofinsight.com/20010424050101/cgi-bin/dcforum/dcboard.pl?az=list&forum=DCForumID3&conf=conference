<script language="javascript">
function MakeArray() {
   this.length = MakeArray.arguments.length
   for (var i = 0; i < this.length; i++)
   this[i+1] = MakeArray.arguments[i]
}

var url = new MakeArray("","http://www.thepowerofinsight.com/cgi-bin/dcforum/dcboard.pl",
"http://www.thepowerofinsight.com/cgi-bin/dcforum/dcboard.pl?conf=conference",
