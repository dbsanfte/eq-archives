			&lt;element name="DefaultWindowDrawTemplate" type="WindowDrawTemplate:item" /&gt;<br />
			&lt;element name="CursorDefault" type="Ui2DAnimation:item" /&gt;<br />
			&lt;element name="CursorResizeNS" type="Ui2DAnimation:item" /&gt;<br />
			&lt;element name="CursorResizeEW" type="Ui2DAnimation:item" /&gt;<br />
			&lt;element name="CursorResizeNESW" type="Ui2DAnimation:item" /&gt;<br />
			&lt;element name="CursorResizeNWSE" type="Ui2DAnimation:item" /&gt;<br />
			&lt;element name="CursorDrag" type="Ui2DAnimation:item" /&gt;<br />
		&lt;/ElementType&gt;<br />
	&lt;/Schema&gt;<br />
&lt;/XML&gt;</font></p></td>
</tr>
