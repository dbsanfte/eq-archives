    &lt;Cycle&gt;true&lt;/Cycle&gt;
    &lt;Frames&gt;
      &lt;Texture&gt;window_pieces06.tga&lt;/Texture&gt;
      &lt;Location&gt;
        &lt;X&gt;100&lt;/X&gt;
        &lt;Y&gt;10&lt;/Y&gt;
      &lt;/Location&gt;
      &lt;Size&gt;
        &lt;CX&gt;40&lt;/CX&gt;
        &lt;CY&gt;40&lt;/CY&gt;
      &lt;/Size&gt;
      &lt;Hotspot&gt;
        &lt;X&gt;0&lt;/X&gt;
        &lt;Y&gt;0&lt;/Y&gt;
      &lt;/Hotspot&gt;
      &lt;Duration&gt;1000&lt;/Duration&gt;
    &lt;/Frames&gt;
  &lt;/Ui2DAnimation&gt;
  &lt;Ui2DAnimation item="A_CombatButton2Flyby"&gt;
    &lt;Cycle&gt;true&lt;/Cycle&gt;
    &lt;Frames&gt;
      &lt;Texture&gt;window_pieces06.tga&lt;/Texture&gt;
      &lt;Location&gt;
        &lt;X&gt;55&lt;/X&gt;
