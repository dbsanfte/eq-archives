<SCRIPT LANGUAGE="JavaScript1.1"><!--
var myMinTimeOut='1440'; // specified in minutes
var maxFreq=1; var freq;
var dz=document;var dt=new Date();
var ck=dz.cookie;
var pos = ck.indexOf("popseen=");
if (pos != -1) {
  var start = pos+8;
  var end = ck.indexOf(";",start);
  if (end == -1) end = ck.length;
  freq = ck.substring(start,end);
}
else {
  freq = 0;
}
if (freq < maxFreq) {
  freq++;
  dt.setTime(dt.getTime()+(myMinTimeOut*60*1000));
  dz.cookie='popseen='+freq+'; domain=.ezboard.com; path=/; expires='+dt.toGMTString(); document.writeln("<script src=\"http://engage.everyone.net/jserver/aamsz=468x60/area=popup.ezboard\"></script>");
}
