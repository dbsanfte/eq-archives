			return('<IMG SRC="http://www.gamerslounge.com/ubb/redlockfolder.gif" BORDER="0">');
		} else if (fiery == "yes") {
			return('<IMG SRC="http://www.gamerslounge.com/ubb/hotclosedb.gif" BORDER="0">');
