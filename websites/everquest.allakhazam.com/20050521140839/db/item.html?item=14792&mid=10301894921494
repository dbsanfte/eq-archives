            function AdsRenderSkin(skin){
                var target = document.getElementById('skin-wrap');
                if (target) {
                    target.innerHTML = '';
                } else {
                    target = document.body;
                }

                // Add class to the body to show the page is skinned
                document.body.className += ' ' + 'skinned' +
                    (skin.usePageLeaderboard ? ' ' + 'skinned' + '-use-page-leaderboard' : '');

                // Add links
                var linkClassPrefix = 'skin-link-';
                var i = 0;
                while (i++ < 2) {
                    var a = target.insertBefore(
                        document.createElement('a'),
                        target.firstChild
                    );
                    a.className = linkClassPrefix + i;
                    a.target = '_blank';
                    a.href = skin.link;
                    a.style.backgroundImage = 'url(' + skin.image + ')';
                }
                if (skin.impressionTracking){
                    var img = target.insertBefore(
                        document.createElement('img'),
                        target.firstChild
                    );
                    img.className = 'skin-pixel';
                    img.src = skin.impressionTracking;
                }

                var removeElems = ['skinned', 'mini-features'];
                if (skin.usePageLeaderboard === undefined || skin.usePageLeaderboard == false)
                {
                    removeElems.push('header-bg')
                }
                for (var i = 0; i < removeElems.length; i++){
                    var elem = document.getElementById(removeElems[i]);
                    if (elem){
                        var parent = elem.parentNode;
                        if (parent){
                            parent.removeChild(elem);
                        }
                    }
                }
            }
