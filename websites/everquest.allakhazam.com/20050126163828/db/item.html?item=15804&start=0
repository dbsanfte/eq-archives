	 function toggle (i,n) {
	   e = document.getElementById(n);
	   if (e.style.display == 'none') {
	     e.style.display = 'block';
	     i.src = '/shared/hide_content.gif';
	   } else {
	     e.style.display = 'none';
	     i.src = '/shared/show_content.gif';
	   }
