	function imgOn(img) {
		if (document.images) {
			imgSrc = document.images["T" + img].src;
			document.images["T" + img].src = imgSrc.substr(0,imgSrc.length-4) + "_on.gif";
		}
	}

	function imgOff(img) {
		if (document.images) {
			imgSrc = document.images["T" + img].src;
			document.images["T" + img].src = imgSrc.substr(0,imgSrc.length-7) + ".gif";
		}
	}

	function go(url) {
		document.location.href = url;
	}

//-->
