function openImage(imageId)
{
	var result;
	var url;

	url = 'http://jg.crgaming.com/screenshots/openImage.asp?ImageId=' + imageId
	result = window.open(url,'cr','toolbar=0,location=0,status=0,menubar=0,scrollbars=1,resizable=0,width=880,height=700');
}
