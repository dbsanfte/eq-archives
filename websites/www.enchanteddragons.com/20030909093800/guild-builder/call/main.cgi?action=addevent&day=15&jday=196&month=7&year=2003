<!--

Stamp = new Date();

MM=Stamp.getMonth() + 1;
DD=Stamp.getDate();
YY=Stamp.getFullYear();
HR=Stamp.getHours();
MN=Stamp.getMinutes();
SS=Stamp.getSeconds();
    with (Math) {  
	GGG = 1;
	if (YY <= 1585) GGG = 0;
	JD = -1 * floor(7 * (floor((MM + 9) / 12) + YY) / 4);
	S = 1;
	if ((MM - 9)<0) S=-1;
	A = abs(MM - 9);
	J1 = floor(YY + S * floor(A / 7));
	J1 = -1 * floor((floor(J1 / 100) + 1) * 3 / 4);
	JD = JD + floor(275 * MM / 9) + DD + (GGG * J1);
	JD = JD + 1721027 + 2 * GGG + 367 * YY;
	JD = JD - 2451910;
	JD = (JD * 86400) + (HR * 3600) + (MN * 60) + SS;
    }


MM=9;
DD=9;
YY=2003;
HR=6;
MN=0;
SS=59;
    with (Math) {  
      GGG = 1;
	if (YY <= 1585) GGG = 0;
	JD2 = -1 * floor(7 * (floor((MM + 9) / 12) + YY) / 4);
	S = 1;
	if ((MM - 9)<0) S=-1;
	A = abs(MM - 9);
	J1 = floor(YY + S * floor(A / 7));
	J1 = -1 * floor((floor(J1 / 100) + 1) * 3 / 4);
	JD2 = JD2 + floor(275 * MM / 9) + DD + (GGG * J1);
	JD2 = JD2 + 1721027 + 2 * GGG + 367 * YY;
	JD2 = JD2 - 2451910;
	JD2 = (JD2 * 86400) + (HR * 3600) + (MN * 60) + SS;
    }

OFFSET = JD2 - JD;
SECOFFSET =JD2 - JD;

with (Math) {
	if (OFFSET < 3600 && OFFSET > -3600) {
	OFFSET = floor (OFFSET / 60);
	TYPE = "minutes";
	}
else	{
	OFFSET = floor(OFFSET / 3600 * 100)/100;
	TYPE = "hours";
	}
}

document.write('<input type=hidden name=offset value="' + SECOFFSET + '">');
//-->
