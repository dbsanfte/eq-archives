		function preventDoublePosts() {
			if (document.all && document.frmMain.cmdSubmit) {
				document.frmMain.cmdSubmit.disabled = true;
				document.frmMain.cmdSubmit.value = 'Processing... Please wait...';
			}

			if (submitDone == 0) {
