OPERA = (navigator.userAgent.indexOf('Opera') != -1);

function addTag(target,tagid, width, height) {
if (arguments.length <3 ) { width = '468'; }
if (arguments.length <4 ) { height = '60'; }

now = new Date();
random = now.getTime();
