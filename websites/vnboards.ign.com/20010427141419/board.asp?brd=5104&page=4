	// Banner Ad Tag Script


hasLayers = (navigator.appName.substring(0,8) == "Netscape" && parseFloat(navigator.appVersion) >= 4.02 && parseFloat(navigator.appVersion) < 5 && (navigator.userAgent.indexOf('Opera') == -1));
