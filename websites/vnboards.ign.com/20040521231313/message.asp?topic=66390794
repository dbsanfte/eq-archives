if (!(WM_readCookie('popunder'))) {
window.open('http:\/\/ads.ign.com/advertisers/warnerbros/cartoon/exit_cartoonclassics.html','popunder','width=250,height=250,left=200,top=200').blur();
window.focus();
WM_setCookie('popunder','true','/','ign.com','false');
}
