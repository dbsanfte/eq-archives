if (!(WM_readCookie('popunder9'))) {
window.open('http:\/\/ads.ign.com/advertisers/nielsen/popunder9.html','popunder9','width=200,height=200,left=20,top=20');
WM_setCookie('popunder9','true','/','ign.com','false');
}
