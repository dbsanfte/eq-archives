function addTag(target,tagid,width,height) {

if (arguments.length < 3) { width = '468'; }
if (arguments.length < 4) { height = '60'; }

now = new Date();
random = now.getTime();

if (hasLayers && obeysNoscript) {
document.write('<ilayer ID=');
document.write(tagid);
document.writeln(' visibility=hide bgcolor="#006666" width=' + width + ' height=' + height + '></ilayer>');

