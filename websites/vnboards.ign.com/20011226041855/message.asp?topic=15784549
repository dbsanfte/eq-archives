resizes[resizes.length] = "document.layer" + tagid + ".moveToAbsolute(document." + tagid + ".pageX, document." + tagid + ".pageY)";


}

function handleResize() {
for (i=0;i<resizes.length;i++) {
eval(resizes[i]);
}
return false;
}

function addTriggers() {
if (hasLayers && obeysNoscript) {
