	function menuSelect(obj_passed) {
		menuI = obj_passed.selectedIndex;
		if (obj_passed.options[menuI].value != "") {
			window.top.location = obj_passed.options[menuI].value;
		}
		obj_passed.selectedIndex = 0;
		return false;
	}
		
