function CookieList()
{
  var FrameName = "";
  if((parent.Memberlist)||(parent.frames[FrameName]))
  {
    if(FrameName == "") { var NewLoc = parent.Memberlist.location.href; }
    else { var NewLoc = parent.frames[FrameName].location.href; }
    if(document.cookie)
    {
      var i = 0;
      var Cook = document.cookie;
      var aCook = new Array();
      aCook = Cook.split("; ");
      Cook = "";

      if(aCook.length != 0)
      {
        for(i = 0; i != aCook.length; i++)
        { if(aCook[i].indexOf("Options=")==0) { Cook = aCook[i]; } }
      }

      if(Cook.indexOf("Options=") == 0)
      {
        Cook = Cook.slice(Cook.indexOf("=")+1,Cook.length);
      }
      else
      {
        Cook = "10100000000000000011111010100001";
      }

      if(Cook.charAt(0) == 1) { NewLoc = NewLoc + "&sort=name"; }
      if(Cook.charAt(0) == 2) { NewLoc = NewLoc + "&sort=level"; }
      if(Cook.charAt(1) == 1) { NewLoc = NewLoc + "&sortbyclass=yes"; }
      if(Cook.charAt(2) == 1) { NewLoc = NewLoc + "&separated=no"; }
      if(Cook.charAt(2) == 2) { NewLoc = NewLoc + "&separated=yes"; }
      if(Cook.charAt(2) == 3) { NewLoc = NewLoc + "&separated=mains"; }
      if(Cook.charAt(3) == 1) { NewLoc = NewLoc + "&class_only_some=only_some"; }
      if(Cook.charAt(4) == 1) { NewLoc = NewLoc + "&class_Bard=Bard"; }
      if(Cook.charAt(5) == 1) { NewLoc = NewLoc + "&class_Cleric=Cleric"; }
      if(Cook.charAt(6) == 1) { NewLoc = NewLoc + "&class_Druid=Druid"; }
      if(Cook.charAt(7) == 1) { NewLoc = NewLoc + "&class_Enchanter=Enchanter"; }
      if(Cook.charAt(8) == 1) { NewLoc = NewLoc + "&class_Magician=Magician"; }
      if(Cook.charAt(9) == 1) { NewLoc = NewLoc + "&class_Monk=Monk"; }
      if(Cook.charAt(10) == 1) { NewLoc = NewLoc + "&class_Necromancer=Necromancer"; }
      if(Cook.charAt(11) == 1) { NewLoc = NewLoc + "&class_Paladin=Paladin"; }
      if(Cook.charAt(12) == 1) { NewLoc = NewLoc + "&class_Ranger=Ranger"; }
      if(Cook.charAt(13) == 1) { NewLoc = NewLoc + "&class_Rogue=Rogue"; }
      if(Cook.charAt(14) == 1) { NewLoc = NewLoc + "&class_Shadowknight=Shadowknight"; }
      if(Cook.charAt(15) == 1) { NewLoc = NewLoc + "&class_Shaman=Shaman"; }
      if(Cook.charAt(16) == 1) { NewLoc = NewLoc + "&class_Warrior=Warrior"; }
      if(Cook.charAt(17) == 1) { NewLoc = NewLoc + "&class_Wizard=Wizard"; }
      if(Cook.charAt(18) == 1) { NewLoc = NewLoc + "&filter_filter_on=filter_on"; }
      if(Cook.charAt(19) == 1) { NewLoc = NewLoc + "&filter_Name=Name"; }
      if(Cook.charAt(20) == 1) { NewLoc = NewLoc + "&filter_Surname=Surname"; }
      if(Cook.charAt(21) == 1) { NewLoc = NewLoc + "&filter_Class=Class"; }
      if(Cook.charAt(22) == 1) { NewLoc = NewLoc + "&filter_Race=Race"; }
      if(Cook.charAt(23) == 1) { NewLoc = NewLoc + "&filter_Deity=Deity"; }
      if(Cook.charAt(24) == 1) { NewLoc = NewLoc + "&filter_Level=Level"; }
      if(Cook.charAt(25) == 1) { NewLoc = NewLoc + "&filter_Tradeskills=Tradeskills"; }
      if(Cook.charAt(26) == 1) { NewLoc = NewLoc + "&filter_Rank=Rank"; }
      if(Cook.charAt(27) == 1) { NewLoc = NewLoc + "&filter_E_Mail=E_Mail"; }
      if(Cook.charAt(28) == 1) { NewLoc = NewLoc + "&filter_ICQ=ICQ"; }
      if(Cook.charAt(29) == 1) { NewLoc = NewLoc + "&filter_Notes=Notes"; }
      if(Cook.charAt(30) == 1) { NewLoc = NewLoc + "&filter_Last_changed=Last_changed"; }
      if(Cook.charAt(31) == 1) { NewLoc = NewLoc + "&filter_MainChar=MainChar"; }
    }
    else
    {
      NewLoc = NewLoc + "&sort=name&separated=no&filter_filter_on=filter_on&filter_Name=Name&filter_Surname=Surname&filter_Class=Class&filter_Race=Race&filter_Level=Level&filter_Rank=Rank&filter_MainChar=MainChar";
    }
    if(FrameName == "") { parent.Memberlist.location.href = NewLoc; }
    else { parent.frames[FrameName].location.href = NewLoc; }
  }
}
function LoginCookie()
{
  if(document.cookie)
  {
    var Cook = document.cookie;
    if((Cook.indexOf("Name")!=-1)&&(Cook.indexOf("Password")!=-1)&&(Cook.indexOf("Options")!=-1))
    {
      var i = 0;
      var aCook = new Array();
      var LoginName = "";
      var LoginPassword = "";
      aCook = Cook.split("; ");
      Cook = "";
      if(aCook.length != 0)
      {
        for(i = 0; i != aCook.length; i++)
        {
          if(aCook[i].indexOf("Name=")==0)
          {
            LoginName = unescape(aCook[i].slice(aCook[i].indexOf("=")+1,aCook[i].length));
          }
        }
        for(i = 0; i != aCook.length; i++)
        {
          if(aCook[i].indexOf("Password=")==0)
          {
            LoginPassword = unescape(aCook[i].slice(aCook[i].indexOf("=")+1,aCook[i].length));
          }
        }
        for(i = 0; i != aCook.length; i++)
        {
          if(aCook[i].indexOf("Options=")==0)
          {
            Cook = aCook[i].slice(aCook[i].indexOf("=")+1,aCook[i].length);
          }
        }
      }
      if(LoginName == "-") { LoginName = ""; }
      if(LoginPassword == "-") { LoginPassword = ""; }
      if(Cook == "") { Cook="10100000000000000011111010100001"; }

      if(document.cookieform)
      {
        document.cookieform.usecookie.checked = true;
      }
      if(document.login)
      {
        document.login.account.value = LoginName;
        document.login.password.value = LoginPassword;
      }
      if(document.options)
      {
        if(Cook.charAt(0) == 1) { document.options.sort[0].checked = true; }
          else { document.options.sort[0].checked = false; }
        if(Cook.charAt(0) == 2) { document.options.sort[1].checked = true; }
          else { document.options.sort[1].checked = false; }
        if(Cook.charAt(1) == 1) { document.options.sortbyclass.checked = true; }
          else { document.options.sortbyclass.checked = false; }
        if(Cook.charAt(2) == 1) { document.options.separated[0].checked = true; }
          else { document.options.separated[0].checked = false; }
        if(Cook.charAt(2) == 2) { document.options.separated[1].checked = true; }
          else { document.options.separated[1].checked = false; }
        if(Cook.charAt(2) == 3) { document.options.separated[2].checked = true; }
          else { document.options.separated[2].checked = false; }
        if(Cook.charAt(3) == 1) { document.options.class_only_some.checked = true; }
          else { document.options.class_only_some.checked = false; }
        if(Cook.charAt(4) == 1) { document.options.class_Bard.checked = true; }
          else { document.options.class_Bard.checked = false; }
        if(Cook.charAt(5) == 1) { document.options.class_Cleric.checked = true; }
          else { document.options.class_Cleric.checked = false; }
        if(Cook.charAt(6) == 1) { document.options.class_Druid.checked = true; }
          else { document.options.class_Druid.checked = false; }
        if(Cook.charAt(7) == 1) { document.options.class_Enchanter.checked = true; }
          else { document.options.class_Enchanter.checked = false; }
        if(Cook.charAt(8) == 1) { document.options.class_Magician.checked = true; }
          else { document.options.class_Magician.checked = false; }
        if(Cook.charAt(9) == 1) { document.options.class_Monk.checked = true; }
          else { document.options.class_Monk.checked = false; }
        if(Cook.charAt(10) == 1) { document.options.class_Necromancer.checked = true; }
          else { document.options.class_Necromancer.checked = false; }
        if(Cook.charAt(11) == 1) { document.options.class_Paladin.checked = true; }
          else { document.options.class_Paladin.checked = false; }
        if(Cook.charAt(12) == 1) { document.options.class_Ranger.checked = true; }
          else { document.options.class_Ranger.checked = false; }
        if(Cook.charAt(13) == 1) { document.options.class_Rogue.checked = true; }
          else { document.options.class_Rogue.checked = false; }
        if(Cook.charAt(14) == 1) { document.options.class_Shadowknight.checked = true; }
          else { document.options.class_Shadowknight.checked = false; }
        if(Cook.charAt(15) == 1) { document.options.class_Shaman.checked = true; }
          else { document.options.class_Shaman.checked = false; }
        if(Cook.charAt(16) == 1) { document.options.class_Warrior.checked = true; }
          else { document.options.class_Warrior.checked = false; }
        if(Cook.charAt(17) == 1) { document.options.class_Wizard.checked = true; }
          else { document.options.class_Wizard.checked = false; }
        if(Cook.charAt(18) == 1) { document.options.filter_filter_on.checked = true; }
          else { document.options.filter_filter_on.checked = false; }
        if(Cook.charAt(19) == 1) { document.options.filter_Name.checked = true; }
          else { document.options.filter_Name.checked = false; }
        if(Cook.charAt(20) == 1) { document.options.filter_Surname.checked = true; }
          else { document.options.filter_Surname.checked = false; }
        if(Cook.charAt(21) == 1) { document.options.filter_Class.checked = true; }
          else { document.options.filter_Class.checked = false; }
        if(Cook.charAt(22) == 1) { document.options.filter_Race.checked = true; }
          else { document.options.filter_Race.checked = false; }
        if(Cook.charAt(23) == 1) { document.options.filter_Deity.checked = true; }
          else { document.options.filter_Deity.checked = false; }
        if(Cook.charAt(24) == 1) { document.options.filter_Level.checked = true; }
          else { document.options.filter_Level.checked = false; }
        if(Cook.charAt(25) == 1) { document.options.filter_Tradeskills.checked = true; }
          else { document.options.filter_Tradeskills.checked = false; }
        if(Cook.charAt(26) == 1) { document.options.filter_Rank.checked = true; }
          else { document.options.filter_Rank.checked = false; }
        if(Cook.charAt(27) == 1) { document.options.filter_E_Mail.checked = true; }
          else { document.options.filter_E_Mail.checked = false; }
        if(Cook.charAt(28) == 1) { document.options.filter_ICQ.checked = true; }
          else { document.options.filter_ICQ.checked = false; }
        if(Cook.charAt(29) == 1) { document.options.filter_Notes.checked = true; }
          else { document.options.filter_Notes.checked = false; }
        if(Cook.charAt(30) == 1) { document.options.filter_Last_changed.checked = true; }
          else { document.options.filter_Last_changed.checked = false; }
        if(Cook.charAt(31) == 1) { document.options.filter_MainChar.checked = true; }
          else { document.options.filter_MainChar.checked = false; }
      }
    }
  }
}
function SetCookie()
{
  var i = 0;
  var Cook = "";
  var aCook = new Array();
  if(document.login)
  {
    if(document.cookie) { Cook = document.cookie; }
      else { Cook = "Name=-; Password=-; Options=10100000000000000011111010100001"; }
    aCook = Cook.split("; ");

    Cook = "Name="+escape(document.login.account.value);
    Cook = Cook + "; Password="+escape(document.login.password.value);

    if(aCook.length != 0)
    {
      for(i = 0; i != (aCook.length-1); i++) { if(aCook[i].indexOf("Options=")==0) { Cook = Cook + "; " + aCook[i]; } }
    }
    if(Cook.indexOf("Options") == -1) { Cook = Cook + "; Options=10100000000000000011111010100001"; }
  }
  if(document.options)
  {
    var i = 0;
    if(document.cookie)
    {
      Cook = document.cookie;
      aCook = Cook.split("; ");
      if(aCook.length != 0)
      {
        for(i = 0; i != aCook.length; i++)
        {
          if(aCook[i].indexOf("Name=")==0) { Cook = aCook[i]; }
        }
      }
      if(Cook.indexOf("Name") == -1) { Cook = "Name=-"; }
      if(aCook.length != 0)
      {
        for(i = 0; i != aCook.length; i++)
        {
          if(aCook[i].indexOf("Password=")==0) { Cook = Cook + "; " + aCook[i]; }
        }
      }
      if(Cook.indexOf("Password") == -1) { Cook = Cook + "; Password=-"; }
      Cook = Cook + "; Options=";
    }
    else { Cook = "Name=-; Password=-; Options="; }

    if(document.options.sort[0].checked) { Cook = Cook + "1"; }
     else { if(document.options.sort[1].checked) { Cook = Cook + "2"; } }
    if(document.options.sortbyclass.checked) { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.separated[0].checked) { Cook = Cook + "1"; }
     else { if(document.options.separated[1].checked) { Cook = Cook + "2"; }
              else { if(document.options.separated[2].checked) { Cook = Cook + "3"; } } }
    if(document.options.class_only_some.checked)    { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.class_Bard.checked)         { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.class_Cleric.checked)       { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.class_Druid.checked)        { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.class_Enchanter.checked)    { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.class_Magician.checked)     { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.class_Monk.checked)         { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.class_Necromancer.checked)  { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.class_Paladin.checked)      { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.class_Ranger.checked)       { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.class_Rogue.checked)        { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.class_Shadowknight.checked) { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.class_Shaman.checked)       { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.class_Warrior.checked)      { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.class_Wizard.checked)       { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.filter_filter_on.checked)   { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.filter_Name.checked)        { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.filter_Surname.checked)     { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.filter_Class.checked)       { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.filter_Race.checked)        { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.filter_Deity.checked)       { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.filter_Level.checked)       { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.filter_Tradeskills.checked) { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.filter_Rank.checked)        { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.filter_E_Mail.checked)      { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.filter_ICQ.checked)         { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.filter_Notes.checked)       { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.filter_Last_changed.checked){ Cook = Cook + "1"; } else { Cook = Cook + "0"; }
    if(document.options.filter_MainChar.checked)    { Cook = Cook + "1"; } else { Cook = Cook + "0"; }
  }
  if(document.cookieform)
  {
    aCook = Cook.split("; ");
    date = new Date(); date.setTime(date.getTime()+1000*60*60*24*30);

    if(document.cookieform.usecookie.checked == true)
    {
      document.cookie = aCook[0] + "; expires=" + date.toGMTString() + ";";
      document.cookie = aCook[1] + "; expires=" + date.toGMTString() + ";";
      document.cookie = aCook[2] + "; expires=" + date.toGMTString() + ";";
    }
    else
    {
      document.cookie = "Name=; expires=Fri, 31 Dec 1999 23:59:59 GMT";
      document.cookie = "Password=; expires=Fri, 31 Dec 1999 23:59:59 GMT";
      document.cookie = "Options=; expires=Fri, 31 Dec 1999 23:59:59 GMT";
    }
  }
}
function DetailWindow(CharName)
{
  NewWin = open("members.pl?type=chardetail&name="+CharName,"CharDetailWindow","depentend=yes,width=500,height=560,screenX=50,screenY=50,scrollbars=yes")
}
function ResetCookie()
{
  document.cookie = "Name=; expires=Fri, 31 Dec 1999 23:59:59 GMT";
  document.cookie = "Password=; expires=Fri, 31 Dec 1999 23:59:59 GMT";
  document.cookie = "Options=; expires=Fri, 31 Dec 1999 23:59:59 GMT";
}
