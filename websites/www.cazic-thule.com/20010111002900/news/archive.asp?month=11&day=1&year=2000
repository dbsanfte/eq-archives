<!-- SELECT TOP 25 newsitems.id, newsauthors.id as authorid, newsauthors.name as name, COALESCE((select siteemail from newsauthorsites where authorid = newsauthors.id and siteid = newsitems.siteid), newsauthors.email) as email, header, headline, datetime, newscategories.name as categoryname, piccaption, picotheday, piclarge, newsitem, comments from newsitems(NOLOCK) join newsauthors(NOLOCK) on newsauthors.id = authorid LEFT JOIN (select newsitemid, min(categoryid) as categoryid from newsitemcategories(NOLOCK) group by newsitemid) as itemcatz ON newsitems.id = itemcatz.newsitemid LEFT JOIN newscategories(NOLOCK) ON itemcatz.categoryid = newscategories.id  where ( datetime < '11/2/2000'  and datetime >= '10/26/2000' AND (/* cazic-thule */ (picotheday = '' or picotheday is null) and categoryid in (182, 185)) ) and newsitems.siteid = 128 order by convert(smalldatetime, convert(varchar(12), datetime)) desc, topstory desc, datetime desc -->

