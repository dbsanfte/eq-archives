function redirectOffsiteLinks()
{
        for(var i=0 ; i<document.links.length ; i++)
                if( document.links[ i ].hostname != self.location.hostname
                && document.links[ i ].hostname != ";")
                        document.links[ i ].target="_blank";
}
</script>
