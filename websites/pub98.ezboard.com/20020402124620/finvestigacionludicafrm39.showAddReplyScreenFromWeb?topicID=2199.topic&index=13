var currentAd = rand(3)-1
var imgCt = 3
function cycle() {
  currentAd++
  if (currentAd == imgCt) {
    currentAd = 0
  }
  document.adBanner.src=imgs[currentAd]
  adLink.href=lnks[currentAd]
  setTimeout("cycle()",12 * 1000)
}
