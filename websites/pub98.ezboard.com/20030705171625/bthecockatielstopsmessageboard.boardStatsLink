<script type="text/javascript">
for(x=0;x<document.links.length;x++){
        if ( document.links[x].href.indexOf(".confirmDeleteSingle") != -1 ) {
                document.links[x].href = document.links[x].href.replace(/confirmD/,"d");
                document.links[x].onclick = new Function ("return confirm('TF Mod Alert: Are you sure you want to delete this reply?');");
                }
        }

if ( !ezPage && !ezQuery && document.getElementsByName("bulkTopicAction").length > 0 ) {
        allForms = document.getElementsByTagName("form");
        for (af = 0; af < allForms.length; af++) {
                if (allForms.item(af).action.indexOf("ulkTopic") != -1) {
                        allForms.item(af).id = "BulkTopicForm";        
                        }        
                }
        allForms = null;
        document.getElementById("BulkTopicForm").onsubmit = new Function("selBulk = document.getElementsByName(\"bulkTopicAction\").item(0); switchBulkAction(selBulk.options[selBulk.selectedIndex].value)");
        }

function switchBulkAction(selValue) {
        switch (selValue) {
                case "bulkDelete":
                case "bulkMove":
                case "bulkCopy":
                        document.getElementById("BulkTopicForm").action = document.location.href + ".bulkTopicsAction";
                        break;
                default:
                        document.getElementById("BulkTopicForm").action = document.location.href + ".reallyPerformBulkTopicAction";
                        sRand = findRand();
                
                        if (sRand.length > 0) {        
                                inHrand = document.createElement("input");
                                inHrand.type = "hidden";
                                inHrand.name = "rand";
                                inHrand.id = "randID";
                                inHrand.value = sRand;
                                inHtopic = document.createElement("input");
                                inHtopic.type = "hidden";
                                inHtopic.name = "topicIDs";
                                inHtopic.id = "topicIDs";
                                document.getElementById("BulkTopicForm").appendChild(inHtopic);
                                document.getElementById("BulkTopicForm").appendChild(inHrand);

                                if (document.getElementById("BulkTopicForm").action == document.location.href + ".reallyPerformBulkTopicAction") {
                                        allInput = document.getElementById("BulkTopicForm").getElementsByTagName("input");
                                        for (ai = 0; ai < allInput.length; ai++) {
                                                if (allInput.item(ai).type == "checkbox") {
                                                        if (allInput.item(ai).checked == true) {
                                                                if (allInput.item(ai).name.indexOf("\.topic") != -1) {
                                                                        document.getElementById("topicIDs").value += allInput.item(ai).name + ",";
                                                                        }
                                                                }
                                                        }
                                                }
                                        }
                                }
                        break;
                }
        }

function findRand() {
        for ( fr = 0; fr < document.links.length; fr++ ) {
                if ( document.links[fr].href.indexOf("rand=") != -1 ) {
                        return document.links[fr].href.substring(document.links[fr].href.indexOf("rand=") + 5);
                        break;
                        }
                }
        }
</script>

