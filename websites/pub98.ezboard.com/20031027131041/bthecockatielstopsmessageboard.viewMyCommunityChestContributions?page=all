<script type="text/javascript">
/* BYPASS CONFIRMATION SCREEN FOR STICKY/CLOSE/REOPEN */
if ( !ezPage && !ezQuery && document.getElementsByName("bulkTopicAction").length > 0 ) {
        allForms = document.getElementsByTagName("form");
        for (af = 0; af < allForms.length; af++) {
                if (allForms.item(af).action.indexOf("ulkTopic") != -1) {
                        allForms.item(af).id = "BulkTopicForm";        
                        }        
                }
        allForms = null;
        document.getElementById("BulkTopicForm").onsubmit = new Function("selBulk = document.getElementsByName(\"bulkTopicAction\").item(0); switchBulkAction(selBulk.options[selBulk.selectedIndex].value)");
        }

/* BYPASS CONFIRMATION SCREEN FOR STICKY/CLOSE/REOPEN  -- functions */
function switchBulkAction(selValue) {
        switch (selValue) {
                case "bulkDelete":
                case "bulkMove":
                case "bulkCopy":
                        document.getElementById("BulkTopicForm").action = document.location.href + ".bulkTopicsAction";
                        break;
                default:
                        document.getElementById("BulkTopicForm").action = document.location.href + ".reallyPerformBulkTopicAction";sRand = findRand();if (sRand.length > 0) { inHrand = document.createElement("input");inHrand.type = "hidden";inHrand.name = "rand";inHrand.id = "randID";inHrand.value = sRand;inHtopic = document.createElement("input");inHtopic.type = "hidden";inHtopic.name = "topicIDs";inHtopic.id = "topicIDs";document.getElementById("BulkTopicForm").appendChild(inHtopic);document.getElementById("BulkTopicForm").appendChild(inHrand);if (document.getElementById("BulkTopicForm").action == document.location.href + ".reallyPerformBulkTopicAction") {allInput = document.getElementById("BulkTopicForm").getElementsByTagName("input");for (ai = 0; ai < allInput.length; ai++) {if (allInput.item(ai).type == "checkbox") {if (allInput.item(ai).checked == true) {if (allInput.item(ai).name.indexOf("\.topic") != -1) {document.getElementById("topicIDs").value += allInput.item(ai).name + ",";}}}}}}break;}}function findRand() {for ( fr = 0; fr < document.links.length; fr++ ) {if ( document.links[fr].href.indexOf("rand=") != -1 ) {return document.links[fr].href.substring(document.links[fr].href.indexOf("rand=") + 5);break;}}}/* END BYPASS CONFIRMATION SCREEN FOR STICKY/CLOSE/REOPEN */</script>
<script type="text/javascript">
/* LAUNCH EZINBOX ALERT FUNCTION */
inboxalert(true);
/* END LAUNCH EZINBOX ALERT FUNCTION */
</script>
</div>
            <!-- End Main Content -->
          </div>
        </td>
        <td width="35" class="right" rowspan="2" height="330">
        </td>
      </tr>
