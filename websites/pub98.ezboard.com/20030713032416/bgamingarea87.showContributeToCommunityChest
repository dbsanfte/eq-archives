                                if ( allTDs.item(i).childNodes.item(nt).nodeType == 1 ) {
                                        if ( allTDs.item(i).childNodes.item(nt).className == "title" ) {
                                                allLinks = allTDs.item(i).childNodes.item(nt).getElementsByTagName("a");
                                                for ( al = 0; al < allLinks.length; al++ ) {
                                                        if ( allLinks.item(al).href.indexOf("PublicProfile") != -1 ) {
                                                                sUser = allLinks.item(al).href.substr(allLinks.item(al).href.indexOf("gid=") + 4);
                                                                if ( regNoSig.test(sUser) || !regAnySig.test(sUser) ) {
                                                                        if ( allTDs.length > i + 1 ) {
                                                                                if ( allTDs.item(i+1).className == "m" ) {
                                                                                        for ( x = 0; x < allTDs.item(i+1).childNodes.length - 1; x++ ) {
                                                                                                if ( allTDs.item(i+1).childNodes.item(x).nodeName == "P" ) {
                                                                                                        iSigLength = 0;
                                                                                                        for ( x = x; x < allTDs.item(i+1).childNodes.length; x++ ) {
                                                                                                                switch ( true ) {
                                                                                                                   case regNoSig.test(allLinks.item(al).href):
                                                                                                                        try { 
                                                                                                                                allTDs.item(i+1).childNodes.item(x).style.display = "none";
                                                                                                                                }
                                                                                                                        catch (x) {
                                                                                                                                allTDs.item(i+1).removeChild(allTDs.item(i+1).childNodes.item(x));
                                                                                                                                }
                                                                                                                        break;                                                                                                        
                                                                                                                   default:
                                                                                                                        if ( bLimitSigs ) { walkChildren(allTDs.item(i+1).childNodes.item(x)); }
                                                                                                                        break;
                                                                                                                        }
                                                                                                                }
                                                                                                        }
                                                                                                }
                                                                                        }
                                                                                }
                                                                        }                
                                                                }
                                                        }
                                                }
                                        }
                                }
                        }
                }
        }

function walkChildren (oNode) {
        iSigLength += sigProcess(oNode);
        if ( oNode.hasChildNodes ) {
                for ( var i = 0; i < oNode.childNodes.length; i++ ) {
                        walkChildren(oNode.childNodes.item(i));
                        }
                }
        }

function sigProcess (oNode) {
        switch ( oNode.nodeType ) {
                case 1: // element (tag)
                        switch ( oNode.nodeName ) {
                                case "IMG":
                                        if ( oNode.height > maxSigImageHeight ) {
                                                nProp = oNode.width / oNode.height;
                                                oNode.height = maxSigImageHeight;
                                                oNode.width = oNode.height * nProp;
                                                }
                                        if ( oNode.width > maxSigImageWidth ) {
                                                nProp = oNode.height / oNode.width;
                                                oNode.width = maxSigImageWidth;
                                                oNode.height = oNode.width * nProp;
                                                }
                                break;
                                }
                        return 0;
                        break;
                case 3: // text node
                        if ( iSigLength + oNode.length > maxSigTextLength ) {
                                oNode.data = iSigLength < maxSigTextLength ? oNode.data.substr(0, maxSigTextLength - iSigLength) : "";
                                return oNode.length;
                        } else {
                                return oNode.length > 0 ? oNode.length : 0;
                                }
                        break;
                case 8: // comment
                        return 0;
                        break;
                default:
                        return 0;
                }
        }
</script>


