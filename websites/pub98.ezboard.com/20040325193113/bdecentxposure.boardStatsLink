{
	with(document.images[k])
	{
		if((src==url + "image/cscicon3.gif") || (src==url + "image/cscicon.gif"))
		{
			src="http://www.decentxposure.com/images/board/csc.gif"
		}

		if(src==url + "image/email.gif")
		{
		src="http://www.decentxposure.com/images/board/email.gif"
		}

		if(src==url + "themes/ultimate/subscribe.gif")
		{
			src="http://www.decentxposure.com/images/board/email.gif"
		}
		if(src==url + "themes/ultimate/unsubscribe.gif")
		{
			src="http://www.decentxposure.com/images/board/email.gif"
		}


	}
}
