<script language="JavaScript">
	function yellAtUser(fieldName) {
		alert("Damnit!!!  We didn't put this form here so you could leave fields blank!  Go fill out the [" + fieldName + "] field and try again fool.");
	}

	function trim(str_inString) {
		var char_ptr = 0;
		var rstring = str_inString;
		var tmpstring = str_inString;
		if (str_inString != "") {
			while(rstring.charAt(char_ptr) == ' ') {
				char_ptr ++;
			}
			tmpstring = rstring.substring(char_ptr, rstring.length);
			rstring = tmpstring;
			char_ptr = rstring.length - 1;
			while(rstring.charAt(char_ptr) == ' ') {
				char_ptr --;
			}
			tmpstring = rstring.substring(0, char_ptr + 1)
			rstring = tmpstring;
			return rstring;
		}
	}

	function checkForm() {
		for (x = 0; x < document.RecruitApp.elements.length; x++) {
			theValue = trim(document.RecruitApp.elements[x].value);
			if ((document.RecruitApp.elements[x].type == "text" || document.RecruitApp.elements[x].type == "textarea") && (theValue == "" || theValue == undefined)) {
				yellAtUser(document.RecruitApp.elements[x].id);
				return false;
			}
		}
		
		return true;
	}
</script>

