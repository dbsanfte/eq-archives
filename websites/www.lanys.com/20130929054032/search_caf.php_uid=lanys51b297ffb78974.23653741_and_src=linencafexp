<script>
 
  function bold_search_terms(query, text) {
   	var term_words = query.split(" ");

   	$.each(term_words, function(index, value) {
      // Need to be a bit clever to make sure we match kits for term kit,
      // and kit for search term kits
      var search_term=value;
      if (value.charAt(value.length-1) == "s") {
        search_term=search_term+"?"; 
      } else {
        search_term=search_term+"s?";
      }
      text = text.replace(new RegExp(search_term, "gi"), "<b>$&</b>");
    }); 

    return text;
  }
  

function insert_websearch(query, num, location) {
  var html;

  $.ajaxSetup({ cache: false });
  $.getJSON(
    '/websearch.php',
    {term: query, max: num},
    function(data) {
      $.ajaxSetup({ cache: true });

      if(data && data.length > 0) {
        html = '<h3>Search Results</h3>';
        html += '<ul>';

        $.each(data, function(idx, r) {
			r.title = bold_search_terms(query, r.title);
			r.description = bold_search_terms(query, r.description);
			html += '<li>';
			html += '<a target="_blank" href="'+r.url+'" class="title gct">'+r.title+'</a><br/>';
 			html += '<span class="description">'+r.description+'</span><br/>';
 			html += '<a target="_blank" href="'+r.url+'" class="display_url gct">'+r.display_url+'</a><br/>';
 			html += '</li>';
        });

        html += '</ul>';

        // add click tracking
        $('#websearch').delegate('a.gct', 'mousedown', function() {
            var url =
                    '/gct.php?uid='+encodeURIComponent(searchClickTrackingOptions.uid) +
                    '&type=' + encodeURIComponent('ws') +
                    '&term=' + encodeURIComponent(searchClickTrackingOptions.term) +
                    '&pt=' + encodeURIComponent(searchClickTrackingOptions.pt) +
                    '&cid=' + encodeURIComponent(searchClickTrackingOptions.cid) +
                    '&ch=' + encodeURIComponent(searchClickTrackingOptions.ch) +
                    '&t=' + (new Date());

            var i = new Image();
            i.src = url;
        });

        $(location).html(html);
      }
    }
  );
}

  function insert_secondary_results(location, terms_to_use, channels, uid) {
    var html;

    $.ajaxSetup({ cache: false });
    $.getJSON(
      '/secondary_feed/get_2ndtier_ads.php',
      {term: terms_to_use, uid: uid}, function (data) {

      $.ajaxSetup({ cache: true });

      if(data && data.length > 0) {
        html = '<div id="secondaryfeed"><ul>';

        $.each(data, function(idx, r) {
            var url = r.url[0];
            var title = r.title[0];
            var description = r.description[0];
            var display_url = r.display_url[0];

            title = bold_search_terms(terms_to_use, title);
            description = bold_search_terms(terms_to_use, description);

            html += '<li>';
            html += '<a target="_blank" href="'+url+'" class="title gct">'+title+'</a><br/>';
            html += '<span class="description">'+description+'</span><br/>';
            html += '<a target="_blank" href="'+url+'" class="display_url gct">'+display_url+'</a><br/>';
            html += '</li>';
        });

        html += '</ul></div>';

        // Add Click Tracking
        $(location).delegate('a.gct', 'mousedown', function() {
            var url =
                    '/2tct.php?uid='+encodeURIComponent(uid) +
                    '&term=' + encodeURIComponent(terms_to_use) +
                    '&ch=' + channels +
                    '&t=' + (new Date());

            var i = new Image();
            i.src = url;
        });

        $(location).html(html);
      } else {
        logStatus('noads-xmlfeed');
        displayCantLoadMessage(); 
      }
    }
  );
}


</script>

