<script type="text/javascript">

var goog_channels = '';

function getUrlVars() {
  var vars = {};
  var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {
         vars[key] = value;
       });
  return vars;
}

</script>



