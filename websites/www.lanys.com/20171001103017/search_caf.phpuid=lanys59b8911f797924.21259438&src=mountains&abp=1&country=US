        <script type="text/javascript">
var pageOptions = {
	'pubId': 'dp-nameadmin01_js',
	'channel': '',
	'hl': 'en',
	'uiOptimize': false,
	'registrarForwardedInfo': '',
	'uid': 'lanys59b8911f797924.21259438',
	'resultsPageBaseUrl': 'http://lanys.com/search_caf.php?src=mountains&uid=lanys59b8911f797924.21259438&abp=1&country=US',
	'kw': '',
	'terms': '',
	'domainRegistrant': 'as-drid-oo-1705318292312488',
	'fontSizeDescription': 16,
	'fontSizeDomainLink': 16,
	'fontFamily': 'Arial',
	'maxTermLength': 40,
	'colorAttribution': '#666666',
	'fontFamilyAttribution': 'Arial',
	'fontSizeAttribution': 16,
	'optimizeTerms': true,
	'pageLoadedCallback': function(requestAccepted, status) {
		if (requestAccepted) { $('#error').find('input[name="loaded"]').val(1); } else {     $('#error').find('input[name="loaded"]').val(1); $('.related-bar').hide(); $('.search-bar').hide(); reportError(status, 'registrar:'); }
	}
}
var searchboxblock = {
	'container': 'searchbox_bot',
	'type': 'searchbox',
	'widthSearchInput': 400,
	'colorBackground': 'transparent'
}
var rsblock = {
	'container': 'rs',
	'type': 'relatedsearch',
	'number': 10,
	'fontSizeTitle': 20,
	'fontFamily': 'Arial',
	'titleBold': true,
	'colorTitleLink': '#2200C1',
	'adIconUrl': 'http://afs.googleusercontent.com/dp-nameadmin/chevron-gray.png',
	'adIconWidth': 24,
	'adIconHeight': 24,
	'colorBackground': 'transparent',
	'verticalSpacing': 18,
	'rolloverLinkColor': '#FE9D0B',
	'columns': 2,
	'columnSpacing': 25,
	'adLoadedCallback': function(containerName, adsLoaded) {
		if (!adsLoaded) { 
                            var pageType = 'AFS_L';
                            if (containerName === 'rs') {
                                if (pageType === 'AFS_L') {
                                    refreshGoogleAds();    
                                }
                            } else {
                                logStatus('noads'); 
                                displayCantLoadMessage();
                            }
                        }
	}
}
function refreshGoogleAds() { 
                   // First step, remove the elements of the front page of a 2 click lander (related links section) that is empty
                   $('#rs').remove();
                   $('#rs_img_left').remove();
                   $('#rs_img_right').remove();

                   // Next, set up an ad block for our fallback ad block
                   var adblock_backfill = {
                       'container': 'ads_backfill',
                       'type': 'ads',
                       'linkTarget': '_blank',
                       'verticalSpacing': 12,
                       'colorBackground': 'transparent',
                       'adIconUrl': 'http://afs.googleusercontent.com/dp-nameadmin/bluearrow.png',
                       'adIconWidth': 22,
                       'adIconHeight': 22,
                       'fontSizeTitle': 22,
                       'colorTitleLink': '#00f',
                       'fontSizeDescription': 16,
                       'colorText': '#000',
                       'lines': 3,
                       'clicktrackUrl': 'https://internettraffic.click/gct2.php?domain=lanys.com&uid=lanys59b8911f797924.21259438&type=ad&term=&pt=2click&cid=dp-nameadmin01_js&ch=',
                       'colorDomainLink': '#11772D',
                       'fontSizeDomainLink': 16,
                       'rolloverLinkColor': '#FF0000',
                       'number': 10,
                       'adLoadedCallback': function(containerName, adsLoaded) {
                                               if (!adsLoaded) {
                                                   logStatus('noads'); 
                                                   displayCantLoadMessage();
                                               }
                                           }
                   };
 
                   // Finally, call the CAF API to reinject ads
                   google.ads.domains.Caf(pageOptions, searchboxblock, adblock_backfill);
               } 


               google.ads.domains.Caf(pageOptions, searchboxblock, rsblock);
              </script>

