<!--
function clock() {
  var date = new Date();
  var now;
  var cst_summer = -5;
  var pst_summer = -7;
  var cst = -6;
  var pst = -8;
  var gmt = 0;
  var jst = 9;
  var month = date.getMonth() + 1;
  
  if (month >= 3 && month <= 10) { cst++; }
  
  now = getdate(date, jst);
  now = now + " " + gettime(date, jst);
  document.forms[0].elements[0].value = now;

  now = getdate(date, cst_summer);
  now = now + " " + gettime(date, cst);
  document.forms[0].elements[1].value = now;

  now = getdate(date, pst_summer);
  now = now + " " + gettime(date, pst);
  document.forms[0].elements[2].value = now;

  now = getdate(date, gmt);
  now = now + " " + gettime(date, gmt);
  document.forms[0].elements[3].value = now;
  
  setTimeout('clock()',500);
}

function getdate(date, tz) {
  var year, month, day;
  var time = new Date();
  
  time.setTime(date.getTime() + (date.getTimezoneOffset() + tz * 60) * 60 * 1000);
  year = time.getYear();
  month = time.getMonth() + 1;
  day = time.getDate();
  if (year < 10) { year = "0" + year; }
  if (month < 10) { month = "0" + month; }
  if (day < 10) { day = "0" + day; }
  
  return month + "/" + day ;
}

function gettime(date, tz) {
  var hour, minute, second;
  var time = new Date();
  
  time.setTime(date.getTime() + (date.getTimezoneOffset() + tz * 60) * 60 * 1000);
  hour   = time.getHours();
  minute = time.getMinutes();
  second = time.getSeconds();
  if (hour < 10) { hour = "0" + hour; }
  if (minute < 10) { minute = "0" + minute; }
  if (second < 10) { second = "0" + second; }
  
  return hour + ":" + minute + ":" + second ;
}

clock();
//-->
